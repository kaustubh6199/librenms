            SYNTAX      Integer32 (1..24)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The LEDs intensity timers index."
            ::= { ctcLedPowerEntry 1 }

        -- 1.3.6.1.4.1.4756.103.30.1.3.1.2
        ctcLedPowerTime OBJECT-TYPE
            SYNTAX      Integer32 (0..23)
            UNITS       "O'clock"
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "The time at which the LEDs intensity shall be set."
            ::= { ctcLedPowerEntry 2 }
        
        -- 1.3.6.1.4.1.4756.103.30.1.3.1.3
        ctcLedIntensity OBJECT-TYPE
            SYNTAX      INTEGER {
                            intensity100(1),
                            intensity90(2),
                            intensity80(3),
                            intensity70(4),
                            intensity60(5),
                            intensity50(6),
                            intensity40(7),
                            intensity30(8),
                            intensity20(9),
                            intensity10(10),
                            intensity0(11)
                        }                             
            MAX-ACCESS  read-write
            STATUS  current
            DESCRIPTION
                "The LEDs intensity (100% = Full power, 0% = LED off)."
            ::= { ctcLedPowerEntry 3 }
                        
    -- 1.3.6.1.4.1.4756.103.30.1.4
    ctcLedOnTime OBJECT-TYPE
        SYNTAX      Integer32 (10..9999)
        UNITS       "Second"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The number of seconds that the LEDs will have full intensity."
        ::= { ctcLedPower 4 }
                       
