            ::= { ctcIgmpPortGroupFilteringEntry 1 }

        -- 1.3.6.1.4.1.4756.103.90.1.32.1.2    
        ctcIgmpPortGroupFilteringPort OBJECT-TYPE
            SYNTAX      Integer32 (1..6)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The logical port for the settings."
            ::= { ctcIgmpPortGroupFilteringEntry 2 }

        -- 1.3.6.1.4.1.4756.103.90.1.32.1.3    
        ctcIgmpPortGroupFilteringGroups OBJECT-TYPE
            SYNTAX      IpAddress
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The IP Multicast Group that will be filtered."
            ::= { ctcIgmpPortGroupFilteringEntry 3 }

