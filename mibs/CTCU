        -- 1.3.6.1.4.1.4756.103.60.2.3.9.1.67
        ctcAceAction OBJECT-TYPE
            SYNTAX      INTEGER {
                            deny(1),
                            permit(2)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Specify the action to take with a frame that hits this ACE.
                 Permit: The frame that hits this ACE is granted permission for the ACE operation.
                 Deny: The frame that hits this ACE is dropped.
                 The default value is 'Permit'."
            ::= { ctcAceConfigEntry 67 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.9.1.68
        ctcAceRateLimiterID OBJECT-TYPE
            SYNTAX      Integer32 (0 | 1..16)
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Specify the rate limiter in number of base units.
                 The allowed range is 0{Disabled) to 16.
                 The value 0(Disabled) indicates that the rate limiter operation is disabled.
                 The default value is 0(Disabled)."
            ::= { ctcAceConfigEntry 68 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.9.1.69
        ctcAcePortRedirect OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Frames that hit the ACE are redirected to the port number specified here.
                 The allowed range is the same as the switch port number range.
                 Disabled indicates that the port redirect operation is disabled and
                 the specific port number of 'Port Redirect' can't be set when action is permitted."
            ::= { ctcAceConfigEntry 69 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.9.1.70
        ctcAceMirror OBJECT-TYPE
            SYNTAX      INTEGER {
                            enable(1),
                            disable(2)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Specify the mirror operation of this port.
                 Frames matching the ACE are mirrored to the destination mirror port.
                 The allowed values are:
                 Enabled: Frames received on the port are mirrored.
                 Disabled: Frames received on the port are not mirrored.
                 The default value is Disabled."
            ::= { ctcAceConfigEntry 70 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.9.1.71
        ctcAceLogging OBJECT-TYPE
            SYNTAX      INTEGER {
                            enable(1),
                            disable(2)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Specify the logging operation of the ACE. The allowed values are:
                 Enabled: Frames matching the ACE are stored in the System Log.
                 Disabled: Frames matching the ACE are not logged. 
                 The default value is Disabled.
                 Please note that the System Log memory size and logging rate is limited."
            ::= { ctcAceConfigEntry 71 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.9.1.72
        ctcAceShutdown OBJECT-TYPE
            SYNTAX      INTEGER {
                            enable(1),
                            disable(2)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Specify the port shut down operation of the ACE. The allowed values are:
                 Enabled: If a frame matches the ACE, the ingress port will be disabled.
                 Disabled: Port shut down is disabled for the ACE. 
                 The default value is Disabled."
            ::= { ctcAceConfigEntry 72 }

    -- 1.3.6.1.4.1.4756.103.60.2.3.10
    ctcAclStatusTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF CtcAclStatusEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table shows the ACL status."
        ::= { ctcACL 10 }

    -- 1.3.6.1.4.1.4756.103.60.2.3.10.1
    ctcAclStatusEntry OBJECT-TYPE
        SYNTAX      CtcAclStatusEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Entry of ACL status."
        INDEX { ctcAclStatusIndex }
        ::= { ctcAclStatusTable 1 }

    CtcAclStatusEntry ::=
        SEQUENCE {
            ctcAclStatusIndex        Integer32,
            ctcAclUser               INTEGER,
            ctcAclIngressPort        DisplayString,
            ctcAclFrameType          DisplayString,
            ctcAclAction             INTEGER,
            ctcAclRateLimiter        Integer32,
            ctcAclPortRedirect       DisplayString,
            ctcAclMirror             INTEGER,
            ctcAclToCPU              INTEGER,
            ctcAclToCpuOnce          INTEGER,
            ctcAclCounter            Counter32,
            ctcAclConflict           INTEGER
        }

        -- 1.3.6.1.4.1.4756.103.60.2.3.10.1.1    
        ctcAclStatusIndex OBJECT-TYPE
            SYNTAX      Integer32 (1..256)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the Index of ACL Status entries."
            ::= { ctcAclStatusEntry 1 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.10.1.2    
        ctcAclUser OBJECT-TYPE
            SYNTAX      INTEGER {
                            static(1),
                            ipManagement(2),
                            ipmc(3),
                            arpInspection(4),
                            upnp(5),
                            dhcp(6)
                        }    
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the ACL user."
            ::= { ctcAclStatusEntry 2 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.10.1.3    
        ctcAclIngressPort OBJECT-TYPE
            SYNTAX      DisplayString  
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the ingress port of the ACE. Possible values are:
                 Any: The ACE will match any ingress port.
                 Policy: The ACE will match ingress ports with a specific policy.
                 Port: The ACE will match a specific ingress port."
            ::= { ctcAclStatusEntry 3 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.10.1.4    
        ctcAclFrameType OBJECT-TYPE
            SYNTAX      DisplayString    
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the frame type of the ACE. Possible values are:
                 Any: The ACE will match any frame type.
                 EType: The ACE will match Ethernet Type frames.
                        Note that an Ethernet Type based ACE will not get matched by IP and ARP frames.
                 ARP: The ACE will match ARP/RARP frames.
                 IPv4: The ACE will match all IPv4 frames.
                 IPv4/ICMP: The ACE will match IPv4 frames with ICMP protocol.
                 IPv4/UDP: The ACE will match IPv4 frames with UDP protocol.
                 IPv4/TCP: The ACE will match IPv4 frames with TCP protocol.
                 IPv4/Other: The ACE will match IPv4 frames, which are not ICMP/UDP/TCP."
            ::= { ctcAclStatusEntry 4 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.10.1.5
        ctcAclAction OBJECT-TYPE
            SYNTAX      INTEGER {
                            deny(1),
                            permit(2)
                        }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the forwarding action of the ACE.
                 Permit: Frames matching the ACE may be forwarded and learned.
                 Deny: Frames matching the ACE are dropped."
            ::= { ctcAclStatusEntry 5 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.10.1.6
        ctcAclRateLimiter OBJECT-TYPE
            SYNTAX      Integer32 (0..16)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the rate limiter number of the ACE.
                 The allowed range is 0(Disable) to 16.
                 When 0(Disabled) is displayed, the rate limiter operation is disabled."
            ::= { ctcAclStatusEntry 6 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.10.1.7
        ctcAclPortRedirect OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the port Redirect operation of the ACE.
                 Frames matching the ACE are Redirected to the port number.
                 The allowed values are 'Disabled' or a specific port number.
                 When 'Disabled' is displayed, the port copy operation is disabled."
            ::= { ctcAclStatusEntry 7 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.10.1.8
        ctcAclMirror OBJECT-TYPE
            SYNTAX      INTEGER {
                            enable(1),
                            disable(2)
                        }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the mirror operation of this port. The allowed values are:
                 Enabled: Frames received on the port are mirrored.
                 Disabled: Frames received on the port are not mirrored."
            ::= { ctcAclStatusEntry 8 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.10.1.9
        ctcAclToCPU OBJECT-TYPE
            SYNTAX      INTEGER {
                            yes(1),
                            no(2)
                        }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Forward packet that matched the specific ACE to CPU."
            ::= { ctcAclStatusEntry 9 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.10.1.10
        ctcAclToCpuOnce OBJECT-TYPE
            SYNTAX      INTEGER {
                            yes(1),
                            no(2)
                        }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Forward first packet that matched the specific ACE to CPU."
            ::= { ctcAclStatusEntry 10 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.10.1.11
        ctcAclCounter OBJECT-TYPE
            SYNTAX      Counter32
            UNITS       "Frames"
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The counter indicates the number of times the ACE was hit by a frame."
            ::= { ctcAclStatusEntry 11 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.10.1.12
        ctcAclConflict OBJECT-TYPE
            SYNTAX      INTEGER {
                            yes(1),
                            no(2)
                        }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the hardware status of the specific ACE.
                 The specific ACE is not applied to the hardware due to hardware limitations."
            ::= { ctcAclStatusEntry 12 }


    -- 1.3.6.1.4.1.4756.103.60.2.4
    ctcDHCP OBJECT IDENTIFIER ::= { ctcSecurityNetwork 4 }

    -- 1.3.6.1.4.1.4756.103.60.2.4.1    
    ctcDhcpSnoopingMode OBJECT-TYPE
        SYNTAX      INTEGER {
                        enabled(1),
                        disabled(2)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Indicates the DHCP snooping mode operation. Possible modes are:
             Enabled: Enable DHCP snooping mode operation.
                      When DHCP snooping mode operation is enabled,
                      the DHCP request messages will be forwarded to trusted ports
                      and only allow reply packets from trusted ports.
             Disabled: Disable DHCP snooping mode operation."
        ::= { ctcDHCP 1 }

    -- 1.3.6.1.4.1.4756.103.60.2.4.2
    ctcDhcpSnoopingPortConfigTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF CtcDhcpSnoopingPortConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "Configure DHCP Snooping on this table."
        ::= { ctcDHCP 2 }

    -- 1.3.6.1.4.1.4756.103.60.2.4.2.1
    ctcDhcpSnoopingPortConfigEntry OBJECT-TYPE
        SYNTAX      CtcDhcpSnoopingPortConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Entry of DHCP Snooping ports."
        INDEX { ctcPortIndex }
        ::= { ctcDhcpSnoopingPortConfigTable 1 }

    CtcDhcpSnoopingPortConfigEntry ::=
        SEQUENCE {
            ctcDhcpSnoopingPortMode             INTEGER
        }

        -- 1.3.6.1.4.1.4756.103.60.2.4.2.1.1
        ctcDhcpSnoopingPortMode OBJECT-TYPE
            SYNTAX      INTEGER {
                            trusted(1),
                            untrusted(2)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Indicates the DHCP snooping port mode. Possible port modes are:
                 Trusted: Configures the port as trusted source of the DHCP messages.
                 Untrusted: Configures the port as untrusted source of the DHCP messages."
            ::= { ctcDhcpSnoopingPortConfigEntry 1 }

    -- 1.3.6.1.4.1.4756.103.60.2.4.3
    ctcDhcpSnoopingPortStatisticsTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF CtcDhcpSnoopingPortStatisticsEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table provides statistics for DHCP snooping."
        ::= { ctcDHCP 3 }

    -- 1.3.6.1.4.1.4756.103.60.2.4.3.1
    ctcDhcpSnoopingPortStatisticsEntry OBJECT-TYPE
        SYNTAX      CtcDhcpSnoopingPortStatisticsEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Entry of DHCP Snooping port statistics."
        INDEX { ctcPortIndex }
        ::= { ctcDhcpSnoopingPortStatisticsTable 1 }

    CtcDhcpSnoopingPortStatisticsEntry ::=
        SEQUENCE {
            ctcDhcpSnoopingPortStatisticsClear   INTEGER,
            ctcDhcpSnoopingRxDiscover            Counter32,
            ctcDhcpSnoopingRxOffer               Counter32,
            ctcDhcpSnoopingRxRequest             Counter32,
            ctcDhcpSnoopingRxDecline             Counter32,
            ctcDhcpSnoopingRxACK                 Counter32,
            ctcDhcpSnoopingRxNAK                 Counter32,
            ctcDhcpSnoopingRxRelease             Counter32,
            ctcDhcpSnoopingRxInform              Counter32,
            ctcDhcpSnoopingRxLeaseQuery          Counter32,
            ctcDhcpSnoopingRxLeaseUnassigned     Counter32,
            ctcDhcpSnoopingRxLeaseUnknown        Counter32,
            ctcDhcpSnoopingRxLeaseActive         Counter32,
            ctcDhcpSnoopingTxDiscover            Counter32,
            ctcDhcpSnoopingTxOffer               Counter32,
            ctcDhcpSnoopingTxRequest             Counter32,
            ctcDhcpSnoopingTxDecline             Counter32,
            ctcDhcpSnoopingTxACK                 Counter32,
            ctcDhcpSnoopingTxNAK                 Counter32,
            ctcDhcpSnoopingTxRelease             Counter32,
            ctcDhcpSnoopingTxInform              Counter32,
            ctcDhcpSnoopingTxLeaseQuery          Counter32,
            ctcDhcpSnoopingTxLeaseUnassigned     Counter32,
            ctcDhcpSnoopingTxLeaseUnknown        Counter32,
            ctcDhcpSnoopingTxLeaseActive         Counter32
        }

        -- 1.3.6.1.4.1.4756.103.60.2.4.3.1.1
        ctcDhcpSnoopingPortStatisticsClear OBJECT-TYPE
            SYNTAX      INTEGER {
                            trigger(1),
                            normal(2)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Clears the counters for this port."
            ::= { ctcDhcpSnoopingPortStatisticsEntry 1 }

        -- 1.3.6.1.4.1.4756.103.60.2.4.3.1.2
