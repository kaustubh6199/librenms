        SYNTAX      DisplayString
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION                                  
            "To delete a VLAN Translation Group database entry.
             Syntax: <group_id> <vid>"
        ::= { ctcVlanTranslation 4 }

    -- 1.3.6.1.4.1.4756.103.105.5
