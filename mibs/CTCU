    -- 1.3.6.1.4.1.4756.103.10.4
    ctcSystemLog OBJECT IDENTIFIER ::= { ctcSystem 4 }

    -- 1.3.6.1.4.1.4756.103.10.4.1
    ctcSystemLogServerMode OBJECT-TYPE
        SYNTAX      INTEGER {
                        enabled(1),
                        disabled(2)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Indicates the server mode operation.
             Enabled: Enable server mode operation.
             Disabled: Disable server mode operation."
        ::= { ctcSystemLog 1 }
           
    -- 1.3.6.1.4.1.4756.103.10.4.2
    ctcSystemLogServerAddress OBJECT-TYPE
        SYNTAX      DisplayString
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Indicates the IPv4 host address of syslog server.
             If the switch provide DNS feature, it also can be a Host Name."
        ::= { ctcSystemLog 2 }

    -- 1.3.6.1.4.1.4756.103.10.4.3
    ctcSystemLogLevel OBJECT-TYPE
        SYNTAX      INTEGER {
                        info(1),
                        warning(2),
                        error(3)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Indicates what kind of message will send to syslog server. Possible modes are:
             Info: Send informations, warnings and errors.
             Warning: Send warnings and errors.
             Error: Send errors."
        ::= { ctcSystemLog 3 }

    -- 1.3.6.1.4.1.4756.103.10.4.4
    ctcSystemLogClear OBJECT-TYPE
        SYNTAX      INTEGER {
                        trigger(1),
                        normal(2)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Flushes all system log entries."
        ::= { ctcSystemLog 4 }

    -- 1.3.6.1.4.1.4756.103.10.4.5
    ctcSystemLogTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF CtcSystemLogEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The switch system log information."
        ::= { ctcSystemLog 5 }

    -- 1.3.6.1.4.1.4756.103.10.4.5.1
    ctcSystemLogEntry OBJECT-TYPE
        SYNTAX      CtcSystemLogEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The system log entry."
        INDEX { ctcSystemLogMessageID }
        ::= { ctcSystemLogTable 1 }

    CtcSystemLogEntry ::=
        SEQUENCE {
            ctcSystemLogMessageID        Integer32,
            ctcSystemLogMessageLevel     INTEGER,
            ctcSystemLogMessageTime      DisplayString,
            ctcSystemLogMessage          DisplayString
        }

        -- 1.3.6.1.4.1.4756.103.10.4.5.1.1
        ctcSystemLogMessageID OBJECT-TYPE
            SYNTAX      Integer32 (1..999)
            MAX-ACCESS  not-accessible
            STATUS      current
            DESCRIPTION
                "The Index of the system log entry."
            ::= { ctcSystemLogEntry 1 }
                        
        -- 1.3.6.1.4.1.4756.103.10.4.5.1.2
        ctcSystemLogMessageLevel OBJECT-TYPE
            SYNTAX      INTEGER {
                            info(1),
                            warning(2),
                            error(3)
                        }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates what kind of message."
            ::= { ctcSystemLogEntry 2 }

        -- 1.3.6.1.4.1.4756.103.10.4.5.1.3
        ctcSystemLogMessageTime OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The time of the system log entry."
            ::= { ctcSystemLogEntry 3 }
                            
        -- 1.3.6.1.4.1.4756.103.10.4.5.1.4
        ctcSystemLogMessage OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The message of the system log entry."
            ::= { ctcSystemLogEntry 4 }
            
  
    -- 1.3.6.1.4.1.4756.103.10.5
    ctcMaintenance OBJECT IDENTIFIER ::= { ctcSystem 5 }

    -- 1.3.6.1.4.1.4756.103.10.5.1
    ctcRestartDevice OBJECT-TYPE
        SYNTAX      INTEGER {
                        trigger(1),
                        normal(2)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "You can restart the device on this node."
        ::= { ctcMaintenance 1 }

    -- 1.3.6.1.4.1.4756.103.10.6
    ctcAutoProvision OBJECT IDENTIFIER ::= { ctcSystem 6 }
                
    -- 1.3.6.1.4.1.4756.103.10.6.1
