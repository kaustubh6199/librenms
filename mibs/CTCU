            SYNTAX      Integer32 (100..3300000)
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Controls the rate for the shaper. The default value is 500.
                 This value is restricted to 100-3300000."
            DEFVAL     { 500 }     
            ::= { ctcQosRxPortShapersEntry 2 }

        -- 1.3.6.1.4.1.4756.103.150.1.3.1.3    
        ctcQosRxPortShaperBurstSize OBJECT-TYPE
            SYNTAX      Integer32 (0..252)
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Controls the ingress shaper burst size, The default value is 4 KBytes,
                 The largest supported value is 252 KBytes."
            DEFVAL     { 4 }     
            ::= { ctcQosRxPortShapersEntry 3 }


    -- 1.3.6.1.4.1.4756.103.150.1.4
