    -- 1.3.6.1.4.1.4756.103.60.3
    ctcSecurityAAA OBJECT IDENTIFIER ::= { ctcSecurity 3 }

    -- 1.3.6.1.4.1.4756.103.60.3.1    
    ctcAaaServerTimeout OBJECT-TYPE
        SYNTAX      Integer32 (3..3600)
        UNITS       "Seconds"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The Timeout, which can be set to a number between 3 and 3600 seconds,
             is the maximum time to wait for a reply from a server.
             If the server does not reply within this timeframe,
             we will consider it to be dead and continue with the next enabled server (if any).
             RADIUS servers are using the UDP protocol, which is unreliable by design.
             In order to cope with lost frames,
             the timeout interval is divided into 3 subintervals of equal length.
             If a reply is not received within the subinterval,
             the request is transmitted again.
             This algorithm causes the RADIUS server to be queried up to 3 times before
             it is considered to be dead."
        ::= { ctcSecurityAAA 1 }

    -- 1.3.6.1.4.1.4756.103.60.3.2    
    ctcAaaServerDeadTime OBJECT-TYPE
        SYNTAX      Integer32 (0..3600)
        UNITS       "Seconds"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The Dead Time, which can be set to a number between 0 and 3600 seconds,
             is the period during which the switch will not send new requests to
             a server that has failed to respond to a previous request.
             This will stop the switch from continually trying to
             contact a server that it has already determined as dead.
             Setting the Dead Time to a value greater than 0(zero) will enable this feature,
             but only if more than one server has been configured."
        ::= { ctcSecurityAAA 2 }

    -- 1.3.6.1.4.1.4756.103.60.3.3
    ctcAuthServerConfigTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF CtcAuthServerConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The table allows you to configure the Authentication Servers."
        ::= { ctcSecurityAAA 3 }

    -- 1.3.6.1.4.1.4756.103.60.3.3.1
    ctcAuthServerConfigEntry OBJECT-TYPE
        SYNTAX      CtcAuthServerConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Entry of the Authentication Server configuration."
        INDEX { ctcAuthServerIndex }
        ::= { ctcAuthServerConfigTable 1 }

    CtcAuthServerConfigEntry ::=
        SEQUENCE {
            ctcAuthServerIndex               Integer32,
            ctcRadiusAuthEnabled             TruthValue,
            ctcRadiusAuthServerAddress       DisplayString,
            ctcRadiusAuthServerPort          Integer32,
            ctcRadiusAuthSecret              DisplayString,
            ctcRadiusAccountingEnabled       TruthValue,
            ctcRadiusAccountingServerAddress DisplayString,
            ctcRadiusAccountingServerPort    Integer32,
            ctcRadiusAccountingSecret        DisplayString,
            ctcTacacsPlusAuthEnabled         TruthValue,
            ctcTacacsPlusAuthServerAddress   DisplayString,
            ctcTacacsPlusAuthServerPort      Integer32,
            ctcTacacsPlusAuthSecret          DisplayString
        }

        -- 1.3.6.1.4.1.4756.103.60.3.3.1.1
        ctcAuthServerIndex OBJECT-TYPE
            SYNTAX      Integer32 (1..5)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The Authentication Server number."
            ::= { ctcAuthServerConfigEntry 1 }

        -- 1.3.6.1.4.1.4756.103.60.3.3.1.2
        ctcRadiusAuthEnabled OBJECT-TYPE
            SYNTAX      TruthValue
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Enable the RADIUS Authentication Server."
            ::= { ctcAuthServerConfigEntry 2 }

        -- 1.3.6.1.4.1.4756.103.60.3.3.1.3
        ctcRadiusAuthServerAddress OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "The IP address or hostname of the RADIUS Authentication Server.
                 IP address is expressed in dotted decimal notation."
            ::= { ctcAuthServerConfigEntry 3 }

        -- 1.3.6.1.4.1.4756.103.60.3.3.1.4
        ctcRadiusAuthServerPort OBJECT-TYPE
            SYNTAX      Integer32 (0..65535)
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "The UDP port to use on the RADIUS Authentication Server.
                 If the port is set to 0(zero),
                 the default port(1812) is used on the RADIUS Authentication Server."
            ::= { ctcAuthServerConfigEntry 4 }

        -- 1.3.6.1.4.1.4756.103.60.3.3.1.5
        ctcRadiusAuthSecret OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "The secret - up to 29 characters long,
                 shared between the RADIUS Authentication Server and the switch."
            ::= { ctcAuthServerConfigEntry 5 }

        -- 1.3.6.1.4.1.4756.103.60.3.3.1.6
        ctcRadiusAccountingEnabled OBJECT-TYPE
            SYNTAX      TruthValue
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Enable the RADIUS Accounting Server."
            ::= { ctcAuthServerConfigEntry 6 }

        -- 1.3.6.1.4.1.4756.103.60.3.3.1.7
