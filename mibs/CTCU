-- VLANs
    -- 1.3.6.1.4.1.4756.103.110
    ctcVLAN  OBJECT IDENTIFIER ::= { gsw1005ms 110 }

    -- 1.3.6.1.4.1.4756.103.110.1
    ctcAddVlanID OBJECT-TYPE
        SYNTAX      Integer32 (1..4095)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION                                  
            "Add a new VLAN ID, and the VLAN can be configured as needed.
             Legal values for a VLAN ID are 1 through 4095,
             and for every new VLAN entry all ports are member.
             Syntax: <vlan_id>"
        ::= { ctcVLAN 1 }

    -- 1.3.6.1.4.1.4756.103.110.2
    ctcDeleteVlanByID OBJECT-TYPE
        SYNTAX      Integer32 (1..4095)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Delete a VLAN from indicated ID.
             Syntax: <vlan_id>"
        ::= { ctcVLAN 2 }

    -- 1.3.6.1.4.1.4756.103.110.3
    ctcDeleteVlanByName OBJECT-TYPE
        SYNTAX      DisplayString
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Delete a VLAN from indicated name.
             Syntax: <vlan_name>"
        ::= { ctcVLAN 3 }

    -- 1.3.6.1.4.1.4756.103.110.4
    ctcVlanMembershipTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF CtcVlanMembershipEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The VLAN membership configuration can be monitored and modified here."
        ::= { ctcVLAN 4 }

    -- 1.3.6.1.4.1.4756.103.110.4.1
    ctcVlanMembershipEntry OBJECT-TYPE
        SYNTAX      CtcVlanMembershipEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Entry of VLAN memberships."
        INDEX { ctcVlanID }
        ::= { ctcVlanMembershipTable 1 }

    CtcVlanMembershipEntry ::=
        SEQUENCE {       
            ctcVlanID                Integer32,
            ctcVlanName              OCTET STRING,
            ctcVlanMembershipPorts   DisplayString,
            ctcVlanForbiddenPorts    DisplayString
        }

        -- 1.3.6.1.4.1.4756.103.110.4.1.1    
        ctcVlanID OBJECT-TYPE
            SYNTAX      Integer32 (1..4095)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the ID of this particular VLAN."
            ::= { ctcVlanMembershipEntry 1 }

        -- 1.3.6.1.4.1.4756.103.110.4.1.2    
        ctcVlanName OBJECT-TYPE
            SYNTAX      OCTET STRING (SIZE (0..32))
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Indicates the name of the VLAN. Maximum length of the VLAN Name String is 32.
                 VLAN Name can only contain alphabets or numbers.
                 VLAN name should contain atleast one alphabet.
                 VLAN name can be edited for the existing VLAN entries."
            ::= { ctcVlanMembershipEntry 2 }

        -- 1.3.6.1.4.1.4756.103.110.4.1.3    
        ctcVlanMembershipPorts OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Add or modify membership port list of VLAN."
            ::= { ctcVlanMembershipEntry 3 }

        -- 1.3.6.1.4.1.4756.103.110.4.1.4    
        ctcVlanForbiddenPorts OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Add or modify forbidden port list of VLAN."
            ::= { ctcVlanMembershipEntry 4 }

    -- 1.3.6.1.4.1.4756.103.110.5
    ctcVlanEthertype OBJECT-TYPE
        SYNTAX      DisplayString
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The ether type used for Custom S-ports.
             This is a global setting for all the Custom S-ports.
             The format of 'Ethernet Type' is restricted to 0x600-0xFFFF"
        ::= { ctcVLAN 5 }

    -- 1.3.6.1.4.1.4756.103.110.6
    ctcVlanPortTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF CtcVlanPortEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table is used for configuring the switch VLAN port."
        ::= { ctcVLAN 6 }

    -- 1.3.6.1.4.1.4756.103.110.6.1
    ctcVlanPortEntry OBJECT-TYPE
        SYNTAX      CtcVlanPortEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Entry of VLAN ports."
        INDEX { ctcPortIndex }
        ::= { ctcVlanPortTable 1 }

    CtcVlanPortEntry ::=
        SEQUENCE {
            ctcVlanPortType          INTEGER,          
            ctcVlanPortRxFiltering   INTEGER,
            ctcVlanPortFrameType     INTEGER,
            ctcVlanPortVlanID        Integer32,
            ctcVlanPortTxTag         INTEGER,
            ctcVlanPortConflicts     TruthValue
        }

        -- 1.3.6.1.4.1.4756.103.110.6.1.1    
        ctcVlanPortType OBJECT-TYPE
            SYNTAX      INTEGER {
                            unaware(1),
                            cPort(2),
                            sPort(3),
                            sCustomPort(4)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Port can be one of the following types:
                 Unaware, Customer port(C-port), Service port(S-port), Custom Service port(S-custom-port)
                 If Port Type is Unaware, all frames are classified to the Port VLAN ID and tags are not removed."
            ::= { ctcVlanPortEntry 1 }

        -- 1.3.6.1.4.1.4756.103.110.6.1.2    
        ctcVlanPortRxFiltering OBJECT-TYPE
            SYNTAX      INTEGER {
                            enabled(1),
                            disabled(2)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "This parameter affects VLAN ingress processing.
                 If ingress filtering is enabled and the ingress port is not a member of
                 the classified VLAN of the frame,
                 the frame is discarded. By default, ingress filtering is disabled."
            DEFVAL      { disabled }     
            ::= { ctcVlanPortEntry 2 }

        -- 1.3.6.1.4.1.4756.103.110.6.1.3    
        ctcVlanPortFrameType OBJECT-TYPE
            SYNTAX      INTEGER {
                            all(1),
                            tagged(2),
                            untagged(3)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Determines whether the port accepts all frames or only tagged/untagged frames.
                 This parameter affects VLAN ingress processing.
                 If the port only accepts tagged frames, untagged frames received on the port are discarded.
                 By default, the frame type is set to All."
            DEFVAL      { all }     
            ::= { ctcVlanPortEntry 3 }

        -- 1.3.6.1.4.1.4756.103.110.6.1.4    
        ctcVlanPortVlanID OBJECT-TYPE
            SYNTAX      Integer32 (0..4095)
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Configures the VLAN identifier for the port.
                 The allowed values are 0 through 4095. The default value is 1.
                 
                 If the value is 0, a VLAN tag with the classified VLAN ID is inserted in frames transmitted on the port.                 
                 This mode is normally used for ports connected to VLAN aware switches.
                 IMPORTANT: 'Port Tx Tag' should be set to 'untagPvid(1)' when this value is used.
                 
                 If the value are 1 through 4095, untagged frames received on the port are classified to the Port VLAN ID.
                 If VLAN awareness is disabled, all frames received on the port are classified to the Port VLAN ID.
                 If the classified VLAN ID of a frame transmitted on the port is different from the Port VLAN ID,
                 a VLAN tag with the classified VLAN ID is inserted in the frame."
            DEFVAL      { 1 }     
            ::= { ctcVlanPortEntry 4 }

        -- 1.3.6.1.4.1.4756.103.110.6.1.5    
        ctcVlanPortTxTag OBJECT-TYPE
            SYNTAX      INTEGER {
                            untagPvid(1),
                            tagAll(2),
                            untagAll(3)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Determines egress tagging of a port.
                 Untag_pvid - All VLANs except the configured PVID will be tagged.
                 Tag_all - All VLANs are tagged.
                 Untag_all - All VLANs are untagged."
            DEFVAL      { untagPvid }     
            ::= { ctcVlanPortEntry 5 }

        -- 1.3.6.1.4.1.4756.103.110.6.1.6    
        ctcVlanPortConflicts OBJECT-TYPE
            SYNTAX      TruthValue
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Shows status of Conflicts whether exists or not.
                 When a Volatile VLAN User requests to set VLAN membership or VLAN port configuration,
                 the following conflicts can occur:
                 Functional Conflicts between features.
                 Conflicts due to hardware limitation.
                 Direct conflict between user modules."
            ::= { ctcVlanPortEntry 6 }

    -- 1.3.6.1.4.1.4756.103.110.10
    ctcVlanMembershipStatusTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF CtcVlanMembershipStatusEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The VLAN membership configuration can be monitored and modified here."
        ::= { ctcVLAN 10 }

    -- 1.3.6.1.4.1.4756.103.110.10.1
    ctcVlanMembershipStatusEntry OBJECT-TYPE
        SYNTAX      CtcVlanMembershipStatusEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Entry of VLAN memberships."
        INDEX { ctcVlanID, ctcPortIndex }
        ::= { ctcVlanMembershipStatusTable 1 }

    CtcVlanMembershipStatusEntry ::=
        SEQUENCE {       
            ctcVlanMembershipStatus   INTEGER
        }

        -- 1.3.6.1.4.1.4756.103.110.10.1.1    
        ctcVlanMembershipStatus OBJECT-TYPE
            SYNTAX      INTEGER {
                            nonMember(1),
                            member(2),
                            forbidden(3),
                            conflict(4)
                        }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The table show the current VLAN port members for all VLANs configured by all the VLAN Users.
                 nonMember: A port is excluded in a VLAN.
                 member: A port is included in a VLAN.
                 forbidden: A port is included in a Forbidden port list.
                 confilect: A port is included in a Forbidden port list and dynamic VLAN user register VLAN on same port."
            ::= { ctcVlanMembershipStatusEntry 1 }


-- Private VLAN
    -- 1.3.6.1.4.1.4756.103.120
    ctcPrivateVLAN  OBJECT IDENTIFIER ::= { gsw1005ms 120 }

    -- 1.3.6.1.4.1.4756.103.120.1
    ctcAddPrivateVlanID OBJECT-TYPE
        SYNTAX      Integer32 (1..6)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION                                  
            "Add a new private VLAN ID, and the private VLAN can be configured as needed.
             The allowed range for a private VLAN ID is the same as the switch port number range.
             and for every new private VLAN entry all ports are member."
        ::= { ctcPrivateVLAN 1 }

    -- 1.3.6.1.4.1.4756.103.120.2
    ctcDeletePrivateVlanID OBJECT-TYPE
        SYNTAX      Integer32 (1..6)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Delete a private VLAN from indicated ID."
        ::= { ctcPrivateVLAN 2 }

    -- 1.3.6.1.4.1.4756.103.120.3
    ctcPrivateVlanPortMemberTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF CtcPrivateVlanPortMemberEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "Private VLANs are based on the source port mask, and there are no connections to VLANs.
             This means that VLAN IDs and Private VLAN IDs can be identical.
             A port must be a member of both a VLAN and a Private VLAN to be able to forward packets.
             By default, all ports are VLAN unaware and members of VLAN 1 and Private VLAN 1.
             A VLAN unaware port can only be a member of one VLAN,
             but it can be a member of multiple Private VLANs."
        ::= { ctcPrivateVLAN 3 }

    -- 1.3.6.1.4.1.4756.103.120.3.1
    ctcPrivateVlanPortMemberEntry OBJECT-TYPE
        SYNTAX      CtcPrivateVlanPortMemberEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Entry of private VLAN."
        INDEX { ctcPrivateVlanID }
        ::= { ctcPrivateVlanPortMemberTable 1 }

    CtcPrivateVlanPortMemberEntry ::=
        SEQUENCE {       
            ctcPrivateVlanID             Integer32,
            ctcPrivateVlanPortMembers    DisplayString
        }

        -- 1.3.6.1.4.1.4756.103.120.3.1.1    
        ctcPrivateVlanID OBJECT-TYPE
            SYNTAX      Integer32 (1..6)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the ID of this particular private VLAN."
            ::= { ctcPrivateVlanPortMemberEntry 1 }

        -- 1.3.6.1.4.1.4756.103.120.3.1.2    
        ctcPrivateVlanPortMembers OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Add or modify member port list of private VLAN."
            ::= { ctcPrivateVlanPortMemberEntry 2 }

    -- 1.3.6.1.4.1.4756.103.120.4    
    ctcIsolationPorts OBJECT-TYPE
        SYNTAX      DisplayString
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "A port member of a VLAN can be isolated to other isolated ports on the same VLAN and Private VLAN.
             By default, port isolation is disabled on all ports."
        ::= { ctcPrivateVLAN 4 }


-- VCL
    -- 1.3.6.1.4.1.4756.103.130
    ctcVCL  OBJECT IDENTIFIER ::= { gsw1005ms 130 }

    -- 1.3.6.1.4.1.4756.103.130.1
