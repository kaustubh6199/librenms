    -- 1.3.6.1.4.1.4756.103.60.2
    ctcSecurityNetwork OBJECT IDENTIFIER ::= { ctcSecurity 2 }
    
    -- 1.3.6.1.4.1.4756.103.60.2.1
    ctcPortSecurity OBJECT IDENTIFIER ::= { ctcSecurityNetwork 1 }

    -- 1.3.6.1.4.1.4756.103.60.2.1.1    
    ctcPsecLimitControlMode OBJECT-TYPE
        SYNTAX      INTEGER {
                        enabled(1),
                        disabled(2)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Indicates if Limit Control is globally enabled or disabled on the switch."
        ::= { ctcPortSecurity 1 }

    -- 1.3.6.1.4.1.4756.103.60.2.1.2    
    ctcPsecLimitControlAgingEnabled OBJECT-TYPE
        SYNTAX      TruthValue
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "If enabled, secured MAC addresses are subject to aging as discussed under Aging Period."
        ::= { ctcPortSecurity 2 }

    -- 1.3.6.1.4.1.4756.103.60.2.1.3    
    ctcPsecLimitControlAgingPeriod OBJECT-TYPE
        SYNTAX      Integer32 (10..10000000)
        UNITS       "Seconds"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "If 'Aging' is Enabled, then the aging period is controlled with this value."
        ::= { ctcPortSecurity 3 }

    -- 1.3.6.1.4.1.4756.103.60.2.1.4
    ctcPsecLimitControlPortConfigTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF CtcPsecLimitControlPortConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table provides Port Security Limit Control Configurations."
        ::= { ctcPortSecurity 4 }

    -- 1.3.6.1.4.1.4756.103.60.2.1.4.1
    ctcPsecLimitControlPortConfigEntry OBJECT-TYPE
        SYNTAX      CtcPsecLimitControlPortConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Entry of Port Security Limit Control Configurations."
        INDEX { ctcPortIndex }
        ::= { ctcPsecLimitControlPortConfigTable 1 }

    CtcPsecLimitControlPortConfigEntry ::=
        SEQUENCE {
            ctcPsecLimitControlPortMode      INTEGER,
            ctcPsecLimitControlMacLimit      Integer32,
            ctcPsecLimitControlAction        INTEGER,
            ctcPsecLimitControlPortReopen    INTEGER
        }

        -- 1.3.6.1.4.1.4756.103.60.2.1.4.1.1
        ctcPsecLimitControlPortMode OBJECT-TYPE
            SYNTAX      INTEGER {
                            enable(1),
                            disable(2)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Controls whether Limit Control is enabled on this port.
                 Both this and the Global Mode must be set to Enabled for Limit Control to be in effect.
                 Notice that other modules may still use the underlying port security features
                 without enabling Limit Control on a given port."
            ::= { ctcPsecLimitControlPortConfigEntry 1 }

        -- 1.3.6.1.4.1.4756.103.60.2.1.4.1.2
        ctcPsecLimitControlMacLimit OBJECT-TYPE
            SYNTAX      Integer32 (1..1024)
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "The maximum number of MAC addresses that can be secured on this port.
                 This number cannot exceed 1024.
                 If the limit is exceeded, the corresponding action is taken.
                 The switch is 'born' with a total number of MAC addresses from which all ports draw
                 whenever a new MAC address is seen on a Port Security-enabled port.
                 Since all ports draw from the same pool,
                 it may happen that a configured maximum cannot be granted,
                 if the remaining ports have already used all available MAC addresses."
            ::= { ctcPsecLimitControlPortConfigEntry 2 }

        -- 1.3.6.1.4.1.4756.103.60.2.1.4.1.3
        ctcPsecLimitControlAction OBJECT-TYPE
            SYNTAX      INTEGER {
                            none(1),
                            trap(2),
                            shutdown(3),
                            trapAndShutdown(4)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "If Limit is reached, the switch can take one of the following actions:
                 None: Do not allow more than Limit MAC addresses on the port,
                       but take no further action.
                 Trap: If Limit + 1 MAC addresses is seen on the port,
                       send an SNMP trap. If Aging is disabled,
                       only one SNMP trap will be sent, but with Aging enabled,
                       new SNMP traps will be sent everytime the limit gets exceeded.
                 Shutdown: If Limit + 1 MAC addresses is seen on the port, shut down the port.
                           This implies that all secured MAC addresses will be removed from the port,
                           and no new address will be learned.
                           Even if the link is physically disconnected and reconnected on the port
                           (by disconnecting the cable), the port will remain shut down.
                           There are three ways to re-open the port:
                           1) Boot the switch,
                           2) Disable and re-enable Limit Control on the port or the switch,
                           3) Trigger Reopen node.
                 Trap & Shutdown: If Limit + 1 MAC addresses is seen on the port, both the 'Trap' and
                                  the 'Shutdown' actions described above will be taken."
            ::= { ctcPsecLimitControlPortConfigEntry 3 }

        -- 1.3.6.1.4.1.4756.103.60.2.1.4.1.4
        ctcPsecLimitControlPortReopen OBJECT-TYPE
            SYNTAX      INTEGER {
                            trigger(1),
                            normal(2)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "If a port is shutdown by this module, you may reopen it by triggering this node."
            ::= { ctcPsecLimitControlPortConfigEntry 4 }

    -- 1.3.6.1.4.1.4756.103.60.2.1.5
    ctcPsecSwitchStatusTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF CtcPsecSwitchStatusEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table provides Port Security Limit Control Switch Status."
        ::= { ctcPortSecurity 5 }

    -- 1.3.6.1.4.1.4756.103.60.2.1.5.1
    ctcPsecSwitchStatusEntry OBJECT-TYPE
        SYNTAX      CtcPsecSwitchStatusEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Entry of Port Security Limit Control Switch Status."
        INDEX { ctcPortIndex }
        ::= { ctcPsecSwitchStatusTable 1 }

    CtcPsecSwitchStatusEntry ::=
        SEQUENCE {
            ctcPsecSwitchStatusPortUsers     DisplayString,
            ctcPsecSwitchStatusPortState     INTEGER,
            ctcPsecSwitchStatusMacCount      Integer32
        }

        -- 1.3.6.1.4.1.4756.103.60.2.1.5.1.1
        ctcPsecSwitchStatusPortUsers OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Each of the user modules has a column that shows whether that module has enabled
                 Port Security or not. A '-' means that the corresponding user module is not enabled,
                 whereas a letter indicates that the user module abbreviated by that letter (see below)
                 has enabled port security.
                 Limit Control: L
                 802.1X       : 8
                 DHCP Snooping: D
                 Voice VLAN   : V "
            ::= { ctcPsecSwitchStatusEntry 1 }

        -- 1.3.6.1.4.1.4756.103.60.2.1.5.1.2
        ctcPsecSwitchStatusPortState OBJECT-TYPE
            SYNTAX      INTEGER {
                            disabled(1),
                            ready(2),
                            limitReached(3),
                            shutdown(4)
                        }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "This node shows the current state of the port.
                 The state takes one of four values:
                 Disabled: Limit Control is either globally disabled or disabled on the port.
                 Ready: The limit is not yet reached. This can be shown for all actions.
                 Limit Reached: Indicates that the limit is reached on this port.
                                This state can only be shown if Action is set to None or Trap.
                 Shutdown: Indicates that the port is shut down by the Limit Control module.
                           This state can only be shown if Action is set to Shutdown or Trap & Shutdown."
            ::= { ctcPsecSwitchStatusEntry 2 }

        -- 1.3.6.1.4.1.4756.103.60.2.1.5.1.3
        ctcPsecSwitchStatusMacCount OBJECT-TYPE
            SYNTAX      Integer32 (-1 | 0..1024) 
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "This indicate the number of currently learned MAC addresses(forwarding as well as blocked)
                 If no user modules are enabled on the port, the Current Count will show a value(-1)."
            ::= { ctcPsecSwitchStatusEntry 3 }

    -- 1.3.6.1.4.1.4756.103.60.2.1.6
    ctcPsecPortStatusTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF CtcPsecPortStatusEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table provides Port Security Port Status."
        ::= { ctcPortSecurity 6 }

    -- 1.3.6.1.4.1.4756.103.60.2.1.6.1
    ctcPsecPortStatusEntry OBJECT-TYPE
        SYNTAX      CtcPsecPortStatusEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Entry of Port Security Port Status."
        INDEX { ctcPortIndex, ctcPsecMacIndex }
        ::= { ctcPsecPortStatusTable 1 }

    CtcPsecPortStatusEntry ::=
        SEQUENCE {
            ctcPsecMacIndex          Integer32,
            ctcPsecMacAddress        MacAddress,
            ctcPsecMacVlanID         Integer32,
            ctcPsecMacState          INTEGER,
            ctcPsecMacAdditionTime   DisplayString,
            ctcPsecMacAge            Integer32
        }

        -- 1.3.6.1.4.1.4756.103.60.2.1.6.1.1
        ctcPsecMacIndex OBJECT-TYPE
            SYNTAX      Integer32 (1..1024) 
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The mac address index of the port."
            ::= { ctcPsecPortStatusEntry 1 }

        -- 1.3.6.1.4.1.4756.103.60.2.1.6.1.2
        ctcPsecMacAddress OBJECT-TYPE
            SYNTAX      MacAddress 
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The MAC address that is seen on this port."
            ::= { ctcPsecPortStatusEntry 2 }

        -- 1.3.6.1.4.1.4756.103.60.2.1.6.1.3
        ctcPsecMacVlanID OBJECT-TYPE
            SYNTAX      Integer32 (1..4095) 
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The VLAN ID that is seen on this port."
            ::= { ctcPsecPortStatusEntry 3 }

        -- 1.3.6.1.4.1.4756.103.60.2.1.6.1.4
        ctcPsecMacState OBJECT-TYPE
            SYNTAX      INTEGER {
                            blocked(1),
                            forwarding(2)
                        }     
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates whether the corresponding MAC address is blocked or forwarding.
                 In the blocked state, it will not be allowed to transmit or receive traffic."
            ::= { ctcPsecPortStatusEntry 4 }

        -- 1.3.6.1.4.1.4756.103.60.2.1.6.1.5
        ctcPsecMacAdditionTime OBJECT-TYPE
            SYNTAX      DisplayString     
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Shows the date and time when this MAC address was first seen on the port."
            ::= { ctcPsecPortStatusEntry 5 }

        -- 1.3.6.1.4.1.4756.103.60.2.1.6.1.6
        ctcPsecMacAge OBJECT-TYPE
            SYNTAX      Integer32 (-1 | 0..1000000)     
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "If at least one user module has decided to block this MAC address,
                 it will stay in the blocked state until the hold time(measured in seconds) expires.
                 If all user modules have decided to allow this MAC address to forward, and aging is enabled,
                 the Port Security module will periodically check that this MAC address still forwards traffic.
                 If the age period (measured in seconds) expires and no frames have been seen,
                 the MAC address will be removed from the MAC table. Otherwise a new age period will begin.
                 If aging is disabled or a user module has decided to hold the MAC address indefinitely,
                 a value (-1) will be shown."
            ::= { ctcPsecPortStatusEntry 6 }
        
                
    -- 1.3.6.1.4.1.4756.103.60.2.2
    ctcNAS OBJECT IDENTIFIER ::= { ctcSecurityNetwork 2 }

    -- 1.3.6.1.4.1.4756.103.60.2.2.1    
    ctcNasMode OBJECT-TYPE
        SYNTAX      INTEGER {
                        enabled(1),
                        disabled(2)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Indicates if NAS is globally enabled or disabled on the switch.
             If globally disabled, all ports are allowed forwarding of frames."
        ::= { ctcNAS 1 }

    -- 1.3.6.1.4.1.4756.103.60.2.2.2    
    ctcNasReauthenticationEnabled OBJECT-TYPE
        SYNTAX      TruthValue
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "If enabled, successfully authenticated supplicants/clients are reauthenticated
             after the interval specified by the Reauthentication Period.
             Reauthentication for 802.1X-enabled ports can be used to detect if a new device
             is plugged into a switch port or if a supplicant is no longer attached. 
             For MAC-based ports, reauthentication is only useful if the RADIUS server configuration has changed.
             It does not involve communication between the switch and the client,
             and therefore doesn't imply that a client is still present on a port."
        ::= { ctcNAS 2 }

    -- 1.3.6.1.4.1.4756.103.60.2.2.3    
    ctcNasReauthenticationPeriod OBJECT-TYPE
        SYNTAX      Integer32 (1..3600)
        UNITS       "Seconds"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Determines the period, in seconds,
             after which a connected client must be reauthenticated.
             This is only active if the Reauthentication is Enabled.
             Valid values are in the range 1 to 3600 seconds."
        ::= { ctcNAS 3 }

    -- 1.3.6.1.4.1.4756.103.60.2.2.4    
    ctcNasEapolTimeout OBJECT-TYPE
        SYNTAX      Integer32 (1..65535)
        UNITS       "Seconds"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Determines the time for retransmission of Request Identity EAPOL frames.
             Valid values are in the range 1 to 65535 seconds.
             This has no effect for MAC-based ports."
        ::= { ctcNAS 4 }

    -- 1.3.6.1.4.1.4756.103.60.2.2.5    
    ctcNasAgingPeriod OBJECT-TYPE
        SYNTAX      Integer32 (10..1000000)
        UNITS       "Seconds"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This setting applies to the following modes,
             i.e. modes using the Port Security functionality to secure MAC addresses:
             Single 802.1X, Multi 802.1X or MAC-Based Auth.
             When the NAS module uses the Port Security module to secure MAC addresses,
             the Port Security module needs to check for activity on the MAC address in question at
             regular intervals and free resources if no activity is seen within a given period of time.
             This parameter controls exactly this period and can be set to a number between 10 and 1000000 seconds.
             If reauthentication is enabled and the port is in an 802.1X-based mode,
             this is not so critical, since supplicants that are no longer attached to
             the port will get removed upon the next reauthentication, which will fail.
             But if reauthentication is not enabled, the only way to free resources is by aging the entries.
             For ports in MAC-based Auth. mode, reauthentication doesn't cause direct communication between
             the switch and the client, so this will not detect whether the client is still attached or not,
             and the only way to free any resources is to age the entry."
        ::= { ctcNAS 5 }

    -- 1.3.6.1.4.1.4756.103.60.2.2.6    
    ctcNasHoldTime OBJECT-TYPE
        SYNTAX      Integer32 (10..1000000)
        UNITS       "Seconds"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This setting applies to the following modes,
             i.e. modes using the Port Security functionality to secure MAC addresses:
             Single 802.1X, Multi 802.1X or MAC-Based Auth.
             If a client is denied access - either because the RADIUS server denies the client access or
             because the RADIUS server request times out, the client is put on hold in the Unauthorized state.
             The hold timer does not count during an on-going authentication.
             In MAC-based Auth. mode, the switch will ignore new frames coming from the client during the hold time.
             The Hold Time can be set to a number between 10 and 1000000 seconds."
        ::= { ctcNAS 6 }

    -- 1.3.6.1.4.1.4756.103.60.2.2.7    
    ctcNasRadiusAssignedQosEnabled OBJECT-TYPE
        SYNTAX      TruthValue
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "RADIUS-assigned QoS provides a means to centrally control the traffic class to which traffic
             coming from a successfully authenticated supplicant is assigned on the switch.
             The RADIUS server must be configured to transmit special RADIUS attributes to
             take advantage of this feature."
        ::= { ctcNAS 7 }

    -- 1.3.6.1.4.1.4756.103.60.2.2.8    
    ctcNasRadiusAssignedVlanEnabled OBJECT-TYPE
        SYNTAX      TruthValue
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "RADIUS-assigned VLAN provides a means to centrally control the VLAN on which a
             successfully authenticated supplicant is placed on the switch.
             Incoming traffic will be classified to and switched on the RADIUS-assigned VLAN.
             The RADIUS server must be configured to transmit special RADIUS attributes to
             take advantage of this feature."
        ::= { ctcNAS 8 }

    -- 1.3.6.1.4.1.4756.103.60.2.2.9    
    ctcNasGuestVlanEnabled OBJECT-TYPE
        SYNTAX      TruthValue
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "A Guest VLAN is a special VLAN - typically with limited network access - on which
             802.1X-unaware clients are placed after a network administrator-defined timeout.
             The switch follows a set of rules for entering and leaving the Guest VLAN."
        ::= { ctcNAS 9 }

    -- 1.3.6.1.4.1.4756.103.60.2.2.10    
    ctcNasGuestVlanID OBJECT-TYPE
        SYNTAX      Integer32 (1..4095)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This is the value that a port's Port VLAN ID is set to if a port is moved into the Guest VLAN.
             It is only changeable if the Guest VLAN option is globally enabled.
             Valid values are in the range 1 to 4095."
        ::= { ctcNAS 10 }

    -- 1.3.6.1.4.1.4756.103.60.2.2.11    
    ctcNasMaxReauthenticationCount OBJECT-TYPE
        SYNTAX      Integer32 (1..255)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The number of times the switch transmits an EAPOL Request Identity frame without response
             before considering entering the Guest VLAN is adjusted with this setting.
             The value can only be changed if the Guest VLAN option is globally enabled.
             Valid values are in the range 1 to 255."
        ::= { ctcNAS 11 }

    -- 1.3.6.1.4.1.4756.103.60.2.2.12    
    ctcNasAllowGuestVlanIfEapolSeen OBJECT-TYPE
        SYNTAX      TruthValue
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The switch remembers if an EAPOL frame has been received on the port for the life-time
             of the port. Once the switch considers whether to enter the Guest VLAN,
             it will first check if this option is enabled or disabled.
             If disabled, the switch will only enter the Guest VLAN if an EAPOL frame has not been
             received on the port for the life-time of the port.
             If enabled, the switch will consider entering the Guest VLAN even if an EAPOL frame has
             been received on the port for the life-time of the port."
        ::= { ctcNAS 12 }

    -- 1.3.6.1.4.1.4756.103.60.2.2.13
    ctcNasPortConfigTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF CtcNasPortConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table provides Network Access Server Port Configurations."
        ::= { ctcNAS 13 }

    -- 1.3.6.1.4.1.4756.103.60.2.2.13.1
    ctcNasPortConfigEntry OBJECT-TYPE
        SYNTAX      CtcNasPortConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Entry of Network Access Server Port Configurations."
        INDEX { ctcPortIndex }
        ::= { ctcNasPortConfigTable 1 }

    CtcNasPortConfigEntry ::=
        SEQUENCE {
            ctcNasPortAdminState                 INTEGER,
            ctcNasPortRadiusAssignedQosEnabled   TruthValue,
            ctcNasPortRadiusAssignedVlanEnabled  TruthValue,
            ctcNasPortGuestVlanEnabled           TruthValue,
            ctcNasPortReauthenticateRestart      INTEGER,
            ctcNasPortReinitializeRestart        INTEGER
        }

        -- 1.3.6.1.4.1.4756.103.60.2.2.13.1.1
        ctcNasPortAdminState OBJECT-TYPE
            SYNTAX      INTEGER {
                            forceAuthorized(1),
                            forceUnauthorized(2),
                            portBased802dot1X(3),
                            single802dot1X(4),
                            multi802dot1X(5),
                            macBasedAuthentication(6)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "If NAS is globally enabled, this node controls the port's authentication mode."
            ::= { ctcNasPortConfigEntry 1 }

        -- 1.3.6.1.4.1.4756.103.60.2.2.13.1.2
        ctcNasPortRadiusAssignedQosEnabled OBJECT-TYPE
            SYNTAX      TruthValue
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "When RADIUS-Assigned QoS is both globally enabled and enabled on a given port,
                 the switch reacts to QoS Class information carried in the RADIUS Access-Accept
                 packet transmitted by the RADIUS server when a supplicant is successfully authenticated.
                 If present and valid, traffic received on the supplicant's port will be classified to
                 the given QoS Class. If (re-)authentication fails or the RADIUS Access-Accept packet
                 no longer carries a QoS Class or it's invalid, or the supplicant is otherwise no longer
                 present on the port, the port's QoS Class is immediately reverted to the original QoS Class
                 (which may be changed by the administrator in the meanwhile without affecting the RADIUS-assigned).
                 This option is only available for single-client modes, i.e. Port-based 802.1X or Single 802.1X."
            ::= { ctcNasPortConfigEntry 2 }

        -- 1.3.6.1.4.1.4756.103.60.2.2.13.1.3
        ctcNasPortRadiusAssignedVlanEnabled OBJECT-TYPE
            SYNTAX      TruthValue
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "When RADIUS-Assigned VLAN is both globally enabled and enabled for a given port,
                 the switch reacts to VLAN ID information carried in the RADIUS Access-Accept
                 packet transmitted by the RADIUS server when a supplicant is successfully authenticated.
                 If present and valid, the port's Port VLAN ID will be changed to this VLAN ID,
                 the port will be set to be a member of that VLAN ID, and the port will be forced into
                 VLAN unaware mode. Once assigned, all traffic arriving on the port will be classified and
                 switched on the RADIUS-assigned VLAN ID. If (re-)authentication fails or the RADIUS Access-Accept
                 packet no longer carries a VLAN ID or it's invalid, or the supplicant is otherwise no longer
                 present on the port, the port's VLAN ID is immediately reverted to the original VLAN ID
                 (which may be changed by the administrator in the meanwhile without affecting the RADIUS-assigned).
                 This option is only available for single-client modes, i.e. Port-based 802.1X or Single 802.1X."
            ::= { ctcNasPortConfigEntry 3 }

        -- 1.3.6.1.4.1.4756.103.60.2.2.13.1.4
        ctcNasPortGuestVlanEnabled OBJECT-TYPE
            SYNTAX      TruthValue
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "When Guest VLAN is both globally enabled and enabled for a given port,
                 the switch considers moving the port into the Guest VLAN according to the rules outlined below.
                 This option is only available for EAPOL-based modes, i.e.:
                 Port-based 802.1X, Single 802.1X or Multi 802.1X."
            ::= { ctcNasPortConfigEntry 4 }

        -- 1.3.6.1.4.1.4756.103.60.2.2.13.1.5
        ctcNasPortReauthenticateRestart OBJECT-TYPE
            SYNTAX      INTEGER {
                            trigger(1),
                            normal(2)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Schedules a reauthentication whenever the quiet-period of the port runs out
                 (EAPOL-based authentication). For MAC-based authentication,
                 reauthentication will be attempted immediately.
                 The node is only has effect when authentication is globally enabled and
                 the port's Admin State is in an EAPOL-based or MAC-based mode."
            ::= { ctcNasPortConfigEntry 5 }

        -- 1.3.6.1.4.1.4756.103.60.2.2.13.1.6
        ctcNasPortReinitializeRestart OBJECT-TYPE
            SYNTAX      INTEGER {
                            trigger(1),
                            normal(2)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Forces a reinitialization of the clients on the port and thereby a reauthentication immediately.
                 The clients will transfer to the unauthorized state while the reauthentication is in progress.
                 The node is only has effect when authentication is globally enabled and
                 the port's Admin State is in an EAPOL-based or MAC-based mode."
            ::= { ctcNasPortConfigEntry 6 }

    -- 1.3.6.1.4.1.4756.103.60.2.2.14
    ctcNasSwitchStatusTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF CtcNasSwitchStatusEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This page provides an overview of the current NAS port states."
        ::= { ctcNAS 14 }

    -- 1.3.6.1.4.1.4756.103.60.2.2.14.1
    ctcNasSwitchStatusEntry OBJECT-TYPE
        SYNTAX      CtcNasSwitchStatusEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Entry of Network Access Server Switch Status."
        INDEX { ctcPortIndex }
        ::= { ctcNasSwitchStatusTable 1 }

    CtcNasSwitchStatusEntry ::=
        SEQUENCE {
            ctcNasSwitchPortAdminState   INTEGER,
            ctcNasSwitchPortState        DisplayString,
            ctcNasSwitchPortLastSource   DisplayString,
            ctcNasSwitchPortLastID       DisplayString,
            ctcNasSwitchPortQosClass     DisplayString,
            ctcNasSwitchPortVlanID       DisplayString
        }

        -- 1.3.6.1.4.1.4756.103.60.2.2.14.1.1
        ctcNasSwitchPortAdminState OBJECT-TYPE
            SYNTAX      INTEGER {
                            forceAuthorized(1),
                            forceUnauthorized(2),
                            portBased802dot1X(3),
                            single802dot1X(4),
                            multi802dot1X(5),
                            macBasedAuthentication(6)
                        }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The port's current administrative state."
            ::= { ctcNasSwitchStatusEntry 1 }

        -- 1.3.6.1.4.1.4756.103.60.2.2.14.1.2
        ctcNasSwitchPortState OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The current state of the port. It can undertake one of the following values:
                 Globally Disabled: NAS is globally disabled.
                 Link Down: NAS is globally enabled, but there is no link on the port.
                 Authorized: The port is in Force Authorized or a single-supplicant mode and
                             the supplicant is authorized.
                 Unauthorized: The port is in Force Unauthorized or a single-supplicant mode and
                               the supplicant is not successfully authorized by the RADIUS server.
                 X Auth/Y Unauth: The port is in a multi-supplicant mode.
                                  Currently X clients are authorized and Y are unauthorized."
            ::= { ctcNasSwitchStatusEntry 2 }

        -- 1.3.6.1.4.1.4756.103.60.2.2.14.1.3
        ctcNasSwitchPortLastSource OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The source MAC address carried in the most recently received EAPOL frame for
                 EAPOL-based authentication, and the most recently received frame from a
                 new client for MAC-based authentication."
            ::= { ctcNasSwitchStatusEntry 3 }

        -- 1.3.6.1.4.1.4756.103.60.2.2.14.1.4
        ctcNasSwitchPortLastID OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The user name (supplicant identity) carried in the most recently received
                 Response Identity EAPOL frame for EAPOL-based authentication,
                 and the source MAC address from the most recently received frame from a
                 new client for MAC-based authentication."
            ::= { ctcNasSwitchStatusEntry 4 }

        -- 1.3.6.1.4.1.4756.103.60.2.2.14.1.5
        ctcNasSwitchPortQosClass OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "QoS Class assigned to the port by the RADIUS server if enabled."
            ::= { ctcNasSwitchStatusEntry 5 }

        -- 1.3.6.1.4.1.4756.103.60.2.2.14.1.6
        ctcNasSwitchPortVlanID OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The VLAN ID that NAS has put the port in.
                 The return string is blank, if the Port VLAN ID is not overridden by NAS."
            ::= { ctcNasSwitchStatusEntry 6 }

    -- 1.3.6.1.4.1.4756.103.60.2.2.15
    ctcNasPortStatisticsTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF CtcNasPortStatisticsEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This page provides an overview of the current NAS port states."
        ::= { ctcNAS 15 }

    -- 1.3.6.1.4.1.4756.103.60.2.2.15.1
    ctcNasPortStatisticsEntry OBJECT-TYPE
        SYNTAX      CtcNasPortStatisticsEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Entry of Network Access Server Switch Status."
        INDEX { ctcPortIndex }
        ::= { ctcNasPortStatisticsTable 1 }

    CtcNasPortStatisticsEntry ::=
        SEQUENCE {
            ctcNasPortStatisticsClearAll     INTEGER,
            ctcNasRxEapolTotal               Counter32,
            ctcNasRxEapolResponseID          Counter32,
            ctcNasRxEapolResponses           Counter32,
            ctcNasRxEapolStart               Counter32,
            ctcNasRxEapolLogoff              Counter32,
            ctcNasRxEapolInvalidType         Counter32,
            ctcNasRxEapolInvalidLength       Counter32,
            ctcNasTxEapolTotal               Counter32,
            ctcNasTxEapolRequestID           Counter32,
            ctcNasTxEapolRequests            Counter32,
            ctcNasRxBackendAccessChallenges  Counter32,
            ctcNasRxBackendOtherRequests     Counter32,
            ctcNasRxBackendAuthSuccesses     Counter32,
            ctcNasRxBackendAuthFailures      Counter32,
            ctcNasTxBackendResponeses        Counter32
        }

        -- 1.3.6.1.4.1.4756.103.60.2.2.15.1.1
        ctcNasPortStatisticsClearAll OBJECT-TYPE
            SYNTAX      INTEGER {
                            trigger(1),
                            normal(2)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Triggers to clear both the port counters and all of the attached client's counters."
            ::= { ctcNasPortStatisticsEntry 1 }

        -- 1.3.6.1.4.1.4756.103.60.2.2.15.1.2
        ctcNasRxEapolTotal OBJECT-TYPE
            SYNTAX      Counter32
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The number of valid EAPOL frames of any type that have been received by the switch."
            ::= { ctcNasPortStatisticsEntry 2 }

        -- 1.3.6.1.4.1.4756.103.60.2.2.15.1.3
        ctcNasRxEapolResponseID OBJECT-TYPE
            SYNTAX      Counter32
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The number of valid EAPOL Response Identity frames that have been received by the switch."
            ::= { ctcNasPortStatisticsEntry 3 }

        -- 1.3.6.1.4.1.4756.103.60.2.2.15.1.4
        ctcNasRxEapolResponses OBJECT-TYPE
            SYNTAX      Counter32
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The number of valid EAPOL response frames (other than Response Identity frames)
                 that have been received by the switch."
            ::= { ctcNasPortStatisticsEntry 4 }

        -- 1.3.6.1.4.1.4756.103.60.2.2.15.1.5
        ctcNasRxEapolStart OBJECT-TYPE
            SYNTAX      Counter32
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The number of EAPOL Start frames that have been received by the switch."
            ::= { ctcNasPortStatisticsEntry 5 }

        -- 1.3.6.1.4.1.4756.103.60.2.2.15.1.6
        ctcNasRxEapolLogoff OBJECT-TYPE
            SYNTAX      Counter32
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The number of valid EAPOL Logoff frames that have been received by the switch."
            ::= { ctcNasPortStatisticsEntry 6 }

        -- 1.3.6.1.4.1.4756.103.60.2.2.15.1.7
        ctcNasRxEapolInvalidType OBJECT-TYPE
            SYNTAX      Counter32
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The number of EAPOL frames that have been received by the switch in which
                 the frame type is not recognized."
            ::= { ctcNasPortStatisticsEntry 7 }

        -- 1.3.6.1.4.1.4756.103.60.2.2.15.1.8
        ctcNasRxEapolInvalidLength OBJECT-TYPE
            SYNTAX      Counter32
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The number of EAPOL frames that have been received by the switch in which
                 the Packet Body Length field is invalid."
            ::= { ctcNasPortStatisticsEntry 8 }

        -- 1.3.6.1.4.1.4756.103.60.2.2.15.1.9
        ctcNasTxEapolTotal OBJECT-TYPE
            SYNTAX      Counter32
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The number of EAPOL frames of any type that have been transmitted by the switch."
            ::= { ctcNasPortStatisticsEntry 9 }

        -- 1.3.6.1.4.1.4756.103.60.2.2.15.1.10
        ctcNasTxEapolRequestID OBJECT-TYPE
            SYNTAX      Counter32
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The number of EAPOL Request Identity frames that have been transmitted by the switch."
            ::= { ctcNasPortStatisticsEntry 10 }

        -- 1.3.6.1.4.1.4756.103.60.2.2.15.1.11
        ctcNasTxEapolRequests OBJECT-TYPE
            SYNTAX      Counter32
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The number of valid EAPOL Request frames (other than Request Identity frames)
                 that have been transmitted by the switch."
            ::= { ctcNasPortStatisticsEntry 11 }

        -- 1.3.6.1.4.1.4756.103.60.2.2.15.1.12
        ctcNasRxBackendAccessChallenges OBJECT-TYPE
            SYNTAX      Counter32
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "802.1X-based: Counts the number of times that the switch receives the first request from
                               the backend server following the first response from the supplicant.
                               Indicates that the backend server has communication with the switch.
                 MAC-based: Counts all Access Challenges received from the backend server for this port
                           (left-most table) or client (right-most table)."
            ::= { ctcNasPortStatisticsEntry 12 }

        -- 1.3.6.1.4.1.4756.103.60.2.2.15.1.13
        ctcNasRxBackendOtherRequests OBJECT-TYPE
            SYNTAX      Counter32
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "802.1X-based: Counts the number of times that the switch sends an EAP Request packet
                               following the first to the supplicant.
                               Indicates that the backend server chose an EAP-method.
                 MAC-based: Not applicable."
            ::= { ctcNasPortStatisticsEntry 13 }

        -- 1.3.6.1.4.1.4756.103.60.2.2.15.1.14
        ctcNasRxBackendAuthSuccesses OBJECT-TYPE
            SYNTAX      Counter32
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "802.1X- and MAC-based: Counts the number of times that the switch receives a
                                        success indication. Indicates that the supplicant/client
                                        has successfully authenticated to the backend server."
            ::= { ctcNasPortStatisticsEntry 14 }

        -- 1.3.6.1.4.1.4756.103.60.2.2.15.1.15
        ctcNasRxBackendAuthFailures OBJECT-TYPE
            SYNTAX      Counter32
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "802.1X- and MAC-based: Counts the number of times that the switch receives a
                                        failure message. This indicates that the supplicant/client
                                        has not authenticated to the backend server."
            ::= { ctcNasPortStatisticsEntry 15 }

        -- 1.3.6.1.4.1.4756.103.60.2.2.15.1.16
        ctcNasTxBackendResponeses OBJECT-TYPE
            SYNTAX      Counter32
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "802.1X-based: Counts the number of times that the switch attempts to send a
                               supplicant's first response packet to the backend server.
                               Indicates the switch attempted communication with the backend server.
                               Possible retransmissions are not counted.
                 MAC-based: Counts all the backend server packets sent from the switch towards
                            the backend server for a given port (left-most table) or client (right-most table).
                            Possible retransmissions are not counted."
            ::= { ctcNasPortStatisticsEntry 16 }

    
    -- 1.3.6.1.4.1.4756.103.60.2.3
    ctcACL OBJECT IDENTIFIER ::= { ctcSecurityNetwork 3 }

    -- 1.3.6.1.4.1.4756.103.60.2.3.1
    ctcAclPortCounterClear OBJECT-TYPE
        SYNTAX      INTEGER {
                        trigger(1),
                        normal(2)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Clear the counters."
        ::= { ctcACL 1 }

    -- 1.3.6.1.4.1.4756.103.60.2.3.2
    ctcAclPortConfigTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF CtcAclPortConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table configure the ACL parameters (ACE) of each switch port.
             These parameters will affect frames received on a port unless the frame matches a specific ACE."
        ::= { ctcACL 2 }

    -- 1.3.6.1.4.1.4756.103.60.2.3.2.1
    ctcAclPortConfigEntry OBJECT-TYPE
        SYNTAX      CtcAclPortConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Entry of ACL ports configuration."
        INDEX { ctcPortIndex }
        ::= { ctcAclPortConfigTable 1 }

    CtcAclPortConfigEntry ::=
        SEQUENCE {
            ctcAclPortPollcyID           Integer32,
            ctcAclPortAction             INTEGER,
            ctcAclPortRateLimiterID      Integer32,
            ctcAclPortRedirectPorts      DisplayString,
            ctcAclPortMirror             INTEGER,
            ctcAclPortLogging            INTEGER,
            ctcAclPortShutdown           INTEGER,
            ctcAclPortState              INTEGER,
            ctcAclPortCounter            Counter32
        }

        -- 1.3.6.1.4.1.4756.103.60.2.3.2.1.1
        ctcAclPortPollcyID OBJECT-TYPE
            SYNTAX      Integer32 (0..255)
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Select the policy to apply to this port. The allowed values are 0 through 255.
                 The default value is 0."
            DEFVAL      { 1 }
            ::= { ctcAclPortConfigEntry 1 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.2.1.2
        ctcAclPortAction OBJECT-TYPE
            SYNTAX      INTEGER {
                            deny(1),
                            permit(2)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Select whether forwarding is permitted (permit) or denied (deny).
                 The default value is permit."
            ::= { ctcAclPortConfigEntry 2 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.2.1.3
        ctcAclPortRateLimiterID OBJECT-TYPE
            SYNTAX      Integer32 (0 | 1..16)
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Select which rate limiter to apply on this port.
                 The allowed values are 0(Disabled) or the values 1 through 16.
                 The default value is 0(Disabled)."
            ::= { ctcAclPortConfigEntry 3 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.2.1.4
        ctcAclPortRedirectPorts OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Select which port frames are redirected on.
                 The allowed values are Disabled or a specific port number and
                 it can't be set when action is permitted. The default value is 'Disabled'."
            ::= { ctcAclPortConfigEntry 4 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.2.1.5
        ctcAclPortMirror OBJECT-TYPE
            SYNTAX      INTEGER {
                            enable(1),
                            disable(2)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Specify the mirror operation of this port. The allowed values are:
                 Enabled: Frames received on the port are mirrored.
                 Disabled: Frames received on the port are not mirrored.
                 The default value is Disabled."
            ::= { ctcAclPortConfigEntry 5 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.2.1.6
        ctcAclPortLogging OBJECT-TYPE
            SYNTAX      INTEGER {
                            enable(1),
                            disable(2)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Specify the logging operation of this port. The allowed values are:
                 Enabled: Frames received on the port are stored in the System Log.
                 Disabled: Frames received on the port are not logged.
                 The default value is Disabled.
                 Please note that the System Log memory size and logging rate is limited."
            ::= { ctcAclPortConfigEntry 6 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.2.1.7
        ctcAclPortShutdown OBJECT-TYPE
            SYNTAX      INTEGER {
                            enable(1),
                            disable(2)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Specify the port shut down operation of this port. The allowed values are:
                 Enabled: If a frame is received on the port, the port will be disabled.
                 Disabled: Port shut down is disabled.
                 The default value is Disabled."
            ::= { ctcAclPortConfigEntry 7 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.2.1.8
        ctcAclPortState OBJECT-TYPE
            SYNTAX      INTEGER {
                            enable(1),
                            disable(2)
                        }
            MAX-ACCESS  read-write
            STATUS      current            
            DESCRIPTION
                "Specify the port state of this port. The allowed values are:
                 Enabled: To reopen ports by changing the volatile port configuration of the ACL user module.
                 Disabled: To close ports by changing the volatile port configuration of the ACL user module.
                 The default value is Enabled."
            DEFVAL      { 1 }     
            ::= { ctcAclPortConfigEntry 8 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.2.1.9
        ctcAclPortCounter OBJECT-TYPE
            SYNTAX      Counter32
            UNITS       "Frames"
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Counts the number of frames that match this ACE."
            ::= { ctcAclPortConfigEntry 9 }

    -- 1.3.6.1.4.1.4756.103.60.2.3.3
    ctcAclRateLimiterConfigTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF CtcAclRateLimiterConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table configure the rate limiter for the ACL of the switch."
        ::= { ctcACL 3 }

    -- 1.3.6.1.4.1.4756.103.60.2.3.3.1
    ctcAclRateLimiterConfigEntry OBJECT-TYPE
        SYNTAX      CtcAclRateLimiterConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Entry of ACL Rate Limiter Configuration."
        INDEX { ctcAclRateLimiterID }
        ::= { ctcAclRateLimiterConfigTable 1 }

    CtcAclRateLimiterConfigEntry ::=
        SEQUENCE {
            ctcAclRateLimiterID          Integer32,
            ctcAclLimitedRate            Integer32,
            ctcAclLimitedUnit            INTEGER
        }

        -- 1.3.6.1.4.1.4756.103.60.2.3.3.1.1
        ctcAclRateLimiterID OBJECT-TYPE
            SYNTAX      Integer32 (1..16)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The ID of rate limiter entry."
            ::= { ctcAclRateLimiterConfigEntry 1 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.3.1.2
        ctcAclLimitedRate OBJECT-TYPE
            SYNTAX      Integer32 (0..3276700)
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "The allowed values are: 0-3276700 in pps 
                 or 0, 100, 200, 300, ..., 1000000 in kbps."
            ::= { ctcAclRateLimiterConfigEntry 2 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.3.1.3
        ctcAclLimitedUnit OBJECT-TYPE
            SYNTAX      INTEGER {
                            pps(1),
                            kbps(2)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Specify the rate unit. The allowed values are:
                 pps: packets per second.
                 kbps: Kbits per second."
            ::= { ctcAclRateLimiterConfigEntry 3 }

    -- 1.3.6.1.4.1.4756.103.60.2.3.4
    ctcAddACE OBJECT-TYPE
        SYNTAX      Integer32 (0 | 1..256)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION                                  
            "Inserts a new ACE to list.
             Value 0: Add ACE to end of list.
             Value 1 ~ 256: Insert new ACE before this ACE index."
        ::= { ctcACL 4 }

    -- 1.3.6.1.4.1.4756.103.60.2.3.5
    ctcDeleteACE OBJECT-TYPE
        SYNTAX      Integer32 (1..256)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Deletes the ACE by Index."
        ::= { ctcACL 5 }

    -- 1.3.6.1.4.1.4756.103.60.2.3.6
    ctcRemoveAllACE OBJECT-TYPE
        SYNTAX      INTEGER {
                        trigger(1),
                        normal(2)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Remove all ACEs."
        ::= { ctcACL 6 }

    -- 1.3.6.1.4.1.4756.103.60.2.3.7
    ctcMoveAceDown OBJECT-TYPE
        SYNTAX      Integer32 (1..256)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Moves the ACE down the list."
        ::= { ctcACL 7 }

    -- 1.3.6.1.4.1.4756.103.60.2.3.8
    ctcAceCounterClear OBJECT-TYPE
        SYNTAX      INTEGER {
                        trigger(1),
                        normal(2)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Clear the counters."
        ::= { ctcACL 8 }

    -- 1.3.6.1.4.1.4756.103.60.2.3.9
    ctcAceConfigTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF CtcAceConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "Configure an ACE (Access Control Entry) on this table.
             An ACE consists of several parameters.
             These parameters vary according to the frame type that you select."
        ::= { ctcACL 9 }

    -- 1.3.6.1.4.1.4756.103.60.2.3.9.1
    ctcAceConfigEntry OBJECT-TYPE
        SYNTAX      CtcAceConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The entry of ACE List."
        INDEX { ctcAceIndex }
        ::= { ctcAceConfigTable 1 }

    CtcAceConfigEntry ::=
        SEQUENCE {
            ctcAceIndex                         Integer32,
            ctcAceIngressPort                   DisplayString,
            ctcAcePolicyFilter                  INTEGER,
            ctcAcePolicyValue                   Integer32,
            ctcAcePolicyBitmask                 DisplayString,
            ctcAceFrameType                     INTEGER,
            ctcAceSMacFilter                    INTEGER,
            ctcAceSMacValue                     MacAddress,
            ctcAceDMacFilter                    INTEGER,
            ctcAceDMacValue                     MacAddress,      -- 10      
            ctcAce802dot1qTagged                INTEGER,
            ctcAceVlanIdFilter                  INTEGER,
            ctcAceVlanID                        Integer32,       
            ctcAceTagPriority                   Integer32,
            ctcAceEtherTypeFilter               INTEGER,
            ctcAceEtherTypeValue                DisplayString,
            ctcAceArpFlagArpRarp                INTEGER,
            ctcAceArpFlagRequestRelay           INTEGER,
            ctcAceArpSenderIpFilter             INTEGER,
            ctcAceArpSenderIpAddress            IpAddress,       -- 20
            ctcAceArpSenderIpMask               IpAddress,
            ctcAceArpTargetIpFilter             INTEGER,
            ctcAceArpTargetIpAddress            IpAddress,       
            ctcAceArpTargetIpMask               IpAddress,
            ctcAceArpSMacMatch                  INTEGER,
            ctcAceRarpDMacMatch                 INTEGER,
            ctcAceArpIpEthernetLength           INTEGER,
            ctcAceArpIp                         INTEGER,
            ctcAceArpEthernet                   INTEGER,
            ctcAceIpProtocolFilter              INTEGER,         -- 30
            ctcAceIpProtocolValue               Integer32,
            ctcAceIpTTL                         INTEGER,
            ctcAceIpFragment                    INTEGER,         
            ctcAceIpOption                      INTEGER,
            ctcAceSIpFilter                     INTEGER,
            ctcAceSIpAddress                    IpAddress,
            ctcAceSIpMask                       IpAddress,
            ctcAceDIpFilter                     INTEGER,
            ctcAceDIpAddress                    IpAddress,
            ctcAceDIpMask                       IpAddress,       -- 40
            ctcAceIcmpTypeFilter                INTEGER,
            ctcAceIcmpTypeValue                 Integer32,
            ctcAceIcmpCodeFilter                INTEGER,         
            ctcAceIcmpCodeValue                 Integer32,
            ctcAceUdpSPortFilter                INTEGER,
            ctcAceUdpSPortNumberSpecific        Integer32,
            ctcAceUdpSPortNumberStart           Integer32,
            ctcAceUdpSPortNumberLast            Integer32,
            ctcAceUdpDPortFilter                INTEGER,
            ctcAceUdpDPortNumberSpecific        Integer32,       -- 50
            ctcAceUdpDPortNumberStart           Integer32,
            ctcAceUdpDPortNumberLast            Integer32,
            ctcAceTcpSPortFilter                INTEGER,         
            ctcAceTcpSPortNumberSpecific        Integer32,
            ctcAceTcpSPortNumberStart           Integer32,
            ctcAceTcpSPortNumberLast            Integer32,
            ctcAceTcpDPortFilter                INTEGER,
            ctcAceTcpDPortNumberSpecific        Integer32,
            ctcAceTcpDPortNumberStart           Integer32,
            ctcAceTcpDPortNumberLast            Integer32,       -- 60
            ctcAceTcpFIN                        INTEGER,
            ctcAceTcpSYN                        INTEGER,
            ctcAceTcpRST                        INTEGER,         
            ctcAceTcpPSH                        INTEGER,
            ctcAceTcpACK                        INTEGER,
            ctcAceTcpURG                        INTEGER,
            ctcAceAction                        INTEGER,
            ctcAceRateLimiterID                 Integer32,
            ctcAcePortRedirect                  DisplayString,
            ctcAceMirror                        INTEGER,         -- 70
            ctcAceLogging                       INTEGER,
            ctcAceShutdown                      INTEGER
        }

        -- 1.3.6.1.4.1.4756.103.60.2.3.9.1.1    
        ctcAceIndex OBJECT-TYPE
            SYNTAX      Integer32 (1..256)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the Index of ACE entries."
            ::= { ctcAceConfigEntry 1 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.9.1.2    
        ctcAceIngressPort OBJECT-TYPE
            SYNTAX      DisplayString    
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Select the ingress port for which this ACE applies. 
                 All: The ACE applies to all port. 
                 Port n/list: The ACE applies to this port(s) number, where n is the number of the switch port.
                 The default value is 'All'.
                 Syntex: [all | (port <port_list>)]"
            ::= { ctcAceConfigEntry 2 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.9.1.4    
        ctcAcePolicyFilter OBJECT-TYPE
            SYNTAX      INTEGER {
                            any(1),
                            specific(2)
                        }    
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Specify the policy number filter for this ACE. 
                 Any: No policy filter is specified. (policy filter status is 'don't-care'.) 
                 Specific: If you want to filter a specific policy with this ACE, choose this value."
            ::= { ctcAceConfigEntry 3 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.9.1.5    
        ctcAcePolicyValue OBJECT-TYPE
            SYNTAX      Integer32 (0..255)    
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "When 'Specific' is selected for the policy filter, you can enter a specific policy value.
                 The allowed range is 0 to 255."
            ::= { ctcAceConfigEntry 4 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.9.1.6   
        ctcAcePolicyBitmask OBJECT-TYPE
            SYNTAX      DisplayString    
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "When 'Specific' is selected for the policy filter, you can enter a specific policy bitmask.
                 The allowed range is 0x0 to 0xff."
            ::= { ctcAceConfigEntry 5 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.9.1.3    
        ctcAceFrameType OBJECT-TYPE
            SYNTAX      INTEGER {
                            any(1),
                            ethernetType(2),
                            arp(3),
                            ipv4(4)
                        }    
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Select the ingress port for which this ACE applies.
                 Any: The ACE applies to any port.
                 Port n: The ACE applies to this port number, where n is the number of the switch port.
                 Policy n: The ACE applies to this policy number, where n can range from 1 through 8.
                 The default value is 'Any'."
            ::= { ctcAceConfigEntry 6 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.9.1.7    
        ctcAceSMacFilter OBJECT-TYPE
            SYNTAX      INTEGER {
                            any(1),
                            specific(2)
                        }    
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "(Only effected when the frame type is Ethernet Type or ARP.)
                 Specify the source MAC filter for this ACE.
                 Any: No SMAC filter is specified. (SMAC filter status is don't-care.)
                 Specific: Filter a specific source MAC address with this ACE.
                 The default value is 'Any'."
            ::= { ctcAceConfigEntry 7 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.9.1.8    
        ctcAceSMacValue OBJECT-TYPE
            SYNTAX      MacAddress    
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "When 'Specific' is selected for the SMAC filter,
                 you can enter a specific source MAC address.
                 The legal format is 'xx-xx-xx-xx-xx-xx'.
                 A frame that hits this ACE matches this SMAC value."
            ::= { ctcAceConfigEntry 8 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.9.1.9    
        ctcAceDMacFilter OBJECT-TYPE
            SYNTAX      INTEGER {
                            any(1),
                            multicast(2),
                            broadcast(3),
                            unicast(4),
                            specific(5)
                        }    
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Specify the destination MAC filter for this ACE.
                 Any: No DMAC filter is specified. (DMAC filter status is don't-care.)
                 Multicast: Frame must be multicast.
                 Broadcast: Frame must be broadcast.
                 Unicast: Frame must be unicast.
                 Specific: If you want to filter a specific destination MAC address with this ACE.
                 The default value is 'Any'."
            ::= { ctcAceConfigEntry 9 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.9.1.10    
        ctcAceDMacValue OBJECT-TYPE
            SYNTAX      MacAddress    
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "When 'Specific' is selected for the DMAC filter,
                 you can enter a specific destination MAC address.
                 The legal format is 'xx-xx-xx-xx-xx-xx'.
                 A frame that hits this ACE matches this DMAC value."
            ::= { ctcAceConfigEntry 10 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.9.1.11    
        ctcAce802dot1qTagged OBJECT-TYPE
            SYNTAX      INTEGER {
                            any(1),
                            disabled(2),
                            enabled(3)
                        }    
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Specify whether frames can hit the action according to the 802.1Q tagged.
                 The allowed values are:
                 Any: Any value is allowed ('don't-care').
                 Enabled: Tagged frame only.
                 Disabled: Untagged frame only.
                 The default value is 'Any'."
            ::= { ctcAceConfigEntry 11 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.9.1.12    
        ctcAceVlanIdFilter OBJECT-TYPE
            SYNTAX      INTEGER {
                            any(1),
                            specific(2)
                        }    
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Specify the VLAN ID filter for this ACE.
                 Any: No VLAN ID filter is specified. (VLAN ID filter status is don't-care.)
                 Specific: Filter a specific VLAN ID with this ACE.
                 The default value is 'Any'."
            ::= { ctcAceConfigEntry 12 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.9.1.13    
        ctcAceVlanID OBJECT-TYPE
            SYNTAX      Integer32 (1..4095)    
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "When Specific is selected for the VLAN ID filter, you can enter a specific VLAN ID number.
                 The allowed range is 1 to 4095. A frame that hits this ACE matches this VLAN ID value."
            ::= { ctcAceConfigEntry 13 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.9.1.14    
        ctcAceTagPriority OBJECT-TYPE
            SYNTAX      Integer32 (-1..7)    
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Specify the tag priority for this ACE.
                 A frame that hits this ACE matches this tag priority.
                 The allowed number range is -1 to 7.
                 The value -1(Any) means that no tag priority is specified (tag priority is don't-care.)
                 The default value is -1(Any)."
            ::= { ctcAceConfigEntry 14 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.9.1.15    
        ctcAceEtherTypeFilter OBJECT-TYPE
            SYNTAX      INTEGER {
                            any(1),
                            specific(2)
                        }    
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Specify the Ethernet type filter for this ACE.
                 Any: No EtherType filter is specified (EtherType filter status is don't-care).
                 Specific: Filter a specific EtherType filter with this ACE.
                 The default value is 'Any'."
            ::= { ctcAceConfigEntry 15 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.9.1.16    
        ctcAceEtherTypeValue OBJECT-TYPE
            SYNTAX      DisplayString    
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "When 'Specific' is selected for the EtherType filter,
                 you can enter a specific EtherType value.
                 The allowed range is 0x600 to 0xFFFF but excluding 0x800(IPv4), 0x806(ARP) and 0x86DD(IPv6).
                 A frame that hits this ACE matches this EtherType value."
            ::= { ctcAceConfigEntry 16 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.9.1.17    
        ctcAceArpFlagArpRarp OBJECT-TYPE
            SYNTAX      INTEGER {
                            any(1),
                            arp(2),
                            rarp(3),
                            other(4)
                        }    
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Specify the available ARP/RARP opcode (OP) flag for this ACE.
                 Any: No ARP/RARP OP flag is specified. (OP is don't-care.)
                 ARP: Frame must have ARP/RARP opcode set to ARP.
                 RARP: Frame must have ARP/RARP opcode set to RARP.
                 Other: Frame has unknown ARP/RARP Opcode flag. 
                 The default value is 'Any'."
            ::= { ctcAceConfigEntry 17 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.9.1.18    
        ctcAceArpFlagRequestRelay OBJECT-TYPE
            SYNTAX      INTEGER {
                            any(1),
                            request(2),
                            reply(3)
                        }    
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Specify the available ARP/RARP opcode (OP) flag for this ACE.
                 Any: No ARP/RARP OP flag is specified. (OP is 'don't-care'.)
                 Request: Frame must have ARP Request or RARP Request OP flag set.
                 Reply: Frame must have ARP Reply or RARP Reply OP flag.  
                 The default value is 'Any'."
            ::= { ctcAceConfigEntry 18 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.9.1.19    
        ctcAceArpSenderIpFilter OBJECT-TYPE
            SYNTAX      INTEGER {
                            any(1),
                            host(2),
                            network(3)
                        }    
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Specify the sender IP filter for this ACE.
                 Any: No sender IP filter is specified. (Sender IP filter is don't-care.)
                 Host: Sender IP filter is set to Host.
                       Specify the sender IP address in the SIP Address field that appears.
                 Network: Sender IP filter is set to Network.
                          Specify the sender IP address and sender IP mask in the SIP Address
                          and SIP Mask fields that appear.  
                 The default value is 'Any'."
            ::= { ctcAceConfigEntry 19 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.9.1.20    
        ctcAceArpSenderIpAddress OBJECT-TYPE
            SYNTAX      IpAddress    
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "When 'Host' or 'Network' is selected for the sender IP filter,
                 you can enter a specific sender IP address in dotted decimal notation."
            ::= { ctcAceConfigEntry 20 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.9.1.21    
        ctcAceArpSenderIpMask OBJECT-TYPE
            SYNTAX      IpAddress    
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "When 'Network' is selected for the sender IP filter,
                 you can enter a specific sender IP mask in dotted decimal notation."
            ::= { ctcAceConfigEntry 21 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.9.1.22    
        ctcAceArpTargetIpFilter OBJECT-TYPE
            SYNTAX      INTEGER {
                            any(1),
                            host(2),
                            network(3)
                        }    
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Specify the target IP filter for this specific ACE.
                 Any: No target IP filter is specified. (Target IP filter is don't-care.)
                 Host: Target IP filter is set to Host.
                       Specify the target IP address in the Target IP Address field that appears.
                 Network: Target IP filter is set to Network.
                          Specify the target IP address and target IP mask in the Target IP Address
                          and Target IP Mask fields that appear.  
                 The default value is 'Any'."
            ::= { ctcAceConfigEntry 22 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.9.1.23    
        ctcAceArpTargetIpAddress OBJECT-TYPE
            SYNTAX      IpAddress    
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "When 'Host' or 'Network' is selected for the target IP filter,
                 you can enter a specific target IP address in dotted decimal notation."
            ::= { ctcAceConfigEntry 23 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.9.1.24    
        ctcAceArpTargetIpMask OBJECT-TYPE
            SYNTAX      IpAddress    
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "When 'Network' is selected for the target IP filter,
                 you can enter a specific target IP mask in dotted decimal notation."
            ::= { ctcAceConfigEntry 24 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.9.1.25    
        ctcAceArpSMacMatch OBJECT-TYPE
            SYNTAX      INTEGER {
                            any(1),
                            unequal(2),
                            equal(3)
                        }    
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Specify whether frames can hit the action according to
                 their sender hardware address field (SHA) settings.
                 Unequal: ARP frames where SHA is not equal to the SMAC address.
                 Equal: ARP frames where SHA is equal to the SMAC address.
                 Any: Any value is allowed (don't-care).   
                 The default value is 'Any'."
            ::= { ctcAceConfigEntry 25 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.9.1.26    
        ctcAceRarpDMacMatch OBJECT-TYPE
            SYNTAX      INTEGER {
                            any(1),
                            unequal(2),
                            equal(3)
                        }    
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Specify whether frames can hit the action according to
                 their target hardware address field (THA) settings.
                 Unequal: RARP frames where THA is not equal to the DMAC address.
                 Equal: RARP frames where THA is equal to the DMAC address.
                 Any: Any value is allowed (don't-care).    
                 The default value is 'Any'."
            ::= { ctcAceConfigEntry 26 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.9.1.27    
        ctcAceArpIpEthernetLength OBJECT-TYPE
            SYNTAX      INTEGER {
                            any(1),
                            unequal(2),
                            equal(3)
                        }    
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Specify whether frames can hit the action according to their
                 ARP/RARP hardware address length (HLN) and protocol address length (PLN) settings.
                 Unequal: ARP/RARP frames where the HLN is not equal to Ethernet (0x06) or
                          the (PLN) is not equal to IPv4 (0x04).
                 Equal: ARP/RARP frames where the HLN is equal to Ethernet (0x06) and
                        the (PLN) is equal to IPv4 (0x04).
                 Any: Any value is allowed (don't-care).     
                 The default value is 'Any'."
            ::= { ctcAceConfigEntry 27 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.9.1.28    
        ctcAceArpIp OBJECT-TYPE
            SYNTAX      INTEGER {
                            any(1),
                            unequal(2),
                            equal(3)
                        }    
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Specify whether frames can hit the action according to
                 their ARP/RARP hardware address space (HRD) settings.
                 Unequal: ARP/RARP frames where the HLD is not equal to Ethernet (1).
                 Equal: ARP/RARP frames where the HLD is equal to Ethernet (1).
                 Any: Any value is allowed (don't-care).      
                 The default value is 'Any'."
            ::= { ctcAceConfigEntry 28 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.9.1.29    
        ctcAceArpEthernet OBJECT-TYPE
            SYNTAX      INTEGER {
                            any(1),
                            unequal(2),
                            equal(3)
                        }    
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Specify whether frames can hit the action according to
                 their ARP/RARP protocol address space (PRO) settings.
                 Unequal: ARP/RARP frames where the PRO is not equal to IP (0x800).
                 Equal: ARP/RARP frames where the PRO is equal to IP (0x800).
                 Any: Any value is allowed (don't-care).       
                 The default value is 'Any'."
            ::= { ctcAceConfigEntry 29 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.9.1.30    
        ctcAceIpProtocolFilter OBJECT-TYPE
            SYNTAX      INTEGER {
                            any(1),
                            icmp(2),
                            udp(3),
                            tcp(4),
                            other(5)
                        }    
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Specify the IP protocol filter for this ACE.
                 Any: No IP protocol filter is specified (don't-care).
                 
                 ICMP: Select ICMP to filter IPv4 ICMP protocol frames.
                       Extra fields for defining ICMP parameters will appear.
                       These fields are explained later in this help file.
                 UDP: Select UDP to filter IPv4 UDP protocol frames.
                      Extra fields for defining UDP parameters will appear.
                      These fields are explained later in this help file.
                 TCP: Select TCP to filter IPv4 TCP protocol frames.
                      Extra fields for defining TCP parameters will appear.
                      These fields are explained later in this help file.
                 Specific: Filter a specific IP protocol filter with this ACE.             
                 The default value is 'Any'."
            ::= { ctcAceConfigEntry 30 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.9.1.31    
        ctcAceIpProtocolValue OBJECT-TYPE
            SYNTAX      Integer32 (0..255)   
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "When 'Specific' is selected for the IP protocol value,
                 you can enter a specific value. The allowed range is 0 to 255.
                 A frame that hits this ACE matches this IP protocol value."
            ::= { ctcAceConfigEntry 31 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.9.1.32   
        ctcAceIpTTL OBJECT-TYPE
            SYNTAX      INTEGER {
                            any(1),
                            nonZero(2),
                            zero(3)
                        }    
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Specify the Time-to-Live settings for this ACE.
                 Zero: IPv4 frames with a Time-to-Live field greater than zero must not be able to match this entry.
                 Non-zero: IPv4 frames with a Time-to-Live field greater than zero must be able to match this entry.
                 Any: Any value is allowed (don't-care).        
                 The default value is 'Any'."
            ::= { ctcAceConfigEntry 32 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.9.1.33   
        ctcAceIpFragment OBJECT-TYPE
            SYNTAX      INTEGER {
                            any(1),
                            yes(2),
                            no(3)
                        }    
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Specify the fragment offset settings for this ACE.
                 This involves the settings for the More Fragments (MF) bit
                 and the Fragment Offset (FRAG OFFSET) field for an IPv4 frame.
                 No: IPv4 frames where the MF bit is set or the FRAG OFFSET field is greater than
                     zero must not be able to match this entry.
                 Yes: IPv4 frames where the MF bit is set or the FRAG OFFSET field is greater than
                      zero must be able to match this entry.
                 Any: Any value is allowed (don't-care).      
                 The default value is 'Any'."
            ::= { ctcAceConfigEntry 33 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.9.1.34   
        ctcAceIpOption OBJECT-TYPE
            SYNTAX      INTEGER {
                            any(1),
                            yes(2),
                            no(3)
                        }    
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Specify the options flag setting for this ACE.
                 No: IPv4 frames where the options flag is set must not be able to match this entry.
                 Yes: IPv4 frames where the options flag is set must be able to match this entry.
                 Any: Any value is allowed (don't-care).       
                 The default value is 'Any'."
            ::= { ctcAceConfigEntry 34 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.9.1.35    
        ctcAceSIpFilter OBJECT-TYPE
            SYNTAX      INTEGER {
                            any(1),
                            host(2),
                            network(3)
                        }    
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Specify the source IP filter for this ACE.
                 Any: No source IP filter is specified. (Source IP filter is don't-care.)
                 Host: Source IP filter is set to Host.
                       Specify the source IP address in the SIP Address field that appears.
                 Network: Source IP filter is set to Network.
                          Specify the source IP address and source IP mask in the SIP Address
                          and SIP Mask fields that appear.   
                 The default value is 'Any'."
            ::= { ctcAceConfigEntry 35 }

        -- 1.3.6.1.4.1.4756.103.60.2.3.9.1.36    
