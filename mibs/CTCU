            ::= { ctcAccessMgmtProtocolEntry 1 }

        -- 1.3.6.1.4.1.4756.103.60.1.9.4.1.2
        ctcAccessMgmtProtocolStartIP OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Indicates the start IP address for the access management entry.
                 It must either be a valid IPv4 or IPv6 address.
                 IPv4 address is in dotted decimal notation ('x.y.z.w').
                 IPv6 address is in 128-bit records represented as eight fields of up to
                 four hexadecimal digits with a colon separating each field (:)."
            ::= { ctcAccessMgmtProtocolEntry 2 }

        -- 1.3.6.1.4.1.4756.103.60.1.9.4.1.3
        ctcAccessMgmtProtocolEndIP OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Indicates the end IP address for the access management entry..
                 It must either be a valid IPv4 or IPv6 address.
                 IPv4 address is in dotted decimal notation ('x.y.z.w').
                 IPv6 address is in 128-bit records represented as eight fields of up to
                 four hexadecimal digits with a colon separating each field (:)."
            ::= { ctcAccessMgmtProtocolEntry 3 }

        -- 1.3.6.1.4.1.4756.103.60.1.9.4.1.4
