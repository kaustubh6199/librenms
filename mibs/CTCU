-- VLAN Translation
    -- 1.3.6.1.4.1.4756.103.105
    ctcVlanTranslation  OBJECT IDENTIFIER ::= { gsw1005ms 105 }

    -- 1.3.6.1.4.1.4756.103.105.1
    ctcAddVlanTransPortToGroup OBJECT-TYPE
        SYNTAX      DisplayString
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION                                  
            "Add a new entry in Port to Group Mapping Table.
             Note that if a VLAN translation is enabled on a management port for management VLAN,
             it may disrupt the management connectivity in some cases.
             Syntax: <port_list> <group_id>"
        ::= { ctcVlanTranslation 1 }

    -- 1.3.6.1.4.1.4756.103.105.2
    ctcVlanTransPortToGroupMappingTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF CtcVlanTransPortToGroupMappingEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table allows you to map set of Port members to a Group ID for all switch ports."
        ::= { ctcVlanTranslation 2 }

    -- 1.3.6.1.4.1.4756.103.105.2.1
    ctcVlanTransPortToGroupMappingEntry OBJECT-TYPE
        SYNTAX      CtcVlanTransPortToGroupMappingEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Entry of Port to Group mapping Table."
        INDEX { ctcVlanTransGroupID }
        ::= { ctcVlanTransPortToGroupMappingTable 1 }

    CtcVlanTransPortToGroupMappingEntry ::=
        SEQUENCE {       
            ctcVlanTransGroupID                  Integer32,
            ctcVlanTransPortToGroupPortMembers   DisplayString
        }

        -- 1.3.6.1.4.1.4756.103.105.2.1.1    
        ctcVlanTransGroupID OBJECT-TYPE
            SYNTAX      Integer32 (1..6)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "A valid Group ID is an integer value form 1 to 6.
                 A set of VLAN Translations are mapped to a group Id.
                 This way a port is mapped to a list of VLAN Translations easily by
                 mapping it to a group."
            ::= { ctcVlanTransPortToGroupMappingEntry 1 }

        -- 1.3.6.1.4.1.4756.103.105.2.1.2    
        ctcVlanTransPortToGroupPortMembers OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "A port can be mapped to any of the groups.
                 Multiple ports can also be mapped to a group with same group Id.
                 Note: By default, each port is mapped to a group with a group Id equal to the port number.
                       For example, port 1 is mapped to the group with ID=1."
            ::= { ctcVlanTransPortToGroupMappingEntry 2 }

    -- 1.3.6.1.4.1.4756.103.105.3
    ctcAddVlanTransMapping OBJECT-TYPE
        SYNTAX      DisplayString
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION                                  
            "Add a new entry in VLAN Translation table.
             Syntax: <group_id> <vid> <trans_vid>"
        ::= { ctcVlanTranslation 3 }

    -- 1.3.6.1.4.1.4756.103.105.4
