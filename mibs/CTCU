            SYNTAX      Counter32
            UNITS       "Packets"
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The number of Received Queries."
            ::= { ctcIgmpSnoopingStatisticsEntry 6 }

        -- 1.3.6.1.4.1.4756.103.90.1.41.1.7    
