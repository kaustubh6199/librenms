                "The RX sensitivity in dBm."
            ::= { ctcSfpStatusEntry 12}

        -- 1.3.6.1.4.1.4756.103.50.3.1.13
        ctcSfpTxBias  OBJECT-TYPE
            SYNTAX      INTEGER
            UNITS		"mA"
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The TX bias current in mA."
            ::= { ctcSfpStatusEntry 13 }

        -- 1.3.6.1.4.1.4756.103.50.3.1.14
