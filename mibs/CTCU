        INDEX { ctcIgmpPortGroupFilteringIndex }
        ::= { ctcIgmpPortGroupFilteringTable 1 }

    CtcIgmpPortGroupFilteringEntry ::=
        SEQUENCE {
            ctcIgmpPortGroupFilteringIndex   Integer32,          
            ctcIgmpPortGroupFilteringPort    Integer32,
            ctcIgmpPortGroupFilteringGroups  IpAddress
        }

        -- 1.3.6.1.4.1.4756.103.90.1.32.1.1    
