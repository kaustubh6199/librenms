-- Ports
    -- 1.3.6.1.4.1.4756.103.50
    ctcPorts OBJECT IDENTIFIER ::= { gsw1005ms 50 }

    -- 1.3.6.1.4.1.4756.103.50.1
    ctcPortsConfigTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF CtcPortsConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table shows current port configurations. Ports can also be configured here."
        ::= { ctcPorts 1 }

    -- 1.3.6.1.4.1.4756.103.50.1.1
    ctcPortsConfigEntry OBJECT-TYPE
        SYNTAX      CtcPortsConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The entry of ports."
        INDEX { ctcPortIndex }
        ::= { ctcPortsConfigTable 1 }

    CtcPortsConfigEntry ::=
        SEQUENCE {
            ctcPortLink              INTEGER,
            ctcPortSpeedCurrent      INTEGER,
            ctcPortSpeedMode         INTEGER,
            ctcPortFlowCtrlCurrentRx TruthValue,
            ctcPortFlowCtrlCurrentTx TruthValue,
            ctcPortFlowCtrlEnabled   TruthValue,
            ctcPortMaxFrameSize      Integer32,
            ctcPortExcCollMode       INTEGER,
            ctcPortPowerControl      INTEGER
        }
                         
        -- 1.3.6.1.4.1.4756.103.50.1.1.1    
        ctcPortLink OBJECT-TYPE
            SYNTAX      INTEGER {
                            linkUp(1),
                            linkDown(2),
                            linkDownByACL(3),
                            linkDownByThermalProtect(4),
                            linkDownByLoopProtect(5)
                        }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The current link state."
            ::= { ctcPortsConfigEntry 1 }
                  
        -- 1.3.6.1.4.1.4756.103.50.1.1.2    
        ctcPortSpeedCurrent OBJECT-TYPE
            SYNTAX      INTEGER {
                            linkDown(1),
                            link10Mhdx(2),
                            link10Mfdx(3),
                            link100Mhdx(4),
                            link100Mfdx(5),
                            link1Gfdx(6)
                        }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Provides the current link speed of the port."
            ::= { ctcPortsConfigEntry 2 }
                  
        -- 1.3.6.1.4.1.4756.103.50.1.1.3    
        ctcPortSpeedMode OBJECT-TYPE
            SYNTAX      INTEGER {
                            disabled(1),
                            auto(2),
                            speed10Mhdx(3),
                            speed10Mfdx(4),
                            speed100Mhdx(5),
                            speed100Mfdx(6),
                            speed1Gfdx(7),
                            detection(8)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Select link speed for the switch port."
            ::= { ctcPortsConfigEntry 3 }
                   
        -- 1.3.6.1.4.1.4756.103.50.1.1.4    
        ctcPortFlowCtrlCurrentRx OBJECT-TYPE
            SYNTAX      TruthValue
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates whether pause frames on the port are obeyed."
            ::= { ctcPortsConfigEntry 4 }
                  
        -- 1.3.6.1.4.1.4756.103.50.1.1.5    
        ctcPortFlowCtrlCurrentTx OBJECT-TYPE
            SYNTAX      TruthValue
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates whether pause frames on the port are transmitted."
            ::= { ctcPortsConfigEntry 5 }
                  
        -- 1.3.6.1.4.1.4756.103.50.1.1.6    
        ctcPortFlowCtrlEnabled OBJECT-TYPE
            SYNTAX      TruthValue
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "When Auto Speed is selected on a port,
                 this section indicates the flow control capability that is advertised to the link partner."
            ::= { ctcPortsConfigEntry 6 }
                  
        -- 1.3.6.1.4.1.4756.103.50.1.1.7    
        ctcPortMaxFrameSize OBJECT-TYPE
            SYNTAX      Integer32 (1518..9600)
            UNITS       "Octet"
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "The maximum frame size allowed for the switch port, including FCS."
            ::= { ctcPortsConfigEntry 7 }
                  
        -- 1.3.6.1.4.1.4756.103.50.1.1.8    
        ctcPortExcCollMode OBJECT-TYPE
            SYNTAX      INTEGER {
                            discard(1),
                            restart(2)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Configure port transmit collision behavior.
                 Discard: Discard frame after 16 collisions (default).
                 Restart: Restart backoff algorithm after 16 collisions."
            DEFVAL      { 1 }
            ::= { ctcPortsConfigEntry 8 }
                  
        -- 1.3.6.1.4.1.4756.103.50.1.1.9    
        ctcPortPowerControl OBJECT-TYPE
            SYNTAX      INTEGER {
                            disabled(1),
                            actiPHY(2),
                            perfectReach(3),
                            enabled(4)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "The Configured allows for changing the power savings mode parameters per port.
                 Disabled: All power savings mechanisms disabled.
                 ActiPHY: Link down power savings enabled.
                 PerfectReach: Link up power savings enabled.
                 Enabled: Both link up and link down power savings enabled."
            ::= { ctcPortsConfigEntry 9 }

    -- 1.3.6.1.4.1.4756.103.50.2
    ctcAutoLaserShutdownTable OBJECT-TYPE
