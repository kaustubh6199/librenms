GSW-1005MS-MIB DEFINITIONS ::= BEGIN 
  
    IMPORTS
        DisplayString,
        DateAndTime,
        MacAddress,
        TruthValue
            FROM SNMPv2-TC         
        MODULE-COMPLIANCE,
        OBJECT-GROUP,
        NOTIFICATION-GROUP
            FROM SNMPv2-CONF            
        enterprises,
        NOTIFICATION-TYPE,
        MODULE-IDENTITY,
        OBJECT-TYPE,
        Counter32,
        Integer32,
        Unsigned32,
        Gauge32,
        Counter64,
        IpAddress
            FROM SNMPv2-SMI
        Float32TC
            FROM FLOAT-TC-MIB       
    ; 
          
    -- 1.3.6.1.4.1.4756
    ctc MODULE-IDENTITY
        LAST-UPDATED "201604110000Z"        -- Apr 11, 2016 at
        ORGANIZATION
            " CTC Union Technologies Co., Ltd. "
        CONTACT-INFO
            " Postal: 8F, No.60, ZhouZi Street, Nei-Hu, Taipei 114, Taiwan
              Tel: +886-2-2659-1021
              Fax: +886-2-2672-5211
              Web: http://www.ctcu.com
              Email: support@ctcu.com " 
        DESCRIPTION
            " GSW-1005MS MIB File "
        
        REVISION "201604110000Z"        -- Apr 11, 2016 at        
        DESCRIPTION
            " V1.05  - Fixed offset of timezone Yangon (Rangoon)."
        
        REVISION "201508120000Z"        -- Aug 12, 2015 at
        DESCRIPTION
            " V1.04  - Added Ingress Port Shaper."
         
        REVISION "201405290000Z"        -- May 29, 2014 at
        DESCRIPTION
            " V1.03  - Added ALS and SFP table."
        
        REVISION "201312020000Z"        -- Dec 02, 2013 at
        DESCRIPTION
            " V1.02  - Removed some nodes that are unsupported."
                
        REVISION "201311050000Z"        -- Nov 05, 2013 at
        DESCRIPTION
            " V1.01  - Added 'Ingress Port Policer FC'."
                            
        REVISION "201309100000Z"        -- Sep 10, 2013 at
        DESCRIPTION
            " V1.00  - Initial Document."
        ::= { enterprises 4756 }
 

              
