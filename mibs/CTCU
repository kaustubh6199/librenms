-- Voice VLAN
    -- 1.3.6.1.4.1.4756.103.140
    ctcVoiceVLAN  OBJECT IDENTIFIER ::= { gsw1005ms 140 }

    -- 1.3.6.1.4.1.4756.103.140.1    
    ctcVoiceVlanMode OBJECT-TYPE
        SYNTAX      INTEGER {
                        enabled(1),
                        disabled(2)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Indicates the Voice VLAN mode operation.
             We must disable MSTP feature before we enable Voice VLAN.
             It can avoid the conflict of ingress filtering."
        ::= { ctcVoiceVLAN 1 }
 
    -- 1.3.6.1.4.1.4756.103.140.2    
    ctcVoiceVlanID OBJECT-TYPE
        SYNTAX      Integer32 (1..4095)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Indicates the Voice VLAN ID.
             It should be a unique VLAN ID in the system and cannot equal each port PVID.
             It is a conflict in configuration if the value equals management VID, MVR VID, PVID etc."
        ::= { ctcVoiceVLAN 2 }
 
    -- 1.3.6.1.4.1.4756.103.140.3    
    ctcVoiceVlanAgeTime OBJECT-TYPE
        SYNTAX      Integer32 (10..10000000)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Indicates the Voice VLAN secure learning aging time.
             It is used when security mode or auto detect mode is enabled.
             In other cases, it will be based on hardware aging time.
             The actual aging time will be situated between the [age_time; 2 * age_time] interval."
        ::= { ctcVoiceVLAN 3 }
 
    -- 1.3.6.1.4.1.4756.103.140.4    
    ctcVoiceVlanTrafficClass OBJECT-TYPE
        SYNTAX      Integer32 (0..7)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Indicates the Voice VLAN traffic class.
             All traffic on the Voice VLAN will apply this class."
        ::= { ctcVoiceVLAN 4 }

    -- 1.3.6.1.4.1.4756.103.140.5
    ctcVoiceVlanPortTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF CtcVoiceVlanPortEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table is used for configuring the switch voice VLAN port."
        ::= { ctcVoiceVLAN 5 }

    -- 1.3.6.1.4.1.4756.103.140.5.1
    ctcVoiceVlanPortEntry OBJECT-TYPE
        SYNTAX      CtcVoiceVlanPortEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Entry of voice VLAN ports."
        INDEX { ctcPortIndex }
        ::= { ctcVoiceVlanPortTable 1 }

    CtcVoiceVlanPortEntry ::=
        SEQUENCE {
            ctcVoiceVlanPortMode         INTEGER,          
            ctcVoiceVlanPortSecurity     INTEGER,
            ctcVoiceVlanPortDiscovery    INTEGER
        }

        -- 1.3.6.1.4.1.4756.103.140.5.1.1    
        ctcVoiceVlanPortMode OBJECT-TYPE
            SYNTAX      INTEGER {
                            disabled(1),
                            auto(2),
                            forced(3)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Indicates the Voice VLAN port mode. 
                 Possible port modes are:
                 Disabled: Disjoin from Voice VLAN.
                 Auto: Enable auto detect mode.
                       It detects whether there is VoIP phone attached to the specific port and
                       configures the Voice VLAN members automatically.
                 Forced: Force join to Voice VLAN."
            ::= { ctcVoiceVlanPortEntry 1 }

        -- 1.3.6.1.4.1.4756.103.140.5.1.1    
        ctcVoiceVlanPortSecurity OBJECT-TYPE
            SYNTAX      INTEGER {
                            enabled(1),
                            disabled(2)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Indicates the Voice VLAN port security mode.
                 When the function is enabled,
                 all non-telephonic MAC addresses in the Voice VLAN will be blocked for 10 seconds."
            ::= { ctcVoiceVlanPortEntry 2 }

        -- 1.3.6.1.4.1.4756.103.140.5.1.1    
        ctcVoiceVlanPortDiscovery OBJECT-TYPE
            SYNTAX      INTEGER {
                            oui(1),
                            lldp(2),
                            both(3)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Indicates the Voice VLAN port discovery protocol.
                 It will only work when auto detect mode is enabled.
                 We should enable LLDP feature before configuring discovery protocol to LLDP or Both.
                 Changing the discovery protocol to OUI or LLDP will restart auto detect process.
                 Possible discovery protocols are:
                 OUI: Detect telephony device by OUI address.
                 LLDP: Detect telephony device by LLDP.
                 Both: Both OUI and LLDP."
            ::= { ctcVoiceVlanPortEntry 3 }

    -- 1.3.6.1.4.1.4756.103.140.6
    ctcAddVoiceVlanOUI OBJECT-TYPE
        SYNTAX      DisplayString
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION                                  
            "Add a new entry to voice VLAN OUI table.
             Syntax: <oui_addr> [<description>]"
        ::= { ctcVoiceVLAN 6 }

    -- 1.3.6.1.4.1.4756.103.140.7
    ctcDeleteVoiceVlanOUI OBJECT-TYPE
        SYNTAX      DisplayString
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Delete an entry from voice VLAN OUI table.
             Syntax: <oui_addr>"
        ::= { ctcVoiceVLAN 7 }

    -- 1.3.6.1.4.1.4756.103.140.8
    ctcVoiceVlanOuiTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF CtcVoiceVlanOuiEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table is used for Configure VOICE VLAN OUI."
        ::= { ctcVoiceVLAN 8 }

    -- 1.3.6.1.4.1.4756.103.140.8.1
    ctcVoiceVlanOuiEntry OBJECT-TYPE
        SYNTAX      CtcVoiceVlanOuiEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Entry of VOICE VLAN OUI table."
        INDEX { ctcVoiceVlanOuiIndex }
        ::= { ctcVoiceVlanOuiTable 1 }

    CtcVoiceVlanOuiEntry ::=
        SEQUENCE {
            ctcVoiceVlanOuiIndex         Integer32,          
            ctcVoiceVlanTelephonyOUI     OCTET STRING,
            ctcVoiceVlanOuiDescription   OCTET STRING
        }

        -- 1.3.6.1.4.1.4756.103.140.8.1.1    
        ctcVoiceVlanOuiIndex OBJECT-TYPE
            SYNTAX      Integer32 (1..16)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The maximum entry number is 16.
                 Modifying the OUI table will restart auto detection of OUI process."
            ::= { ctcVoiceVlanOuiEntry 1 }

        -- 1.3.6.1.4.1.4756.103.140.8.1.2    
        ctcVoiceVlanTelephonyOUI OBJECT-TYPE
            SYNTAX      OCTET STRING (SIZE (8))
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "A telephony OUI address is a globally unique identifier assigned to a vendor by IEEE.
                 It must be 6 characters long and the input format is xx-xx-xx (x is a hexadecimal digit)."
            ::= { ctcVoiceVlanOuiEntry 2 }

        -- 1.3.6.1.4.1.4756.103.140.8.1.3    
        ctcVoiceVlanOuiDescription OBJECT-TYPE
            SYNTAX      OCTET STRING (SIZE (0..32))
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "The description of OUI address.
                 Normally, it describes which vendor telephony device it belongs to.
                 The allowed string length is 0 to 32."
            ::= { ctcVoiceVlanOuiEntry 3 }


-- QoS
    -- 1.3.6.1.4.1.4756.103.150
    ctcQoS  OBJECT IDENTIFIER ::= { gsw1005ms 150 }

    -- 1.3.6.1.4.1.4756.103.150.1
    ctcQosIngress  OBJECT IDENTIFIER ::= { ctcQoS 1 }

    -- 1.3.6.1.4.1.4756.103.150.1.1
    ctcQosRxPortClassifiedTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF CtcQosRxPortClassifiedEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table allows you to configure the basic QoS Ingress Classification settings for all ports."
        ::= { ctcQosIngress 1 }

    -- 1.3.6.1.4.1.4756.103.150.1.1.1
    ctcQosRxPortClassifiedEntry OBJECT-TYPE
        SYNTAX      CtcQosRxPortClassifiedEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The port entry for which the configuration in table."
        INDEX { ctcPortIndex }
        ::= { ctcQosRxPortClassifiedTable 1 }

    CtcQosRxPortClassifiedEntry ::=
        SEQUENCE {
            ctcQosRxPortQosClass         Integer32,
            ctcQosRxPortDpLevel          Integer32,
            ctcQosRxPortPCP              Integer32,
            ctcQosRxPortDEI              Integer32,
            ctcQosRxPortTagClassified    INTEGER,
            ctcQosRxPortDscpBased        TruthValue
        }

        -- 1.3.6.1.4.1.4756.103.150.1.1.1.1    
        ctcQosRxPortQosClass OBJECT-TYPE
            SYNTAX      Integer32 (0..7)
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Controls the default QoS class,
                 i.e., the QoS class for frames not classified in any other way.
                 There is a one to one mapping between QoS class, queue and priority.
                 A QoS class of 0 (zero) has the lowest priority."
            ::= { ctcQosRxPortClassifiedEntry 1 }

        -- 1.3.6.1.4.1.4756.103.150.1.1.1.2    
        ctcQosRxPortDpLevel OBJECT-TYPE
            SYNTAX      Integer32 (0..1)
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Controls the default DP(Drop Precedence) level,
                 i.e., the DP level for frames not classified in any other way."
            ::= { ctcQosRxPortClassifiedEntry 2 }

        -- 1.3.6.1.4.1.4756.103.150.1.1.1.3    
        ctcQosRxPortPCP OBJECT-TYPE
            SYNTAX      Integer32 (0..7)
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Controls the default PCP(Priority Code Point) for untagged frames."
            ::= { ctcQosRxPortClassifiedEntry 3 }

        -- 1.3.6.1.4.1.4756.103.150.1.1.1.4    
        ctcQosRxPortDEI OBJECT-TYPE
            SYNTAX      Integer32 (0..1)
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Controls the default DEI(Data Encryption Standard) for untagged frames."
            ::= { ctcQosRxPortClassifiedEntry 4 }

        -- 1.3.6.1.4.1.4756.103.150.1.1.1.5    
        ctcQosRxPortTagClassified OBJECT-TYPE
            SYNTAX      INTEGER {
                            enabled(1),
                            disabled(2)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "The classification mode for tagged frames on this port.
                 Disabled: Use default QoS class and DP level for tagged frames.
                 Enabled: Use mapped versions of PCP and DEI for tagged frames."
            ::= { ctcQosRxPortClassifiedEntry 5 }

        -- 1.3.6.1.4.1.4756.103.150.1.1.1.6    
        ctcQosRxPortDscpBased OBJECT-TYPE
            SYNTAX      TruthValue
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "To enable DSCP Based QoS Ingress Port Classification."
            ::= { ctcQosRxPortClassifiedEntry 6 }

    -- 1.3.6.1.4.1.4756.103.150.1.2
