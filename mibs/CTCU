    -- 1.3.6.1.4.1.4756.103
    gsw1005ms OBJECT IDENTIFIER ::= { ctc 103 }
                              
-- System                              
    -- 1.3.6.1.4.1.4756.103.10
    ctcSystem OBJECT IDENTIFIER ::= { gsw1005ms 10 }

    -- 1.3.6.1.4.1.4756.103.10.1
    ctcSystemInformation OBJECT IDENTIFIER ::= { ctcSystem 1 }
            
    -- 1.3.6.1.4.1.4756.103.10.1.1
    ctcChipRevision OBJECT-TYPE
        SYNTAX      DisplayString
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The chip revision of this switch."
        ::= { ctcSystemInformation 1 }
             
    -- 1.3.6.1.4.1.4756.103.10.1.2
    ctcBaseMacAddress OBJECT-TYPE
        SYNTAX      MacAddress
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The MAC Address of this switch."
        ::= { ctcSystemInformation 2 }
             
    -- 1.3.6.1.4.1.4756.103.10.1.3
    ctcSoftWareVersion OBJECT-TYPE
        SYNTAX      DisplayString
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The software version of this switch."
        ::= { ctcSystemInformation 3 }
            
    -- 1.3.6.1.4.1.4756.103.10.1.4
    ctcSoftWareDate OBJECT-TYPE
        SYNTAX      DisplayString
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The date when the switch software was produced."
        ::= { ctcSystemInformation 4 }
            
    -- 1.3.6.1.4.1.4756.103.10.1.5
    ctcConfigurationFile OBJECT-TYPE
        SYNTAX      DisplayString
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The filename when the switch configuration was produced."
        ::= { ctcSystemInformation 5 }

    -- 1.3.6.1.4.1.4756.103.10.1.7
    ctcDhcpOption60 OBJECT-TYPE
        SYNTAX      DisplayString
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Configure the DHCP option 60 vendor class ID.
             The allowed string length is 0 to 60."
        ::= { ctcSystemInformation 7 }
              
    -- 1.3.6.1.4.1.4756.103.10.1.8
    ctcDnsServer OBJECT-TYPE
        SYNTAX      IpAddress
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Provide the IP address of the DNS Server in dotted decimal notation."
        ::= { ctcSystemInformation 8 }
            
    -- 1.3.6.1.4.1.4756.103.10.1.9
    ctcSystemTime OBJECT-TYPE
        SYNTAX      DisplayString
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The current (GMT) system time and date."
        ::= { ctcSystemInformation 9 }
            
    -- 1.3.6.1.4.1.4756.103.10.2
    ctcNTP OBJECT IDENTIFIER ::= { ctcSystem 2 }

    -- 1.3.6.1.4.1.4756.103.10.2.1
    ctcNtpMode OBJECT-TYPE
        SYNTAX      INTEGER {
                        enabled(1),
                        disabled(2)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Indicates the NTP mode operation. Possible modes are:
             Enabled: Enable NTP mode operation. When NTP mode operation is enabled,
                      the agent forwards NTP messages between the clients and the server
                      when they are not on the same subnet domain.
             Disabled: Disable NTP mode operation. "
        ::= { ctcNTP 1 }

    -- 1.3.6.1.4.1.4756.103.10.2.2
    ctcNtpServerTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF CtcNtpServerEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The switch system log information."
        ::= { ctcNTP 2 }

    -- 1.3.6.1.4.1.4756.103.10.2.2.1
    ctcNtpServerEntry OBJECT-TYPE
        SYNTAX      CtcNtpServerEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The system log entry."
        INDEX { ctcNtpServerIndex }
        ::= { ctcNtpServerTable 1 }

    CtcNtpServerEntry ::=
        SEQUENCE {
            ctcNtpServerIndex        Integer32,
            ctcNtpServerAddress      DisplayString
        }

        -- 1.3.6.1.4.1.4756.103.10.2.2.1.1
        ctcNtpServerIndex OBJECT-TYPE
            SYNTAX      Integer32 (1..5)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The Index of the NTP server."
            ::= { ctcNtpServerEntry 1 }

        -- 1.3.6.1.4.1.4756.103.10.2.2.1.2
        ctcNtpServerAddress OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "It must either be a valid IP address or a valid hostname.
                 A valid hostname is a string drawn from the alphabet(A-Za-z), digits(0-9), dot(.), dash(-).
                 Spaces are not allowed, the first character must be an alpha character,
                 and the first and last characters must not be a dot or a dash.
                 IPv4 address is in dotted decimal notation ('x.y.z.w').
                 IPv6 address is in 128-bit records represented as eight fields of up to
                 four hexadecimal digits with a colon separating each field (:)."
            ::= { ctcNtpServerEntry 2 }
 


