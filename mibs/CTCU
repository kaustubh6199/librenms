    -- 1.3.6.1.4.1.4756.103.90.1.60
    ctcIgmpSfmInfoTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF CtcIgmpSfmInfoEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table shows IGMP SSM(Source-Filtered Multicast) information.
             The IGMP SFM Information Table also contains the SSM(Source-Specific Multicast) information."
        ::= { ctcIgmpSnooping 60 }

    -- 1.3.6.1.4.1.4756.103.90.1.60.1
    ctcIgmpSfmInfoEntry OBJECT-TYPE
        SYNTAX      CtcIgmpSfmInfoEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Entry of IGMP SSM information."
        INDEX { ctcIgmpSfmInfoIndex }
        ::= { ctcIgmpSfmInfoTable 1 }

    CtcIgmpSfmInfoEntry ::=
        SEQUENCE {
            ctcIgmpSfmInfoIndex              Integer32,
            ctcIgmpSfmInfoVlanID             Integer32,
            ctcIgmpSfmInfoGroup              IpAddress,          
            ctcIgmpSfmPortNumber             Integer32,
            ctcIgmpSfmMode                   INTEGER,
            ctcIgmpSfmSourceAddress          IpAddress,
            ctcIgmpSfmType                   INTEGER,
            ctcIgmpSfmHardwareFilterSwitch   INTEGER
        }

        -- 1.3.6.1.4.1.4756.103.90.1.60.1.1    
        ctcIgmpSfmInfoIndex OBJECT-TYPE
            SYNTAX      Integer32 (1..4095)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Entries in the IGMP SSM information table."
            ::= { ctcIgmpSfmInfoEntry 1 }

        -- 1.3.6.1.4.1.4756.103.90.1.60.1.2    
        ctcIgmpSfmInfoVlanID OBJECT-TYPE
            SYNTAX      Integer32 (1..4095)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "VLAN ID of the group."
            ::= { ctcIgmpSfmInfoEntry 2 }

        -- 1.3.6.1.4.1.4756.103.90.1.60.1.3    
        ctcIgmpSfmInfoGroup OBJECT-TYPE
            SYNTAX      IpAddress
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Group address of the entry."
            ::= { ctcIgmpSfmInfoEntry 3 }

        -- 1.3.6.1.4.1.4756.103.90.1.60.1.4    
        ctcIgmpSfmPortNumber OBJECT-TYPE
            SYNTAX      Integer32 (1..6)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Switch port number."
            ::= { ctcIgmpSfmInfoEntry 4 }

        -- 1.3.6.1.4.1.4756.103.90.1.60.1.5    
        ctcIgmpSfmMode OBJECT-TYPE
            SYNTAX      INTEGER {
                            exclude(1),
                            include(2)
                        }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the filtering mode maintained per (VLAN ID, port number, Group Address) basis.
                 It can be either Include or Exclude."
            ::= { ctcIgmpSfmInfoEntry 5 }

        -- 1.3.6.1.4.1.4756.103.90.1.60.1.6    
        ctcIgmpSfmSourceAddress OBJECT-TYPE
            SYNTAX      IpAddress
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "IP Address of the source.
                 Currently, system limits the total number of IP source addresses for filtering to be 128."
            ::= { ctcIgmpSfmInfoEntry 6 }

        -- 1.3.6.1.4.1.4756.103.90.1.60.1.7    
        ctcIgmpSfmType OBJECT-TYPE
            SYNTAX      INTEGER {
                            deny(1),
                            allow(2)
                        }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the Type. It can be either Allow or Deny."
            ::= { ctcIgmpSfmInfoEntry 7 }

        -- 1.3.6.1.4.1.4756.103.90.1.60.1.8    
        ctcIgmpSfmHardwareFilterSwitch OBJECT-TYPE
            SYNTAX      INTEGER {
                            no(1),
                            yes(2)
                        }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates whether data plane destined to the specific group address from
                 the source IPv4 address could be handled by chip or not."
            ::= { ctcIgmpSfmInfoEntry 8 }

