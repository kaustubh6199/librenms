            SYNTAX      INTEGER {
                            yes(1),
                            no(2)
                        }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates whether data plane destined to the specific group address from
                 the source IPv4/IPv6 address could be handled by chip or not."   
            ::= { ctcMvrSfmInformationEntry 6 }

             
-- IPMC
    -- 1.3.6.1.4.1.4756.103.90
    ctcIPMC  OBJECT IDENTIFIER ::= { gsw1005ms 90 }

    -- 1.3.6.1.4.1.4756.103.90.1
    ctcIgmpSnooping  OBJECT IDENTIFIER ::= { ctcIPMC 1 }

    -- 1.3.6.1.4.1.4756.103.90.1.1
    ctcIgmpSnoopingEnabled OBJECT-TYPE
        SYNTAX      TruthValue
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION                                  
            "Enable the Global IGMP Snooping."
        ::= { ctcIgmpSnooping 1 }

    -- 1.3.6.1.4.1.4756.103.90.1.2
    ctcUnregisteredIpmcV4FloodingEnabled OBJECT-TYPE
        SYNTAX      TruthValue
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION                                  
            "Enable unregistered IPMC traffic flooding."
        ::= { ctcIgmpSnooping 2 }

    -- 1.3.6.1.4.1.4756.103.90.1.3
    ctcIgmpSsmRange OBJECT-TYPE
        SYNTAX      DisplayString
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION                                  
            "SSM (Source-Specific Multicast) Range allows the SSM-aware hosts and
             routers run the SSM service model for the groups in the address range.
             Syntax: (Range <mask_len>)"
        ::= { ctcIgmpSnooping 3 }

    -- 1.3.6.1.4.1.4756.103.90.1.4
    ctcIgmpLeaveProxyEnabled OBJECT-TYPE
        SYNTAX      TruthValue
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION                                  
            "Enable IGMP Leave Proxy.
             This feature can be used to avoid forwarding unnecessary leave messages to
             the router side."
        ::= { ctcIgmpSnooping 4 }

    -- 1.3.6.1.4.1.4756.103.90.1.5
    ctcIgmpProxyEnabled OBJECT-TYPE
        SYNTAX      TruthValue
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION                                  
            "Enable IGMP Proxy.
             This feature can be used to avoid forwarding unnecessary join and
             leave messages to the router side."
        ::= { ctcIgmpSnooping 5 }

    -- 1.3.6.1.4.1.4756.103.90.1.10
    ctcIgmpPortRelatedTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF CtcIgmpPortRelatedEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table is used for configuring the Port Related."
        ::= { ctcIgmpSnooping 10 }

    -- 1.3.6.1.4.1.4756.103.90.1.10.1
    ctcIgmpPortRelatedEntry OBJECT-TYPE
        SYNTAX      CtcIgmpPortRelatedEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Entry of Port Related configurations."
        INDEX { ctcPortIndex }
        ::= { ctcIgmpPortRelatedTable 1 }

    CtcIgmpPortRelatedEntry ::=
        SEQUENCE {
            ctcIgmpRouterPortEnabled TruthValue,
            ctcIgmpRouterPortStatus  INTEGER,          
            ctcIgmpFastLeaveEnabled  TruthValue,
            ctcIgmpThrottling        Integer32
        }

        -- 1.3.6.1.4.1.4756.103.90.1.10.1.1    
        ctcIgmpRouterPortEnabled OBJECT-TYPE
            SYNTAX      TruthValue
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Specify which ports act as router ports.
                 A router port is a port on the Ethernet switch that
                 leads towards the Layer 3 multicast device or IGMP querier.
                 If an aggregation member port is selected as a router port,
                 the whole aggregation will act as a router port."
            ::= { ctcIgmpPortRelatedEntry 1 }

        -- 1.3.6.1.4.1.4756.103.90.1.10.1.2    
        ctcIgmpRouterPortStatus OBJECT-TYPE
            SYNTAX      INTEGER {
                            none(1),
                            static(2),
                            dynamic(3),
                            both(4)
                        }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Shows IGMP router port mode."
            ::= { ctcIgmpPortRelatedEntry 2 }

        -- 1.3.6.1.4.1.4756.103.90.1.10.1.3    
        ctcIgmpFastLeaveEnabled OBJECT-TYPE
            SYNTAX      TruthValue
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Enable the fast leave on the port."
            ::= { ctcIgmpPortRelatedEntry 3 }

        -- 1.3.6.1.4.1.4756.103.90.1.10.1.4    
        ctcIgmpThrottling OBJECT-TYPE
            SYNTAX      Integer32 (0..10)
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Enable to limit the number of multicast groups to which a switch port can belong.
                 Set to zero for unlimited."
            ::= { ctcIgmpPortRelatedEntry 4 }

    -- 1.3.6.1.4.1.4756.103.90.1.20
    ctcAddIgmpVlan OBJECT-TYPE
        SYNTAX      Integer32 (1..4095)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION                                  
            "Add a new entry to the IGMP VLAN table.
             Specify the VID and configure the new entry.
             The specific IGMP VLAN starts working after the corresponding static VLAN is also created."
        ::= { ctcIgmpSnooping 20 }

    -- 1.3.6.1.4.1.4756.103.90.1.21
    ctcDeleteIgmpVlan OBJECT-TYPE
        SYNTAX      Integer32 (1..4095)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Delete an IGMP VLAN from table."
        ::= { ctcIgmpSnooping 21 }

    -- 1.3.6.1.4.1.4756.103.90.1.22
    ctcIgmpVlanConfigTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF CtcIgmpVlanConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table allows for configure IGMP snooping VLAN."
        ::= { ctcIgmpSnooping 22 }

    -- 1.3.6.1.4.1.4756.103.90.1.22.1
    ctcIgmpVlanConfigEntry OBJECT-TYPE
        SYNTAX      CtcIgmpVlanConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Entry of IGMP snooping VLAN."
        INDEX { ctcIgmpVlanID }
        ::= { ctcIgmpVlanConfigTable 1 }

    CtcIgmpVlanConfigEntry ::=
        SEQUENCE {     
            ctcIgmpVlanID            Integer32,
            ctcIgmpPerVlanEnabled    TruthValue,
            ctcIgmpQuerierEnabled    TruthValue,
            ctcIgmpCompatibility     INTEGER,
            ctcIgmpRV                Integer32,
            ctcIgmpQI                Integer32,
            ctcIgmpQRI               Integer32,
            ctcIgmpLLQI              Integer32,
            ctcIgmpURI               Integer32           
        }

        -- 1.3.6.1.4.1.4756.103.90.1.22.1.1    
        ctcIgmpVlanID OBJECT-TYPE
            SYNTAX      Integer32 (1..4095)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the ID of this particular IGMP snooping VLAN."
            ::= { ctcIgmpVlanConfigEntry 1 }

        -- 1.3.6.1.4.1.4756.103.90.1.22.1.2    
        ctcIgmpPerVlanEnabled OBJECT-TYPE
            SYNTAX      TruthValue
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Enable the per-VLAN IGMP Snooping. Only up to 64 VLANs can be selected."
            ::= { ctcIgmpVlanConfigEntry 2 }

        -- 1.3.6.1.4.1.4756.103.90.1.22.1.3    
        ctcIgmpQuerierEnabled OBJECT-TYPE
            SYNTAX      TruthValue
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Enable the IGMP Querier in the VLAN."
            ::= { ctcIgmpVlanConfigEntry 3 }

        -- 1.3.6.1.4.1.4756.103.90.1.22.1.4    
        ctcIgmpCompatibility OBJECT-TYPE
            SYNTAX      INTEGER {
                            forcedIgmpV1(1),
                            forcedIgmpV2(2),
                            forcedIgmpV3(3),
            	            igmpAuto(4)
            	        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Compatibility is maintained by hosts and routers taking appropriate actions depending on
                 the versions of IGMP operating on hosts and routers within a network.
                 The allowed selection is IGMP-Auto, Forced IGMPv1, Forced IGMPv2, Forced IGMPv3,
                 default compatibility value is IGMP-Auto."
            DEFVAL      { igmpAuto }
            ::= { ctcIgmpVlanConfigEntry 4 }

        -- 1.3.6.1.4.1.4756.103.90.1.22.1.5    
        ctcIgmpRV OBJECT-TYPE
            SYNTAX      Integer32 (1..255)
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Robustness Variable.
                 The Robustness Variable allows tuning for the expected packet loss on a network.
                 The allowed range is 1 to 255, default robustness variable value is 2,
                 Robustness Variable SHOULD NOT be one."
            DEFVAL     { 2 }
            ::= { ctcIgmpVlanConfigEntry 5 }

        -- 1.3.6.1.4.1.4756.103.90.1.22.1.6    
        ctcIgmpQI OBJECT-TYPE
            SYNTAX      Integer32 (1..255)
            UNITS       "Seconds"
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Query Interval.
                 The Query Interval is the interval between General Queries sent by the Querier.
                 The allowed range is 1 to 255 seconds, default query interval is 125 seconds.
                 Note: The number of seconds represented by QRI must be less than QI!"
            DEFVAL     { 125 }     
            ::= { ctcIgmpVlanConfigEntry 6 }

        -- 1.3.6.1.4.1.4756.103.90.1.22.1.7    
        ctcIgmpQRI OBJECT-TYPE
            SYNTAX      Integer32 (0..31744)
            UNITS       "100 millisecond"
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Query Response Interval.
                 The Max Response Time used to calculate the Max Resp Code inserted into the periodic General Queries.
                 The allowed range is 0 to 31744 in tenths of seconds,
                 default query response interval is 100 in tenths of seconds (10 seconds).
                 The number of seconds represented by QRI must be less than QI!"
            DEFVAL     { 100 }     
            ::= { ctcIgmpVlanConfigEntry 7 }

        -- 1.3.6.1.4.1.4756.103.90.1.22.1.8    
        ctcIgmpLLQI OBJECT-TYPE
            SYNTAX      Integer32 (0..31744)
            UNITS       "100 millisecond"
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Last Member Query Interval.
                 The Last Member Query Time is the time value represented by the Last Member Query Interval,
                 multiplied by the Last Member Query Count.
                 The allowed range is 0 to 31744 in tenths of seconds,
                 default last member query interval is 10 in tenths of seconds (1 second)."
            DEFVAL     { 10 }     
            ::= { ctcIgmpVlanConfigEntry 8 }

        -- 1.3.6.1.4.1.4756.103.90.1.22.1.9    
        ctcIgmpURI OBJECT-TYPE
            SYNTAX      Integer32 (0..31744)
            UNITS       "Seconds"
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Unsolicited Report Interval.
                 The Unsolicited Report Interval is the time between repetitions of
                 a host's initial report of membership in a group.
                 The allowed range is 0 to 31744 seconds,
                 default unsolicited report interval is 1 second."
            DEFVAL     { 1 }     
            ::= { ctcIgmpVlanConfigEntry 9 }

    -- 1.3.6.1.4.1.4756.103.90.1.30
    ctcAddIgmpPortFilteringGroup OBJECT-TYPE
        SYNTAX      DisplayString
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION                                  
            "Add a new entry to the Group Filtering table.
             Specify the Port, and Filtering Group of the new entry.
             Syntax: <port_no> <group_addr>
             The IGMP Snooping can support maximum 5 entries per port."
