    -- 1.3.6.1.4.1.4756.103.50.10
    ctcPortsQclStatusTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF CtcPortsQclStatusEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table shows the QCL status by different QCL users."
        ::= { ctcPorts 10 }

    -- 1.3.6.1.4.1.4756.103.50.10.1
    ctcPortsQclStatusEntry OBJECT-TYPE
        SYNTAX      CtcPortsQclStatusEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "Each entry describes the QCE that is defined."
        INDEX { ctcQclStatusIndex }
        ::= { ctcPortsQclStatusTable 1 }

    CtcPortsQclStatusEntry ::=
        SEQUENCE {
            ctcQclStatusIndex    Integer32,
            ctcQclUser           INTEGER,
            ctcQCEntryNumber     Integer32,
            ctcQclFrameType      INTEGER,
            ctcQclPort           DisplayString,
            ctcQclActionClass    Integer32,
            ctcQclActionDPL      Integer32,
            ctcQclActionDSCP     Integer32,
            ctcQclConflict       TruthValue
        }

        -- 1.3.6.1.4.1.4756.103.50.10.1.1
        ctcQclStatusIndex OBJECT-TYPE
            SYNTAX      Integer32 (1..240)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The index of QoS Control List Status."
            ::= { ctcPortsQclStatusEntry 1 }
                  
        -- 1.3.6.1.4.1.4756.103.50.10.1.2    
        ctcQclUser OBJECT-TYPE
            SYNTAX      INTEGER {
                            static(1),
                            voiceVLAN(2)
                        }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the QCL user."
            ::= { ctcPortsQclStatusEntry 2 }
                  
        -- 1.3.6.1.4.1.4756.103.50.10.1.3    
        ctcQCEntryNumber OBJECT-TYPE
            SYNTAX      Integer32
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the index of QCE."
            ::= { ctcPortsQclStatusEntry 3 }
                  
        -- 1.3.6.1.4.1.4756.103.50.10.1.4    
        ctcQclFrameType OBJECT-TYPE
            SYNTAX      INTEGER {
                            any(1),
                            ethernet(2),
                            llc(3),
                            snap(4),
                            ipv4(5),
                            ipv6(6)
                        }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the type of frame to look for incomming frames. Possible frame types are:
                 Any: The QCE will match all frame type.
                 Ethernet: Only Ethernet frames (with Ether Type 0x600-0xFFFF) are allowed.
                 LLC: Only (LLC) frames are allowed.
                 LLC: Only (SNAP) frames are allowed.
                 IPv4: The QCE will match only IPV4 frames.
                 IPv6: The QCE will match only IPV6 frames."
            ::= { ctcPortsQclStatusEntry 4 }
                  
        -- 1.3.6.1.4.1.4756.103.50.10.1.5    
        ctcQclPort OBJECT-TYPE
            SYNTAX      DisplayString (SIZE (1..128))
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the list of ports configured with the QCE."
            ::= { ctcPortsQclStatusEntry 5 }     
                  
        -- 1.3.6.1.4.1.4756.103.50.10.1.6    
        ctcQclActionClass OBJECT-TYPE
            SYNTAX      Integer32  (-1 | 0..7)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Classified QoS Class, if a frame matches the QCE it will be put in the queue.
                 -1: default
                 0 to 7: class0 to class7"
            DEFVAL      { -1 }
            ::= { ctcPortsQclStatusEntry 6 }
                  
        -- 1.3.6.1.4.1.4756.103.50.10.1.7    
        ctcQclActionDPL OBJECT-TYPE
            SYNTAX      Integer32 (-1 | 0..1)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Drop Precedence Level,
                 if a frame matches the QCE then DP level will set to value displayed under DPL column.
                 -1: default
                 0, 1: DPL0 and DPL1"
            DEFVAL      { -1 }
            ::= { ctcPortsQclStatusEntry 7 }
                  
        -- 1.3.6.1.4.1.4756.103.50.10.1.8    
        ctcQclActionDSCP OBJECT-TYPE
            SYNTAX      Integer32 (-1 | 0..63)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "If a frame matches the QCE then DSCP will be classified with the value displayed under DSCP column.
                 -1: default
                 0 to 63: DSCP0 to DSCP63"
            DEFVAL      { -1 }
            ::= { ctcPortsQclStatusEntry 8 }
                  
        -- 1.3.6.1.4.1.4756.103.50.10.1.9    
        ctcQclConflict OBJECT-TYPE
            SYNTAX      TruthValue
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Displays QCE status. It may happen that resources required to add a QCE may not available."
            ::= { ctcPortsQclStatusEntry 9 }


-- Security
    -- 1.3.6.1.4.1.4756.103.60
    ctcSecurity OBJECT IDENTIFIER ::= { gsw1005ms 60 }

    -- 1.3.6.1.4.1.4756.103.60.1
    ctcSecuritySwitch OBJECT IDENTIFIER ::= { ctcSecurity 1 }

    -- 1.3.6.1.4.1.4756.103.60.1.1    
    ctcAddUser OBJECT-TYPE
        SYNTAX      OCTET STRING (SIZE (1..32))
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "A string identifying the user name that this entry should belong to.
             The allowed string length is 1 to 32.
             The valid user name is a combination of letters, numbers and underscores."
        ::= { ctcSecuritySwitch 1 }

    -- 1.3.6.1.4.1.4756.103.60.1.2    
    ctcDeleteUser OBJECT-TYPE
        SYNTAX      OCTET STRING (SIZE (1..32))
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Delete the user by name."
        ::= { ctcSecuritySwitch 2 }

    -- 1.3.6.1.4.1.4756.103.60.1.3
    ctcUserConfigTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF CtcUserConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table provides User Configurations."
        ::= { ctcSecuritySwitch 3 }

    -- 1.3.6.1.4.1.4756.103.60.1.3.1
    ctcUserConfigEntry OBJECT-TYPE
        SYNTAX      CtcUserConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Entry of User Configurations."
        INDEX { ctcUserIndex }
        ::= { ctcUserConfigTable 1 }

    CtcUserConfigEntry ::=
        SEQUENCE {
            ctcUserIndex             Integer32,
            ctcUserName              OCTET STRING,
            ctcUserPassword          OCTET STRING,
            ctcUserPrivilegeLevel    Integer32
        }

        -- 1.3.6.1.4.1.4756.103.60.1.3.1.1
        ctcUserIndex OBJECT-TYPE
            SYNTAX      Integer32 (1..20)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The index of user entries."
            ::= { ctcUserConfigEntry 1 }

        -- 1.3.6.1.4.1.4756.103.60.1.3.1.2
        ctcUserName OBJECT-TYPE
            SYNTAX      OCTET STRING (SIZE (1..32))
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The name identifying the user."
            ::= { ctcUserConfigEntry 2 }

        -- 1.3.6.1.4.1.4756.103.60.1.3.1.3
        ctcUserPassword OBJECT-TYPE
            SYNTAX      OCTET STRING (SIZE (0..32))
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "The password of the user. The allowed string length is 0 to 32."
            ::= { ctcUserConfigEntry 3 }

        -- 1.3.6.1.4.1.4756.103.60.1.3.1.4
        ctcUserPrivilegeLevel OBJECT-TYPE
            SYNTAX      Integer32 (1..15)
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "The privilege level of the user. The allowed range is 1 to 15.
                 If the privilege level value is 15, it can access all groups,
                 i.e. that is granted the fully control of the device.
                 But others value need to refer to each group privilege level.
                 User's privilege should be same or greater than the group privilege level to have the access of that group.
                 By default setting, most groups privilege level 5 has the read-only access
                 and privilege level 10 has the read-write access. And the system maintenance
                 (software upload, factory defaults and etc.) need user privilege level 15.
                 Generally, the privilege level 15 can be used for an administrator account,
                 privilege level 10 for a standard user account and privilege level 5 for a guest account."
            ::= { ctcUserConfigEntry 4 }

    -- 1.3.6.1.4.1.4756.103.60.1.4
    ctcPrivilegeLevelTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF CtcPrivilegeLevelEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table provides privilege levels configurations."
        ::= { ctcSecuritySwitch 4 }

    -- 1.3.6.1.4.1.4756.103.60.1.4.1
    ctcPrivilegeLevelEntry OBJECT-TYPE
        SYNTAX      CtcPrivilegeLevelEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The entry of privilege groups."
        INDEX { ctcPrivilegeGroupID }
        ::= { ctcPrivilegeLevelTable 1 }

    CtcPrivilegeLevelEntry ::=
        SEQUENCE {
            ctcPrivilegeGroupID                  Integer32,
            ctcPrivilegeGroupName                DisplayString,
            ctcPrivilegeConfigReadOnlyLevel      Integer32,
            ctcPrivilegeConfigReadWriteLevel     Integer32,
            ctcPrivilegeStatusReadOnlyLevel      Integer32,
            ctcPrivilegeStatusReadWriteLevel     Integer32
        }

        -- 1.3.6.1.4.1.4756.103.60.1.4.1.1
        ctcPrivilegeGroupID OBJECT-TYPE
            SYNTAX      Integer32 (1..26)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The ID of privilege groups."
            ::= { ctcPrivilegeLevelEntry 1 }

        -- 1.3.6.1.4.1.4756.103.60.1.4.1.2
        ctcPrivilegeGroupName OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The name identifying the privilege group. In most cases,
                 a privilege level group consists of a single module (e.g. Ports, VLANs or QoS),
                 but a few of them contains more than one."
            ::= { ctcPrivilegeLevelEntry 2 }

        -- 1.3.6.1.4.1.4756.103.60.1.4.1.3
        ctcPrivilegeConfigReadOnlyLevel OBJECT-TYPE
            SYNTAX      Integer32 (1..15)
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "The authorization privilege level for configuration read-only of the group.
                 The privilege level of 'Read-only' should be less or equal 'Read/write'."
            ::= { ctcPrivilegeLevelEntry 3 }

        -- 1.3.6.1.4.1.4756.103.60.1.4.1.4
        ctcPrivilegeConfigReadWriteLevel OBJECT-TYPE
            SYNTAX      Integer32 (1..15)
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "The authorization privilege level for configuration/execute read-write of the group.
                 The privilege level of 'Configuration/Execute Read/write'
                 should be great or equal 'Status/Statistics Read-only'"
            ::= { ctcPrivilegeLevelEntry 4 }

        -- 1.3.6.1.4.1.4756.103.60.1.4.1.5
        ctcPrivilegeStatusReadOnlyLevel OBJECT-TYPE
            SYNTAX      Integer32 (1..15)
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "The authorization privilege level for status/statistics read-only of the group.
                 The privilege level of 'Read-only' should be less or equal 'Read/write'."
            ::= { ctcPrivilegeLevelEntry 5 }

        -- 1.3.6.1.4.1.4756.103.60.1.4.1.6
        ctcPrivilegeStatusReadWriteLevel OBJECT-TYPE
            SYNTAX      Integer32 (1..15)
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "The authorization privilege level for status/statistics read-write
                 (e.g. for clearing of statistics) of the group."
            ::= { ctcPrivilegeLevelEntry 6 }

    -- 1.3.6.1.4.1.4756.103.60.1.5
    ctcAuthenticationMethodTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF CtcAuthenticationMethodEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table allows you to configure how a user is authenticated when
             he logs into the switch via one of the management client interfaces."
        ::= { ctcSecuritySwitch 5 }

    -- 1.3.6.1.4.1.4756.103.60.1.5.1
    ctcAuthenticationMethodEntry OBJECT-TYPE
        SYNTAX      CtcAuthenticationMethodEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The entry of the management client interfaces."
        INDEX { ctcAuthenticationClientIndex }
        ::= { ctcAuthenticationMethodTable 1 }

    CtcAuthenticationMethodEntry ::=
        SEQUENCE {
            ctcAuthenticationClientIndex         Integer32,
            ctcAuthenticationClientInterface     DisplayString,
            ctcAuthenticationMethod              INTEGER,
            ctcAuthenticationFallbackEnable      TruthValue
        }

        -- 1.3.6.1.4.1.4756.103.60.1.5.1.1
        ctcAuthenticationClientIndex OBJECT-TYPE
            SYNTAX      Integer32 (1..4)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The index of the management client interfaces."
            ::= { ctcAuthenticationMethodEntry 1 }

        -- 1.3.6.1.4.1.4756.103.60.1.5.1.2
        ctcAuthenticationClientInterface OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The management client interface."
            ::= { ctcAuthenticationMethodEntry 2 }

        -- 1.3.6.1.4.1.4756.103.60.1.5.1.3
        ctcAuthenticationMethod OBJECT-TYPE
            SYNTAX      INTEGER {
                            none(1),
                            local(2),
                            radius(3),
                            tacacsPlus(4)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Authentication Method can be set to one of the following values:
                 none : authentication is disabled and login is not possible.
                 local : use the local user database on the switch for authentication.
                 radius : use a remote RADIUS server for authentication.
                 tacacs+ : use a remote TACACS+ server for authentication."
            ::= { ctcAuthenticationMethodEntry 3 }

        -- 1.3.6.1.4.1.4756.103.60.1.5.1.4
        ctcAuthenticationFallbackEnable OBJECT-TYPE
            SYNTAX      TruthValue
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Enable fallback to local authentication.
                 If none of the configured authentication servers are alive,
                 the local user database is used for authentication.
                 This is only possible if the Authentication Method is set to a value other than 'none' or 'local'."
            ::= { ctcAuthenticationMethodEntry 4 }

    -- 1.3.6.1.4.1.4756.103.60.1.6    
    ctcSshMode OBJECT-TYPE
        SYNTAX      INTEGER {
                        enabled(1),
                        disabled(2)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Indicates the SSH mode operation. Possible modes are: 
             Enabled: Enable SSH mode operation. 
             Disabled: Disable SSH mode operation."
        ::= { ctcSecuritySwitch 6 }

    -- 1.3.6.1.4.1.4756.103.60.1.7    
    ctcHttpsMode OBJECT-TYPE
        SYNTAX      INTEGER {
                        enabled(1),
                        disabled(2)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Indicates the HTTPS mode operation. Possible modes are: 
             Enabled: Enable HTTPS mode operation. 
             Disabled: Disable HTTPS mode operation."
        ::= { ctcSecuritySwitch 7 }

    -- 1.3.6.1.4.1.4756.103.60.1.8    
    ctcHttpsAutomaticRedirect OBJECT-TYPE
        SYNTAX      INTEGER {
                        enabled(1),
                        disabled(2)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Indicates the HTTPS redirect mode operation.
             Automatically redirect web browser to HTTPS when HTTPS mode is enabled. Possible modes are: 
             Enabled: Enable HTTPS redirect mode operation. 
             Disabled: Disable HTTPS redirect mode operation."
        ::= { ctcSecuritySwitch 8 }

    -- 1.3.6.1.4.1.4756.103.60.1.9
    ctcAccessManagement OBJECT IDENTIFIER ::= { ctcSecuritySwitch 9 }

    -- 1.3.6.1.4.1.4756.103.60.1.9.1    
    ctcAccessMgmtProtocolMode OBJECT-TYPE
        SYNTAX      INTEGER {
                        enabled(1),
                        disabled(2)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Indicates the access management protocol mode operation. Possible modes are: 
             Enabled: Enable access management protocol mode operation,
                      and there must at least one entry provide SNMP service. 
             Disabled: Disable access management protocol mode operation."
        ::= { ctcAccessManagement 1 }

    -- 1.3.6.1.4.1.4756.103.60.1.9.2    
    ctcAddAccessMgmtProtocol OBJECT-TYPE
        SYNTAX      Integer32 (0 | 1..16)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Add a new access management entry: 
             Value 0: Add entry by the first valid ID of list. 
             Value 1~16: Insert new entry by this ID."
        ::= { ctcAccessManagement 2 }

    -- 1.3.6.1.4.1.4756.103.60.1.9.3    
