        ::= { ctcLedPower 2 }

    -- 1.3.6.1.4.1.4756.103.30.1.3
    ctcLedPowerTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF CtcLedPowerEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The LEDs power consumption can be reduced by lowering the LEDs intensity.
             LEDs intensity could for example be lowered during night time,
             or they could be turn completely off."
        ::= { ctcLedPower 3 }

    -- 1.3.6.1.4.1.4756.103.30.1.3.1
    ctcLedPowerEntry OBJECT-TYPE
        SYNTAX      CtcLedPowerEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "It is possible to configure 24 different hours of the day,
             at where the LEDs intensity should be set."
        INDEX { ctcLedTimerIndex }
        ::= { ctcLedPowerTable 1 }

    CtcLedPowerEntry ::=
        SEQUENCE {
            ctcLedTimerIndex    Integer32,
            ctcLedPowerTime     Integer32,
            ctcLedIntensity	    INTEGER
        }

        -- 1.3.6.1.4.1.4756.103.30.1.3.1.1
