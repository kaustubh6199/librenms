        SYNTAX      INTEGER {
                        enabled(1),
                        disabled(2)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Enable or disable the Auto-Provisioning.
             This feature automatic downloading of new software and configurations using DHCP and TFTP/HTTP/FTP."
        ::= { ctcAutoProvision 1 }
                
    -- 1.3.6.1.4.1.4756.103.10.6.2
