    -- 1.3.6.1.4.1.4756.103.150.3
    ctcQosDSCP  OBJECT IDENTIFIER ::= { ctcQoS 3 }

    -- 1.3.6.1.4.1.4756.103.150.3.1
    ctcQosPortDscpConfigTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF CtcQosPortDscpConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table allows you to configure the basic QoS Port DSCP Configuration settings
             for all ports."
        ::= { ctcQosDSCP 1 }

    -- 1.3.6.1.4.1.4756.103.150.3.1.1
    ctcQosPortDscpConfigEntry OBJECT-TYPE
        SYNTAX      CtcQosPortDscpConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The port entry for which the configuration in table."
        INDEX { ctcPortIndex }
        ::= { ctcQosPortDscpConfigTable 1 }

    CtcQosPortDscpConfigEntry ::=
        SEQUENCE {
            ctcQosPortDscpRxTranlateEnabled  TruthValue,
            ctcQosPortDscpRxClassifyMode     INTEGER,
            ctcQosPortDscpTxRewriteMode      INTEGER            
        }

        -- 1.3.6.1.4.1.4756.103.150.3.1.1.1    
        ctcQosPortDscpRxTranlateEnabled OBJECT-TYPE
            SYNTAX      TruthValue
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "To Enable the Ingress Translation."
            ::= { ctcQosPortDscpConfigEntry 1 }

        -- 1.3.6.1.4.1.4756.103.150.3.1.1.2    
        ctcQosPortDscpRxClassifyMode OBJECT-TYPE
            SYNTAX      INTEGER {
                            disabled(1),
                            dscp0(2),
                            selected(3),
                            all(4)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Classification for a port have 4 different values:
                 Disable: No Ingress DSCP Classification.
                 DSCP=0: Classify if incoming (or translated if enabled) DSCP is 0.
                 Selected: Classify only selected DSCP for which classification is enabled as
                           specified in DSCP Translation window for the specific DSCP.
                 All: Classify all DSCP."
            ::= { ctcQosPortDscpConfigEntry 2 }

        -- 1.3.6.1.4.1.4756.103.150.3.1.1.3    
        ctcQosPortDscpTxRewriteMode OBJECT-TYPE
            SYNTAX      INTEGER {
                            disabled(1),
                            enabled(2),
                            remapDpUnaware(3),
                            remapDpAware(4)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Port Egress Rewriting can be one of -
                 Disable: No Egress rewrite.
                 Enable: Rewrite enable without remapped.
                 Remap DP Unaware: DSCP remarked with DSCP value from
                                   analyzer remapped through remap table .
                 Remap DP Aware: DSCP remarked with DSCP value from
                                 analyzer remapped through remap DP aware tables."
            ::= { ctcQosPortDscpConfigEntry 3 }

    -- 1.3.6.1.4.1.4756.103.150.3.2
    ctcQosDscpBasedRxClassifiedTable OBJECT-TYPE
