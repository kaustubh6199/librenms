        STATUS      current
        DESCRIPTION
            "The Entry of MLD snooping VLAN."
        INDEX { ctcMldVlanID }
        ::= { ctcMldVlanConfigTable 1 }

    CtcMldVlanConfigEntry ::=
        SEQUENCE {     
            ctcMldVlanID             Integer32,
            ctcMldPerVlanEnabled     TruthValue,
            ctcMldQuerierEnabled     TruthValue,
            ctcMldCompatibility      INTEGER,
            ctcMldRV                 Integer32,
            ctcMldQI                 Integer32,
            ctcMldQRI                Integer32,
            ctcMldLLQI               Integer32,
            ctcMldURI                Integer32           
        }

        -- 1.3.6.1.4.1.4756.103.90.2.22.1.1    
        ctcMldVlanID OBJECT-TYPE
            SYNTAX      Integer32 (1..4095)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the ID of this particular MLD snooping VLAN."
            ::= { ctcMldVlanConfigEntry 1 }

        -- 1.3.6.1.4.1.4756.103.90.2.22.1.2    
        ctcMldPerVlanEnabled OBJECT-TYPE
            SYNTAX      TruthValue
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Enable the per-VLAN MLD Snooping. Only up to 64 VLANs can be selected."
            ::= { ctcMldVlanConfigEntry 2 }

        -- 1.3.6.1.4.1.4756.103.90.2.22.1.3    
        ctcMldQuerierEnabled OBJECT-TYPE
            SYNTAX      TruthValue
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Enable the MLD Querier in the VLAN."
            ::= { ctcMldVlanConfigEntry 3 }

        -- 1.3.6.1.4.1.4756.103.90.2.22.1.4    
        ctcMldCompatibility OBJECT-TYPE
            SYNTAX      INTEGER {
                            forcedMldV1(1),
                            forcedMldV2(2),
            	            mldAuto(3)
            	        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Compatibility is maintained by hosts and routers taking appropriate actions depending on
                 the versions of MLD operating on hosts and routers within a network.
                 The allowed selection is MLD-Auto, Forced MLDv1, Forced MLDv2,
                 default compatibility value is MLD-Auto."
            DEFVAL      { mldAuto }
            ::= { ctcMldVlanConfigEntry 4 }

        -- 1.3.6.1.4.1.4756.103.90.2.22.1.5    
        ctcMldRV OBJECT-TYPE
            SYNTAX      Integer32 (1..255)
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Robustness Variable.
                 The Robustness Variable allows tuning for the expected packet loss on a link.
                 The allowed range is 1 to 255, default robustness variable value is 2,
                 Robustness Variable SHOULD NOT be one."
            DEFVAL      { 2 }     
            ::= { ctcMldVlanConfigEntry 5 }

        -- 1.3.6.1.4.1.4756.103.90.2.22.1.6    
        ctcMldQI OBJECT-TYPE
            SYNTAX      Integer32 (1..255)
            UNITS       "Seconds"
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Query Interval.
                 The Query Interval is the interval between General Queries sent by the Querier.
                 The allowed range is 1 to 255 seconds, default query interval is 125 seconds.
                 Note: The number of seconds represented by QRI must be less than QI!"
            DEFVAL      { 125 }    
            ::= { ctcMldVlanConfigEntry 6 }

        -- 1.3.6.1.4.1.4756.103.90.2.22.1.7    
        ctcMldQRI OBJECT-TYPE
            SYNTAX      Integer32 (0..31744)
            UNITS       "100 millisecond"
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Query Response Interval.
                 The Max Response Time used to calculate the Max Resp Code inserted into the periodic General Queries.
                 The allowed range is 0 to 31744 in tenths of seconds,
                 default query response interval is 100 in tenths of seconds (10 seconds).
                 The number of seconds represented by QRI must be less than QI!"
            DEFVAL      { 100 }     
            ::= { ctcMldVlanConfigEntry 7 }

        -- 1.3.6.1.4.1.4756.103.90.2.22.1.8    
        ctcMldLLQI OBJECT-TYPE
            SYNTAX      Integer32 (0..31744)
            UNITS       "100 millisecond"
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Last Member Query Interval.
                 The Last Member Query Time is the time value represented by the Last Member Query Interval,
                 multiplied by the Last Member Query Count.
                 The allowed range is 0 to 31744 in tenths of seconds,
                 default last member query interval is 10 in tenths of seconds (1 second)."
            DEFVAL      { 10 }     
            ::= { ctcMldVlanConfigEntry 8 }

        -- 1.3.6.1.4.1.4756.103.90.2.22.1.9    
        ctcMldURI OBJECT-TYPE
            SYNTAX      Integer32 (0..31744)
            UNITS       "Seconds"
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Unsolicited Report Interval.
                 The Unsolicited Report Interval is the time between repetitions of
                 a host's initial report of membership in a group.
                 The allowed range is 0 to 31744 seconds,
                 default unsolicited report interval is 1 second."
            DEFVAL      { 1 }     
            ::= { ctcMldVlanConfigEntry 9 }

