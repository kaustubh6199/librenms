        SYNTAX      OCTET STRING (SIZE (0..20))
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The name identifying the HTTP/FTP Login user. The allowed string length is 0 to 20.
             the valid charactores are '0'~'9', 'a'~'z', 'A'~'Z' and '_'."
        ::= { ctcAutoProvision 3 }
    -- 1.3.6.1.4.1.4756.103.60.1.3.1.3
    
