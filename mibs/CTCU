            ctcMldRouterPortEnabled  TruthValue,
            ctcMldRouterPortStatus   INTEGER,          
            ctcMldFastLeaveEnabled   TruthValue,
            ctcMldThrottling         Integer32
        }

        -- 1.3.6.1.4.1.4756.103.90.2.10.1.1    
        ctcMldRouterPortEnabled OBJECT-TYPE
            SYNTAX      TruthValue
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Specify which ports act as router ports.
                 A router port is a port on the Ethernet switch that
                 leads towards the Layer 3 multicast device or MLD querier.
                 If an aggregation member port is selected as a router port,
                 the whole aggregation will act as a router port."
            ::= { ctcMldPortRelatedEntry 1 }

        -- 1.3.6.1.4.1.4756.103.90.2.10.1.2    
        ctcMldRouterPortStatus OBJECT-TYPE
            SYNTAX      INTEGER {
                            none(1),
                            static(2),
                            dynamic(3),
                            both(4)
                        }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Shows MLD router port mode."
            ::= { ctcMldPortRelatedEntry 2 }

        -- 1.3.6.1.4.1.4756.103.90.2.10.1.3    
