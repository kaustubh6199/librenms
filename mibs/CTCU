        ::= { ctcIgmpSnooping 41 }

    -- 1.3.6.1.4.1.4756.103.90.1.41.1
    ctcIgmpSnoopingStatisticsEntry OBJECT-TYPE
        SYNTAX      CtcIgmpSnoopingStatisticsEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Entry of VLAN ID."
        INDEX { ctcIgmpSnoopingVlanID }
        ::= { ctcIgmpSnoopingStatisticsTable 1 }

    CtcIgmpSnoopingStatisticsEntry ::=
        SEQUENCE {       
            ctcIgmpSnoopingVlanID        Integer32,
            ctcIgmpQuerierVersion        Integer32,
            ctcIgmpHostVersion           Integer32,
            ctcIgmpQuerierStatus         INTEGER,
            ctcIgmpQueriesTransmitted    Counter32,
            ctcIgmpQueriesReceived       Counter32,
            ctcIgmpV1ReportsReceived     Counter32,
            ctcIgmpV2ReportsReceived     Counter32,
            ctcIgmpV3ReportsReceived     Counter32,
            ctcIgmpV2LeavesReceived      Counter32
        }
        
        -- 1.3.6.1.4.1.4756.103.90.1.41.1.1    
        ctcIgmpSnoopingVlanID OBJECT-TYPE
            SYNTAX      Integer32 (1..4095)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                 "The VLAN ID of the entry."
            ::= { ctcIgmpSnoopingStatisticsEntry 1 }

        -- 1.3.6.1.4.1.4756.103.90.1.41.1.2    
        ctcIgmpQuerierVersion OBJECT-TYPE
            SYNTAX      Integer32 (1..3)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Working Querier Version currently."
            ::= { ctcIgmpSnoopingStatisticsEntry 2 }

        -- 1.3.6.1.4.1.4756.103.90.1.41.1.3    
        ctcIgmpHostVersion OBJECT-TYPE
            SYNTAX      Integer32 (1..3)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Working Host Version currently."
            ::= { ctcIgmpSnoopingStatisticsEntry 3 }

        -- 1.3.6.1.4.1.4756.103.90.1.41.1.4    
        ctcIgmpQuerierStatus OBJECT-TYPE
            SYNTAX      INTEGER {
                            idle(1),
                            active(2),
                            disable(3)
                        }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Shows the Querier status is ACTIVE or IDLE.
                 DISABLE denotes the specific interface is administratively disabled."
            ::= { ctcIgmpSnoopingStatisticsEntry 4 }

        -- 1.3.6.1.4.1.4756.103.90.1.41.1.5    
        ctcIgmpQueriesTransmitted OBJECT-TYPE
            SYNTAX      Counter32
            UNITS       "Packets"
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The number of Transmitted Queries."
            ::= { ctcIgmpSnoopingStatisticsEntry 5 }

        -- 1.3.6.1.4.1.4756.103.90.1.41.1.6    
