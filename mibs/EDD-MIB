        SYNTAX      TruthValue
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The number of received packets per queue."
        ::= { iStaticAggregation 3 }

    -- 1.3.6.1.4.1.4756.121.50.10.4    
