    -- 1.3.6.1.4.1.4756.121.50.20.3.1
    iLacpPortStatusEntry OBJECT-TYPE
        SYNTAX      ILacpPortStatusEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Entry of LACP status for all ports."
        INDEX { iPortIndex }
        ::= { iLacpPortStatusTable 1 }

    ILacpPortStatusEntry ::=
        SEQUENCE {
        	iLacpPortEnabledState		    INTEGER,
            iLacpPortKeyState             Integer32,          
            iLacpPortAggrID           	Integer32,
            iLacpPortPartnerSystemID      DisplayString,
            iLacpPortPartnerPort      	Integer32,
            iLacpPortPartnerPriority      Integer32
        }

        -- 1.3.6.1.4.1.4756.121.50.20.3.1.1    
        iLacpPortEnabledState OBJECT-TYPE
            SYNTAX      INTEGER {
                            no(1),
                            yes(2),
                            standby(3)
                        }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "'Yes' means that LACP is enabled and the port link is up.
                 'No' means that LACP is not enabled or that the port link is down.
                 'Standby' means that the port could not join the aggregation group but will join if other port leaves.
                  Meanwhile it's LACP status is disabled."
            ::= { iLacpPortStatusEntry 1 }

        -- 1.3.6.1.4.1.4756.121.50.20.3.1.2    
        iLacpPortKeyState OBJECT-TYPE
            SYNTAX      Integer32 (0..65535)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The key assigned to this port. Only ports with the same key can aggregate together."
            ::= { iLacpPortStatusEntry 2 }

        -- 1.3.6.1.4.1.4756.121.50.20.3.1.3
        iLacpPortAggrID OBJECT-TYPE
            SYNTAX      Integer32 (1..3)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The Aggregation ID assigned to this aggregation group."
            ::= { iLacpPortStatusEntry 3 }

        -- 1.3.6.1.4.1.4756.121.50.20.3.1.4
        iLacpPortPartnerSystemID OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The partner's System ID (MAC address)."
            ::= { iLacpPortStatusEntry 4 }

        -- 1.3.6.1.4.1.4756.121.50.20.3.1.5
        iLacpPortPartnerPort OBJECT-TYPE
            SYNTAX      Integer32 (1..5)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The partner's port number connected to this port."
            ::= { iLacpPortStatusEntry 5 }

        -- 1.3.6.1.4.1.4756.121.50.20.3.1.6    
        iLacpPortPartnerPriority OBJECT-TYPE
            SYNTAX      Integer32 (1..65535)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The partner's port priority."
            ::= { iLacpPortStatusEntry 6 }

    -- 1.3.6.1.4.1.4756.121.50.20.4
