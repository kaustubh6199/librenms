	    iSystemLog OBJECT IDENTIFIER ::= { iSystem 6 }  
	    
	    iSystemLogServTable OBJECT-TYPE
	        SYNTAX      SEQUENCE OF ISystemLogServEntry
	        MAX-ACCESS  not-accessible
	        STATUS      current
	        DESCRIPTION
	            "The switch system log server information."
	        ::= { iSystemLog 1 }
	
	    iSystemLogServEntry OBJECT-TYPE
	        SYNTAX      ISystemLogServEntry
	        MAX-ACCESS  not-accessible
	        STATUS      current
	        DESCRIPTION
	            "The system log server entry."
	        INDEX { iSystemLogServIdx }
	        ::= { iSystemLogServTable 1 }
	
	    ISystemLogServEntry ::=
	        SEQUENCE {
	        	iSystemLogServIdx	        Integer32,
	            iSystemLogServerMode        INTEGER,
	            iSystemLogServerAddress     DisplayString,
	            iSystemLogLevel      		INTEGER
	        }  
	        
	     iSystemLogServIdx OBJECT-TYPE
	        SYNTAX      Integer32 (1..3)
	        MAX-ACCESS  not-accessible
	        STATUS      current
	        DESCRIPTION
	            "The Index of the system log server entry."
	        ::= { iSystemLogServEntry 1 }

	
	    iSystemLogServerMode OBJECT-TYPE
	        SYNTAX      INTEGER {
	                        enabled(1),
	                        disabled(2)
	                    }
	        MAX-ACCESS  read-write
	        STATUS      current
	        DESCRIPTION
	            "Indicates the server mode operation.
	             Enabled: Enable server mode operation.
	             Disabled: Disable server mode operation."
	        ::= { iSystemLogServEntry 2 }
	           
	    iSystemLogServerAddress OBJECT-TYPE
	        SYNTAX      DisplayString
	        MAX-ACCESS  read-write
	        STATUS      current
	        DESCRIPTION
	            "Indicates the IPv4 host address of syslog server.
	             If the switch provide DNS feature, it also can be a Host Name."
	        ::= { iSystemLogServEntry 3 }
	
	    iSystemLogLevel OBJECT-TYPE
	        SYNTAX      INTEGER {
	                        info(1),
	                        warning(2),
	                        error(3)
	                    }
	        MAX-ACCESS  read-write
	        STATUS      current
	        DESCRIPTION
	            "Indicates what kind of message will send to syslog server. Possible modes are:
	             Info: Send informations, warnings and errors.
	             Warning: Send warnings and errors.
	             Error: Send errors."
	        ::= { iSystemLogServEntry 4 }
	
	    -- 1.3.6.1.4.1.4756.121.10.6.4
	    iSystemLogClear OBJECT-TYPE
	        SYNTAX      INTEGER {
	                        trigger(1),
	                        normal(2)
	                    }
	        MAX-ACCESS  read-write
	        STATUS      current
	        DESCRIPTION
	            "Flushes all system log entries."
	        ::= { iSystemLog 2 }
	
	    -- 1.3.6.1.4.1.4756.121.10.6.5
