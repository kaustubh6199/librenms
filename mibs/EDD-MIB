-- Loop Protection
    -- 1.3.6.1.4.1.4756.121.60.20
    iLoopProtection  OBJECT IDENTIFIER ::= { eddCard 60 }

    -- 1.3.6.1.4.1.4756.121.60.20.1
    iLoopProtectionEnable OBJECT-TYPE
        SYNTAX      TruthValue
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Controls whether loop protections is enabled (as a whole)."
        ::= { iLoopProtection 1 }

    -- 1.3.6.1.4.1.4756.121.60.20.2
    iLoopProtectionTxTime OBJECT-TYPE
        SYNTAX      Integer32 (1..10)
        UNITS       "Seconds"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The interval between each loop protection PDU sent on each port.
             valid values are 1 to 10 seconds."
       ::= { iLoopProtection 2 }

    -- 1.3.6.1.4.1.4756.121.60.20.3
    iLoopProtectionShutdownTime OBJECT-TYPE
        SYNTAX      Integer32 (10..50)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The period (in seconds) for which a port will be kept disabled in the event of
             a loop is detected (and the port action shuts down the port).
             Valid values are 0 to 604800 seconds (7 days).
             A value of zero will keep a port disabled (until next device restart)."
        ::= { iLoopProtection 3 }

    -- 1.3.6.1.4.1.4756.121.60.20.4
    iLoopProtectionPortConfigTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF ILoopProtectionPortConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table provides Loop Protection configurations for all ports."
        ::= { iLoopProtection 4 }

    -- 1.3.6.1.4.1.4756.121.60.20.4.1
    iLoopProtectionPortConfigEntry OBJECT-TYPE
        SYNTAX      ILoopProtectionPortConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Entry of Loop Protection port configurations."
        INDEX { iPortIndex }
        ::= { iLoopProtectionPortConfigTable 1 }

    ILoopProtectionPortConfigEntry ::=
        SEQUENCE {
            iLoopProtectionPortEnabled     TruthValue,          
            iLoopProtectionPortAction      INTEGER,
            iLoopProtectionPortTxMode      INTEGER 
        }

        -- 1.3.6.1.4.1.4756.121.60.20.4.1.1    
        iLoopProtectionPortEnabled OBJECT-TYPE
            SYNTAX      TruthValue
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Controls whether loop protection is enabled on this switch port."
            ::= { iLoopProtectionPortConfigEntry 1 }

        -- 1.3.6.1.4.1.4756.121.60.20.4.1.2    
        iLoopProtectionPortAction OBJECT-TYPE
            SYNTAX      INTEGER {
                            shutdownPort(1),
                            shutdownPortAndLog(2),
                            logOnly(3)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Configures the action performed when a loop is detected on a port.
                 Valid values are Shutdown Port, Shutdown Port and Log or Log Only."
            ::= { iLoopProtectionPortConfigEntry 2 }
            
        -- 1.3.6.1.4.1.4756.121.60.20.4.1.3    
        iLoopProtectionPortTxMode OBJECT-TYPE
            SYNTAX      INTEGER {
                            enabled(1),
                            disabled(2)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Controls whether the port is actively generating loop protection PDU's,
                 or whether it is just passively looking for looped PDU's."
            ::= { iLoopProtectionPortConfigEntry 3 }

    -- 1.3.6.1.4.1.4756.121.60.20.5
    iLoopProtectionPortStatusTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF ILoopProtectionPortStatusEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table provides Loop Protection status for all ports."
        ::= { iLoopProtection 5 }

    -- 1.3.6.1.4.1.4756.121.60.20.5.1
    iLoopProtectionPortStatusEntry OBJECT-TYPE
        SYNTAX      ILoopProtectionPortStatusEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Entry of Loop Protection port status."
        INDEX { iPortIndex }
        ::= { iLoopProtectionPortStatusTable 1 }

    ILoopProtectionPortStatusEntry ::=
        SEQUENCE {
            iLoopProtectionPortLoops     Integer32,          
            iLoopProtectionPortStatus    INTEGER,
            iLoopProtectionPortLoop      INTEGER,
            iLoopProtectionPortLastLoop  DisplayString
        }

        -- 1.3.6.1.4.1.4756.121.60.20.5.1.1    
        iLoopProtectionPortLoops OBJECT-TYPE
            SYNTAX      Integer32
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The number of loops detected on this port."
            ::= { iLoopProtectionPortStatusEntry 1 }

        -- 1.3.6.1.4.1.4756.121.60.20.5.1.2    
        iLoopProtectionPortStatus OBJECT-TYPE
            SYNTAX      INTEGER {
                            up(1),
                            down(2),
                            disabled(3)
                        }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The current loop protection status of the port."
            ::= { iLoopProtectionPortStatusEntry 2 }
            
        -- 1.3.6.1.4.1.4756.121.60.20.5.1.3    
