    -- 1.3.6.1.4.1.4756.121.40.2.6
    iArpInspection OBJECT IDENTIFIER ::= { iSecurityNetwork 6 }

    -- 1.3.6.1.4.1.4756.121.40.2.6.1    
    iArpInspectionMode OBJECT-TYPE
        SYNTAX      INTEGER {
                        enabled(1),
                        disabled(2)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Enable the Global ARP Inspection or disable the Global ARP Inspection."
        ::= { iArpInspection 1 }

    -- 1.3.6.1.4.1.4756.121.40.2.6.2    
    iArpTranslateDynamicToStatic OBJECT-TYPE
        SYNTAX      INTEGER {
                        trigger(1),
                        normal(2)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "To translate all dynamic entries to static entries."
        ::= { iArpInspection 2 }

    -- 1.3.6.1.4.1.4756.121.40.2.6.3
    iArpInspectionPortConfigTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF IArpInspectionPortConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table provides ARP Inspection port configuration."
        ::= { iArpInspection 3 }

    -- 1.3.6.1.4.1.4756.121.40.2.6.3.1
    iArpInspectionPortConfigEntry OBJECT-TYPE
        SYNTAX      IArpInspectionPortConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Entry of ARP Inspection ports."
        INDEX { iPortIndex }
        ::= { iArpInspectionPortConfigTable 1 }

    IArpInspectionPortConfigEntry ::=
        SEQUENCE {
            iArpInspectionPortMode         INTEGER,    
            iArpInspectionCheckVLAN        INTEGER,
            iArpInspectionLogType         INTEGER
        }

        -- 1.3.6.1.4.1.4756.121.40.2.6.3.1.1
        iArpInspectionPortMode OBJECT-TYPE
            SYNTAX      INTEGER {
                            enable(1),
                            disable(2)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Specify ARP Inspection is enabled on this port.
                 Only when both Global Mode and Port Mode on a given port are enabled,
                 ARP Inspection is enabled on this given port."
            ::= { iArpInspectionPortConfigEntry 1 }
        
        -- 1.3.6.1.4.1.4756.121.40.2.6.3.1.2
        iArpInspectionCheckVLAN OBJECT-TYPE
            SYNTAX      INTEGER {
                            enable(1),
                            disable(2)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "If you want to inspect the VLAN configuration, you have to enable the setting of 'Check VLAN'. 
                 The default setting of 'Check VLAN' is disabled. 
                 When the setting of 'Check VLAN' is disabled, the log type of ARP Inspection will refer to the port setting. 
                 And the setting of 'Check VLAN' is enabled, the log type of ARP Inspection will refer to the VLAN setting. 
                 Possible setting of 'Check VLAN' are: 
					Enabled: Enable check VLAN operation. 
					Disabled: Disable check VLAN operation. 
				"
            ::= { iArpInspectionPortConfigEntry 2 }

		-- 1.3.6.1.4.1.4756.121.40.2.6.3.1.3
        iArpInspectionLogType OBJECT-TYPE
            SYNTAX      INTEGER {
                            none(1),
                            deny(2),
                            permit(3),
                            all(4)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Only the Global Mode and Port Mode on a given port are enabled, and the setting of 'Check VLAN' is disabled, the log type of ARP Inspection will refer to the port setting. There are four log types and possible types are: 
					None: Log nothing. 
					Deny: Log denied entries. 
					Permit: Log permitted entries. 
					ALL: Log all entries. 
				"
            ::= { iArpInspectionPortConfigEntry 3 }
	
	-- 1.3.6.1.4.1.4756.121.40.2.6.4
    iAddArpInspectionVLANConfiguration  OBJECT-TYPE
        SYNTAX      DisplayString
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION                                  
            "Add a new VLAN to the ARP Inspection VLAN table
             Syntax: <vid> <[none|deny|permit|all]>
            "
        ::= { iArpInspection 4 }

    -- 1.3.6.1.4.1.4756.121.40.2.6.5
