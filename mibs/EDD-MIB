        STATUS      current
        DESCRIPTION
            "Added the Peer MEP ID Entry."
        INDEX { iMEPInstanceIndex }
        ::= { iAddPeerMEPIDTable 1 }

    IAddPeerMEPIDEntry ::= 
    SEQUENCE {
    			iAddNewPeerMEPID	DisplayString
    		 }
    
    	-- 1.3.6.1.4.1.4756.121.60.40.4.1.1
	    iAddNewPeerMEPID OBJECT-TYPE
	        SYNTAX      DisplayString
	        MAX-ACCESS  read-write
	        STATUS      current
	        DESCRIPTION
	            "Added a new peer MEP id.
	            	Syntax: <MEP id>
	            	MEP id range: 1 ~ 8191
            	"
	        ::= { iAddPeerMEPIDEntry 1 }
    
	-- 1.3.6.1.4.1.4756.121.60.40.5
    iPeerMEPConfigTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF IPeerMEPConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Peer MEP config Table."
        ::= { iMEP 5 }
     
    -- 1.3.6.1.4.1.4756.121.60.40.5.1
    iPeerMEPConfigEntry OBJECT-TYPE
        SYNTAX      IPeerMEPConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Peer MEP config Entry."
        INDEX { iMEPInstanceIndex , iPeerMEPIndex }
        ::= { iPeerMEPConfigTable 1 }
        

    IPeerMEPConfigEntry ::= 
    SEQUENCE {
    		iPeerMEPIndex       Integer32,
    		iDeletePeerMEPID	TruthValue,
    		iPeerMEPID			Integer32,
    		iUnicastPeerMAC		DisplayString,
    		iPeerMEPIDLLFLogic	 INTEGER,
    		iPeerMEPIDLLFcLoc	 TruthValue,
    		iPeerMEPIDLLFPeerPorts  Integer32,
    		iPeerMEPIDLLFPort1	 TruthValue,
    		iPeerMEPIDLLFPort2	 TruthValue,
    		iPeerMEPIDLLFPort3	 TruthValue,
    		iPeerMEPIDLLFPort4	 TruthValue,
    		iPeerMEPIDLLFPort5	 TruthValue,
    		iPeerMEPIDLLFPort6	 TruthValue,
    		iPeerMEPIDLLFPort7	 TruthValue,
    		iPeerMEPIDLLFPort8	 TruthValue,
			icLOC 				INTEGER,
			icRDI 				INTEGER,
			icPeriod 			INTEGER,
			icPriority 			INTEGER
    	}
    	
    	-- 1.3.6.1.4.1.4756.121.60.40.5.1.1
        iPeerMEPIndex OBJECT-TYPE
         	SYNTAX Integer32 (1..5)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "This value will become an expected MEP ID in a received CCM - see 'cMEP'."
            ::= { iPeerMEPConfigEntry 1 }
    	
    	-- 1.3.6.1.4.1.4756.121.60.40.5.1.2
		iDeletePeerMEPID OBJECT-TYPE
		    SYNTAX      TruthValue
		    MAX-ACCESS  read-write
		    STATUS      current
		    DESCRIPTION
		        "Deleted this peer MEP id."
		    ::= { iPeerMEPConfigEntry 2 }
		    
    	-- 1.3.6.1.4.1.4756.121.60.40.5.1.3
        iPeerMEPID OBJECT-TYPE
         	SYNTAX Integer32 (1..8191)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "This value will become an expected MEP ID in a received CCM - see 'cMEP'."
            ::= { iPeerMEPConfigEntry 3 }
		    
        -- 1.3.6.1.4.1.4756.121.60.40.5.1.4
        iUnicastPeerMAC OBJECT-TYPE
         	SYNTAX DisplayString
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "This MAC will be used when unicast is selected with this peer MEP. 
                 Also this MAC is used to create HW checking of receiving CCM PDU (LOC detection) from this MEP.
                "
            ::= { iPeerMEPConfigEntry 4 }  
            		    
		iPeerMEPIDLLFLogic OBJECT-TYPE
         	SYNTAX INTEGER {
         						and(1),
         						or(2)
         				   }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Configure LLF logic of the Peer MEP id."
            ::= { iPeerMEPConfigEntry 5 }

          iPeerMEPIDLLFcLoc OBJECT-TYPE
		    SYNTAX      TruthValue
		    MAX-ACCESS  read-write
		    STATUS      current
		    DESCRIPTION
		        "Configure cLoc of LLF of the Peer MEP id."
		    ::= { iPeerMEPConfigEntry 6 }
		    
		  iPeerMEPIDLLFPeerPorts OBJECT-TYPE
            SYNTAX      Integer32 (0..8) 
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
            "The peer ports of LLF of the Peer MEP id."
            ::= { iPeerMEPConfigEntry 7 } 
            
           iPeerMEPIDLLFPort1 OBJECT-TYPE
		    SYNTAX      TruthValue
		    MAX-ACCESS  read-write
		    STATUS      current
		    DESCRIPTION
		        "Configure the logic port1 of LLF of the Peer MEP id."
		    ::= { iPeerMEPConfigEntry 8 }
	
	       iPeerMEPIDLLFPort2 OBJECT-TYPE
		    SYNTAX      TruthValue
		    MAX-ACCESS  read-write
		    STATUS      current
		    DESCRIPTION
		        "Configure the logic port2 of LLF of the Peer MEP id."
		    ::= { iPeerMEPConfigEntry 9 }
	        
	       iPeerMEPIDLLFPort3 OBJECT-TYPE
		    SYNTAX      TruthValue
		    MAX-ACCESS  read-write
		    STATUS      current
		    DESCRIPTION
		        "Configure the logic port3 of LLF of the Peer MEP id."
		    ::= { iPeerMEPConfigEntry 10 } 
	
		   iPeerMEPIDLLFPort4 OBJECT-TYPE
		    SYNTAX      TruthValue
		    MAX-ACCESS  read-write
		    STATUS      current
		    DESCRIPTION
		        "Configure the logic port4 of LLF of the Peer MEP id."
		    ::= { iPeerMEPConfigEntry 11 } 

		   iPeerMEPIDLLFPort5 OBJECT-TYPE
		    SYNTAX      TruthValue
		    MAX-ACCESS  read-write
		    STATUS      current
		    DESCRIPTION
		        "Configure the logic port5 of LLF of the Peer MEP id."
		    ::= { iPeerMEPConfigEntry 12 } 

		   iPeerMEPIDLLFPort6 OBJECT-TYPE
		    SYNTAX      TruthValue
		    MAX-ACCESS  read-write
		    STATUS      current
		    DESCRIPTION
		        "Configure the logic port6 of LLF of the Peer MEP id."
		    ::= { iPeerMEPConfigEntry 13 }  
		    
		   iPeerMEPIDLLFPort7 OBJECT-TYPE
		    SYNTAX      TruthValue
		    MAX-ACCESS  read-write
		    STATUS      current
		    DESCRIPTION
		        "Configure the logic port7 of LLF of the Peer MEP id."
		    ::= { iPeerMEPConfigEntry 14 }   
		    
		   iPeerMEPIDLLFPort8 OBJECT-TYPE
		    SYNTAX      TruthValue
		    MAX-ACCESS  read-write
		    STATUS      current
		    DESCRIPTION
		        "Configure the logic port8 of LLF of the Peer MEP id."
		    ::= { iPeerMEPConfigEntry 15 } 
            
        -- 1.3.6.1.4.1.4756.121.60.40.5.1.5
        icLOC OBJECT-TYPE
         	SYNTAX INTEGER {
         						success(1),
         						fail(2)
         				   }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Fault Cause indicating that no CCM has been received (in 3,5 periods) - from this peer MEP."
            ::= { iPeerMEPConfigEntry 16 }
        
        -- 1.3.6.1.4.1.4756.121.60.40.5.1.6
        icRDI OBJECT-TYPE
         	SYNTAX INTEGER {
         						success(1),
         						fail(2)
         				   }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Fault Cause indicating that a CCM is received with Remote Defect Indication - from this peer MEP."
            ::= { iPeerMEPConfigEntry 17 }
            
        -- 1.3.6.1.4.1.4756.121.60.40.5.1.7
        icPeriod OBJECT-TYPE
         	SYNTAX INTEGER {
         						success(1),
         						fail(2)
         				   }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Fault Cause indicating that a CCM is received with a period different what is configured for this MEP - from this peer MEP."
            ::= { iPeerMEPConfigEntry 18 }

        -- 1.3.6.1.4.1.4756.121.60.40.5.1.8
        icPriority OBJECT-TYPE
         	SYNTAX INTEGER {
         						success(1),
         						fail(2)
         				   }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Fault Cause indicating that a CCM is received with a priority different what is configured for this MEP - from this peer MEP."
            ::= { iPeerMEPConfigEntry 19 }

		-- 1.3.6.1.4.1.4756.121.60.40.6
    iMEPFunctionalConfigTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF IMEPFunctionalConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Peer MEP config Table."
        ::= { iMEP 6 }
     
    -- 1.3.6.1.4.1.4756.121.60.40.6.1
    iMEPFunctionalConfigEntry OBJECT-TYPE
        SYNTAX      IMEPFunctionalConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Peer MEP config Entry."
        INDEX { iMEPInstanceIndex }
        ::= { iMEPFunctionalConfigTable 1 }

    IMEPFunctionalConfigEntry ::= 
    SEQUENCE {
    			iContinuityCheckEnable			TruthValue,
    			iContinuityCheckPriority		Integer32,
    			iContinuityCheckFrameRate		INTEGER,
    			iAPSProtocolEnable				TruthValue,
    			iAPSProtocolPriority			Integer32,
    			iAPSProtocolCast				INTEGER,
    			iAPSProtocolType				INTEGER,
    			iAPSProtocolLastOctet			Integer32
    		}
    		
    		-- 1.3.6.1.4.1.4756.121.60.40.6.1.1
        iContinuityCheckEnable OBJECT-TYPE
            SYNTAX      TruthValue
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Continuity Check based on transmitting/receiving CCM PDU can be enabled/disabled. 
                 The CCM PDU is always transmitted as Multi-cast Class 1."
            ::= { iMEPFunctionalConfigEntry 1 } 
            
        -- 1.3.6.1.4.1.4756.121.60.40.6.1.2
        iContinuityCheckPriority OBJECT-TYPE
            SYNTAX      Integer32 (0..9) 
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "The priority to be inserted as PCP bits in TAG (if any).
                 In case of enable of Continuity Check and Loss Measurement both implemented on SW based CCM, 'Priority' has to be the same."
            ::= { iMEPFunctionalConfigEntry 2 }
        
        -- 1.3.6.1.4.1.4756.121.60.40.6.1.3
        iContinuityCheckFrameRate OBJECT-TYPE
            SYNTAX      INTEGER {
                            tx-300FramesPerSecond(1),
                            tx-100FramesPerSecond(2),
                            tx-10FramesPerSecond(3),
                            tx-1FramesPerSecond(4),
                            tx-6FramesPerMinute(5),
                            tx-1FramesPerMinute(6),
                            tx-6FramesPerHour(7)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Selecting the frame rate of CCM PDU. This is the inverse of transmission period as described in Y.1731. 
                 This value has the following uses: 
				 * The transmission rate of the CCM PDU.
				 * Fault Cause cLOC is declared if no CCM PDU has been received within 3.5 periods - see 'cLOC'.
				 * Fault Cause cPeriod is declared if a CCM PDU has been received with different period - see 'cPeriod'.
				 
				 Selecting 300f/sec or 100f/sec will configure HW based CCM (if possible). 
				 Selecting other frame rates will configure SW based CCM. 
				 In case of enable of Continuity Check and Loss Measurement both implemented on SW based CCM, 'Frame Rate' has to be the same. 
				"
            ::= { iMEPFunctionalConfigEntry 3 }
            
        -- 1.3.6.1.4.1.4756.121.60.40.6.1.4
        iAPSProtocolEnable OBJECT-TYPE
            SYNTAX      TruthValue
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Automatic Protection Switching protocol information transportation based on transmitting/receiving R-APS/L-APS PDU can be enabled/disabled.
                 Must be enabled to support ERPS/ELPS implementing APS. This is only valid with one Peer MEP configured. 
								"
            ::= { iMEPFunctionalConfigEntry 4 }
            
        -- 1.3.6.1.4.1.4756.121.60.40.6.1.5
        iAPSProtocolPriority OBJECT-TYPE
            SYNTAX      Integer32 (0..9) 
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "The priority to be inserted as PCP bits in TAG (if any)."
            ::= { iMEPFunctionalConfigEntry 5 }
            
        -- 1.3.6.1.4.1.4756.121.60.40.6.1.6
        iAPSProtocolCast OBJECT-TYPE
            SYNTAX      INTEGER {
                            unicast(1),
                            multicast(2)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Selection of APS PDU transmitted unicast or multi-cast. 
                 The unicast MAC will be taken from the 'Unicast Peer MAC' configuration. Unicast is only valid for L-APS - see 'Type'. 
                 The R-APS PDU is always transmitted with multi-cast MAC described in G.8032. 
								"
            ::= { iMEPFunctionalConfigEntry 6 }

		-- 1.3.6.1.4.1.4756.121.60.40.6.1.7
        iAPSProtocolType OBJECT-TYPE
            SYNTAX      INTEGER {
                            liniar-aps(1),
                            ring-aps(2)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "L-APS: APS PDU is transmitted as L-APS - this is for ELPS.
                 R-APS: APS PDU is transmitted as R-APS - this is for ERPS.
				"
            ::= { iMEPFunctionalConfigEntry 7 }            

		-- 1.3.6.1.4.1.4756.121.60.40.6.1.8
        iAPSProtocolLastOctet OBJECT-TYPE
            SYNTAX      Integer32 (0..255) 
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "This is the last octet of the transmitted and expected RAPS multi-cast MAC. 
                 In G.8031 (03/2010) a RAPS multi-cast MAC is defined as 01-19-A7-00-00-XX. 
                 In current standard the value for this last octet is '01' and the usage of other values is for further study. 
								"
            ::= { iMEPFunctionalConfigEntry 8 }

-- ERPS
    
    -- 1.3.6.1.4.1.4756.121.60.10
    iERPS  OBJECT IDENTIFIER ::= { eddCard 130 }
    
    -- 1.3.6.1.4.1.4756.121.60.10.1
    iAddNewERPSID OBJECT-TYPE
        SYNTAX      DisplayString
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Added a new ERPS ID.
             	Syntax: <ERPS ID> <Port0> <Port1>
             	Port0 means east , the value is the switch port (range 1 ~ 5)
             	Port1 means west , the value is the switch port (range 1 ~ 5)
            "
        ::= { iERPS 1 }
        
     -- 1.3.6.1.4.1.4756.121.60.10.2
    iDeleteERPSID OBJECT-TYPE
        SYNTAX      DisplayString
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Deleted a ERPS ID.
            	Syntax: <ERPS ID>
            "
        ::= { iERPS 2 }
    
    -- 1.3.6.1.4.1.4756.121.60.10.3
    iERPSIDTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF IERPSIDEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The ERPS ID Table."
        ::= { iERPS 3 }
     
     -- 1.3.6.1.4.1.4756.121.60.10.3.1
    iERPSIDEntry OBJECT-TYPE
        SYNTAX      IERPSIDEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The ERPS ID Entry."
        INDEX { iERPSIDIndex }
        ::= { iERPSIDTable 1 }

    IERPSIDEntry ::= 
    SEQUENCE {        
    		iERPSIDIndex	 			 Integer32,
    		iERPSIDPort0	 	 		 Integer32,
    		iERPSIDPort1	 	 		 Integer32,
    		iERPSIDPort0APSMEP	 		 Integer32,
    		iERPSIDPort1APSMEP	 		 Integer32,
    		iERPSIDPort0SFMEP	 		 Integer32, 
    		iERPSIDPort1SFMEP			 Integer32,
    		iERPSIDRingType	 			 INTEGER,
    		iERPSIDInterconnectedNode	 INTEGER,
    		iERPSIDVirtualChannel	 	 INTEGER,
    		iERPSIDMajorID	 			 DisplayString,
    		iERPSIDAlarm	 	 		 INTEGER
    	}
        
        -- 1.3.6.1.4.1.4756.121.60.10.3.1.1    
        iERPSIDIndex OBJECT-TYPE
         	SYNTAX Integer32 (1..64)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The ID of the Protection group."
            ::= { iERPSIDEntry 1 }
        
        -- 1.3.6.1.4.1.4756.121.60.10.3.1.2    
        iERPSIDPort0 OBJECT-TYPE
         	SYNTAX Integer32 (1..5)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "This will create a Port 0 of the switch in the ring."
            ::= { iERPSIDEntry 2 }
        
        -- 1.3.6.1.4.1.4756.121.60.10.3.1.3    
        iERPSIDPort1 OBJECT-TYPE
         	SYNTAX Integer32 (1..5)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "This will create 'Port 1' of the switch in the Ring. 
                 As interconnected sub-ring will have only one ring port, 'Port 1' is configured as '0' for interconnected sub-ring. 
                 '0' in this field indicates that no 'Port 1' is associated with this instance"
            ::= { iERPSIDEntry 3 }
            
        -- 1.3.6.1.4.1.4756.121.60.10.3.1.4    
        iERPSIDPort0APSMEP OBJECT-TYPE
         	SYNTAX Integer32 (1..99)
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "The Port 0 APS PDU handling MEP."
            ::= { iERPSIDEntry 4 }
        
        -- 1.3.6.1.4.1.4756.121.60.10.3.1.5    
