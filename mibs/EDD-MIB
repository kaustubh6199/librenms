    -- 1.3.6.1.4.1.4756.121.40.2.5
    iIpSourceGuard OBJECT IDENTIFIER ::= { iSecurityNetwork 5 }

    -- 1.3.6.1.4.1.4756.121.40.2.5.1    
    iIpSourceGuardMode OBJECT-TYPE
        SYNTAX      INTEGER {
                        enabled(1),
                        disabled(2)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Enable the Global IP Source Guard or disable the Global IP Source Guard."
        ::= { iIpSourceGuard 1 }

    -- 1.3.6.1.4.1.4756.121.40.2.5.2    
    iIpTranslateDynamicToStatic OBJECT-TYPE
        SYNTAX      INTEGER {
                        trigger(1),
                        normal(2)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "To translate all dynamic IP Source Guard entries to static entries."
        ::= { iIpSourceGuard 2 }

    -- 1.3.6.1.4.1.4756.121.40.2.5.3
    iIpSourceGuardPortConfigTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF IIpSourceGuardPortConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table provides IP Source Guard port configuration."
        ::= { iIpSourceGuard 3 }

    -- 1.3.6.1.4.1.4756.121.40.2.5.3.1
    iIpSourceGuardPortConfigEntry OBJECT-TYPE
        SYNTAX      IIpSourceGuardPortConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Entry of IP Source Guard ports."
        INDEX { iPortIndex }
        ::= { iIpSourceGuardPortConfigTable 1 }

    IIpSourceGuardPortConfigEntry ::=
        SEQUENCE {
            iIpSourceGuardPortMode             INTEGER,
            iIpSourceGuardMaxDynamicClients    INTEGER
        }

        -- 1.3.6.1.4.1.4756.121.40.2.5.3.1.1
        iIpSourceGuardPortMode OBJECT-TYPE
            SYNTAX      INTEGER {
                            enable(1),
                            disable(2)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Specify IP Source Guard is enabled on which ports.
                 Only when both Global Mode and Port Mode on a given port are enabled,
                 IP Source Guard is enabled on this given port."
            ::= { iIpSourceGuardPortConfigEntry 1 }

        -- 1.3.6.1.4.1.4756.121.40.2.5.3.1.2
        iIpSourceGuardMaxDynamicClients OBJECT-TYPE
            SYNTAX      INTEGER {
                            unlimited(1),
                            limited0(2),
                            limited1(3),
                            limited2(4)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Specify the maximum number of dynamic clients that can be learned on given port.
                 This value can be 0, 1, 2 or unlimited.
                 If the port mode is enabled and the value of max dynamic client is equal to 0,
                 it means only allow the IP packets forwarding that are matched in static entries
                 on the specific port."
            ::= { iIpSourceGuardPortConfigEntry 2 }

    -- 1.3.6.1.4.1.4756.121.40.2.5.4
    iAddStaticIpSourceGuard OBJECT-TYPE
        SYNTAX      DisplayString
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION                                  
            "Add a new entry to the Static IP Source Guard table.
             Specify the Port, VLAN ID, IP address, and MAC address for the new entry.
             Syntax: <port_no> <vid> <allowed_ip> <allowed_mac> 
             The Static ARP Inspection table can support maximum 64 entries per port."
        ::= { iIpSourceGuard 4 }

