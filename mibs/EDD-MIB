         	SYNTAX INTEGER {
         						active(1),
         						inactive(2)
         				   }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "There is an active alarm on the MEP."
            ::= { iMEPConfigEntry 8 }
        
