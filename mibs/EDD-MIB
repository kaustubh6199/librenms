        SYNTAX      TruthValue
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The TCP/UDP port number can be used to calculate the destination port for the frame."
        DEFVAL      { true }
        ::= { iStaticAggregation 4 }

    -- 1.3.6.1.4.1.4756.121.50.10.5
    iAggrGroupTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF IAggrGroupEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
