            "The ERPS config Table."
        ::= { iERPS 4 }
     
     -- 1.3.6.1.4.1.4756.121.60.10.4.1
    iERPSIDConfigEntry OBJECT-TYPE
        SYNTAX      IERPSIDConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The ERPS config Entry."
        INDEX { iERPSIDIndex }
        ::= { iERPSIDConfigTable 1 }

    IERPSIDConfigEntry ::= 
    SEQUENCE {
           	iERPSConfigStatus		INTEGER,
        	iERPSGuardTime			Integer32,
        	iERPSWTRTime			INTEGER,
        	iERPSHoldOffTime		Integer32,
        	iERPSVersion			INTEGER,
        	iERPSRevertive			TruthValue
        }
                
        -- 1.3.6.1.4.1.4756.121.60.10.4.1.1    
        iERPSConfigStatus OBJECT-TYPE
         	SYNTAX INTEGER {
         						active(1),
         						inactive(2)
         				}
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                " active: This ERPS is configured - is active.
                  inactive: This ERPS is only created and has not yet been configured - is not active.
				"
            ::= { iERPSIDConfigEntry 1 }
            
        -- 1.3.6.1.4.1.4756.121.60.10.4.1.2
        iERPSGuardTime OBJECT-TYPE
         	SYNTAX Integer32 (10..2000)
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                " Guard timeout value to be used to prevent ring nodes from receiving outdated R-APS messages.
				  The period of the guard timer can be configured in 10 ms steps between 10 ms and 2 seconds, with a default value of 500 ms 
				"
            ::= { iERPSIDConfigEntry 2 }
        
        -- 1.3.6.1.4.1.4756.121.60.10.4.1.3
        iERPSWTRTime OBJECT-TYPE
         	SYNTAX INTEGER {
         						one-minute(1),
         						five-minutes(2),
         						six-minutes(3),
         						seven-minutes(4),
         						eight-minutes(5),
         						nine-minutes(6),
         						ten-minutes(7),
         						eleven-minutes(8),
         						twelve-minutes(9)
         				}
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                " The Wait To Restore timing value to be used in revertive switching.
				  The period of the WTR time can be configured by the operator in 1 minute steps between 5 and 12 minutes with a default value of 5 minutes.
				"
            ::= { iERPSIDConfigEntry 3 }
            
        -- 1.3.6.1.4.1.4756.121.60.10.4.1.4
        iERPSHoldOffTime OBJECT-TYPE
         	SYNTAX Integer32 (0..10000)
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                " The timing value to be used to make persistent check on Signal Fail before switching.
				  The range of the hold off timer is 0 to 10 seconds in steps of 100 ms.
				"
            ::= { iERPSIDConfigEntry 4 }
            
        -- 1.3.6.1.4.1.4756.121.60.10.4.1.5
        iERPSVersion OBJECT-TYPE
         	SYNTAX INTEGER {
         						v1(1),
         						v2(2)
						}
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                " ERPS Protocol Version - v1 or v2"
            ::= { iERPSIDConfigEntry 5 }
            
        -- 1.3.6.1.4.1.4756.121.60.10.4.1.6
        iERPSRevertive OBJECT-TYPE
         	SYNTAX TruthValue
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "In Revertive mode, after the conditions causing a protection switch has cleared, the traffic channel is restored to the working transport entity, i.e., blocked on the RPL.
				 In Non-Revertive mode, the traffic channel continues to use the RPL, if it is not failed, after a protection switch condition has cleared.
				"
            ::= { iERPSIDConfigEntry 6 }
    
    -- 1.3.6.1.4.1.4756.121.60.40.5
