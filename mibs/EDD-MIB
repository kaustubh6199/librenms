    -- 1.3.6.1.4.1.4756.121.10.3
    iIP OBJECT IDENTIFIER ::= { iSystem 3 } 
    
    -- 1.3.6.1.4.1.4756.121.10.3.1
    iMode OBJECT-TYPE
        SYNTAX      INTEGER {
                        host(1),
                        router(2)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Configure whether the IP stack should act as a Host or a Router. In Host mode, 
             IP traffic between interfaces will not be routed. In Router mode traffic is routed between all interfaces. 
			"
        ::= { iIP 1 }
        
    -- 1.3.6.1.4.1.4756.121.10.3.2
    iDNSServerType OBJECT-TYPE
        SYNTAX      INTEGER {
                        from-any-dhcp-interfaces(1),
                        no-DNS-server(2),
                        configured(3),
                        from-this-dhcp-interface(4)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This setting controls the DNS name resolution done by the switch. The following modes are supported: 
				1:From any DHCP interfaces 
					The first DNS server offered from a DHCP lease to a DHCP-enabled interface will be used. 
				
				2:No DNS server 
					No DNS server will be used.
				
				3:Configured 
					Explicitly provide the IP address of the DNS Server in iDNSServerIP Node. 
				
				4:From this DHCP interface 
					Specify from which DHCP-enabled interface a provided DNS server should be preferred. 
			"
        ::= { iIP 2 }
    
    -- 1.3.6.1.4.1.4756.121.10.23.3
    iDNSServerSetting OBJECT-TYPE
        SYNTAX      DisplayString
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION                         
            "
             If iDNSServerType setting '3:Configured',this value must setting IP address format as below:
                Dotted Decimal Notation refers to a method of writing IP addresses using decimal numbers and dots as separators between octets.
			    An IPv4 dotted decimal address has the form x.y.z.w, where x, y, z, and w are decimal numbers between 0 and 255.               
			 
			 If DNSServerType setting '4:From this DHCP interface', this value must setting VLAN ID.(Range 1~4095).
			"
        ::= { iIP 3 }
    
    -- 1.3.6.1.4.1.4756.121.10.3.4
    iDNSproxy OBJECT-TYPE
        SYNTAX      INTEGER {
                        disabled(1),
                        enabled(2)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "When DNS proxy is enabled, system will relay DNS requests to the currently configured DNS server, 
             and reply as a DNS resolver to the client devices on the network.  
			"
        ::= { iIP 4 }
    
    -- 1.3.6.1.4.1.4756.121.10.3.5
    iAddInterface OBJECT-TYPE
        SYNTAX      DisplayString
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Add a new IP VLAN Interface:
             Syntax: [<vlan_list>]
            "
        ::= { iIP 5 }
        
    -- 1.3.6.1.4.1.4756.121.10.3.6
    iDeleteInterface OBJECT-TYPE
        SYNTAX      DisplayString
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Delete a IP VLAN Interface:
             Syntax: [<vlan_list>]
            "
        ::= { iIP 6 }
    
    -- 1.3.6.1.4.1.4756.121.10.3.7
    iIPinterfaceTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF IIPinterfaceEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The switch system IP Interface."
        ::= { iIP 7 }
    
    -- 1.3.6.1.4.1.4756.121.10.3.7.1
    iIPinterfaceEntry OBJECT-TYPE
        SYNTAX      IIPinterfaceEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The system log entry."
        INDEX { iIPinterfaceVLAN }
        ::= { iIPinterfaceTable 1 }

    IIPinterfaceEntry ::=
        SEQUENCE {
            iIPinterfaceVLAN    Integer32,
            iIPinterfaceDHCP    INTEGER,
            iIPv4Address   		DisplayString,
            iIPv4MaskLenth 		Integer32,     
            iIPv4CurrentLease 	DisplayString,
            iIPv6Address 		DisplayString,
            iIPv6MaskLenth    	Integer32
        }
        
     -- 1.3.6.1.4.1.4756.121.10.3.7.1.1
     iIPinterfaceVLAN OBJECT-TYPE
         SYNTAX	Integer32 (1..4095)
         MAX-ACCESS  read-only
         STATUS      current
         DESCRIPTION
             "The VLAN associated with the IP interface. Only ports in this VLAN will be able to access the IP interface. 
              This field is only available for input when creating an new interface. 
			 "
         ::= { iIPinterfaceEntry 1 }       
         
     -- 1.3.6.1.4.1.4756.121.10.3.7.1.2
     iIPinterfaceDHCP OBJECT-TYPE
         SYNTAX	INTEGER {
                     		disable(1),
                     		enabled(2)
                 		}
         MAX-ACCESS  read-write
         STATUS      current
         DESCRIPTION
             "Enable the DHCP client by checking this box. If this option is enabled, 
              the system will configure the IPv4 address and mask of the interface using the DHCP protocol. 
              The DHCP client will announce the configured System Name as hostname to provide DNS lookup. 
			 "
         ::= { iIPinterfaceEntry 2 }    
     
     -- 1.3.6.1.4.1.4756.121.10.3.7.1.3
     iIPv4Address OBJECT-TYPE
         SYNTAX		 DisplayString
         MAX-ACCESS  read-write
         STATUS      current
         DESCRIPTION
             "The IPv4 address of the interface.
              IP addresses using decimal numbers and dots as separators between octets.
			  An IPv4 dotted decimal address has the form x.y.z.w, where x, y, z, and w are decimal numbers between 0 and 255. 
			  If DHCP is enabled, this field is not used. 
			  The field may also be left blank if IPv4 operation on the interface is not desired. 
			 "
         ::= { iIPinterfaceEntry 3 }     
     
     
     -- 1.3.6.1.4.1.4756.121.10.3.7.1.4
     iIPv4MaskLenth OBJECT-TYPE
         SYNTAX		 Integer32 (0..30)
         MAX-ACCESS  read-write
         STATUS      current
         DESCRIPTION
             "The IPv4 network mask, in number of bits (prefix length). 
              Valid values are between 0 and 30 bits for a IPv4 address. 
			   If DHCP is enabled, this field is not used. 
			   The field may also be left blank if IPv4 operation on the interface is not desired.
			 "
         ::= { iIPinterfaceEntry 4 }    
      
     -- 1.3.6.1.4.1.4756.121.10.3.7.1.5
     iIPv4CurrentLease OBJECT-TYPE
         SYNTAX		 DisplayString
         MAX-ACCESS  read-only
         STATUS      current
         DESCRIPTION
             "For DHCP interfaces with an active lease, this column show the current interface address, as provided by the DHCP server."
         ::= { iIPinterfaceEntry 5 }
         
     -- 1.3.6.1.4.1.4756.121.10.3.7.1.6
     iIPv6Address OBJECT-TYPE
         SYNTAX		 DisplayString
         MAX-ACCESS  read-write
         STATUS      current
         DESCRIPTION
             "The IPv6 address of the interface. 
              A IPv6 address is in 128-bit records represented as eight fields of up to four hexadecimal digits with a colon separating each field (:). For example, fe80::215:c5ff:fe03:4dc7. The symbol :: is a special syntax that can be used as a shorthand way of representing multiple 16-bit groups of contiguous zeros; but it can appear only once. It can also represent a legally valid IPv4 address. For example, ::192.1.2.34. 
              The field may be left blank if IPv6 operation on the interface is not desired. 
			 "
         ::= { iIPinterfaceEntry 6 }
    
      -- 1.3.6.1.4.1.4756.121.10.3.7.1.7
     iIPv6MaskLenth OBJECT-TYPE
         SYNTAX		 Integer32 (1..128)
         MAX-ACCESS  read-write
         STATUS      current
         DESCRIPTION
             "The IPv6 network mask, in number of bits (prefix length). 
              Valid values are between 1 and 128 bits for a IPv6 address. 
			  The field may be left blank if IPv6 operation on the interface is not desired.  
			 "
         ::= { iIPinterfaceEntry 7 }
     
    
    -- 1.3.6.1.4.1.4756.121.10.3.8
    iAddRoutes OBJECT-TYPE
        SYNTAX      DisplayString
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Add a new IP Routes:
             Syntax: <ip_net> <ip_gateway>
            "
        ::= { iIP 8 }
    
    -- 1.3.6.1.4.1.4756.121.10.3.9
    iDeleteRoutes OBJECT-TYPE
        SYNTAX      DisplayString
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Delete a IP VLAN Interface:
             Syntax: <ip_net> <ip_gateway>
            "
        ::= { iIP 9 }
     
    -- 1.3.6.1.4.1.4756.121.10.3.10
    iIProuteTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF IIProuteEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The switch system IP Interface."
        ::= { iIP 10 }
    
    -- 1.3.6.1.4.1.4756.121.10.3.10.1
    iIProuteEntry OBJECT-TYPE
        SYNTAX      IIProuteEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The IP route entry."
        INDEX { iIProuteIndex }
        ::= { iIProuteTable 1 }

    IIProuteEntry ::=
        SEQUENCE {
            iIProuteIndex  		Integer32,
            iNetwork     	 	DisplayString,
            iMaskLenth       	Integer32,
            iGateway     	 	DisplayString
        }
     
     -- 1.3.6.1.4.1.4756.121.10.3.10.1.1
     iIProuteIndex OBJECT-TYPE
         SYNTAX      Integer32 (1..8)
         MAX-ACCESS  read-only
         STATUS      current
         DESCRIPTION
             "The Index of the IP routes."
         ::= { iIProuteEntry 1 }
 
     
     -- 1.3.6.1.4.1.4756.121.10.3.10.1.2
     iNetwork OBJECT-TYPE
         SYNTAX	DisplayString
         MAX-ACCESS  read-only
         STATUS      current
         DESCRIPTION
             "The destination IP network or host address of this route. 
              Valid format is dotted decimal notationor a valid IPv6 notation. 
              A default route can use the value 0.0.0.0or IPv6 :: notation. 
             "
         ::= { iIProuteEntry 2 } 
     
     -- 1.3.6.1.4.1.4756.121.10.3.10.1.3
     iMaskLenth OBJECT-TYPE
         SYNTAX	Integer32 (0..128)
         MAX-ACCESS  read-only
         STATUS      current
         DESCRIPTION
             "The destination IP network or host mask, in number of bits (prefix length). 
              It defines how much of a network address that must match, in order to qualify for this route. 
              Valid values are between 0 and 32 bits respectively 128 for IPv6 routes. Only a default route will have a mask length of 0 (as it will match anything). 
			 "
         ::= { iIProuteEntry 3 } 
     
     -- 1.3.6.1.4.1.4756.121.10.3.6.1.4
     iGateway OBJECT-TYPE
         SYNTAX	DisplayString
         MAX-ACCESS  read-only
         STATUS      current
         DESCRIPTION
             "
			   The IP address of the IP gateway. Valid format is dotted decimal notationor a valid IPv6 notation. 
               Gateway and Network must be of the same type. 
			 "
         ::= { iIProuteEntry 4 } 

    -- 1.3.6.1.4.1.4756.121.10.4
    iNTP OBJECT IDENTIFIER ::= { iSystem 4 }

    -- 1.3.6.1.4.1.4756.121.10.4.1
    iNtpMode OBJECT-TYPE
        SYNTAX  INTEGER {
                        enabled(1),
                        disabled(2)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Indicates the NTP mode operation. Possible modes are:
             Enabled: Enable NTP mode operation. When NTP mode operation is enabled,
                      the agent forwards NTP messages between the clients and the server
                      when they are not on the same subnet domain.
             Disabled: Disable NTP mode operation. "
        ::= { iNTP 1 }

    -- 1.3.6.1.4.1.4756.121.10.4.2
    iNtpServerTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF INtpServerEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The switch system log information."
        ::= { iNTP 2 }

    -- 1.3.6.1.4.1.4756.121.10.4.2.1
    iNtpServerEntry OBJECT-TYPE
        SYNTAX      INtpServerEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The NTP Server entry."
        INDEX { iNtpServerIndex }
        ::= { iNtpServerTable 1 }

    INtpServerEntry ::=
        SEQUENCE {
            iNtpServerIndex        Integer32,
            iNtpServerAddress      DisplayString
        }

        -- 1.3.6.1.4.1.4756.121.10.4.1.1
        iNtpServerIndex OBJECT-TYPE
            SYNTAX      Integer32 (1..5)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The Index of the NTP server."
            ::= { iNtpServerEntry 1 }

        -- 1.3.6.1.4.1.4756.121.10.4.1.2
        iNtpServerAddress OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "It must either be a valid IP address or a valid hostname.
                 A valid hostname is a string drawn from the alphabet(A-Za-z), digits(0-9), dot(.), dash(-).
                 Spaces are not allowed, the first character must be an alpha character,
                 and the first and last characters must not be a dot or a dash.
                 IPv4 address is in dotted decimal notation ('x.y.z.w').
                 IPv6 address is in 128-bit records represented as eight fields of up to
                 four hexadecimal digits with a colon separating each field (:)."
            ::= { iNtpServerEntry 2 }
 
