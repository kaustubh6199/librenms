-- Security
    -- 1.3.6.1.4.1.4756.121.40
    iSecurity OBJECT IDENTIFIER ::= { eddCard 30 }

    -- 1.3.6.1.4.1.4756.121.40.1
    iSecuritySwitch OBJECT IDENTIFIER ::= { iSecurity 1 }

    -- 1.3.6.1.4.1.4756.121.40.1.1    
    iAddUser OBJECT-TYPE
        SYNTAX      OCTET STRING (SIZE (1..32))
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "A string identifying the user name that this entry should belong to.
             The allowed string length is 1 to 32.
             The valid user name is a combination of letters, numbers and underscores.
            "
        ::= { iSecuritySwitch 1 }

    -- 1.3.6.1.4.1.4756.121.40.1.2    
    iDeleteUser OBJECT-TYPE
        SYNTAX      DisplayString
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Delete the user by name.
             Syntax: 
             		<user_name>
            "
        ::= { iSecuritySwitch 2 }

    -- 1.3.6.1.4.1.4756.121.40.1.3
    iUserConfigTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF IUserConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table provides User Configurations."
        ::= { iSecuritySwitch 3 }

    -- 1.3.6.1.4.1.4756.121.40.1.3.1
    iUserConfigEntry OBJECT-TYPE
        SYNTAX      IUserConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Entry of User Configurations."
        INDEX { iUserIndex }
        ::= { iUserConfigTable 1 }

    IUserConfigEntry ::=
        SEQUENCE {
            iUserIndex             Integer32,
            iUserName              OCTET STRING,
            iUserPassword          OCTET STRING,
            iUserPrivilegeLevel    Integer32
        }

        -- 1.3.6.1.4.1.4756.121.40.1.3.1.1
        iUserIndex OBJECT-TYPE
            SYNTAX      Integer32 (1..20)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The index of user entries."
            ::= { iUserConfigEntry 1 }

        -- 1.3.6.1.4.1.4756.121.40.1.3.1.2
        iUserName OBJECT-TYPE
            SYNTAX      OCTET STRING (SIZE (1..32))
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The name identifying the user."
            ::= { iUserConfigEntry 2 }

        -- 1.3.6.1.4.1.4756.121.40.1.3.1.3
        iUserPassword OBJECT-TYPE
            SYNTAX      OCTET STRING (SIZE (0..32))
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "The password of the user. The allowed string length is 0 to 32."
            ::= { iUserConfigEntry 3 }

        -- 1.3.6.1.4.1.4756.121.40.1.3.1.4
        iUserPrivilegeLevel OBJECT-TYPE
            SYNTAX      Integer32 (1..15)
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "The privilege level of the user. The allowed range is 1 to 15.
                 If the privilege level value is 15, it can access all groups,
                 i.e. that is granted the fully control of the device.
                 But others value need to refer to each group privilege level.
                 User's privilege should be same or greater than the group privilege level to have the access of that group.
                 By default setting, most groups privilege level 5 has the read-only access
                 and privilege level 10 has the read-write access. And the system maintenance
                 (software upload, factory defaults and etc.) need user privilege level 15.
                 Generally, the privilege level 15 can be used for an administrator account,
                 privilege level 10 for a standard user account and privilege level 5 for a guest account."
            ::= { iUserConfigEntry 4 }

    -- 1.3.6.1.4.1.4756.121.40.1.4
    iPrivilegeLevelTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF IPrivilegeLevelEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table provides privilege levels configurations."
        ::= { iSecuritySwitch 4 }

    -- 1.3.6.1.4.1.4756.121.40.1.4.1
    iPrivilegeLevelEntry OBJECT-TYPE
        SYNTAX      IPrivilegeLevelEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The entry of privilege groups."
        INDEX { iPrivilegeGroupID }
        ::= { iPrivilegeLevelTable 1 }

    IPrivilegeLevelEntry ::=
        SEQUENCE {
            iPrivilegeGroupID                  Integer32,
            iPrivilegeGroupName                DisplayString,
            iPrivilegeConfigReadOnlyLevel      Integer32,
            iPrivilegeConfigReadWriteLevel     Integer32,
            iPrivilegeStatusReadOnlyLevel      Integer32,
            iPrivilegeStatusReadWriteLevel     Integer32
        }

        -- 1.3.6.1.4.1.4756.121.40.1.4.1.1
        iPrivilegeGroupID OBJECT-TYPE
            SYNTAX      Integer32 (1..26)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The ID of privilege groups."
            ::= { iPrivilegeLevelEntry 1 }

        -- 1.3.6.1.4.1.4756.121.40.1.4.1.2
        iPrivilegeGroupName OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The name identifying the privilege group. In most cases,
                 a privilege level group consists of a single module (e.g. LACP, RSTP or QoS),
                 but a few of them contains more than one."
            ::= { iPrivilegeLevelEntry 2 }

        -- 1.3.6.1.4.1.4756.121.40.1.4.1.3
        iPrivilegeConfigReadOnlyLevel OBJECT-TYPE
            SYNTAX      Integer32 (1..15)
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "The authorization privilege level for configuration read-only of the group.
                 The privilege level of 'Read-only' should be less or equal 'Read/write'."
            ::= { iPrivilegeLevelEntry 3 }

        -- 1.3.6.1.4.1.4756.121.40.1.4.1.4
        iPrivilegeConfigReadWriteLevel OBJECT-TYPE
            SYNTAX      Integer32 (1..15)
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "The authorization privilege level for configuration/execute read-write of the group.
                 The privilege level of 'Configuration/Execute Read/write'
                 should be great or equal 'Status/Statistics Read-only'"
            ::= { iPrivilegeLevelEntry 4 }

        -- 1.3.6.1.4.1.4756.121.40.1.4.1.5
        iPrivilegeStatusReadOnlyLevel OBJECT-TYPE
            SYNTAX      Integer32 (1..15)
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "The authorization privilege level for status/statistics read-only of the group.
                 The privilege level of 'Read-only' should be less or equal 'Read/write'."
            ::= { iPrivilegeLevelEntry 5 }

        -- 1.3.6.1.4.1.4756.121.40.1.4.1.6
        iPrivilegeStatusReadWriteLevel OBJECT-TYPE
            SYNTAX      Integer32 (1..15)
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "The authorization privilege level for status/statistics read-write
                 (e.g. for clearing of statistics) of the group."
            ::= { iPrivilegeLevelEntry 6 }

    -- 1.3.6.1.4.1.4756.121.40.1.5
    iAuthenticationMethodTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF IAuthenticationMethodEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table allows you to configure how a user is authenticated when
             he logs into the switch via one of the management client interfaces."
        ::= { iSecuritySwitch 5 }

    -- 1.3.6.1.4.1.4756.121.40.1.5.1
    iAuthenticationMethodEntry OBJECT-TYPE
        SYNTAX      IAuthenticationMethodEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The entry of the management client interfaces."
        INDEX { iAuthenticationClientIndex }
        ::= { iAuthenticationMethodTable 1 }

    IAuthenticationMethodEntry ::=
        SEQUENCE {
            iAuthenticationClientIndex         Integer32,
            iAuthenticationClientInterface     DisplayString,
            iAuthenticationMethod1             INTEGER,
            iAuthenticationMethod2             INTEGER,
            iAuthenticationMethod3             INTEGER
        }

        -- 1.3.6.1.4.1.4756.121.40.1.5.1.1
        iAuthenticationClientIndex OBJECT-TYPE
            SYNTAX      Integer32 (1..4)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The index of the management client interfaces."
            ::= { iAuthenticationMethodEntry 1 }

        -- 1.3.6.1.4.1.4756.121.40.1.5.1.2
        iAuthenticationClientInterface OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The management client interface."
            ::= { iAuthenticationMethodEntry 2 }

        -- 1.3.6.1.4.1.4756.121.40.1.5.1.3
        iAuthenticationMethod1 OBJECT-TYPE
            SYNTAX      INTEGER {
                            none(1),
                            local(2),
                            radius(3),
                            tacacsPlus(4)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Authentication Method can be set to one of the following values:
                 none : authentication is disabled and login is not possible.
                 local : use the local user database on the switch for authentication.
                 radius : use a remote RADIUS server for authentication.
                 tacacs+ : use a remote TACACS+ server for authentication."
            ::= { iAuthenticationMethodEntry 3 }
        
        -- 1.3.6.1.4.1.4756.121.40.1.5.1.4
        iAuthenticationMethod2 OBJECT-TYPE
            SYNTAX      INTEGER {
                            none(1),
                            local(2),
                            radius(3),
                            tacacsPlus(4)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Authentication Method can be set to one of the following values:
                 none : authentication is disabled and login is not possible.
                 local : use the local user database on the switch for authentication.
                 radius : use a remote RADIUS server for authentication.
                 tacacs+ : use a remote TACACS+ server for authentication."
            ::= { iAuthenticationMethodEntry 4 }
         
         -- 1.3.6.1.4.1.4756.121.40.1.5.1.5
        iAuthenticationMethod3 OBJECT-TYPE
            SYNTAX      INTEGER {
                            none(1),
                            local(2),
                            radius(3),
                            tacacsPlus(4)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Authentication Method can be set to one of the following values:
                 none : authentication is disabled and login is not possible.
                 local : use the local user database on the switch for authentication.
                 radius : use a remote RADIUS server for authentication.
                 tacacs+ : use a remote TACACS+ server for authentication."
            ::= { iAuthenticationMethodEntry 5 }

    -- 1.3.6.1.4.1.4756.121.40.1.6    
    iSshMode OBJECT-TYPE
        SYNTAX      INTEGER {
                        enabled(1),
                        disabled(2)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Indicates the SSH mode operation. Possible modes are: 
             Enabled: Enable SSH mode operation. 
             Disabled: Disable SSH mode operation."
        ::= { iSecuritySwitch 6 }

    -- 1.3.6.1.4.1.4756.121.40.1.7    
