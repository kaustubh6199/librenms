		-- 1.3.6.1.4.1.4756.121.60.30.8
    iBridgeStatusTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF IBridgeStatusEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table provides status of bridge."
        ::= { iSpanningTree 8 }

    -- 1.3.6.1.4.1.4756.121.60.30.8.1
    iBridgeStatusEntry OBJECT-TYPE
        SYNTAX      IBridgeStatusEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Entry of MSTI configurations."
        INDEX { iMSTIPrioritiesIndex }
        ::= { iBridgeStatusTable 1 }

    IBridgeStatusEntry ::=
        SEQUENCE {
						iBridgeStatusMSTIName		DisplayString,
						iBridgeID					DisplayString,
						iRootID						DisplayString,
						iRootCost					Integer32,
						iRootPort					DisplayString,
						iRegionalRoot				DisplayString,
						iInternalRootCost			Integer32,
						iTopologyFlag				DisplayString,
						iTopologyChangeCount		Integer32,
						iTopologyChangeLast     	DisplayString
        }
        
        -- 1.3.6.1.4.1.4756.121.60.30.8.1.1
        iBridgeStatusMSTIName OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The Bridge instance - CIST, MST1, ... "
            ::= { iBridgeStatusEntry 1 }
        
        -- 1.3.6.1.4.1.4756.121.60.30.8.1.2
        iBridgeID OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The Bridge ID of this Bridge instance."
            ::= { iBridgeStatusEntry 2 }
            
        -- 1.3.6.1.4.1.4756.121.60.30.8.1.3
        iRootID OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The Bridge ID of the currently elected root bridge. "
            ::= { iBridgeStatusEntry 3 }
            
        -- 1.3.6.1.4.1.4756.121.60.30.8.1.4
        iRootCost OBJECT-TYPE
            SYNTAX      Integer32
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Root Path Cost. For the Root Bridge this is zero. 
                 For all other Bridges, it is the sum of the Port Path Costs on the least cost path to the Root Bridge. "
            ::= { iBridgeStatusEntry 4 }
            
        -- 1.3.6.1.4.1.4756.121.60.30.8.1.5
        iRootPort OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The switch port currently assigned the root port role."
            ::= { iBridgeStatusEntry 5 }
            
        -- 1.3.6.1.4.1.4756.121.60.30.8.1.6
        iRegionalRoot OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The Bridge ID of the currently elected regional root bridge, inside the MSTP region of this bridge. (For the CIST instance only)."
            ::= { iBridgeStatusEntry 6 }
            
        -- 1.3.6.1.4.1.4756.121.60.30.8.1.7
        iInternalRootCost OBJECT-TYPE
            SYNTAX      Integer32
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The Regional Root Path Cost. For the Regional Root Bridge this is zero. 
                 For all other CIST instances in the same MSTP region, 
                 it is the sum of the Internal Port Path Costs on the least cost path to the Internal Root Bridge. (For the CIST instance only). "
            ::= { iBridgeStatusEntry 7 }
            
        -- 1.3.6.1.4.1.4756.121.60.30.8.1.8
        iTopologyFlag OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The current state of the Topology Change Flag of this Bridge instance. "
            ::= { iBridgeStatusEntry 8 }
            
       	-- 1.3.6.1.4.1.4756.121.60.30.8.1.9
        iTopologyChangeCount OBJECT-TYPE
            SYNTAX      Integer32
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The number of times where the topology change flag has been set (during a one-second interval). "
            ::= { iBridgeStatusEntry 9 }
        
        -- 1.3.6.1.4.1.4756.121.60.30.8.1.10
        iTopologyChangeLast OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The time passed since the Topology Flag was last set."
            ::= { iBridgeStatusEntry 10 }

		-- 1.3.6.1.4.1.4756.121.60.30.9
    iPortsAndAggregationsStateTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF IPortsAndAggregationsStateEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table provides detailed information on a single STP bridge instance, along with port state for all active ports associateds for all ports."
        ::= { iSpanningTree 9 }

    -- 1.3.6.1.4.1.4756.121.60.30.9.1
    iPortsAndAggregationsStateEntry OBJECT-TYPE
        SYNTAX      IPortsAndAggregationsStateEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Entry of CIST Ports & Aggregations State"
        INDEX { iMSTIPrioritiesIndex, iPortIndex }
        ::= { iPortsAndAggregationsStateTable 1 }

    IPortsAndAggregationsStateEntry ::=
        SEQUENCE {              
        				iPortMSTIName			DisplayString,
						iPortID					DisplayString,
						iPortRole				DisplayString,
						iPortState				DisplayString,
						iPortPathCost			DisplayString,
						iPortEdge				DisplayString,
						iPortPointToPoint		DisplayString,
						iPortUpTime				DisplayString
        }
        
        -- 1.3.6.1.4.1.4756.121.60.30.8.1.1
        iPortMSTIName OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The Bridge instance - CIST, MST1, ... "
            ::= { iPortsAndAggregationsStateEntry 1 }

        
        -- 1.3.6.1.4.1.4756.121.60.30.9.1.2
        iPortID OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The port id as used by the STP protocol. This is the priority part and the logical port index of the bridge port."
            ::= { iPortsAndAggregationsStateEntry 2 }
            
        -- 1.3.6.1.4.1.4756.121.60.30.9.1.3
        iPortRole OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The current STP port role. The port role can be one of the following values: AlternatePort BackupPort RootPort DesignatedPort. "
            ::= { iPortsAndAggregationsStateEntry 3 }
            
        -- 1.3.6.1.4.1.4756.121.60.30.9.1.4
        iPortState OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The current STP port state. The port state can be one of the following values: Discarding Learning Forwarding. "
            ::= { iPortsAndAggregationsStateEntry 4 }
            
        -- 1.3.6.1.4.1.4756.121.60.30.9.1.5
        iPortPathCost OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The current STP port path cost. This will either be a value computed from the Auto setting, or any explicitly configured value."
            ::= { iPortsAndAggregationsStateEntry 5 }
            
        -- 1.3.6.1.4.1.4756.121.60.30.9.1.6
        iPortEdge OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The current STP port (operational) Edge Flag. An Edge Port is a switch port to which no Bridges are attached. 
                 The flag may be automatically computed or explicitly configured. 
                 Each Edge Port transits directly to the Forwarding Port State, since there is no possibility of it participating in a loop. "
            ::= { iPortsAndAggregationsStateEntry 6 }
            
        -- 1.3.6.1.4.1.4756.121.60.30.9.1.7
        iPortPointToPoint OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The current STP port point-to-point flag. A point-to-point port connects to a non-shared LAN media. 
                 The flag may be automatically computed or explicitly configured. 
                 The point-to-point properties of a port affect how fast it can transit to STP state. 
                "
            ::= { iPortsAndAggregationsStateEntry 7 }
            
        -- 1.3.6.1.4.1.4756.121.60.30.9.1.8
        iPortUpTime OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The time since the bridge port was last initialized."
            ::= { iPortsAndAggregationsStateEntry 8 }
            
