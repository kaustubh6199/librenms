-- VLAN Translation
    -- 1.3.6.1.4.1.4756.121.130
    iVlanTranslation  OBJECT IDENTIFIER ::= { eddCard 150 }

    -- 1.3.6.1.4.1.4756.121.130.1
    iAddVlanTransPortToGroup OBJECT-TYPE
        SYNTAX      DisplayString
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION                                  
            "Add a new entry in Port to Group Mapping Table.
             Note that if a VLAN translation is enabled on a management port for management VLAN,
             it may disrupt the management connectivity in some cases.
             Syntax: <port_list> <group_id>"
        ::= { iVlanTranslation 1 }

    -- 1.3.6.1.4.1.4756.121.130.2
    iVlanTransPortToGroupMappingTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF IVlanTransPortToGroupMappingEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table allows you to map set of Port members to a Group ID for all switch ports."
        ::= { iVlanTranslation 2 }

    -- 1.3.6.1.4.1.4756.121.130.2.1
    iVlanTransPortToGroupMappingEntry OBJECT-TYPE
        SYNTAX      IVlanTransPortToGroupMappingEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Entry of Port to Group mapping Table."
        INDEX { iVlanTransGroupID }
        ::= { iVlanTransPortToGroupMappingTable 1 }

    IVlanTransPortToGroupMappingEntry ::=
        SEQUENCE {       
            iVlanTransGroupID                  Integer32,
            iVlanTransPortToGroupPortMembers   DisplayString
        }

        -- 1.3.6.1.4.1.4756.121.130.2.1.1    
        iVlanTransGroupID OBJECT-TYPE
            SYNTAX      Integer32 (1..5)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "A valid Group ID is an integer value form 1 to 6.
                 A set of VLAN Translations are mapped to a group Id.
                 This way a port is mapped to a list of VLAN Translations easily by
                 mapping it to a group."
            ::= { iVlanTransPortToGroupMappingEntry 1 }

        -- 1.3.6.1.4.1.4756.121.130.2.1.2    
        iVlanTransPortToGroupPortMembers OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "A port can be mapped to any of the groups.
                 Multiple ports can also be mapped to a group with same group Id.
                 Note: By default, each port is mapped to a group with a group Id equal to the port number.
                       For example, port 1 is mapped to the group with ID=1."
            ::= { iVlanTransPortToGroupMappingEntry 2 }

    -- 1.3.6.1.4.1.4756.121.130.3
    iAddVlanTransMapping OBJECT-TYPE
        SYNTAX      DisplayString
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION                                  
            "Add a new entry in VLAN Translation table.
             Syntax: <group_id> <vid> <trans_vid>"
        ::= { iVlanTranslation 3 }

    -- 1.3.6.1.4.1.4756.121.130.4
