    -- 1.3.6.1.4.1.4756.121.60.40.1
    iAddNewMEP OBJECT-TYPE
        SYNTAX      DisplayString
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Added a new instance of MEP.
             Syntax: <Instance> <Flow Instance> <Tagged ID>
           	 Instance range: 1 ~ 100
             Flow Instance : Switch port number
             Tagged ID     : 1 ~ 4095
            "
        ::= { iMEP 1 }
        
     -- 1.3.6.1.4.1.4756.121.60.40.2
    iDeleteMEP OBJECT-TYPE
        SYNTAX      DisplayString
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Deleted a instance of MEP.
            	Syntax: <Instance>
            "
        ::= { iMEP 2 }
        
    -- 1.3.6.1.4.1.4756.121.60.40.3
    iMEPConfigTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF IMEPConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Maintenance Entity Point instances Table."
        ::= { iMEP 3 }
     
     -- 1.3.6.1.4.1.4756.121.60.40.3.1
    iMEPConfigEntry OBJECT-TYPE
        SYNTAX      IMEPConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Maintenance Entity Point instances Entry."
        INDEX { iMEPInstanceIndex }
        ::= { iMEPConfigTable 1 }

    IMEPConfigEntry ::= 
    SEQUENCE {
        	iMEPInstanceIndex		  Integer32,
            iMEPDomain      	  	  INTEGER,
            iMEPMode				  INTEGER,
            iMEPDirection			  INTEGER,
            iMEPResidencePort		  Integer32,
            iMEPFlowInstance		  Integer32,
			iMEPThisMAC				  DisplayString,
			iMEPAlarm				  INTEGER,
			iMEPLevel				  Integer32,
			iMEPFormat				  INTEGER,
			iMEPDomainName			  DisplayString,
			iMEGID				  	  DisplayString,
			iMEPID                    Integer32,
			iMEPTaggedID			  Integer32,
			iMEPLLF					  TruthValue,
			icLevel					  INTEGER,
			icMEG					  INTEGER,
			icMEP					  INTEGER,
			icAIS					  INTEGER,
			icLCK					  INTEGER,
			icSSF					  INTEGER,
			iaBLK					  INTEGER,
			iaTSF 					  INTEGER
        }
        
    	-- 1.3.6.1.4.1.4756.121.60.40.3.1.1    
        iMEPInstanceIndex OBJECT-TYPE
         	SYNTAX Integer32 (1..100)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The ID of the MEP. Click on the ID of a MEP."
            ::= { iMEPConfigEntry 1 }
            
        -- 1.3.6.1.4.1.4756.121.60.40.3.1.2
        iMEPDomain OBJECT-TYPE
         	SYNTAX INTEGER {
         					  port(1)
         				   }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Port: This is a MEP in the Port Domain. 'Flow Instance' is a Port."
            ::= { iMEPConfigEntry 2 }
            
        -- 1.3.6.1.4.1.4756.121.60.40.3.1.3
        iMEPMode OBJECT-TYPE
         	SYNTAX INTEGER {
         						mep(1),
         						mip(2)
         				   }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "MEP: This is a Maintenance Entity End Point.
				 MIP: This is a Maintenance Entity Intermediate Point.
                "
            ::= { iMEPConfigEntry 3 }
            
        -- 1.3.6.1.4.1.4756.121.60.40.3.1.4
        iMEPDirection OBJECT-TYPE
         	SYNTAX INTEGER {
         						ingress(1),
         						egress(2)
         				   }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Ingress: This is a Ingress (down) MEP - monitoring ingress traffic on 'Residence Port'.
				 Egress: This is a Egress (up) MEP - monitoring egress traffic on 'Residence Port'.
                "
            ::= { iMEPConfigEntry 4 }
            
        -- 1.3.6.1.4.1.4756.121.60.40.3.1.5
        iMEPResidencePort OBJECT-TYPE
         	SYNTAX Integer32 (1..5)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The port where MEP is monitoring - see 'iMEPDirection'."
            ::= { iMEPConfigEntry 5 }

		-- 1.3.6.1.4.1.4756.121.60.40.3.1.6
        iMEPFlowInstance OBJECT-TYPE
         	SYNTAX Integer32 (1..5)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The MEP is related to this flow - See 'iMEPDomain'."
            ::= { iMEPConfigEntry 6 }
            
        -- 1.3.6.1.4.1.4756.121.60.40.3.1.7
