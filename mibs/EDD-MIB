-- Ports
    -- 1.3.6.1.4.1.4756.121.30
    iPorts OBJECT IDENTIFIER ::= { eddCard 20 }
    
    -- 1.3.6.1.4.1.4756.121.30.1
    iPortsConfigTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF IPortsConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table shows current port configurations. Ports can also be configured here."
        ::= { iPorts 1 }

    -- 1.3.6.1.4.1.4756.121.30.1.1
    iPortsConfigEntry OBJECT-TYPE
        SYNTAX      IPortsConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The entry of ports."
        INDEX { iPortIndex }
        ::= { iPortsConfigTable 1 }

    IPortsConfigEntry ::=
        SEQUENCE {
			iPortIndex         	   Integer32,
            iPortLink              INTEGER,
            iPortSpeedCurrent      INTEGER,
            iPortSpeedMode         INTEGER,
            iPortFlowCtrlCurrentRx TruthValue,
            iPortFlowCtrlCurrentTx TruthValue,
            iPortFlowCtrlEnabled   TruthValue,
            iPortMaxFrameSize      Integer32,
            iPortExcCollMode       INTEGER,
            iPortALS		       INTEGER,
            iPortLLFLogic		   INTEGER,
            iPortLLFP1	           INTEGER,
            iPortLLFP2	           INTEGER,
            iPortLLFP3	           INTEGER,
            iPortLLFP4	           INTEGER,
            iPortMEPInstance	   Integer32,
            iPortMEPLogic   	   INTEGER

        }
        
        iPortIndex OBJECT-TYPE
            SYNTAX      Integer32 (1..5)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "This is the logical port number for this and below rows."
            ::= { iPortsConfigEntry 1 }

                         
        -- 1.3.6.1.4.1.4756.121.30.1.1.1    
        iPortLink OBJECT-TYPE
            SYNTAX      INTEGER {
                            linkUp(1),
                            linkDown(2),
                            linkDownByACL(3),
                            linkDownByLoopProtect(4),
                            linkDownByMEP(5),
                            linkDownByLLF(6)
                        }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The current link state.
                	1: linkUp
                	2: linkDown
                	3: linkDownByACL
                	4: linkDownByLoopProtect
                	5: linkDownByMEP
                	6: linkDownByLLF
                "
            ::= { iPortsConfigEntry 2 }
                  
        -- 1.3.6.1.4.1.4756.121.30.1.1.2    
        iPortSpeedCurrent OBJECT-TYPE
            SYNTAX      INTEGER {
                            linkDown(1),
                            link10Mhdx(2),
                            link10Mfdx(3),
                            link100Mhdx(4),
                            link100Mfdx(5),
                            link1Gfdx(6),
                            link2-5Gfdx(7)
                        }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Provides the current link speed of the port.
                	1: linkDown
                	2: link10Mhdx
                	3: link10Mfdx
                	4: link100Mhdx
                	5: link100Mfdx
                	6: link1Gfdx
                	7: link2-5Gfdx
                "
            ::= { iPortsConfigEntry 3 }
                  
        -- 1.3.6.1.4.1.4756.121.30.1.1.3    
        iPortSpeedMode OBJECT-TYPE
            SYNTAX      INTEGER {
                            disabled(1),
                            auto(2),
                            speed10Mhdx(3),
                            speed10Mfdx(4),
                            speed100Mhdx(5),
                            speed100Mfdx(6),
                            speed1Gfdx(7),
                            speed2-5Gfdx(8)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Select link speed for the switch port."
            ::= { iPortsConfigEntry 4 }
                   
        -- 1.3.6.1.4.1.4756.121.30.1.1.4    
        iPortFlowCtrlCurrentRx OBJECT-TYPE
            SYNTAX      TruthValue
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates whether pause frames on the port are obeyed."
            ::= { iPortsConfigEntry 5 }
                  
        -- 1.3.6.1.4.1.4756.121.30.1.1.5    
        iPortFlowCtrlCurrentTx OBJECT-TYPE
            SYNTAX      TruthValue
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates whether pause frames on the port are transmitted."
            ::= { iPortsConfigEntry 6 }
                  
        -- 1.3.6.1.4.1.4756.121.30.1.1.6    
        iPortFlowCtrlEnabled OBJECT-TYPE
            SYNTAX      TruthValue
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "When Auto Speed is selected on a port,
                 this section indicates the flow control capability that is advertised to the link partner."
            ::= { iPortsConfigEntry 7 }
                  
        -- 1.3.6.1.4.1.4756.121.30.1.1.7    
        iPortMaxFrameSize OBJECT-TYPE
            SYNTAX      Integer32 (1518..9600)
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "The maximum frame size allowed for the switch port, including FCS."
            ::= { iPortsConfigEntry 8 }
                  
        -- 1.3.6.1.4.1.4756.121.30.1.1.8    
        iPortExcCollMode OBJECT-TYPE
            SYNTAX      INTEGER {
                            discard(1),
                            restart(2)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Configure port transmit collision behavior. only support on copper ports
                 Discard: Discard frame after 16 collisions (default).
                 Restart: Restart backoff algorithm after 16 collisions."
            DEFVAL      { 1 }
            ::= { iPortsConfigEntry 9 }
                  
        -- 1.3.6.1.4.1.4756.121.30.1.1.9    
        iPortALS OBJECT-TYPE
            SYNTAX      INTEGER {
                            disable(1),
                            enable(2)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Configure Auto Laser Shutdown."
            DEFVAL      { 1 }
            ::= { iPortsConfigEntry 10 }
            
        iPortLLFLogic OBJECT-TYPE
            SYNTAX      INTEGER {
                            and(1),
                            or(2)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Configure Link Loss Forwarding logic."
            DEFVAL      { 1 }
            ::= { iPortsConfigEntry 11 }
                                    
         iPortLLFP1 OBJECT-TYPE
            SYNTAX      INTEGER {
                            dont-care(1),
                            selected(2)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Configure Link Loss Forwarding logical port1."
            DEFVAL      { 1 }
            ::= { iPortsConfigEntry 12 }  
            
          iPortLLFP2 OBJECT-TYPE
            SYNTAX      INTEGER {
                            dont-care(1),
                            selected(2)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Configure Link Loss Forwarding logical port2."
            DEFVAL      { 1 }
            ::= { iPortsConfigEntry 13 }
             
           iPortLLFP3 OBJECT-TYPE
            SYNTAX      INTEGER {
                            dont-care(1),
                            selected(2)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Configure Link Loss Forwarding logical port3."
            DEFVAL      { 1 }
            ::= { iPortsConfigEntry 14 }   
            
           iPortLLFP4 OBJECT-TYPE
            SYNTAX      INTEGER {
                            dont-care(1),
                            selected(2)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Configure Link Loss Forwarding logical port4."
            DEFVAL      { 1 }
            ::= { iPortsConfigEntry 15 }

           iPortMEPInstance OBJECT-TYPE
            SYNTAX      Integer32 (0..8191)
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "The port LLF depends on MEP Instance."
            ::= { iPortsConfigEntry 16 }

        iPortMEPLogic OBJECT-TYPE
            SYNTAX      INTEGER {
                            and(1),
                            or(2)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Configure Link Loss Forwarding MEP logic."
            DEFVAL      { 1 }
            ::= { iPortsConfigEntry 17 } 
            
    -- 1.3.6.1.4.1.4756.121.30.2
