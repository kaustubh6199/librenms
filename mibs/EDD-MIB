            iVlanPort2Member   	   INTEGER,
            iVlanPort3Member       INTEGER,
            iVlanPort4Member   	   INTEGER,
            iVlanPort5Member       INTEGER
        }
 
        iVlanID OBJECT-TYPE
            SYNTAX      Integer32 (1..4095)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the ID of this particular VLAN."
            ::= { iVlanMembershipEntry 1 }
  
