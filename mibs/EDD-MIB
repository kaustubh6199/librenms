	-- 1.3.6.1.4.1.4756.121.60.30.6
    iCISTPorts OBJECT IDENTIFIER ::= { iSpanningTree 6 }
	
	  -- 1.3.6.1.4.1.4756.121.60.30.6.1
      iCISTAggregatedPort OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The switch port number of the logical STP port. "
            ::= { iCISTPorts 1 }
      
      -- 1.3.6.1.4.1.4756.121.60.30.6.2
      iCISTAggregatedSTPEnabled OBJECT-TYPE
            SYNTAX    	TruthValue
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Controls whether STP is enabled on this switch port.  "
            ::= { iCISTPorts 2 }
	
			-- 1.3.6.1.4.1.4756.121.60.30.6.3
      iCISTAggregatedPathCost OBJECT-TYPE
            SYNTAX     Integer32 (0..2000000000)
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Controls the path cost incurred by the port. 
                 The Auto setting will set the path cost as appropriate by the physical link speed, using the 802.1D recommended values. 
                 Using the Specific setting, a user-defined value can be entered. 
                 The path cost is used when establishing the active topology of the network. 
                 Lower path cost ports are chosen as forwarding ports in favour of higher path cost ports.                  
                 Valid values are in the range 0 to 200000000.
                 Setting 0 means Auto path cost.
                 Setting 1 ~ 2000000000 means path cost.
								"
            ::= { iCISTPorts 3 }
			
			-- 1.3.6.1.4.1.4756.121.60.30.6.4
      iCISTAggregatedPriority OBJECT-TYPE
            SYNTAX      INTEGER {
	                        priority-0(1),
	                        priority-16(2),
	                        priority-32(3),
	                        priority-48(4),
	                        priority-64(5),
	                        priority-80(6),
	                        priority-96(7),
	                        priority-112(8),
	                        priority-128(9),
	                        priority-144(10),
	                        priority-160(11),
	                        priority-176(12),
	                        priority-192(13),
	                        priority-208(14),
	                        priority-224(15),
	                        priority-240(16)
	                    }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Controls the port priority. This can be used to control priority of ports having identical port cost."
            ::= { iCISTPorts 4 }
	
			-- 1.3.6.1.4.1.4756.121.60.30.6.5
      iCISTAggregatedAdminEdge OBJECT-TYPE
            SYNTAX      INTEGER {
	                        non-edge(1),
	                        edge(2)
	                    }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Operational flag describing whether the port is connecting directly to edge devices. 
                (No Bridges attached). Transition to the forwarding state is faster for edge ports (having operEdge true) than for other ports. 
                 The value of this flag is based on AdminEdge and AutoEdge fields. 
                 This flag is displayed as Edge in Monitor->Spanning Tree -> STP Detailed Bridge Status. 
                 Controls whether the operEdge flag should start as set or cleared. "
            ::= { iCISTPorts 5 }
	
			-- 1.3.6.1.4.1.4756.121.60.30.6.6
      iCISTAggregatedAutoEdge OBJECT-TYPE
            SYNTAX    	TruthValue
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Controls whether the bridge should enable automatic edge detection on the bridge port. 
                 This allows operEdge to be derived from whether BPDU's are received on the port or not"
            ::= { iCISTPorts 6 }
            
      -- 1.3.6.1.4.1.4756.121.60.30.6.7
      iCISTAggregatedRestrictedRole OBJECT-TYPE
            SYNTAX    	TruthValue
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "If enabled, causes the port not to be selected as Root Port for the CIST or any MSTI, even if it has the best spanning tree priority vector. 
                 Such a port will be selected as an Alternate Port after the Root Port has been selected. 
                 If set, it can cause lack of spanning tree connectivity. 
                 It can be set by a network administrator to prevent bridges external to a core region of the network influence the spanning tree active topology, possibly because those bridges are not under the full control of the administrator. 
                 This feature is also known as Root Guard. 
								"
            ::= { iCISTPorts 7 }
            
      -- 1.3.6.1.4.1.4756.121.60.30.6.8
      iCISTAggregatedRestrictedTCN OBJECT-TYPE
            SYNTAX    	TruthValue
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "If enabled, causes the port not to propagate received topology change notifications and topology changes to other ports. 
                 If set it can cause temporary loss of connectivity after changes in a spanning tree's active topology as a result of persistently incorrect learned station location information. 
                 It is set by a network administrator to prevent bridges external to a core region of the network, causing address flushing in that region, possibly because those bridges are not under the full control of the administrator or the physical link state of the attached LANs transits frequently. 
								"
            ::= { iCISTPorts 8 }
            
      -- 1.3.6.1.4.1.4756.121.60.30.6.9
      iCISTAggregatedBPDUGuard OBJECT-TYPE
            SYNTAX    	TruthValue
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "If enabled, causes the port to disable itself upon receiving valid BPDU's. 
                 Contrary to the similar bridge setting, the port Edge status does not effect this setting. 
								 A port entering error-disabled state due to this setting is subject to the bridge Port Error Recovery setting as well. 
								"
            ::= { iCISTPorts 9 }
	
			-- 1.3.6.1.4.1.4756.121.60.30.6.10
      iCISTAggregatedPointToPoint OBJECT-TYPE
            SYNTAX      INTEGER {
	                        forced-true(1),
	                        forced-false(2),
	                        auto(3)
	                    }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Controls whether the port connects to a point-to-point LAN rather than to a shared medium. 
                 This can be automatically determined, or forced either true or false. 
                 Transition to the forwarding state is faster for point-to-point LANs than for shared media. 
								"
            ::= { iCISTPorts 10 }
	
    -- 1.3.6.1.4.1.4756.121.60.30.6.11
    iCISTNormalPortConfigTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF ICISTNormalPortConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table provides CIST Normal configurations for all ports."
        ::= { iCISTPorts 11 }

    -- 1.3.6.1.4.1.4756.121.60.30.6.11.1
    iCISTNormalPortConfigEntry OBJECT-TYPE
        SYNTAX      ICISTNormalPortConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Entry of CIST Normal Port Configuration."
        INDEX { iPortIndex }
        ::= { iCISTNormalPortConfigTable 1 }

    ICISTNormalPortConfigEntry ::=
        SEQUENCE {
        	iCISTPortSTPEnabled				TruthValue,
					iCISTPortPathCost		Integer32,
					iCISTPortPriority       INTEGER,
					iCISTPortAdminEdge		INTEGER,
					iCISTPortAutoEdge       TruthValue,
					iCISTPortRestrictedRole TruthValue,
					iCISTPortRestrictedTCN  TruthValue,
					iCISTPortBPDUGuard 		TruthValue,
					iCISTPortPointToPoint   INTEGER
        }
        
      
      -- 1.3.6.1.4.1.4756.121.60.30.6.11.1
      iCISTPortSTPEnabled OBJECT-TYPE
            SYNTAX    	TruthValue
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Controls whether STP is enabled on this switch port.  "
            ::= { iCISTNormalPortConfigEntry 1 }
	
			-- 1.3.6.1.4.1.4756.121.60.30.6.11.2
      iCISTPortPathCost OBJECT-TYPE
            SYNTAX     Integer32 (0..2000000000)
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Controls the path cost incurred by the port. 
                 The Auto setting will set the path cost as appropriate by the physical link speed, using the 802.1D recommended values. 
                 Using the Specific setting, a user-defined value can be entered. 
                 The path cost is used when establishing the active topology of the network. 
                 Lower path cost ports are chosen as forwarding ports in favour of higher path cost ports.                  
                 Valid values are in the range 0 to 200000000.
                 Setting 0 means Auto path cost.
                 Setting 1 ~ 2000000000 means path cost.
								"
            ::= { iCISTNormalPortConfigEntry 2 }
			
			-- 1.3.6.1.4.1.4756.121.60.30.6.11.3
      iCISTPortPriority OBJECT-TYPE
            SYNTAX      INTEGER {
	                        priority-0(1),
	                        priority-16(2),
	                        priority-32(3),
	                        priority-48(4),
	                        priority-64(5),
	                        priority-80(6),
	                        priority-96(7),
	                        priority-112(8),
	                        priority-128(9),
	                        priority-144(10),
	                        priority-160(11),
	                        priority-176(12),
	                        priority-192(13),
	                        priority-208(14),
	                        priority-224(15),
	                        priority-240(16)
	                    }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Controls the port priority. This can be used to control priority of ports having identical port cost."
            ::= { iCISTNormalPortConfigEntry 3 }
	
			-- 1.3.6.1.4.1.4756.121.60.30.6.11.4
      iCISTPortAdminEdge OBJECT-TYPE
            SYNTAX      INTEGER {
	                        non-edge(1),
	                        edge(2)
	                    }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Operational flag describing whether the port is connecting directly to edge devices. 
                (No Bridges attached). Transition to the forwarding state is faster for edge ports (having operEdge true) than for other ports. 
                 The value of this flag is based on AdminEdge and AutoEdge fields. 
                 This flag is displayed as Edge in Monitor->Spanning Tree -> STP Detailed Bridge Status. 
                 Controls whether the operEdge flag should start as set or cleared. "
            ::= { iCISTNormalPortConfigEntry 4 }
	
			-- 1.3.6.1.4.1.4756.121.60.30.6.11.5
      iCISTPortAutoEdge OBJECT-TYPE
            SYNTAX    	TruthValue
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Controls whether the bridge should enable automatic edge detection on the bridge port. 
                 This allows operEdge to be derived from whether BPDU's are received on the port or not"
            ::= { iCISTNormalPortConfigEntry 5 }
            
      -- 1.3.6.1.4.1.4756.121.60.30.6.11.6
      iCISTPortRestrictedRole OBJECT-TYPE
            SYNTAX    	TruthValue
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "If enabled, causes the port not to be selected as Root Port for the CIST or any MSTI, even if it has the best spanning tree priority vector. 
                 Such a port will be selected as an Alternate Port after the Root Port has been selected. 
                 If set, it can cause lack of spanning tree connectivity. 
                 It can be set by a network administrator to prevent bridges external to a core region of the network influence the spanning tree active topology, possibly because those bridges are not under the full control of the administrator. 
                 This feature is also known as Root Guard. 
								"
            ::= { iCISTNormalPortConfigEntry 6 }
            
      -- 1.3.6.1.4.1.4756.121.60.30.6.11.7
      iCISTPortRestrictedTCN OBJECT-TYPE
            SYNTAX    	TruthValue
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "If enabled, causes the port not to propagate received topology change notifications and topology changes to other ports. 
                 If set it can cause temporary loss of connectivity after changes in a spanning tree's active topology as a result of persistently incorrect learned station location information. 
                 It is set by a network administrator to prevent bridges external to a core region of the network, causing address flushing in that region, possibly because those bridges are not under the full control of the administrator or the physical link state of the attached LANs transits frequently. 
								"
            ::= { iCISTNormalPortConfigEntry 7 }
            
      -- 1.3.6.1.4.1.4756.121.60.30.6.11.8
      iCISTPortBPDUGuard OBJECT-TYPE
            SYNTAX    	TruthValue
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "If enabled, causes the port to disable itself upon receiving valid BPDU's. 
                 Contrary to the similar bridge setting, the port Edge status does not effect this setting. 
								 A port entering error-disabled state due to this setting is subject to the bridge Port Error Recovery setting as well. 
								"
            ::= { iCISTNormalPortConfigEntry 8 }
	
			-- 1.3.6.1.4.1.4756.121.60.30.6.11.9
      iCISTPortPointToPoint OBJECT-TYPE
            SYNTAX      INTEGER {
	                        forced-true(1),
	                        forced-false(2),
	                        auto(3)
	                    }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Controls whether the port connects to a point-to-point LAN rather than to a shared medium. 
                 This can be automatically determined, or forced either true or false. 
                 Transition to the forwarding state is faster for point-to-point LANs than for shared media. 
								"
            ::= { iCISTNormalPortConfigEntry 9 }
            
    -- 1.3.6.1.4.1.4756.121.60.30.7
    iMSTIPortsTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF IMSTIPortsEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table provides configurations for MSTI."
        ::= { iSpanningTree 7 }

    -- 1.3.6.1.4.1.4756.121.60.30.7.1
    iMSTIPortsEntry OBJECT-TYPE
        SYNTAX      IMSTIPortsEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Entry of MSTI configurations."
        INDEX { iMSTIIndex }
        ::= { iMSTIPortsTable 1 }

    IMSTIPortsEntry ::=
        SEQUENCE {
					iMSTIAggregatedPorts 		DisplayString,
					iMSTIAggregatedPathCost 	Integer32,
					iMSTIAggregatedPriority		INTEGER,
					iMSTIPort1PathCost 			Integer32,
					iMSTIPort1Priority			INTEGER,
					iMSTIPort2PathCost 			Integer32,
					iMSTIPort2Priority			INTEGER,
					iMSTIPort3PathCost 			Integer32,
					iMSTIPort3Priority			INTEGER,
					iMSTIPort4PathCost 			Integer32,
					iMSTIPort4Priority			INTEGER,
					iMSTIPort5PathCost 			Integer32,
					iMSTIPort5Priority			INTEGER,
					iMSTIPort6PathCost 			Integer32,
					iMSTIPort6Priority			INTEGER,
					iMSTIPort7PathCost 			Integer32,
					iMSTIPort7Priority			INTEGER,
					iMSTIPort8PathCost 			Integer32,
					iMSTIPort8Priority			INTEGER,
					iMSTIPort9PathCost 			Integer32,
					iMSTIPort9Priority			INTEGER,
					iMSTIPort10PathCost 		Integer32,
					iMSTIPort10Priority			INTEGER,
					iMSTIPort11PathCost 		Integer32,
					iMSTIPort11Priority			INTEGER
        }
        
        -- 1.3.6.1.4.1.4756.121.60.30.7.1.1
	      iMSTIAggregatedPorts OBJECT-TYPE
	            SYNTAX      DisplayString
	            MAX-ACCESS  read-only
	            STATUS      current
	            DESCRIPTION
	                "The switch port number of the logical STP port. "
	            ::= { iMSTIPortsEntry 1 }
        
   			-- 1.3.6.1.4.1.4756.121.60.30.7.1.2
      	iMSTIAggregatedPathCost OBJECT-TYPE
            SYNTAX     Integer32 (0..2000000000)
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Controls the path cost incurred by the port. 
                 The Auto setting will set the path cost as appropriate by the physical link speed, using the 802.1D recommended values. 
                 Using the Specific setting, a user-defined value can be entered. 
                 The path cost is used when establishing the active topology of the network. 
                 Lower path cost ports are chosen as forwarding ports in favour of higher path cost ports.                  
                 Valid values are in the range 0 to 200000000.
                 Setting 0 means Auto path cost.
                 Setting 1 ~ 2000000000 means path cost.
								"
            ::= { iMSTIPortsEntry 2 }
			
