        -- 1.3.6.1.4.1.4756.133.40.2.3.9.1.68
        iAceAction OBJECT-TYPE
            SYNTAX      INTEGER {
                            deny(1),
                            permit(2),
                            filter(3)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Specify the action to take with a frame that hits this ACE.
                 Permit: The frame that hits this ACE is granted permission for the ACE operation.
                 Deny: The frame that hits this ACE is dropped.
                 Filter: Frames matching the ACE are filtered. 
                 The default value is 'Permit'."
            ::= { iAceConfigEntry 68 }
        
        -- 1.3.6.1.4.1.4756.133.40.2.3.9.1.69
        iAceFilterPort OBJECT-TYPE
            SYNTAX      DisplayString    
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Select the filter port for which this ACE applies. 
                 All: The ACE applies to all port. 
                 Port n/list: The ACE applies to this port(s) number, where n is the number of the switch port.
                 The default value is 'All'.
                 Syntex: [all | (port <port_list>)]"
            ::= { iAceConfigEntry 69 }
            
        -- 1.3.6.1.4.1.4756.133.40.2.3.9.1.70
        iAceRateLimiterID OBJECT-TYPE
            SYNTAX      Integer32 (0 | 1..16)
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Specify the rate limiter in number of base units.
                 The allowed range is 0{Disabled) to 16.
                 The value 0(Disabled) indicates that the rate limiter operation is disabled.
                 The default value is 0(Disabled)."
            ::= { iAceConfigEntry 70 }

        -- 1.3.6.1.4.1.4756.133.40.2.3.9.1.71
        iAcePortRedirect OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Frames that hit the ACE are redirected to the port number specified here.
                 The allowed range is the same as the switch port number range.
                 Disabled indicates that the port redirect operation is disabled and
                 the specific port number of 'Port Redirect' can't be set when action is permitted."
            ::= { iAceConfigEntry 71 }

        -- 1.3.6.1.4.1.4756.133.40.2.3.9.1.72
        iAceMirror OBJECT-TYPE
            SYNTAX      INTEGER {
                            enable(1),
                            disable(2)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Specify the mirror operation of this port.
                 Frames matching the ACE are mirrored to the destination mirror port.
                 The allowed values are:
                 Enabled: Frames received on the port are mirrored.
                 Disabled: Frames received on the port are not mirrored.
                 The default value is Disabled."
            ::= { iAceConfigEntry 72 }

        -- 1.3.6.1.4.1.4756.133.40.2.3.9.1.73
        iAceLogging OBJECT-TYPE
            SYNTAX      INTEGER {
                            enable(1),
                            disable(2)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Specify the logging operation of the ACE. The allowed values are:
                 Enabled: Frames matching the ACE are stored in the System Log.
                 Disabled: Frames matching the ACE are not logged. 
                 The default value is Disabled.
                 Please note that the System Log memory size and logging rate is limited."
            ::= { iAceConfigEntry 73 }

        -- 1.3.6.1.4.1.4756.133.40.2.3.9.1.74
        iAceShutdown OBJECT-TYPE
            SYNTAX      INTEGER {
                            enable(1),
                            disable(2)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Specify the port shut down operation of the ACE. The allowed values are:
                 Enabled: If a frame matches the ACE, the ingress port will be disabled.
                 Disabled: Port shut down is disabled for the ACE. 
                 The default value is Disabled."
            ::= { iAceConfigEntry 74 }

    -- 1.3.6.1.4.1.4756.121.40.2.3.10
    iAclStatusTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF IAclStatusEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table shows the ACL status."
        ::= { iACL 10 }

    -- 1.3.6.1.4.1.4756.121.40.2.3.10.1
    iAclStatusEntry OBJECT-TYPE
        SYNTAX      IAclStatusEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Entry of ACL status."
        INDEX { iAclStatusIndex }
        ::= { iAclStatusTable 1 }

    IAclStatusEntry ::=
        SEQUENCE {
            iAclStatusIndex        Integer32,
            iAclUser               INTEGER,
            iAclIngressPort        DisplayString,
            iAclFrameType          DisplayString,
            iAclAction             INTEGER,
            iAclRateLimiter        Integer32,
            iAclPortRedirect       DisplayString,
            iAclMirror             INTEGER,
            iAclToCPU              INTEGER,
            iAclToCpuOnce          INTEGER,
            iAclCounter            Counter32,
            iAclConflict           INTEGER
        }

        -- 1.3.6.1.4.1.4756.121.40.2.3.10.1.1    
        iAclStatusIndex OBJECT-TYPE
            SYNTAX      Integer32 (1..256)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the Index of ACL Status entries."
            ::= { iAclStatusEntry 1 }

        -- 1.3.6.1.4.1.4756.121.40.2.3.10.1.2    
        iAclUser OBJECT-TYPE
            SYNTAX      INTEGER {
                            static(1),
                            ipSourceGuard(2),
                            ipmc(3),
                            mep(4),
                            arpInspection(5),
                            upnp(6),
                            dhcp(7),
                            loopProtect(8),
                            linkoam(9),
                            u-Ring(10)
                        }    
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the ACL user."
            ::= { iAclStatusEntry 2 }

        -- 1.3.6.1.4.1.4756.121.40.2.3.10.1.3    
        iAclIngressPort OBJECT-TYPE
            SYNTAX      DisplayString  
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the ingress port of the ACE. Possible values are:
                 Any: The ACE will match any ingress port.
                 Policy: The ACE will match ingress ports with a specific policy.
                 Port: The ACE will match a specific ingress port."
            ::= { iAclStatusEntry 3 }

        -- 1.3.6.1.4.1.4756.121.40.2.3.10.1.4    
        iAclFrameType OBJECT-TYPE
            SYNTAX      DisplayString    
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the frame type of the ACE. Possible values are:
                 Any: The ACE will match any frame type.
                 EType: The ACE will match Ethernet Type frames.
                        Note that an Ethernet Type based ACE will not get matched by IP and ARP frames.
                 ARP: The ACE will match ARP/RARP frames.
                 IPv4: The ACE will match all IPv4 frames.
                 IPv4/ICMP: The ACE will match IPv4 frames with ICMP protocol.
                 IPv4/UDP: The ACE will match IPv4 frames with UDP protocol.
                 IPv4/TCP: The ACE will match IPv4 frames with TCP protocol.
                 IPv4/Other: The ACE will match IPv4 frames, which are not ICMP/UDP/TCP."
            ::= { iAclStatusEntry 4 }

        -- 1.3.6.1.4.1.4756.121.40.2.3.10.1.5
        iAclAction OBJECT-TYPE
            SYNTAX      INTEGER {
                            deny(1),
                            permit(2)
                        }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the forwarding action of the ACE.
                 Permit: Frames matching the ACE may be forwarded and learned.
                 Deny: Frames matching the ACE are dropped."
            ::= { iAclStatusEntry 5 }

        -- 1.3.6.1.4.1.4756.121.40.2.3.10.1.6
        iAclRateLimiter OBJECT-TYPE
            SYNTAX      Integer32 (0..16)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the rate limiter number of the ACE.
                 The allowed range is 0(Disable) to 16.
                 When 0(Disabled) is displayed, the rate limiter operation is disabled."
            ::= { iAclStatusEntry 6 }

        -- 1.3.6.1.4.1.4756.121.40.2.3.10.1.7
        iAclPortRedirect OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the port Redirect operation of the ACE.
                 Frames matching the ACE are Redirected to the port number.
                 The allowed values are 'Disabled' or a specific port number.
                 When 'Disabled' is displayed, the port copy operation is disabled."
            ::= { iAclStatusEntry 7 }

        -- 1.3.6.1.4.1.4756.121.40.2.3.10.1.8
        iAclMirror OBJECT-TYPE
            SYNTAX      INTEGER {
                            enable(1),
                            disable(2)
                        }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the mirror operation of this port. The allowed values are:
                 Enabled: Frames received on the port are mirrored.
                 Disabled: Frames received on the port are not mirrored."
            ::= { iAclStatusEntry 8 }

        -- 1.3.6.1.4.1.4756.121.40.2.3.10.1.9
        iAclToCPU OBJECT-TYPE
            SYNTAX      INTEGER {
                            yes(1),
                            no(2)
                        }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Forward packet that matched the specific ACE to CPU."
            ::= { iAclStatusEntry 9 }

        -- 1.3.6.1.4.1.4756.121.40.2.3.10.1.10
        iAclToCpuOnce OBJECT-TYPE
            SYNTAX      INTEGER {
                            yes(1),
                            no(2)
                        }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Forward first packet that matched the specific ACE to CPU."
            ::= { iAclStatusEntry 10 }

        -- 1.3.6.1.4.1.4756.121.40.2.3.10.1.11
        iAclCounter OBJECT-TYPE
            SYNTAX      Counter32
            UNITS       "Frames"
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The counter indicates the number of times the ACE was hit by a frame."
            ::= { iAclStatusEntry 11 }

        -- 1.3.6.1.4.1.4756.121.40.2.3.10.1.12
        iAclConflict OBJECT-TYPE
            SYNTAX      INTEGER {
                            yes(1),
                            no(2)
                        }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the hardware status of the specific ACE.
                 The specific ACE is not applied to the hardware due to hardware limitations."
            ::= { iAclStatusEntry 12 }


    -- 1.3.6.1.4.1.4756.121.40.2.4
    iDHCP OBJECT IDENTIFIER ::= { iSecurityNetwork 4 }

    -- 1.3.6.1.4.1.4756.121.40.2.4.1    
    iDhcpSnoopingMode OBJECT-TYPE
        SYNTAX      INTEGER {
                        enabled(1),
                        disabled(2)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Indicates the DHCP snooping mode operation. Possible modes are:
             Enabled: Enable DHCP snooping mode operation.
                      When DHCP snooping mode operation is enabled,
                      the DHCP request messages will be forwarded to trusted ports
                      and only allow reply packets from trusted ports.
             Disabled: Disable DHCP snooping mode operation."
        ::= { iDHCP 1 }

    -- 1.3.6.1.4.1.4756.121.40.2.4.2
    iDhcpSnoopingPortConfigTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF IDhcpSnoopingPortConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "Configure DHCP Snooping on this table."
        ::= { iDHCP 2 }

    -- 1.3.6.1.4.1.4756.121.40.2.4.2.1
    iDhcpSnoopingPortConfigEntry OBJECT-TYPE
        SYNTAX      IDhcpSnoopingPortConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Entry of DHCP Snooping ports."
        INDEX { iPortIndex }
        ::= { iDhcpSnoopingPortConfigTable 1 }

    IDhcpSnoopingPortConfigEntry ::=
        SEQUENCE {
            iDhcpSnoopingPortMode             INTEGER
        }

        -- 1.3.6.1.4.1.4756.121.40.2.4.2.1.1
        iDhcpSnoopingPortMode OBJECT-TYPE
            SYNTAX      INTEGER {
                            trusted(1),
                            untrusted(2)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Indicates the DHCP snooping port mode. Possible port modes are:
                 Trusted: Configures the port as trusted source of the DHCP messages.
                 Untrusted: Configures the port as untrusted source of the DHCP messages."
            ::= { iDhcpSnoopingPortConfigEntry 1 }

    -- 1.3.6.1.4.1.4756.121.40.2.4.3
    iDhcpSnoopingPortStatisticsTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF IDhcpSnoopingPortStatisticsEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table provides statistics for DHCP snooping."
        ::= { iDHCP 3 }

    -- 1.3.6.1.4.1.4756.121.40.2.4.3.1
    iDhcpSnoopingPortStatisticsEntry OBJECT-TYPE
        SYNTAX      IDhcpSnoopingPortStatisticsEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Entry of DHCP Snooping port statistics."
        INDEX { iPortIndex }
        ::= { iDhcpSnoopingPortStatisticsTable 1 }

    IDhcpSnoopingPortStatisticsEntry ::=
        SEQUENCE {
            iDhcpSnoopingPortStatisticsClear   INTEGER,
            iDhcpSnoopingRxDiscover            Counter32,
            iDhcpSnoopingRxOffer               Counter32,
            iDhcpSnoopingRxRequest             Counter32,
            iDhcpSnoopingRxDecline             Counter32,
            iDhcpSnoopingRxACK                 Counter32,
            iDhcpSnoopingRxNAK                 Counter32,
            iDhcpSnoopingRxRelease             Counter32,
            iDhcpSnoopingRxInform              Counter32,
            iDhcpSnoopingRxLeaseQuery          Counter32,
            iDhcpSnoopingRxLeaseUnassigned     Counter32,
            iDhcpSnoopingRxLeaseUnknown        Counter32,
            iDhcpSnoopingRxLeaseActive         Counter32,
            iDhcpSnoopingTxDiscover            Counter32,
            iDhcpSnoopingTxOffer               Counter32,
            iDhcpSnoopingTxRequest             Counter32,
            iDhcpSnoopingTxDecline             Counter32,
            iDhcpSnoopingTxACK                 Counter32,
            iDhcpSnoopingTxNAK                 Counter32,
            iDhcpSnoopingTxRelease             Counter32,
            iDhcpSnoopingTxInform              Counter32,
            iDhcpSnoopingTxLeaseQuery          Counter32,
            iDhcpSnoopingTxLeaseUnassigned     Counter32,
            iDhcpSnoopingTxLeaseUnknown        Counter32,
            iDhcpSnoopingTxLeaseActive         Counter32
        }

        -- 1.3.6.1.4.1.4756.121.40.2.4.3.1.1
        iDhcpSnoopingPortStatisticsClear OBJECT-TYPE
            SYNTAX      INTEGER {
                            trigger(1),
                            normal(2)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Clears the counters for this port."
            ::= { iDhcpSnoopingPortStatisticsEntry 1 }

        -- 1.3.6.1.4.1.4756.121.40.2.4.3.1.2
