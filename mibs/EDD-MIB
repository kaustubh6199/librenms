            STATUS      current
            DESCRIPTION
                "User IP address of the entry."
            ::= { iDynamicArpInspectionEntry 5 }


-- 1.3.6.1.4.1.4756.121.40.3
    iSecurityRADIUS OBJECT IDENTIFIER ::= { iSecurity 3 }

    -- 1.3.6.1.4.1.4756.121.40.3.1    
    iServerTimeout OBJECT-TYPE
        SYNTAX      Integer32 (1..1000)
        UNITS       "Seconds"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Timeout is the number of seconds, in the range 1 to 1000, to wait for a reply from a RADIUS server before retransmitting the request."
        ::= { iSecurityRADIUS 1 }

    -- 1.3.6.1.4.1.4756.121.40.3.2    
    iServerRetransmit OBJECT-TYPE
        SYNTAX      Integer32 (1..1000)
        UNITS       "Seconds"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Retransmit is the number of times, in the range 1 to 1000, a RADIUS request is retransmitted to a server that is not responding. 
             If the server has not responded after the last retransmit it is considered to be dead. "
        ::= { iSecurityRADIUS 2 }
    
    -- 1.3.6.1.4.1.4756.121.40.3.10
    iServerDeadTime OBJECT-TYPE
        SYNTAX      Integer32 (0..1440)
        UNITS       "Seconds"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Deadtime, which can be set to a number between 0 to 1440 minutes, is the period during which the switch will not send new requests to a server that has failed to respond to a previous request. 
            This will stop the switch from continually trying to contact a server that it has already determined as dead.
						 Setting the Deadtime to a value greater than 0 (zero) will enable this feature, but only if more than one server has been configured. 
					  "
        ::= { iSecurityRADIUS 3 }
    
    -- 1.3.6.1.4.1.4756.121.40.3.4
    iServerKey OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION 
           	    "The secret key - up to 63 characters long - shared between the RADIUS server and the switch."
        ::= { iSecurityRADIUS 4 }
    
    -- 1.3.6.1.4.1.4756.121.40.3.12
    iServerNasIpAddress OBJECT-TYPE
            SYNTAX      DisplayString    
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
            "The IPv4 address to be used as attribute 4 in RADIUS Access-Request packets. 
             If this field is left blank, the IP address of the outgoing interface is used."
        ::= { iSecurityRADIUS 5 }
    
    -- 1.3.6.1.4.1.4756.121.40.3.6
    iServerNasIPv6Address OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
            "The IPv6 address to be used as attribute 95 in RADIUS Access-Request packets. 
             If this field is left blank, the IP address of the outgoing interface is used. "
        ::= { iSecurityRADIUS 6 }
    
    -- 1.3.6.1.4.1.4756.121.40.3.7
    iServerNasIdentifier OBJECT-TYPE
            SYNTAX      DisplayString    
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
            "The identifier - up to 255 characters long - to be used as attribute 32 in RADIUS Access-Request packets. 
            If this field is left blank, the NAS-Identifier is not included in the packet."
        ::= { iSecurityRADIUS 7 }
        
    -- 1.3.6.1.4.1.4756.121.40.3.8
    iAddRadiusServer OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION 
           	    "Add a new RADIUS server. An empty row is added to the table, and the RADIUS server can be configured as needed. 
           	     Up to 5 servers are supported.
           	     Syntax: <ip_addr_string> [<auth_port>] [<acct_port>] [<timeout>] [<retransmit>] [<key>] 
           	    "
        ::= { iSecurityRADIUS 8 }

		-- 1.3.6.1.4.1.4756.121.40.3.9
    iDeleteRadiusServer OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION 
           	    "Delete can be used to undo the addition of the new server.
           	     Syntax: <ip_addr_string> [<auth_port>] [<acct_port>]
           	    "
        ::= { iSecurityRADIUS 9 }

    -- 1.3.6.1.4.1.4756.121.40.3.10
    iAuthServerConfigTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF IAuthServerConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The table allows you to configure the Authentication Servers."
        ::= { iSecurityRADIUS 10 }

    -- 1.3.6.1.4.1.4756.121.40.3.10.1
    iAuthServerConfigEntry OBJECT-TYPE
        SYNTAX      IAuthServerConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Entry of the Authentication Server configuration."
        INDEX { iRadiusServerIndex }
        ::= { iAuthServerConfigTable 1 }

    IAuthServerConfigEntry ::=
        SEQUENCE {
            iRadiusServerIndex             Integer32,
            iRadiusServerHostname      	   DisplayString,
            iRadiusAuthServerPort          Integer32,
            iRadiusAccountingServerPort    Integer32,
            iRadiusServerTimeout    	   Integer32,
            iRadiusServerRetransmit    	   Integer32,
            iRadiusAuthkey                 DisplayString
        }

        -- 1.3.6.1.4.1.4756.121.40.3.10.1.1
        iRadiusServerIndex OBJECT-TYPE
            SYNTAX      Integer32 (1..5)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The RADIUS Server number index."
            ::= { iAuthServerConfigEntry 1 }

        -- 1.3.6.1.4.1.4756.121.40.3.10.1.2
        iRadiusServerHostname OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The IP address or hostname of the RADIUS server."
            ::= { iAuthServerConfigEntry 2 }

        -- 1.3.6.1.4.1.4756.121.40.3.10.1.3
        iRadiusAuthServerPort OBJECT-TYPE
            SYNTAX      Integer32 (0..65535)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The UDP port to use on the RADIUS Authentication Server.
                 If the port is set to 0(zero),
                 the default port(1812) is used on the RADIUS Authentication Server."
            ::= { iAuthServerConfigEntry 3 }        

        -- 1.3.6.1.4.1.4756.121.40.3.10.1.4
        iRadiusAccountingServerPort OBJECT-TYPE
            SYNTAX      Integer32 (0..65535)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The UDP port to use on the RADIUS Accounting Server.
                 If the port is set to 0(zero),
                 the default port(1813) is used on the RADIUS Accounting Server."
            ::= { iAuthServerConfigEntry 4 }

		-- 1.3.6.1.4.1.4756.121.40.3.10.1.5
		iRadiusServerTimeout 	OBJECT-TYPE
            SYNTAX      Integer32 (1..1000)
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "This optional setting overrides the global timeout value. Leaving it blank will use the global timeout value."
            ::= { iAuthServerConfigEntry 5 }
        
        -- 1.3.6.1.4.1.4756.121.40.3.10.1.6
        iRadiusServerRetransmit 	OBJECT-TYPE
            SYNTAX      Integer32 (0..1440)
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "This optional setting overrides the global retransmit value. Leaving it blank will use the global retransmit value."
            ::= { iAuthServerConfigEntry 6 }    
            
        -- 1.3.6.1.4.1.4756.121.40.3.10.1.7
        iRadiusAuthkey OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "This optional setting overrides the global key. Leaving it blank will use the global key."
            ::= { iAuthServerConfigEntry 7 }


    -- 1.3.6.1.4.1.4756.121.40.3.11
    iRadiusServerStatusTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF IRadiusServerStatusEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table provides the status of the RADIUS servers."
        ::= { iSecurityRADIUS 11 }

    -- 1.3.6.1.4.1.4756.121.40.3.11.1
    iRadiusServerStatusEntry OBJECT-TYPE
        SYNTAX      IRadiusServerStatusEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Entry of RADIUS Server status."
        INDEX { iRadiusServerIndex }
        ::= { iRadiusServerStatusTable 1 }

    IRadiusServerStatusEntry ::=
        SEQUENCE {
            iRadiusAuthIpAddress           DisplayString,
            iRadiusAuthState               INTEGER,
            iRadiusAccountingIpAddress     DisplayString,
            iRadiusAccountingState         INTEGER
        }

        -- 1.3.6.1.4.1.4756.121.40.3.11.1.1
        iRadiusAuthIpAddress OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The IP address of the RADIUS Authentication server."
            ::= { iRadiusServerStatusEntry 1 }

        -- 1.3.6.1.4.1.4756.121.40.3.11.1.2
        iRadiusAuthState OBJECT-TYPE
            SYNTAX      INTEGER {
                            disabled(1),
                            notReady(2),
                            ready(3),
                            dead(4)
                        }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The current state of the server. This node takes one of the following values:
                 Disabled: The server is disabled.
                 Not Ready: The server is enabled, but IP communication is not yet up and running.
                 Ready: The server is enabled, IP communication is up and running,
                        and the RADIUS module is ready to accept access attempts.
                 Dead: Access attempts were made to this server,
                       but it did not reply within the configured timeout.
                       The server has temporarily been disabled,
                       but will get re-enabled when the dead-time expires.
                       The number of seconds left before this occurs is displayed in parentheses.
                       This state is only reachable when more than one server is enabled."
            ::= { iRadiusServerStatusEntry 2 }        

        -- 1.3.6.1.4.1.4756.121.40.3.11.1.3
        iRadiusAccountingIpAddress OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The IP address of the RADIUS Accounting server."
            ::= { iRadiusServerStatusEntry 3 }

        -- 1.3.6.1.4.1.4756.121.40.3.11.1.4
        iRadiusAccountingState OBJECT-TYPE
            SYNTAX      INTEGER {
                            disabled(1),
                            notReady(2),
                            ready(3),
                            dead(4)
                        }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The current state of the server. This node takes one of the following values:
                 Disabled: The server is disabled.
                 Not Ready: The server is enabled, but IP communication is not yet up and running.
                 Ready: The server is enabled, IP communication is up and running,
                        and the RADIUS module is ready to accept access attempts.
                 Dead: Access attempts were made to this server,
                       but it did not reply within the configured timeout.
                       The server has temporarily been disabled,
                       but will get re-enabled when the dead-time expires.
                       The number of seconds left before this occurs is displayed in parentheses.
                       This state is only reachable when more than one server is enabled."
            ::= { iRadiusServerStatusEntry 4 }
        

    -- 1.3.6.1.4.1.4756.121.40.3.12
    iRadiusAuthStatisticsTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF IRadiusAuthStatisticsEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table provides the status of the RADIUS servers."
        ::= { iSecurityRADIUS 12 }

    -- 1.3.6.1.4.1.4756.121.40.3.12.1
    iRadiusAuthStatisticsEntry OBJECT-TYPE
        SYNTAX      IRadiusAuthStatisticsEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Entry of RADIUS Authentication Server status."
        INDEX { iRadiusServerIndex }
        ::= { iRadiusAuthStatisticsTable 1 }

    IRadiusAuthStatisticsEntry ::=
        SEQUENCE {
            iRadiusStatisticsClear                  INTEGER,
            iRadiusAuthRxAccessAccepts              Counter32,
            iRadiusAuthRxAccessRejects              Counter32,
            iRadiusAuthRxAccessChallenges           Counter32,
            iRadiusAuthRxMalformedAccessRespones    Counter32,
            iRadiusAuthRxBadAuthentications         Counter32,
            iRadiusAuthRxUnknowTypes                Counter32,
            iRadiusAuthRxDroppeds                   Counter32,
            iRadiusAuthTxAccessRequests             Counter32,
            iRadiusAuthTxAccessRetransmissions      Counter32,
            iRadiusAuthTxPendingRequests            Counter32,
            iRadiusAuthTxTimeouts                   Counter32,
            iRadiusAuthOtherInfoIPAddress		    DisplayString,
            iRadiusAuthOtherInfoStatus				INTEGER,
            iRadiusAuthOtherInfoRoundTripTime		DisplayString,
            iRadiusAccountingRxResponses            Counter32,
            iRadiusAccountingRxMalformedResponses   Counter32,
            iRadiusAccountingRxBadAuthentications   Counter32,
            iRadiusAccountingRxUnknowTypes          Counter32,
            iRadiusAccountingRxDroppeds             Counter32,
            iRadiusAccountingTxRequests             Counter32,
            iRadiusAccountingTxRetransmissions      Counter32,
            iRadiusAccountingTxPendingRequests      Counter32,
            iRadiusAccountingTxTimeouts            	Counter32,
            iRadiusAccountingOtherInfoIPAddress			DisplayString,
            iRadiusAccountingOtherInfoStatus				INTEGER,
            iRadiusAccountingOtherInfoRoundTripTime	DisplayString
        }

        -- 1.3.6.1.4.1.4756.121.40.3.12.1.1
        iRadiusStatisticsClear OBJECT-TYPE
            SYNTAX      INTEGER {
                            trigger(1),
                            normal(2)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Clears the counters for all ports."
            ::= { iRadiusAuthStatisticsEntry 1 }

        -- 1.3.6.1.4.1.4756.121.40.3.12.1.2
        iRadiusAuthRxAccessAccepts OBJECT-TYPE
            SYNTAX      Counter32
            UNITS       "Packets"
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The number of RADIUS Access-Accept packets(valid or invalid) received from the server."
            ::= { iRadiusAuthStatisticsEntry 2 }

        -- 1.3.6.1.4.1.4756.121.40.3.12.1.3
        iRadiusAuthRxAccessRejects OBJECT-TYPE
            SYNTAX      Counter32
            UNITS       "Packets"
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The number of RADIUS Access-Reject packets(valid or invalid) received from the server."
            ::= { iRadiusAuthStatisticsEntry 3 }

        -- 1.3.6.1.4.1.4756.121.40.3.12.1.4
        iRadiusAuthRxAccessChallenges OBJECT-TYPE
            SYNTAX      Counter32
            UNITS       "Packets"
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The number of RADIUS Access-Challenge packets(valid or invalid) received from the server."
            ::= { iRadiusAuthStatisticsEntry 4 }

        -- 1.3.6.1.4.1.4756.121.40.3.12.1.5
        iRadiusAuthRxMalformedAccessRespones OBJECT-TYPE
            SYNTAX      Counter32
            UNITS       "Packets"
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The number of malformed RADIUS Access-Response packets received from the server.
                 Malformed packets include packets with an invalid length.
                 Bad authenticators or Message Authenticator attributes or
                 unknown types are not included as malformed access responses."
            ::= { iRadiusAuthStatisticsEntry 5 }

        -- 1.3.6.1.4.1.4756.121.40.3.12.1.6
        iRadiusAuthRxBadAuthentications OBJECT-TYPE
            SYNTAX      Counter32
            UNITS       "Packets"
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The number of RADIUS Access-Response packets containing invalid authenticators or
                 Message Authenticator attributes received from the server."
            ::= { iRadiusAuthStatisticsEntry 6 }

        -- 1.3.6.1.4.1.4756.121.40.3.12.1.7
        iRadiusAuthRxUnknowTypes OBJECT-TYPE
            SYNTAX      Counter32
            UNITS       "Packets"
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The number of RADIUS packets that were received with unknown types from
                 the server on the authentication port and dropped."
            ::= { iRadiusAuthStatisticsEntry 7 }

        -- 1.3.6.1.4.1.4756.121.40.3.12.1.8
        iRadiusAuthRxDroppeds OBJECT-TYPE
            SYNTAX      Counter32
            UNITS       "Packets"
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The number of RADIUS packets that were received from the server on
                 the authentication port and dropped for some other reason."
            ::= { iRadiusAuthStatisticsEntry 8 }

        -- 1.3.6.1.4.1.4756.121.40.3.12.1.9
        iRadiusAuthTxAccessRequests OBJECT-TYPE
            SYNTAX      Counter32
            UNITS       "Packets"
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The number of RADIUS Access-Request packets sent to the server.
                 This does not include retransmissions."
            ::= { iRadiusAuthStatisticsEntry 9 }

        -- 1.3.6.1.4.1.4756.121.40.3.12.1.10
        iRadiusAuthTxAccessRetransmissions OBJECT-TYPE
            SYNTAX      Counter32
            UNITS       "Packets"
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The number of RADIUS Access-Request packets retransmitted to
                 the RADIUS authentication server."
            ::= { iRadiusAuthStatisticsEntry 10 }

        -- 1.3.6.1.4.1.4756.121.40.3.12.1.11
        iRadiusAuthTxPendingRequests OBJECT-TYPE
            SYNTAX      Counter32
            UNITS       "Packets"
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The number of RADIUS Access-Request packets destined for the server
                 that have not yet timed out or received a response.
                 This variable is incremented when an Access-Request is sent and decremented due to
                 receipt of an Access-Accept, Access-Reject, Access-Challenge, timeout, or retransmission."
            ::= { iRadiusAuthStatisticsEntry 11 }

        -- 1.3.6.1.4.1.4756.121.40.3.12.1.12
        iRadiusAuthTxTimeouts OBJECT-TYPE
            SYNTAX      Counter32
            UNITS       "Packets"
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The number of authentication timeouts to the server. After a timeout,
                 the client may retry to the same server, send to a different server, or give up.
                 A retry to the same server is counted as a retransmit as well as a timeout.
                 A send to a different server is counted as a Request as well as a timeout."
            ::= { iRadiusAuthStatisticsEntry 12 }

				 -- 1.3.6.1.4.1.4756.121.40.3.12.1.13
        iRadiusAuthOtherInfoIPAddress OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The IP address of the RADIUS Authentication server."
            ::= { iRadiusAuthStatisticsEntry 13 }

        -- 1.3.6.1.4.1.4756.121.40.3.12.1.14
        iRadiusAuthOtherInfoStatus OBJECT-TYPE
            SYNTAX      INTEGER {
                            disabled(1),
                            notReady(2),
                            ready(3),
                            dead(4)
                        }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The current state of the server. This node takes one of the following values:
                 Disabled: The server is disabled.
                 Not Ready: The server is enabled, but IP communication is not yet up and running.
                 Ready: The server is enabled, IP communication is up and running,
                        and the RADIUS module is ready to accept access attempts.
                 Dead: Access attempts were made to this server,
                       but it did not reply within the configured timeout.
                       The server has temporarily been disabled,
                       but will get re-enabled when the dead-time expires.
                       The number of seconds left before this occurs is displayed in parentheses.
                       This state is only reachable when more than one server is enabled."
            ::= { iRadiusAuthStatisticsEntry 14 }
				
				 -- 1.3.6.1.4.1.4756.121.40.3.12.1.15
        iRadiusAuthOtherInfoRoundTripTime OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The time interval (measured in milliseconds) between the most recent Access-Reply/Access-Challenge and the Access-Request that matched it from the RADIUS authentication server. 
                 The granularity of this measurement is 100 ms. A value of 0 ms indicates that there hasn't been round-trip communication with the server yet. "
            ::= { iRadiusAuthStatisticsEntry 15 }

        -- 1.3.6.1.4.1.4756.121.40.3.12.1.16
