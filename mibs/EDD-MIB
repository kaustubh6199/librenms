-- MAC Table             
    -- 1.3.6.1.4.1.4756.121.120
    iMacTableInfo  OBJECT IDENTIFIER ::= { eddCard 140 }

    -- 1.3.6.1.4.1.4756.121.120.1
    iDisableAutoAging OBJECT-TYPE
        SYNTAX      TruthValue
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION                                  
            "Disable the automatic aging of dynamic entries."
        ::= { iMacTableInfo 1 }

    -- 1.3.6.1.4.1.4756.121.120.2
    iAgingTime OBJECT-TYPE
        SYNTAX      Integer32 (10..1000000)
        UNITS       "seconds"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION                                  
            "By default, dynamic entries are removed from the MAC table after 300 seconds.
             This removal is also called aging.
             Configure aging time by entering a value here in seconds,
             The allowed range is 10 to 1000000 seconds."
        DEFVAL     { 300 }     
        ::= { iMacTableInfo 2 }
 
    -- 1.3.6.1.4.1.4756.121.120.3
    iMacTableLearningTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF IMacTableLearningEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table allows for configure port learning mode."
        ::= { iMacTableInfo 3 }

    -- 1.3.6.1.4.1.4756.121.120.3.1
    iMacTableLearningEntry OBJECT-TYPE
        SYNTAX      IMacTableLearningEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The entry of ports."
        INDEX { iPortIndex }
        ::= { iMacTableLearningTable 1 }

    IMacTableLearningEntry ::=
        SEQUENCE {       
            iPortLearningMode      INTEGER
        }

        -- 1.3.6.1.4.1.4756.121.120.3.1.1    
        iPortLearningMode OBJECT-TYPE
            SYNTAX      INTEGER {
                            auto(1),
                            disable(2),
                            secure(3)
                        }    
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "If another module is in control of the mode, then it cannot be changed by the user.
                 An example of such a module is the MAC-Based Authentication under 802.1X.
                 Each port can do learning based upon the following settings:
                 Auto: Learning is done automatically as soon as a frame with unknown SMAC is received.
                 Disable: No learning is done.
                 Secure: Only static MAC entries are learned, all other frames are dropped.
                         <NOTE> Make sure that the link used for managing the switch is added to
                                the Static Mac Table before changing to secure learning mode,
                                otherwise the management link is lost and can only be restored by
                                using another non-secure port"
            ::= { iMacTableLearningEntry 1 }

    -- 1.3.6.1.4.1.4756.121.120.4
    iAddStaticMAC OBJECT-TYPE
        SYNTAX      DisplayString
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION                                  
            "Add a new entry to the static MAC table. The static MAC table can contain 64 entries.
             By default, no ports are members.
             Syntax: <mac_addr> [<vid>]"
        ::= { iMacTableInfo 4 }

    -- 1.3.6.1.4.1.4756.121.120.5
    iDeleteStaticMAC OBJECT-TYPE
        SYNTAX      DisplayString
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Delete an entry from the static MAC table.
             Syntax: <mac_addr> [<vid>]"
        ::= { iMacTableInfo 5 }

    -- 1.3.6.1.4.1.4756.121.120.6
    iStaticMacTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF IStaticMacEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The static entries in the MAC table are shown in this table."
        ::= { iMacTableInfo 6 }

    -- 1.3.6.1.4.1.4756.121.120.6.1
    iStaticMacEntry OBJECT-TYPE
        SYNTAX      IStaticMacEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The entry of static MAC addresses of table."
        INDEX { iStaticMacVlanID, iStaticMacAddress }
        ::= { iStaticMacTable 1 }

    IStaticMacEntry ::=
        SEQUENCE {        
            iStaticMacVlanID       Integer32,
            iStaticMacAddress      MacAddress,
            iStaticMacPortMembers  DisplayString
        }

        -- 1.3.6.1.4.1.4756.121.120.6.1.1    
        iStaticMacVlanID OBJECT-TYPE
            SYNTAX      Integer32 (1..4095)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The VLAN ID of the entry."
            ::= { iStaticMacEntry 1 }

        -- 1.3.6.1.4.1.4756.121.120.6.1.2    
        iStaticMacAddress OBJECT-TYPE
            SYNTAX      MacAddress
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The MAC address of the entry."
            ::= { iStaticMacEntry 2 }

        -- 1.3.6.1.4.1.4756.121.120.6.1.3    
        iStaticMacPortMembers OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Indicates which ports are members of the entry."
            ::= { iStaticMacEntry 3 }

    -- 1.3.6.1.4.1.4756.121.120.7
    iFlushDynamicMacEntries OBJECT-TYPE
        SYNTAX      INTEGER {
                        trigger(1),
                        normal(2)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Flushes all dynamic entries."
        ::= { iMacTableInfo 7 }

    -- 1.3.6.1.4.1.4756.121.120.8
    iMacAddressTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF IMacAddressEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The entries in the MAC addresses table are shown in this table."
        ::= { iMacTableInfo 8 }

    -- 1.3.6.1.4.1.4756.121.120.8.1
    iMacAddressEntry OBJECT-TYPE
        SYNTAX      IMacAddressEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The entry of MAC addresses table."
        INDEX { iMacAddressVlanID, iMacAddress }
        ::= { iMacAddressTable 1 }

