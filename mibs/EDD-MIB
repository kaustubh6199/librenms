        DESCRIPTION
            "The Entry of Static ARP Inspection."
        INDEX { iArpInspectionVLANID }
        ::= { iArpInspectionVLANConfigTable 1 }

    IArpInspectionVLANConfigEntry ::=
        SEQUENCE {
            iArpInspectionVLANID  		  Integer32,
            iArpInspectionVLANLogType     INTEGER
        }
    
        -- 1.3.6.1.4.1.4756.121.40.2.6.6.1
        iArpInspectionVLANID OBJECT-TYPE
        SYNTAX      Integer32 (1..4095)
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION                                  
            "Show the setting VLAN ID"
        ::= { iArpInspectionVLANConfigEntry 1 }
        
        -- 1.3.6.1.4.1.4756.121.40.2.6.6.2
        iArpInspectionVLANLogType OBJECT-TYPE
        SYNTAX      INTEGER {
        			         none(1),
        			         deny(2),
        			         permit(3),
        			         all(4)
        			}
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION                                  
            "Specify ARP Inspection is enabled on which VLANs. 
             First, you have to enable the port setting on Port mode configuration web page. 
             Only when both Global Mode and Port Mode on a given port are enabled, ARP Inspection is enabled on this given port. 
             Second, you can specify which VLAN will be inspected on VLAN mode configuration web page. 
             The log type also can be configured on per VLAN setting. 
			 Possible types are: 
			 None: Log nothing. 
			 Deny: Log denied entries. 
			 Permit: Log permitted entries. 
			 ALL: Log all entries. 
			"
        ::= { iArpInspectionVLANConfigEntry 2 }
	
