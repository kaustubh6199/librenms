    -- 1.3.6.1.4.1.4756.121.10
    iSystem OBJECT IDENTIFIER ::= { eddCard 10 }

	-- 1.3.6.1.4.1.4756.121.10.1
    iModelType OBJECT-TYPE
        SYNTAX      INTEGER {
                        msw202(1),
                        mx210(2)                    
        }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "This serial Model Type:
             1:MSW202
   			 2:MX210
            "
        ::= { iSystem 1 }

    -- 1.3.6.1.4.1.4756.121.10.2
    iSystemInformation OBJECT IDENTIFIER ::= { iSystem 2 }
                            
    -- 1.3.6.1.4.1.4756.121.10.2.1
    iBaseMacAddress OBJECT-TYPE
        SYNTAX      DisplayString
        MAX-ACCESS  read-only
