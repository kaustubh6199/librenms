    -- 1.3.6.1.4.1.37552.4.2.110.1.22
    iIgmpVlanConfigTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF IIgmpVlanConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table allows for configure IGMP snooping VLAN."
        ::= { iIgmpSnooping 22 }

    -- 1.3.6.1.4.1.37552.4.2.110.1.22.1
    iIgmpVlanConfigEntry OBJECT-TYPE
        SYNTAX      IIgmpVlanConfigEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Entry of IGMP snooping VLAN."
        INDEX { iIgmpVlanID }
        ::= { iIgmpVlanConfigTable 1 }

    IIgmpVlanConfigEntry ::=
        SEQUENCE {     
            iIgmpVlanID            Integer32,
            iIgmpPerVlanEnabled    TruthValue,
            iIgmpQuerierEnabled    TruthValue,
            iIgmpCompatibility     INTEGER,
            iIgmpRV                Integer32,
            iIgmpQI                Integer32,
            iIgmpQRI               Integer32,
            iIgmpLLQI              Integer32,
            iIgmpURI               Integer32,
            iIgmpQuerierAddress    IpAddress,
            iIgmpPRI            	 Integer32
        }

        -- 1.3.6.1.4.1.37552.4.2.110.1.22.1.1    
        iIgmpVlanID OBJECT-TYPE
            SYNTAX      Integer32 (1..4095)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the ID of this particular IGMP snooping VLAN."
            ::= { iIgmpVlanConfigEntry 1 }

        -- 1.3.6.1.4.1.37552.4.2.110.1.22.1.2    
        iIgmpPerVlanEnabled OBJECT-TYPE
            SYNTAX      TruthValue
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Enable the per-VLAN IGMP Snooping. Only up to 64 VLANs can be selected."
            ::= { iIgmpVlanConfigEntry 2 }

        -- 1.3.6.1.4.1.37552.4.2.110.1.22.1.3    
        iIgmpQuerierEnabled OBJECT-TYPE
            SYNTAX      TruthValue
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Enable the IGMP Querier in the VLAN."
            ::= { iIgmpVlanConfigEntry 3 }

        -- 1.3.6.1.4.1.37552.4.2.110.1.22.1.4    
        iIgmpCompatibility OBJECT-TYPE
            SYNTAX      INTEGER {
                            forcedIgmpV1(1),
                            forcedIgmpV2(2),
                            forcedIgmpV3(3),
            	            igmpAuto(4)
            	        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Compatibility is maintained by hosts and routers taking appropriate actions depending on
                 the versions of IGMP operating on hosts and routers within a network.
                 The allowed selection is IGMP-Auto, Forced IGMPv1, Forced IGMPv2, Forced IGMPv3,
                 default compatibility value is IGMP-Auto."
            DEFVAL      { igmpAuto }
            ::= { iIgmpVlanConfigEntry 4 }

        -- 1.3.6.1.4.1.37552.4.2.110.1.22.1.5    
        iIgmpRV OBJECT-TYPE
            SYNTAX      Integer32 (1..255)
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Robustness Variable.
                 The Robustness Variable allows tuning for the expected packet loss on a network.
                 The allowed range is 1 to 255, default robustness variable value is 2,
                 Robustness Variable SHOULD NOT be one."
            DEFVAL     { 2 }
            ::= { iIgmpVlanConfigEntry 5 }

        -- 1.3.6.1.4.1.37552.4.2.110.1.22.1.6    
        iIgmpQI OBJECT-TYPE
            SYNTAX      Integer32 (1..255)
            UNITS       "Seconds"
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Query Interval.
                 The Query Interval is the interval between General Queries sent by the Querier.
                 The allowed range is 1 to 255 seconds, default query interval is 125 seconds.
                 Note: The number of seconds represented by QRI must be less than QI!"
            DEFVAL     { 125 }     
            ::= { iIgmpVlanConfigEntry 6 }

        -- 1.3.6.1.4.1.37552.4.2.110.1.22.1.7    
        iIgmpQRI OBJECT-TYPE
            SYNTAX      Integer32 (0..31744)
            UNITS       "100 millisecond"
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Query Response Interval.
                 The Max Response Time used to calculate the Max Resp Code inserted into the periodic General Queries.
                 The allowed range is 0 to 31744 in tenths of seconds,
                 default query response interval is 100 in tenths of seconds (10 seconds).
                 The number of seconds represented by QRI must be less than QI!"
            DEFVAL     { 100 }     
            ::= { iIgmpVlanConfigEntry 7 }

        -- 1.3.6.1.4.1.37552.4.2.110.1.22.1.8    
        iIgmpLLQI OBJECT-TYPE
            SYNTAX      Integer32 (0..31744)
            UNITS       "100 millisecond"
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Last Member Query Interval.
                 The Last Member Query Time is the time value represented by the Last Member Query Interval,
                 multiplied by the Last Member Query Count.
                 The allowed range is 0 to 31744 in tenths of seconds,
                 default last member query interval is 10 in tenths of seconds (1 second)."
            DEFVAL     { 10 }     
            ::= { iIgmpVlanConfigEntry 8 }

        -- 1.3.6.1.4.1.37552.4.2.110.1.22.1.9    
        iIgmpURI OBJECT-TYPE
            SYNTAX      Integer32 (0..31744)
            UNITS       "Seconds"
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Unsolicited Report Interval.
                 The Unsolicited Report Interval is the time between repetitions of
                 a host's initial report of membership in a group.
                 The allowed range is 0 to 31744 seconds,
                 default unsolicited report interval is 1 second."
            DEFVAL     { 1 }     
            ::= { iIgmpVlanConfigEntry 9 }

        -- 1.3.6.1.4.1.37552.4.2.110.1.22.1.10       
        iIgmpQuerierAddress OBJECT-TYPE
            SYNTAX      IpAddress
            MAX-ACCESS  read-write
            STATUS      current                          
            DESCRIPTION
                "
                "
            ::= { iIgmpVlanConfigEntry 10 }

        -- 1.3.6.1.4.1.37552.4.2.110.1.22.1.11   
        iIgmpPRI OBJECT-TYPE
            SYNTAX      Integer32 (0..7)
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "
                "
            ::= { iIgmpVlanConfigEntry 11 }



