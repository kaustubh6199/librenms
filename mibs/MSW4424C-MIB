-- IPMC
    -- 1.3.6.1.4.1.37552.4.2.110
    iIPMC  OBJECT IDENTIFIER ::= { msw4424C 110 }

    -- 1.3.6.1.4.1.37552.4.2.110.1
    iIgmpSnooping  OBJECT IDENTIFIER ::= { iIPMC 1 }

    -- 1.3.6.1.4.1.37552.4.2.110.1.1
    iIgmpSnoopingEnabled OBJECT-TYPE
        SYNTAX      TruthValue
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION                                  
            "Enable the Global IGMP Snooping."
        ::= { iIgmpSnooping 1 }

    -- 1.3.6.1.4.1.37552.4.2.110.1.2
    iUnregisteredIpmcV4FloodingEnabled OBJECT-TYPE
        SYNTAX      TruthValue
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION                                  
            "Enable unregistered IPMC traffic flooding."
        ::= { iIgmpSnooping 2 }

    -- 1.3.6.1.4.1.37552.4.2.110.1.3
    iIgmpSsmRange OBJECT-TYPE
        SYNTAX      DisplayString
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION                                  
            "SSM (Source-Specific Multicast) Range allows the SSM-aware hosts and
             routers run the SSM service model for the groups in the address range.
             Syntax: (Range <mask_len>)"
        ::= { iIgmpSnooping 3 }

    -- 1.3.6.1.4.1.37552.4.2.110.1.4
    iIgmpLeaveProxyEnabled OBJECT-TYPE
        SYNTAX      TruthValue
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION                                  
            "Enable IGMP Leave Proxy.
             This feature can be used to avoid forwarding unnecessary leave messages to
             the router side."
        ::= { iIgmpSnooping 4 }

    -- 1.3.6.1.4.1.37552.4.2.110.1.5
    iIgmpProxyEnabled OBJECT-TYPE
        SYNTAX      TruthValue
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION                                  
            "Enable IGMP Proxy.
             This feature can be used to avoid forwarding unnecessary join and
             leave messages to the router side."
        ::= { iIgmpSnooping 5 }

    -- 1.3.6.1.4.1.37552.4.2.110.1.10
    iIgmpPortRelatedTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF IIgmpPortRelatedEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table is used for configuring the Port Related."
        ::= { iIgmpSnooping 10 }

    -- 1.3.6.1.4.1.37552.4.2.110.1.10.1
    iIgmpPortRelatedEntry OBJECT-TYPE
        SYNTAX      IIgmpPortRelatedEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Entry of Port Related configurations."
        INDEX { iPortIndex }
        ::= { iIgmpPortRelatedTable 1 }

    IIgmpPortRelatedEntry ::=
        SEQUENCE {
            iIgmpRouterPortEnabled TruthValue,                
            iIgmpFastLeaveEnabled  TruthValue,
            iIgmpThrottling        Integer32
        }

        -- 1.3.6.1.4.1.37552.4.2.110.1.10.1.1    
        iIgmpRouterPortEnabled OBJECT-TYPE
            SYNTAX      TruthValue
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Specify which ports act as router ports.
                 A router port is a port on the Ethernet switch that
                 leads towards the Layer 3 multicast device or IGMP querier.
                 If an aggregation member port is selected as a router port,
                 the whole aggregation will act as a router port."
            ::= { iIgmpPortRelatedEntry 1 }


        -- 1.3.6.1.4.1.37552.4.2.110.1.10.1.3    
        iIgmpFastLeaveEnabled OBJECT-TYPE
            SYNTAX      TruthValue
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Enable the fast leave on the port."
            ::= { iIgmpPortRelatedEntry 3 }

        -- 1.3.6.1.4.1.37552.4.2.110.1.10.1.4    
        iIgmpThrottling OBJECT-TYPE
            SYNTAX      Integer32 (0..10)
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Enable to limit the number of multicast groups to which a switch port can belong.
                 Set to zero for unlimited."
            ::= { iIgmpPortRelatedEntry 4 }



    -- 1.3.6.1.4.1.37552.4.2.110.1.20
