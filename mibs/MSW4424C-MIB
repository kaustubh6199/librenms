-- Link OAM
    -- 1.3.6.1.4.1.4756.136.45
    iLinkOAM OBJECT IDENTIFIER ::= { msw4424C 45 }
    



    -- 1.3.6.1.4.1.4756.136.45.20
    iLinkOAMStatisticsTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF ILinkOAMStatisticsEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table shows current port configurations. Ports can also be configured here."
        ::= { iLinkOAM 4 }

    -- 1.3.6.1.4.1.4756.136.45.4.1
    iLinkOAMStatisticsEntry OBJECT-TYPE
        SYNTAX      ILinkOAMStatisticsEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The entry of ports."
        INDEX { iPortIndex }
