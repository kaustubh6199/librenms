            ::= { iQceConfigEntry 62 }


    -- 1.3.6.1.4.1.37552.4.2.170.4
    iStormControl   OBJECT IDENTIFIER ::= { iQoS 4 }

    -- 1.3.6.1.4.1.37552.4.2.170.4.1
    iStormControlTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF IStormControlEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "Storm control for the switch is configured on this table.
             There is a unicast storm rate control, multicast storm rate control,
             and a broadcast storm rate control. These only affect flooded frames,
             i.e. frames with a (VLAN ID, DMAC) pair not present on the MAC Address table.
             The configuration indicates the permitted packet rate for unicast, multicast or
             broadcast traffic across the switch."
        ::= { iStormControl 3 }

    -- 1.3.6.1.4.1.37552.4.2.170.4.1.1
    iStormControlEntry OBJECT-TYPE
        SYNTAX      IStormControlEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The entry of frame type."
        INDEX { iStormControlIndex }
        ::= { iStormControlTable 1 }

    IStormControlEntry ::=
        SEQUENCE {
        		iStormControlIndex     Integer32,
            iStormControlUuicastEnable    TruthValue,
            iStormControlUuicastRate      INTEGER,
            iStormControlUuicastUnit      INTEGER,
            iStormControlBroadcastEnable    TruthValue,
            iStormControlBroadcastRate      INTEGER,
            iStormControlBroadcastUnit      INTEGER,
            iStormControlUnknownEnable    TruthValue,
            iStormControlUnknownRate      INTEGER,
            iStormControlUnknownUnit      INTEGER                        
        }
        
    	  -- 1.3.6.1.4.1.37552.4.2.170.4.1.1.1
        iStormControlIndex OBJECT-TYPE
            SYNTAX      Integer32 (1..29)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The Index of the StormContro Port

                "
            ::= { iStormControlEntry 1 }
            

        -- 1.3.6.1.4.1.37552.4.2.170.4.1.1.2    
        iStormControlUuicastEnable OBJECT-TYPE
            SYNTAX      TruthValue
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Enable or disable the storm control status for the given frame type."
            ::= { iStormControlEntry 2 }

    	  -- 1.3.6.1.4.1.37552.4.2.170.4.1.1.3
        iStormControlUuicastRate OBJECT-TYPE
            SYNTAX      INTEGER 
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "

                "
            ::= { iStormControlEntry 3 }       

        -- 1.3.6.1.4.1.37552.4.2.170.4.1.1.4    
        iStormControlUuicastUnit OBJECT-TYPE
            SYNTAX      INTEGER {
                            kbps(1),
                            mbps(2),
                            fps(3),
                            kfps(4)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "
                "
            ::= { iStormControlEntry 4 }            

        -- 1.3.6.1.4.1.37552.4.2.170.4.1.1.5    
        iStormControlBroadcastEnable OBJECT-TYPE
            SYNTAX      TruthValue
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Enable or disable the storm control status for the given frame type."
            ::= { iStormControlEntry 5 }

    	  -- 1.3.6.1.4.1.37552.4.2.170.4.1.1.6
        iStormControlBroadcastRate OBJECT-TYPE
            SYNTAX      INTEGER 
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "

                "
            ::= { iStormControlEntry 6 }       

        -- 1.3.6.1.4.1.37552.4.2.170.4.1.1.7    
        iStormControlBroadcastUnit OBJECT-TYPE
            SYNTAX      INTEGER {
                            kbps(1),
                            mbps(2),
                            fps(3),
                            kfps(4)
                        }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "
                "
            ::= { iStormControlEntry 7 }
            
        -- 1.3.6.1.4.1.37552.4.2.170.4.1.1.8    
        iStormControlUnknownEnable OBJECT-TYPE
            SYNTAX      TruthValue
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Enable or disable the storm control status for the given frame type."
            ::= { iStormControlEntry 8 }

    	  -- 1.3.6.1.4.1.37552.4.2.170.4.1.1.9
        iStormControlUnknownRate OBJECT-TYPE
            SYNTAX      INTEGER 
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "

                "
            ::= { iStormControlEntry 9 }       

        -- 1.3.6.1.4.1.37552.4.2.170.4.1.1.10    
        iStormControlUnknownUnit OBJECT-TYPE
            SYNTAX      INTEGER {
                            kbps(1),
                            mbps(2),
