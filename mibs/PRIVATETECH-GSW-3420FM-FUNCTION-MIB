swDHCPSnoopingAgentURL  OBJECT-TYPE
	SYNTAX	DisplayString( SIZE(0..358))
	MAX-ACCESS	read-write
	STATUS	current
	DESCRIPTION
		" Enter the location of the DHCP snooping database. The location should be expressed like this: tftp://(domain name or IP Address)}/directory, if applicable/file name; for example, tftp://192.168.1.1/database.txt."
	::= { swDHCPSnoopingConf 3 }


swDHCPSnoopingTimeoutInterval	 OBJECT-TYPE
	SYNTAX	INTEGER(10..65535)
	MAX-ACCESS	read-write
	STATUS	current
	DESCRIPTION
		" Enter how hong (10-65535 seconds) the switch tries to complete a specific update in the DHCP dnooping database before it gives up."
	::= { swDHCPSnoopingConf 4 }



swDHCPSnoopingWriteDelayInterval	 OBJECT-TYPE
	SYNTAX	INTEGER(10..65535)
	MAX-ACCESS	read-write
	STATUS	current
	DESCRIPTION
		" Enter how hong (10-65535 seconds) the switch waits to update the DHCP dnooping database."
	::= { swDHCPSnoopingConf 5 }
	

