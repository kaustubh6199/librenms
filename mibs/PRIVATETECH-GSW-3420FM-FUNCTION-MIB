swPort                                                  OBJECT IDENTIFIER ::= { swConfiguration 6 }

swPortConfigurationTable OBJECT-TYPE
   SYNTAX      SEQUENCE OF SwPortConfigurationEntry
   MAX-ACCESS  not-accessible
   STATUS      current
   DESCRIPTION
       " Show port configurations. "
   ::= { swPort 1 }

swPortConfigurationEntry OBJECT-TYPE
   SYNTAX      SwPortConfigurationEntry
   MAX-ACCESS  not-accessible
   STATUS      current
   DESCRIPTION
       ""
   INDEX    { swPortConfPort  }
   ::= { swPortConfigurationTable 1 }

SwPortConfigurationEntry ::= SEQUENCE {
   swPortConfPort                     INTEGER,
   swPortConfPortMedia                DisplayString,
   swPortConfLink                     DisplayString,
   swPortConfCurrentSpeed             DisplayString,
   swPortConfSpeed                    INTEGER,
   swPortConfCurrentFlowControlRx     INTEGER,
   swPortConfCurrentFlowControlTx     INTEGER,
   swPortConfFlowControl              INTEGER,
   swPortConfMaxFrameSize             INTEGER,
   swPortConfExcessiveCollisionMode   INTEGER,
   swPortConfPowerControl             INTEGER 
      
}

swPortConfPort OBJECT-TYPE
   SYNTAX      INTEGER (1..4095)
   MAX-ACCESS  not-accessible
   STATUS      current
   DESCRIPTION
       " port, range : 1 - port number. "
   ::= { swPortConfigurationEntry 1 }

swPortConfPortMedia OBJECT-TYPE
   SYNTAX      DisplayString(SIZE (1..4))
   MAX-ACCESS  read-only
   STATUS      current
   DESCRIPTION
       " Show port media. "
   ::= { swPortConfigurationEntry 2 }

swPortConfLink OBJECT-TYPE
   SYNTAX      DisplayString(SIZE (2..4))
   MAX-ACCESS  read-only
   STATUS      current
   DESCRIPTION
       " The current link state. "
   ::= { swPortConfigurationEntry 3 }

swPortConfCurrentSpeed OBJECT-TYPE
   SYNTAX      DisplayString(SIZE (6..12))
   MAX-ACCESS  read-only
   STATUS      current
   DESCRIPTION
       " The current link speed of the port. "
   ::= { swPortConfigurationEntry 4 }

swPortConfSpeed OBJECT-TYPE
   SYNTAX      INTEGER (0..6)
   MAX-ACCESS  read-write
   STATUS      current
   DESCRIPTION
       "default: 1,
        0: disable state
        1: auto
        2: 10 Half
        3: 10 Full
        4: 100 Half
        5: 100 Full
        6: 1G full 
        7: SFP_Auto_AMS
        8: 100-FX
        9: 100-FX_AMS
        10:1000-X
        11:1000-X_AMS
       "
   ::= { swPortConfigurationEntry 5 }

swPortConfCurrentFlowControlRx OBJECT-TYPE
   SYNTAX      INTEGER (0|1)
   MAX-ACCESS  read-only
   STATUS      current
   DESCRIPTION
       "0: disable
        1: enable "
   ::= { swPortConfigurationEntry 6 }

swPortConfCurrentFlowControlTx OBJECT-TYPE
   SYNTAX      INTEGER (0|1)
   MAX-ACCESS  read-only
   STATUS      current
   DESCRIPTION
       "0: disable
        1: enable "
   ::= { swPortConfigurationEntry 7 }

swPortConfFlowControl OBJECT-TYPE
   SYNTAX      INTEGER (0|1)
   MAX-ACCESS  read-write
   STATUS      current
   DESCRIPTION
       "default: 0,
        0: disable
        1: enable "
   ::= { swPortConfigurationEntry 8 }

swPortConfMaxFrameSize OBJECT-TYPE
   SYNTAX      INTEGER (1518..9600)
   MAX-ACCESS  read-write
   STATUS      current
   DESCRIPTION
       "default: 9600 , range : 1518 - 9600 "
   ::= { swPortConfigurationEntry 9 }

swPortConfExcessiveCollisionMode OBJECT-TYPE
   SYNTAX      INTEGER  (0|1)
   MAX-ACCESS  read-write
   STATUS      current
   DESCRIPTION
      "default: Discard,
        0: Discard
        1: Restart "     
   ::= { swPortConfigurationEntry 10 }


swPortConfPowerControl OBJECT-TYPE
   SYNTAX      INTEGER  (0..3)
   MAX-ACCESS  read-write
   STATUS      current
   DESCRIPTION
       "default: 0,        
        0: Disable,
        1: Actiphy,
        2: Dynamic (perfectReach),
        3: Enable "
   ::= { swPortConfigurationEntry 11 }







