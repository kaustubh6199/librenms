swIPInterfaces                                          OBJECT IDENTIFIER ::= {swIP 6 }


swIPVlanConfiguredTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF SwIPVlanConfiguredEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A table containing IP Interfaces information for
        each VLAN configured into the device by (local or
        network) management.  All entries are permanent and will
        be restored after the device is reset."
    ::= { swIPInterfaces 1 }

swIPVlanConfiguredEntry OBJECT-TYPE
    SYNTAX      SwIPVlanConfiguredEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "IP interfaces information for a VLAN configured into the
        device by (local or network) management."
    INDEX   { swIPVlanId }
    ::= { swIPVlanConfiguredTable 1 }

SwIPVlanConfiguredEntry ::=
    SEQUENCE {
        swIPVlanId
             INTEGER,
        swIPVlanV4DHCPClient
             INTEGER,
        swIPVlanV4DHCPFallbackTimeout    
             Unsigned32,     
       swIPVlanIpType      
             INTEGER,              
        swIPVlanV4Address      
             IpAddress,
        swIPVlanV4MaskLen   
             INTEGER, 
        swIPVlanV6Address      
             DisplayString,
        swIPVlanV6Prefix   
             INTEGER,                              
        swIPVlanRowStatus
            RowStatus
    }
    
swIPVlanId OBJECT-TYPE
   SYNTAX      INTEGER (1..4094)
   MAX-ACCESS  not-accessible
   STATUS      current
   DESCRIPTION
       "The VLAN-ID or other identifier referring to this VLAN."
   ::= { swIPVlanConfiguredEntry 1 }   
   
swIPVlanV4DHCPClient OBJECT-TYPE
   SYNTAX      INTEGER (0|1)
   MAX-ACCESS  read-write
   STATUS      current
   DESCRIPTION
       "default:0
       0: disable
       1: enable" 
   ::= { swIPVlanConfiguredEntry 2 } 
   
swIPVlanV4DHCPFallbackTimeout OBJECT-TYPE
   SYNTAX      Unsigned32 (0..4294967295)
   MAX-ACCESS  read-write
   STATUS      current
   DESCRIPTION
       "The number of seconds for trying to obtain a DHCP lease" 
   ::= { swIPVlanConfiguredEntry 3 }    
   
swIPVlanIpType OBJECT-TYPE
   SYNTAX      INTEGER (1|2)
   MAX-ACCESS  read-write
   STATUS      current
   DESCRIPTION
       " 1: IPV4
         2: IPV6" 
   ::= { swIPVlanConfiguredEntry 4 }    

swIPVlanV4Address OBJECT-TYPE
   SYNTAX      IpAddress
   MAX-ACCESS  read-write
   STATUS      current
   DESCRIPTION
       ""
   ::= { swIPVlanConfiguredEntry 5 }

swIPVlanV4MaskLen OBJECT-TYPE
   SYNTAX      INTEGER (0..32)
   MAX-ACCESS  read-write
   STATUS      current
   DESCRIPTION
       ""
   ::= { swIPVlanConfiguredEntry 6 }   
  
swIPVlanV6Address OBJECT-TYPE
   SYNTAX      DisplayString (SIZE (0..30))
   MAX-ACCESS  read-write
   STATUS      current
   DESCRIPTION
       ""
   ::= { swIPVlanConfiguredEntry 7 }

swIPVlanV6Prefix OBJECT-TYPE
   SYNTAX      INTEGER (1..128)
   MAX-ACCESS  read-write
   STATUS      current
   DESCRIPTION
       "default: 96"
   ::= { swIPVlanConfiguredEntry 8 }

swIPVlanRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object indicates the status of this entry."
    ::= { swIPVlanConfiguredEntry 9 }
    
    
    
swIPRoutes                                          OBJECT IDENTIFIER ::= {swIP 7 }


swIPRoutesConfiguredTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF SwIPRoutesConfiguredEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A table containing IP Interfaces information for
        each VLAN configured into the device by (local or
        network) management.  All entries are permanent and will
        be restored after the device is reset."
    ::= { swIPRoutes 1 }

swIPRoutesConfiguredEntry OBJECT-TYPE
    SYNTAX      SwIPRoutesConfiguredEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "IP interfaces information for a VLAN configured into the
        device by (local or network) management."
    INDEX   { swIPRoutesIndex }
    ::= { swIPRoutesConfiguredTable 1 }

SwIPRoutesConfiguredEntry ::=
    SEQUENCE {
        swIPRoutesIndex
             INTEGER,
        swIPRoutesIpType
             INTEGER,
        swIPRoutesV4Network
             IpAddress,
        swIPRoutesV4MaskLen   
             INTEGER, 
        swIPRoutesV4Gateway
             IpAddress,             
        swIPRoutesV6Network
             DisplayString,
        swIPRoutesV6MaskLen
             INTEGER, 
        swIPRoutesV6Gateway      
             DisplayString,
        swIPRoutesNextHopVLAN
             INTEGER                                        
    }
    
swIPRoutesIndex OBJECT-TYPE
   SYNTAX      INTEGER (1..32)
   MAX-ACCESS  not-accessible
   STATUS      current
   DESCRIPTION
       "The Route Table index no."
   ::= { swIPRoutesConfiguredEntry 1 }   
   
swIPRoutesIpType OBJECT-TYPE
   SYNTAX      INTEGER (0|1|2)
   MAX-ACCESS  read-write
   STATUS      current
   DESCRIPTION
       "default:0
       0: Invalid 
       1: IPV4
       2: IPV6" 
   ::= { swIPRoutesConfiguredEntry 2 } 
   
 

swIPRoutesV4Network OBJECT-TYPE
   SYNTAX      IpAddress
   MAX-ACCESS  read-write
   STATUS      current
   DESCRIPTION
       "The destination IP network or host address of this route
       Valid format is dotted decimal notationor . A default route can use the value 0.0.0.0
       "
    ::= { swIPRoutesConfiguredEntry 3 } 

swIPRoutesV4MaskLen OBJECT-TYPE
   SYNTAX      INTEGER (0..32)
   MAX-ACCESS  read-write
   STATUS      current
   DESCRIPTION
       "The destination IP network or host mask
        Valid values are between 0 and 32 bits"
    ::= { swIPRoutesConfiguredEntry 4 } 
        
swIPRoutesV4Gateway  OBJECT-TYPE
   SYNTAX      IpAddress
   MAX-ACCESS  read-write
   STATUS      current
   DESCRIPTION
       "The IP address of the IP gateway.
        Valid format is dotted decimal notation.
        Gateway and Network must be of the same type."
    ::= { swIPRoutesConfiguredEntry 5 }     
    
  
swIPRoutesV6Network OBJECT-TYPE
   SYNTAX      DisplayString (SIZE (0..30))
   MAX-ACCESS  read-write
   STATUS      current
   DESCRIPTION
       "The destination IP network or host address of this route
        Valid format is a valid IPv6 notation. A default route can use the value 
        IPv6 :: notation"
     ::= { swIPRoutesConfiguredEntry 6 }    

swIPRoutesV6MaskLen  OBJECT-TYPE
   SYNTAX      INTEGER (1..128)
   MAX-ACCESS  read-write
   STATUS      current
   DESCRIPTION
       "The destination IP network or host mask
        respectively 128 for IPv6 routes
       "       
   ::= { swIPRoutesConfiguredEntry 7 }    
   
swIPRoutesV6Gateway OBJECT-TYPE
   SYNTAX      DisplayString (SIZE (0..30))
   MAX-ACCESS  read-write
   STATUS      current
   DESCRIPTION
       "The IP address of the IP gateway.
        Valid format is a valid IPv6 notation.
        Gateway and Network must be of the same type.
       "
     ::= { swIPRoutesConfiguredEntry 8 }     

swIPRoutesNextHopVLAN OBJECT-TYPE
   SYNTAX      INTEGER (1..4094)
   MAX-ACCESS  read-write
   STATUS      current
   DESCRIPTION
       "The VLAN ID (VID) of the specific IPv6 interface associated with the gateway"
   ::= { swIPRoutesConfiguredEntry 9 }        

