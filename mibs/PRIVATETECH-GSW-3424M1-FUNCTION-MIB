swMonitorMacAddr                                     OBJECT IDENTIFIER ::= { swMonitorMac 2 }

swMonitorMacAddrTable OBJECT-TYPE
   SYNTAX      SEQUENCE OF SwMonitorMacAddrEntry
   MAX-ACCESS  not-accessible
   STATUS      current
   DESCRIPTION
       " "
   ::= { swMonitorMacAddr 1 }

swMonitorMacAddrEntry OBJECT-TYPE
   SYNTAX      SwMonitorMacAddrEntry
   MAX-ACCESS  not-accessible
   STATUS      current
   DESCRIPTION
       ""
   INDEX    { swMonitorMacAddrIndex  }
   ::= { swMonitorMacAddrTable 1 }

SwMonitorMacAddrEntry ::= SEQUENCE {
   swMonitorMacAddrIndex             INTEGER,
   swMonitorMacAddrTypeVidMacAddressPorts      DisplayString
}

swMonitorMacAddrIndex OBJECT-TYPE
   SYNTAX      INTEGER (0..8192)
   MAX-ACCESS  not-accessible
   STATUS      current
   DESCRIPTION
       " The index of the addr entry. "
   ::= { swMonitorMacAddrEntry 1 }


--swMonitorMacAddrPort OBJECT-TYPE
--   SYNTAX       INTEGER(0..50)
--   MAX-ACCESS   read-write
--   STATUS       current
--   DESCRIPTION
--       "Default: 0,
--        0 : IPv4,
--        1 : IPv6."
--   ::= { swMonitorMacAddrEntry 2 }



swMonitorMacAddrTypeVidMacAddressPorts  OBJECT-TYPE
	SYNTAX	DisplayString( SIZE(0..300))
	MAX-ACCESS	read-only
	STATUS	current
	DESCRIPTION
		" MAC Address Table. Format:Type    VID  MAC Address        Ports  Example:Dynamic 1    00-01-02-03-04-05  1  "
   ::= { swMonitorMacAddrEntry 2 }

