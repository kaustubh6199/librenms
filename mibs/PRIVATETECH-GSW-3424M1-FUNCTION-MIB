swConfigTime                                       OBJECT IDENTIFIER ::= { swConfiguration 1 }

swConfigTimeClockSource OBJECT-TYPE
   SYNTAX       INTEGER  (0|1)
   MAX-ACCESS   read-write
   STATUS       current
   DESCRIPTION
       "default: 0,
        0 : Use Local Settings,
        1 : Use NTP Server."
   ::= { swConfigTime 1 }
   
 

swConfigTimeNTP                                     OBJECT IDENTIFIER ::= { swConfigTime 2 }

swConfigTimeNTPTable OBJECT-TYPE
   SYNTAX      SEQUENCE OF SwConfigTimeNTPEntry
   MAX-ACCESS  not-accessible
   STATUS      current
   DESCRIPTION
       " "
   ::= { swConfigTimeNTP 1 }

swConfigTimeNTPEntry OBJECT-TYPE
   SYNTAX      SwConfigTimeNTPEntry
   MAX-ACCESS  not-accessible
   STATUS      current
   DESCRIPTION
       ""
   INDEX    { swConfigTimeNTPIndex  }
   ::= { swConfigTimeNTPTable 1 }

SwConfigTimeNTPEntry ::= SEQUENCE {
   swConfigTimeNTPIndex             INTEGER,
   swConfigTimeNTPServerIPType      INTEGER,
   swConfigTimeNTPServer            DisplayString
}

swConfigTimeNTPIndex OBJECT-TYPE
   SYNTAX      INTEGER (1..5)
   MAX-ACCESS  not-accessible
   STATUS      current
   DESCRIPTION
       " The index of the ntp entry. "
   ::= { swConfigTimeNTPEntry 1 }

swConfigTimeNTPServerIPType OBJECT-TYPE
   SYNTAX       INTEGER(0|1)
   MAX-ACCESS   read-write
   STATUS       current
   DESCRIPTION
       "Default: 0,
        0 : IPv4,
        1 : IPv6."
   ::= { swConfigTimeNTPEntry 2 }

swConfigTimeNTPServer OBJECT-TYPE
   SYNTAX       DisplayString
   MAX-ACCESS   read-write
   STATUS       current
   DESCRIPTION
       "Get/Set the NTP Time Server1. You can use the IP address or domain name."
   ::= { swConfigTimeNTPEntry 3 }


 
swConfigTimeLocalTime  OBJECT-TYPE
	SYNTAX	DisplayString( SIZE(0..20))
	MAX-ACCESS	read-write
	STATUS	current
	DESCRIPTION
		"User can set current time. 
                 Format: YYYY-MM-DD HH:MM:SS
                 year: 1970-2037,
                 month: 1-23,
                 day: 1-31,
                 hour: 0-23,
                 minitue: 0-59,
                 second: 0-59,
                 Note: time can't small than (1970-1-1 0:0:0 + System Uptime)"
	::= { swConfigTime 3 }

