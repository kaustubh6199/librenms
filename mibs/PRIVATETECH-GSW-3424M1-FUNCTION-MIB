swTrapHostConfTable OBJECT-TYPE
   SYNTAX      SEQUENCE OF SwTrapHostConfEntry
   MAX-ACCESS  not-accessible
   STATUS      current
   DESCRIPTION
       "A list of trap host entry. The numbers of entries are given by the value of swTrapHostConfNumber."
   ::= { swSnmpConf 4 }
   
swTrapHostConfEntry OBJECT-TYPE
   SYNTAX      SwTrapHostConfEntry
   MAX-ACCESS  not-accessible
   STATUS      current
   DESCRIPTION
       "This is the entry information for trap host."
   INDEX    { swTrapHostConfIndex  }
   ::= { swTrapHostConfTable 1 }

SwTrapHostConfEntry ::= SEQUENCE {
    swTrapHostConfIndex             INTEGER,
    swTrapHostMode                  INTEGER,
    swTrapHostConfVersion           INTEGER,
    swTrapHostConfIPv4              DisplayString,
    swTrapHostConfIPv6              DisplayString, 
    swTrapHostConfCommunity         DisplayString 
    
}
   
