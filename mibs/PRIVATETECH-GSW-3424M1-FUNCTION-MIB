   swDDMWarn NOTIFICATION-TYPE
      STATUS   current
      DESCRIPTION
		  " Warning flags associated with Fx transceiver temperature,supply voltage, Tx bias, Tx outpout
		    power and received power."
      ::= { swProductId 302 }        



 -- Traps for use by port limit reached

   swLimitReachedTrap NOTIFICATION-TYPE
      STATUS   current
      DESCRIPTION
		  "If Limit + 1 MAC addresses is seen on the port, send an SNMP trap"
      ::= { swProductId 401 }
 


