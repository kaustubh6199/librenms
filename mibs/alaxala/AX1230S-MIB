--ax1230sMauth OBJECT-TYPE
--	SYNTAX  INTEGER
--	MAX-ACCESS  not-accessible
--	STATUS  current
--	DESCRIPTION
--		"Private trap of Mac authentication."
--	::= { ax1230sAuth 3 }

ax1230sMauthFailure OBJECT-TYPE
	SYNTAX  SEQUENCE OF Ax1230sAuthInfoTableEntry
	MAX-ACCESS  not-accessible
	STATUS  current
	DESCRIPTION
		"Private trap of Mac authentication failure."
	::= { ax1230sMauth 1 }

ax1230sMauthEvent OBJECT-TYPE
	SYNTAX  SEQUENCE OF Ax1230sAuthInfoTableEntry
	MAX-ACCESS  not-accessible
	STATUS  current
	DESCRIPTION
		"Private trap of Mac authentication event."
	::= { ax1230sMauth 2 }

ax1230sMauthSystem OBJECT-TYPE
	SYNTAX  SEQUENCE OF Ax1230sAuthInfoTableEntry
	MAX-ACCESS  not-accessible
	STATUS  current
	DESCRIPTION
		"Private trap of Mac authentication system."
	::= { ax1230sMauth 3 }

	Ax1230sAuthInfoTableEntry ::=
		SEQUENCE {
		  ax1230sAuthSysName
			DisplayString,
		  ax1230sAuthIfIndex
			INTEGER,
		  ax1230sAuthSupplicantMac
			MacAddress,
		  ax1230sAuthMessage
			DisplayString
		}

