--ax1230sAuthInfo OBJECT-TYPE
--	SYNTAX  INTEGER
--	MAX-ACCESS  not-accessible
--	STATUS  current
--	DESCRIPTION
--		"Private trap of Authentication."
--	::= { ax1230sAuth 1 }

ax1230sAuthSysName OBJECT-TYPE
	SYNTAX  DisplayString (SIZE (0..60))
	MAX-ACCESS  not-accessible
	STATUS  current
	DESCRIPTION
		"System name of authentication."
	::= { ax1230sAuthInfo 1 }

ax1230sAuthIfIndex OBJECT-TYPE
	SYNTAX  INTEGER
	MAX-ACCESS  not-accessible
	STATUS  current
	DESCRIPTION
		"ifIndex of authentication."
	::= { ax1230sAuthInfo 2 }

ax1230sAuthSupplicantMac OBJECT-TYPE
	SYNTAX  MacAddress
	MAX-ACCESS  not-accessible
	STATUS  current
	DESCRIPTION
		"Mac address of authentication."
	::= { ax1230sAuthInfo 3 }

ax1230sAuthMessage OBJECT-TYPE
	SYNTAX  DisplayString (SIZE (0..280))
	MAX-ACCESS  not-accessible
	STATUS  current
	DESCRIPTION
		"Message of authentication."
	::= { ax1230sAuthInfo 4 }

--ax1230sDot1x OBJECT-TYPE
--	SYNTAX  INTEGER
--	MAX-ACCESS  not-accessible
--	STATUS  current
--	DESCRIPTION
--		"Private trap of 802.1X authentication."
--	::= { ax1230sAuth 2 }

