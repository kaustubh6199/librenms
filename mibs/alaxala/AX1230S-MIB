	SYNTAX  SEQUENCE OF Ax1230sAuthInfoTableEntry
	MAX-ACCESS  not-accessible
	STATUS  current
	DESCRIPTION
		"Private trap of 802.1X authentication event."
	::= { ax1230sDot1x 2 }

ax1230sDot1xSystem OBJECT-TYPE
	SYNTAX  SEQUENCE OF Ax1230sAuthInfoTableEntry
	MAX-ACCESS  not-accessible
	STATUS  current
	DESCRIPTION
		"Private trap of 802.1X authentication system."
	::= { ax1230sDot1x 3 }

