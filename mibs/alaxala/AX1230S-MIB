	SYNTAX  INTEGER   { 
				enabled(1),
				disabled(2)
		    }
	MAX-ACCESS  read-only
	STATUS  current
	DESCRIPTION
		"The  administrative  status  of  OSPF  in  the
		router.  "
	::= { axsOspfGeneralEntry 3 }

axsOspfVersionNumber OBJECT-TYPE
	SYNTAX  INTEGER
	MAX-ACCESS  read-only
	STATUS  current
	DESCRIPTION
		"The current version number of the OSPF  
		protocol is 2."
	::= { axsOspfGeneralEntry 4 }

axsOspfAreaBdrRtrStatus OBJECT-TYPE
	SYNTAX  INTEGER { 
				true(1),
				false(2)
			}
	MAX-ACCESS  read-only
	STATUS  current
	DESCRIPTION
		"A flag to note whether this router is an  area
		 border router."
	::= { axsOspfGeneralEntry 5 }

axsOspfASBdrRtrStatus OBJECT-TYPE
	SYNTAX  INTEGER {
				true(1),
				false(2)
			}
	MAX-ACCESS  read-only
	STATUS  current
	DESCRIPTION
