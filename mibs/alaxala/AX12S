ax2130sTemperatureStatusTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF Ax2130sTemperatureStatusEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
		"The table of ambient temperature status maintained by the
		environmental monitor."
	::= { ax2130sChassis 3 }

ax2130sTemperatureStatusEntry OBJECT-TYPE
	SYNTAX  Ax2130sTemperatureStatusEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
		"An entry in the ambient temperature status table, representing
		the status of the associated testpoint maintained by the
		environmental monitor."
	INDEX  { ax2130sChassisIndex, ax2130sTemperatureStatusIndex }
	::= { ax2130sTemperatureStatusTable 1 }

	Ax2130sTemperatureStatusEntry ::=
		SEQUENCE {
		  ax2130sTemperatureStatusIndex
			INTEGER,
		  ax2130sTemperatureStatusDescr
			DisplayString,
		  ax2130sTemperatureStatusValue
			Integer32,
		  ax2130sTemperatureThreshold
			Integer32,
		  ax2130sTemperatureState
			INTEGER
		}

ax2130sTemperatureStatusIndex OBJECT-TYPE
	SYNTAX  INTEGER
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
		"Unique index for the testpoint being instrumented.
		This index is for SNMP purposes only, and has no
		intrinsic meaning."
	::= { ax2130sTemperatureStatusEntry 1 }

ax2130sTemperatureStatusDescr OBJECT-TYPE
	SYNTAX  DisplayString(SIZE(0..32))
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
		"Textual description of the testpoint being instrumented.
		This description is a short textual label, suitable as a
		human-sensible identification for the rest of the
		information in the entry."
	::= { ax2130sTemperatureStatusEntry 2 }

ax2130sTemperatureStatusValue OBJECT-TYPE
	SYNTAX  Integer32
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
		"The current measurement of the testpoint being instrumented."
	::= { ax2130sTemperatureStatusEntry 3 }

ax2130sTemperatureThreshold OBJECT-TYPE
	SYNTAX  Integer32
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
		"The highest value that the associated instance of the
		object ax2130sTemperatureStatusValue may obtain
		before an emergency shutdown of the managed device is
		initiated."
	::= { ax2130sTemperatureStatusEntry 4 }

ax2130sTemperatureState OBJECT-TYPE
	SYNTAX  INTEGER {
		normal(1),
		caution(2)
		}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
		"The current state of the testpoint being instrumented."
        ::= { ax2130sTemperatureStatusEntry 5 }

