ax1240sL2ldLinkDown TRAP-TYPE
	ENTERPRISE      ax1240s
	VARIABLES       { axsL2ldPortIfIndex,axsL2ldPortSourcePortIfindex,axsL2ldPortDestinationPortIfindex,axsL2ldPortSourceVlan }
	DESCRIPTION
           "When the operation of the interface changes from ACTIVE(state that can be communicated)
           into DISABLE (state that cannot be communicated) by the L2 loop detection."
	::= 51

--axsL2ldLinkUp TRAP-TYPE
ax1240sL2ldLinkUp TRAP-TYPE
	ENTERPRISE      ax1240s
	VARIABLES       { axsL2ldPortIfIndex }
	DESCRIPTION
           "When the operation of the interface changes from DISABLE (state that cannot be communicated)
           into ACTIVE (state that can be communicated) by the automatic restoration function of the L2 loop detection."
	::= 52

--axsL2ldLoopDetection TRAP-TYPE
ax1240sL2ldLoopDetection TRAP-TYPE
	ENTERPRISE      ax1240s
	VARIABLES       { axsL2ldPortIndex,axsL2ldPortIfIndex,axsL2ldPortSourcePortIfindex,axsL2ldPortSourceVlan }
	DESCRIPTION
           "When you detect the L2 loop. It notifies every 60 seconds when the state of the L2 loop continues."
	::= 53

--axsUlrChangeSecondary TRAP-TYPE
ax1240sUlrChangeSecondary TRAP-TYPE
	ENTERPRISE      ax1240s
	VARIABLES       { axsUlrPortIfIndex,axsUlrPairedPortIfIndex }
	DESCRIPTION
           "When secondary port changed to active port."
	::= 87

--axsUlrChangePrimary TRAP-TYPE
ax1240sUlrChangePrimary TRAP-TYPE
	ENTERPRISE      ax1240s
	VARIABLES       { axsUlrPortIfIndex,axsUlrPairedPortIfIndex }
	DESCRIPTION
           "When primary port changed to active port."
	::= 88

ax1240sDeviceErrorTrap TRAP-TYPE
	ENTERPRISE      ax1240s
	VARIABLES       { ax1240sMemoryError }
	DESCRIPTION
             "This indicates decection on Device Error." 
	::= 90

END

AXS-AX1250S-TRAP DEFINITIONS ::= BEGIN
IMPORTS
enterprises
        FROM RFC1155-SMI
OBJECT-TYPE
        FROM RFC-1212
TRAP-TYPE
        FROM RFC-1215
ifIndex, DisplayString
        FROM RFC1213-MIB
MacAddress
        FROM SNMPv2-TC
ax1250s, ax1250sSystemMsgType, ax1250sSystemMsgTimeStamp,
ax1250sSystemMsgLevel, ax1250sSystemMsgEventPoint,
ax1250sSystemMsgEventInterfaceID, ax1250sSystemMsgEventCode,
ax1250sSystemMsgAdditionalCode, ax1250sSystemMsgText,
ax1250sChassisIndex, ax1250sTemperatureStatusIndex, ax1250sTemperatureStatusDescr,
ax1250sTemperatureStatusValue, ax1250sTemperatureState,
axsLoginName, axsLoginTime, axsLoginLocation, axsLoginLine,
axsLoginFailureTime, axsLogoutTime, axsLogoutStatus,
axsL2ldPortIndex, axsL2ldPortIfIndex, axsL2ldPortSourcePortIfindex,
axsL2ldPortDestinationPortIfindex, axsL2ldPortSourceVlan,
axsUlrPortIfIndex, axsUlrPairedPortIfIndex, ax1250sMemoryError, ax1250sAuthInfo
        FROM AX1240S
;

