ax2230sSystemMsgTimeStamp OBJECT-TYPE
	SYNTAX  DisplayString (SIZE (0..14))
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
		"The time that event occured. And that is expressed as type of
		MM/DD hh:mm:ss
			MM:month(01-12)
			DD:day(01-31)
			hh:hour(00-23)
			mm:minute(00-59)
			ss:second(00-59)
		Insert space between DD and hh."
	::= { ax2230sSystemMsg 3 }

ax2230sSystemMsgLevel OBJECT-TYPE
	SYNTAX  OCTET STRING (SIZE (1))
	ACCESS  read-only
	STATUS  mandatory
