        FROM RFC1213-MIB
MacAddress
        FROM SNMPv2-TC
ax2230s, ax2230sSystemMsgType, ax2230sSystemMsgTimeStamp,
ax2230sSystemMsgLevel, ax2230sSystemMsgEventPoint,
ax2230sSystemMsgEventInterfaceID, ax2230sSystemMsgEventCode,
ax2230sSystemMsgAdditionalCode, ax2230sSystemMsgText,
ax2230sChassisIndex, ax2230sTemperatureStatusIndex, ax2230sTemperatureStatusDescr,
ax2230sTemperatureStatusValue, ax2230sTemperatureState,
axsLoginName, axsLoginTime, axsLoginLocation, axsLoginLine,
axsLoginFailureTime, axsLogoutTime, axsLogoutStatus,
axsL2ldPortIndex, axsL2ldPortIfIndex, axsL2ldPortSourcePortIfindex,
axsL2ldPortDestinationPortIfindex, axsL2ldPortSourceVlan,
axsUlrPortIfIndex, axsUlrPairedPortIfIndex, ax2230sMemoryError, ax2230sAuthInfo
        FROM AX1240S
;

-- Traps for use by AlaxalA networks AX2230S

-- Traps are defined using the conventions in RFC 1215.

-- ax2230sAuth group

ax2230sAuthSysName OBJECT-TYPE
	SYNTAX  DisplayString (SIZE (0..60))
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
		"System name of authentication."
	::= { ax2230sAuthInfo 1 }

ax2230sAuthIfIndex OBJECT-TYPE
	SYNTAX  INTEGER
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
		"ifIndex of authentication."
	::= { ax2230sAuthInfo 2 }

ax2230sAuthSupplicantMac OBJECT-TYPE
	SYNTAX  MacAddress
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
		"Mac address of authentication."
	::= { ax2230sAuthInfo 3 }

ax2230sAuthMessage OBJECT-TYPE
	SYNTAX  DisplayString (SIZE (0..280))
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
		"Message of authentication."
	::= { ax2230sAuthInfo 4 }

ax2230sSystemMsgTrap TRAP-TYPE
	ENTERPRISE      ax2230s
	VARIABLES       { ax2230sSystemMsgType, ax2230sSystemMsgTimeStamp,
                          ax2230sSystemMsgLevel, ax2230sSystemMsgEventPoint,
                          ax2230sSystemMsgEventInterfaceID, ax2230sSystemMsgEventCode,
                          ax2230sSystemMsgAdditionalCode, ax2230sSystemMsgText }
	DESCRIPTION
		"System message is displayed."
	::= 1

ax2230sTemperatureTrap TRAP-TYPE
	ENTERPRISE ax2230s
	VARIABLES {
		ax2230sChassisIndex,
		ax2230sTemperatureStatusIndex,
		ax2230sTemperatureStatusDescr, 
		ax2230sTemperatureStatusValue, 
		ax2230sTemperatureState }
	DESCRIPTION
		"A ax2230sTemperatureTrap is sent if the 
		temperature measured at a given testpoint is outside 
		the normal range for the testpoint (i.e. is at the warning, 
		critical, or shutdown stage)."
	::= 4

ax2230sAirFanStopTrap TRAP-TYPE
	ENTERPRISE      ax2230s
	DESCRIPTION
             "This trap indicates a fan failure." 
	::= 8 

ax2230sPowerSupplyFailureTrap TRAP-TYPE
	ENTERPRISE      ax2230s
	DESCRIPTION
             "This trap indicates one ( or more ) of power supplies failure." 
	::= 9 

ax2230sLoginSuccessTrap TRAP-TYPE
	ENTERPRISE      ax2230s
	VARIABLES       { axsLoginName, axsLoginTime, axsLoginLocation, axsLoginLine }
	DESCRIPTION
             "This trap indicates success of user log in." 
	::= 10 

ax2230sLoginFailureTrap TRAP-TYPE
	ENTERPRISE      ax2230s
	VARIABLES       { axsLoginName, axsLoginFailureTime, 
                          axsLoginLocation, axsLoginLine }
	DESCRIPTION
             "This trap indicates failure of user log in." 
	::= 11

ax2230sLogoutTrap TRAP-TYPE
	ENTERPRISE      ax2230s
	VARIABLES       { axsLoginName, axsLoginTime, axsLogoutTime, 
                          axsLoginLocation, axsLoginLine, axsLogoutStatus }
	DESCRIPTION
             "This trap indicates success of following log out.
	       'logout' command, 'exit' command, 'quit' command." 
	::= 12

ax2230sMemoryUsageTrap  TRAP-TYPE
	ENTERPRISE      ax2230s
	DESCRIPTION
             "This indicates available memory is fewer than lower limit." 
	::= 13

ax2230sFrameErrorReceiveTrap  TRAP-TYPE
	ENTERPRISE      ax2230s
	VARIABLES       { ifIndex }
	DESCRIPTION
             "This indicates frame receive error." 
	::= 18

ax2230sFrameErrorSendTrap  TRAP-TYPE
	ENTERPRISE      ax2230s
	VARIABLES       { ifIndex }
	DESCRIPTION
             "This indicates frame send error." 
	::= 19

