	::= { ax1240sChassis 5 }

ax1240sFanEntry OBJECT-TYPE
	SYNTAX  Ax1240sFanTableEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
		"This entry containing information for a fan."
	INDEX   { ax1240sChassisIndex,ax1240sFanIndex }
	::= { ax1240sFanTable 1 }

	Ax1240sFanTableEntry ::=
		SEQUENCE {
		  ax1240sFanIndex
			INTEGER,
		  ax1240sFanStatus
			INTEGER
		}

