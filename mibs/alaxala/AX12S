-- ax2130sDevice group

ax2130sChassisMaxNumber OBJECT-TYPE
	SYNTAX  INTEGER
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
		"Max number of cluster chassis that can connect to this router."
	::= { ax2130sChassis 1 }


ax2130sChassisTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF Ax2130sChassisTableEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
		"A table containing chassis information."
	::= { ax2130sChassis 2 }

ax2130sChassisEntry OBJECT-TYPE
	SYNTAX  Ax2130sChassisTableEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
		"An entry containing specific chassis."
	INDEX   { ax2130sChassisIndex }
	::= { ax2130sChassisTable 1 }

	Ax2130sChassisTableEntry ::=
		SEQUENCE {
		  ax2130sChassisIndex
			INTEGER,
		  ax2130sChassisType
			INTEGER,
		  ax2130sChassisStatus
			INTEGER,
		  ax2130sStsLedStatus
			INTEGER,
                  ax2130sCpuName
                        DisplayString,
                  ax2130sCpuClock
                        INTEGER,
                  ax2130sMemoryTotalSize
                        INTEGER,
                  ax2130sMemoryUsedSize
                        INTEGER,
                  ax2130sMemoryFreeSize
                        INTEGER,
                  ax2130sRomVersion
                        DisplayString,
                  ax2130sCpuLoad1m
                        INTEGER,
                  ax2130sFlashTotalSize
                        INTEGER,
                  ax2130sFlashUsedSize
                        INTEGER,
                  ax2130sFlashFreeSize
                        INTEGER,
                  ax2130sSdCardStatus
                        INTEGER,
                  ax2130sSdCardTotalSize
                        INTEGER,
                  ax2130sSdCardUsedSize
                        INTEGER,
                  ax2130sSdCardFreeSize
                        INTEGER,
                  ax2130sPhysLineNumber
                        INTEGER,
                  ax2130sTemperatureStatusNumber
                        INTEGER,
		          ax2130sPowerUnitNumber
			            INTEGER,
                  ax2130sRedundantPsNumber
                        INTEGER,
 	              ax2130sFanNumber
			            INTEGER,
                  ax2130sTotalAccumRunTime
                        INTEGER,
                  ax2130sCriticalAccumRunTime
                        INTEGER
		}

ax2130sChassisIndex OBJECT-TYPE
	SYNTAX  INTEGER
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
		"A number to specify ax2130sChassisEntry."
	::= { ax2130sChassisEntry 1 }

ax2130sChassisType OBJECT-TYPE
	SYNTAX  INTEGER {
		other(1),
		model-AX2130S-24T(2100),
		model-AX2130S-24P(2101),
		model-AX2130S-16T(2102),
		model-AX2130S-16TZ(2103),
		model-AX2130S-16PZ(2104),
		model-AX2130S-16P(2105),
		model-AX2130S-24TH(2106),
		model-AX2130S-24PH(2107)
	      }
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
		"Chassis type of this system."
	::= { ax2130sChassisEntry 2 }

