	SYNTAX  Counter32
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
		"The number of FDB entries learned on the port."
	::= { axsFdbCounterEntry 3 }

axsFdbCounterType OBJECT-TYPE
	SYNTAX  INTEGER	{
				unlimited(0),
				limited-and-Forward(1),
				limited-and-Discard(2)
			}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
		"The type of FDB entries learning."
	::= { axsFdbCounterEntry 4 }

