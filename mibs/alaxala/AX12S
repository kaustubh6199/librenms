	SYNTAX  INTEGER {
                            active(1)
                        }
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
		"This variable displays the status of the entry.
		This value is only valid(1)."
	::= { axsOspfStubAreaEntry 5 }

