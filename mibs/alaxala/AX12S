	SYNTAX  TimeTicks
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
		"This object indicates the time of the most recent
		 Fdb clearing success.(sysUpTime)"
	::= { ax2230sFdbClearMIB 3 }

