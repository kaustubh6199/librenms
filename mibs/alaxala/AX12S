			INTEGER,
                  ax2230sCpuName
                        DisplayString,
                  ax2230sCpuClock
                        INTEGER,
                  ax2230sMemoryTotalSize
                        INTEGER,
                  ax2230sMemoryUsedSize
                        INTEGER,
                  ax2230sMemoryFreeSize
                        INTEGER,
