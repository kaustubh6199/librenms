	::= { ax2230sPhysLine 1 }

ax2230sPhysLineEntry OBJECT-TYPE
	SYNTAX  Ax2230sPhysLineTableEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
		"An entry containing physical port."
	INDEX   { ax2230sChassisIndex, ax2230sPhysLineIndex }
	::= { ax2230sPhysLineTable 1 }

	Ax2230sPhysLineTableEntry ::=
		SEQUENCE {
		  ax2230sPhysLineIndex
			INTEGER,
		  ax2230sPhysLineConnectorType
			INTEGER,
		  ax2230sPhysLineOperStatus
			INTEGER,
		  ax2230sPhysLineIfIndexNumber
			INTEGER,
		  ax2230sPhysLineTransceiverStatus
		        INTEGER
		}

ax2230sPhysLineIndex OBJECT-TYPE
	SYNTAX  INTEGER
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
		"Physical port number. The value is from 1 to ax2230sNifLineNumber."
	::= { ax2230sPhysLineEntry 1 }

ax2230sPhysLineConnectorType OBJECT-TYPE
	SYNTAX  INTEGER {
		other(1),
		type1000BASE-LX(301),
		type1000BASE-SX(302),
		type1000BASE-LH(303),
		type1000BASE-BX10-D(304),
		type1000BASE-BX10-U(305),
		type1000BASE-BX40-D(306),
		type1000BASE-BX40-U(307),
		type1000BASE-SX2(308)
	      }
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
		"Connector type is connected physical port. The value is 1(other)
		when connector is disconnected or connector type is unknown.When
		displaying in pannel, requests board name of NIF,then will display it.
		The value is -1	when PSU or NIF board is disconnected."
	::= { ax2230sPhysLineEntry 2 }

ax2230sPhysLineOperStatus OBJECT-TYPE
	SYNTAX  INTEGER {
		other(1),
		active(2),
		initialization(3),
		down(4),
		lock(6),
		line-fault(8),
		test(9),
		inactive(20)
	      }
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
		"Status of physical port. The value is -1 when PSU or NIF board is
		disconnected."
	::= { ax2230sPhysLineEntry 3 }

ax2230sPhysLineIfIndexNumber OBJECT-TYPE
	SYNTAX  INTEGER
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
		"Number of ifIndex which is included in physical port.The value is -1
		when PSU board or NIF board is disconnected."
	::= { ax2230sPhysLineEntry 4 }

ax2230sPhysLineTransceiverStatus OBJECT-TYPE
	SYNTAX  INTEGER{
                                unchangeable-transceiver(1),
				sfp-mounted(20),
				sfp-unmounted(21),
				unsupported-sfp-mounted(22),
				sfp-status-unknown(23)
			}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
		"Type and status of replaceable transceiver."
	::= { ax2230sPhysLineEntry 5 }


