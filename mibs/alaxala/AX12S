ax2230sTemperatureStatusTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF Ax2230sTemperatureStatusEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
		"The table of ambient temperature status maintained by the
		environmental monitor."
	::= { ax2230sChassis 3 }

ax2230sTemperatureStatusEntry OBJECT-TYPE
	SYNTAX  Ax2230sTemperatureStatusEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
		"An entry in the ambient temperature status table, representing
		the status of the associated testpoint maintained by the
		environmental monitor."
	INDEX  { ax2230sChassisIndex, ax2230sTemperatureStatusIndex }
	::= { ax2230sTemperatureStatusTable 1 }

	Ax2230sTemperatureStatusEntry ::=
		SEQUENCE {
		  ax2230sTemperatureStatusIndex
			INTEGER,
		  ax2230sTemperatureStatusDescr
			DisplayString,
		  ax2230sTemperatureStatusValue
			Integer32,
		  ax2230sTemperatureThreshold
			Integer32,
		  ax2230sTemperatureState
			INTEGER
		}

ax2230sTemperatureStatusIndex OBJECT-TYPE
	SYNTAX  INTEGER
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
		"Unique index for the testpoint being instrumented.
		This index is for SNMP purposes only, and has no
		intrinsic meaning."
	::= { ax2230sTemperatureStatusEntry 1 }

ax2230sTemperatureStatusDescr OBJECT-TYPE
	SYNTAX  DisplayString(SIZE(0..32))
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
		"Textual description of the testpoint being instrumented.
		This description is a short textual label, suitable as a
		human-sensible identification for the rest of the
		information in the entry."
	::= { ax2230sTemperatureStatusEntry 2 }

ax2230sTemperatureStatusValue OBJECT-TYPE
	SYNTAX  Integer32
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
		"The current measurement of the testpoint being instrumented."
	::= { ax2230sTemperatureStatusEntry 3 }

ax2230sTemperatureThreshold OBJECT-TYPE
	SYNTAX  Integer32
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
		"The highest value that the associated instance of the
		object ax2230sTemperatureStatusValue may obtain
		before an emergency shutdown of the managed device is
		initiated."
	::= { ax2230sTemperatureStatusEntry 4 }

