ax1250sTemperatureStatusEntry OBJECT-TYPE
	SYNTAX  Ax1250sTemperatureStatusEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
		"An entry in the ambient temperature status table, representing
		the status of the associated testpoint maintained by the
		environmental monitor."
	INDEX  { ax1250sChassisIndex, ax1250sTemperatureStatusIndex }
	::= { ax1250sTemperatureStatusTable 1 }

	Ax1250sTemperatureStatusEntry ::=
		SEQUENCE {
		  ax1250sTemperatureStatusIndex
			INTEGER,
		  ax1250sTemperatureStatusDescr
			DisplayString,
		  ax1250sTemperatureStatusValue
			Integer32,
		  ax1250sTemperatureThreshold
			Integer32,
		  ax1250sTemperatureState
			INTEGER
		}

ax1250sTemperatureStatusIndex OBJECT-TYPE
	SYNTAX  INTEGER
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
		"Unique index for the testpoint being instrumented.
		This index is for SNMP purposes only, and has no
		intrinsic meaning."
	::= { ax1250sTemperatureStatusEntry 1 }

ax1250sTemperatureStatusDescr OBJECT-TYPE
	SYNTAX  DisplayString(SIZE(0..32))
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
		"Textual description of the testpoint being instrumented.
		This description is a short textual label, suitable as a
		human-sensible identification for the rest of the
		information in the entry."
	::= { ax1250sTemperatureStatusEntry 2 }

ax1250sTemperatureStatusValue OBJECT-TYPE
	SYNTAX  Integer32
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
		"The current measurement of the testpoint being instrumented."
	::= { ax1250sTemperatureStatusEntry 3 }

ax1250sTemperatureThreshold OBJECT-TYPE
	SYNTAX  Integer32
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
		"The highest value that the associated instance of the
		object ax1250sTemperatureStatusValue may obtain
		before an emergency shutdown of the managed device is
		initiated."
	::= { ax1250sTemperatureStatusEntry 4 }

ax1250sTemperatureState OBJECT-TYPE
	SYNTAX  INTEGER {
		normal(1),
		caution(2)
		}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
		"The current state of the testpoint being instrumented."
        ::= { ax1250sTemperatureStatusEntry 5 }

