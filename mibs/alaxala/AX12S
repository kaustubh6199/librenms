ax1250sPhysLineTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF Ax1250sPhysLineTableEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
		"A table containing physical port information."
	::= { ax1250sPhysLine 1 }

ax1250sPhysLineEntry OBJECT-TYPE
	SYNTAX  Ax1250sPhysLineTableEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
		"An entry containing physical port."
	INDEX   { ax1250sChassisIndex, ax1250sPhysLineIndex }
	::= { ax1250sPhysLineTable 1 }

	Ax1250sPhysLineTableEntry ::=
		SEQUENCE {
		  ax1250sPhysLineIndex
			INTEGER,
		  ax1250sPhysLineConnectorType
			INTEGER,
		  ax1250sPhysLineOperStatus
			INTEGER,
		  ax1250sPhysLineIfIndexNumber
			INTEGER,
		  ax1250sPhysLineTransceiverStatus
		        INTEGER
		}

ax1250sPhysLineIndex OBJECT-TYPE
	SYNTAX  INTEGER
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
		"Physical port number. The value is from 1 to ax1250sNifLineNumber."
	::= { ax1250sPhysLineEntry 1 }

ax1250sPhysLineConnectorType OBJECT-TYPE
	SYNTAX  INTEGER {
		other(1),
		type100BASE-FX(201),
		type1000BASE-LX(301),
		type1000BASE-SX(302),
		type1000BASE-LH(303),
		type1000BASE-BX10-D(304),
		type1000BASE-BX10-U(305),
		type1000BASE-BX40-D(306),
		type1000BASE-BX40-U(307),
		type1000BASE-SX2(308)
	      }
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
		"Connector type is connected physical port. The value is 1(other)
		when connector is disconnected or connector type is unknown.When
		displaying in pannel, requests board name of NIF,then will display it.
		The value is -1	when PSU or NIF board is disconnected."
	::= { ax1250sPhysLineEntry 2 }

ax1250sPhysLineOperStatus OBJECT-TYPE
	SYNTAX  INTEGER {
		other(1),
		active(2),
		initialization(3),
		down(4),
		lock(6),
		line-fault(8),
		test(9),
		inactive(20)
	      }
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
		"Status of physical port. The value is -1 when PSU or NIF board is
		disconnected."
	::= { ax1250sPhysLineEntry 3 }

ax1250sPhysLineIfIndexNumber OBJECT-TYPE
	SYNTAX  INTEGER
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
		"Number of ifIndex which is included in physical port.The value is -1
		when PSU board or NIF board is disconnected."
	::= { ax1250sPhysLineEntry 4 }

ax1250sPhysLineTransceiverStatus OBJECT-TYPE
	SYNTAX  INTEGER{
                                unchangeable-transceiver(1),
				sfp-mounted(20),
				sfp-unmounted(21),
				unsupported-sfp-mounted(22),
				sfp-status-unknown(23)
			}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
		"Type and status of replaceable transceiver."
	::= { ax1250sPhysLineEntry 5 }


