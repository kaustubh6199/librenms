	INDEX   { ax1240sChassisIndex }
	::= { ax1240sChassisTable 1 }

	Ax1240sChassisTableEntry ::=
		SEQUENCE {
		  ax1240sChassisIndex
			INTEGER,
		  ax1240sChassisType
			INTEGER,
		  ax1240sChassisStatus
			INTEGER,
		  ax1240sStsLedStatus
			INTEGER,
                  ax1240sCpuName
                        DisplayString,
                  ax1240sCpuClock
                        INTEGER,
                  ax1240sMemoryTotalSize
                        INTEGER,
                  ax1240sMemoryUsedSize
                        INTEGER,
                  ax1240sMemoryFreeSize
                        INTEGER,
                  ax1240sRomVersion
                        DisplayString,
                  ax1240sCpuLoad1m
                        INTEGER,
                  ax1240sFlashTotalSize
                        INTEGER,
                  ax1240sFlashUsedSize
                        INTEGER,
                  ax1240sFlashFreeSize
                        INTEGER,
                  ax1240sSdCardStatus
                        INTEGER,
                  ax1240sSdCardTotalSize
                        INTEGER,
                  ax1240sSdCardUsedSize
                        INTEGER,
                  ax1240sSdCardFreeSize
                        INTEGER,
                  ax1240sPhysLineNumber
                        INTEGER,
                  ax1240sTemperatureStatusNumber
                        INTEGER,
		          ax1240sPowerUnitNumber
			            INTEGER,
                  ax1240sRedundantPsNumber
                        INTEGER,
 	              ax1240sFanNumber
			            INTEGER,
                  ax1240sTotalAccumRunTime
                        INTEGER,
                  ax1240sCriticalAccumRunTime
                        INTEGER
		}

