	SYNTAX  SEQUENCE OF Ax1240sTemperatureStatusEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
		"The table of ambient temperature status maintained by the
		environmental monitor."
	::= { ax1240sChassis 3 }

ax1240sTemperatureStatusEntry OBJECT-TYPE
	SYNTAX  Ax1240sTemperatureStatusEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
		"An entry in the ambient temperature status table, representing
		the status of the associated testpoint maintained by the
		environmental monitor."
	INDEX  { ax1240sChassisIndex, ax1240sTemperatureStatusIndex }
	::= { ax1240sTemperatureStatusTable 1 }

	Ax1240sTemperatureStatusEntry ::=
		SEQUENCE {
		  ax1240sTemperatureStatusIndex
			INTEGER,
		  ax1240sTemperatureStatusDescr
			DisplayString,
		  ax1240sTemperatureStatusValue
			Integer32,
		  ax1240sTemperatureThreshold
			Integer32,
		  ax1240sTemperatureState
			INTEGER
		}

ax1240sTemperatureStatusIndex OBJECT-TYPE
	SYNTAX  INTEGER
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
		"Unique index for the testpoint being instrumented.
		This index is for SNMP purposes only, and has no
		intrinsic meaning."
	::= { ax1240sTemperatureStatusEntry 1 }

ax1240sTemperatureStatusDescr OBJECT-TYPE
	SYNTAX  DisplayString(SIZE(0..32))
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
		"Textual description of the testpoint being instrumented.
		This description is a short textual label, suitable as a
		human-sensible identification for the rest of the
		information in the entry."
	::= { ax1240sTemperatureStatusEntry 2 }

ax1240sTemperatureStatusValue OBJECT-TYPE
	SYNTAX  Integer32
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
		"The current measurement of the testpoint being instrumented."
	::= { ax1240sTemperatureStatusEntry 3 }

ax1240sTemperatureThreshold OBJECT-TYPE
	SYNTAX  Integer32
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
		"The highest value that the associated instance of the
		object ax1240sTemperatureStatusValue may obtain
		before an emergency shutdown of the managed device is
		initiated."
	::= { ax1240sTemperatureStatusEntry 4 }

ax1240sTemperatureState OBJECT-TYPE
	SYNTAX  INTEGER {
		normal(1),
		caution(2)
		}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
		"The current state of the testpoint being instrumented."
        ::= { ax1240sTemperatureStatusEntry 5 }

