axrLogoutStatus OBJECT-TYPE
	SYNTAX  INTEGER {
		error(1),
		success(2),
		timeout(3),
		disconnect(4),
		force(5)
	      }
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
