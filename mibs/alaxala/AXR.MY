axrOspfv3VirtNbrIfIndex OBJECT-TYPE
	SYNTAX  Ipv6IfIndex
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
		"The local link ID for the virtual link over
		which the neighbor can be reached."
	::= { axrOspfv3VirtNbrEntry 4 }

axrOspfv3VirtNbrIpv6Addr OBJECT-TYPE
