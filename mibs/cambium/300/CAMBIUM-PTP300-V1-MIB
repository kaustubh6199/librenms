                       gmt-plus-0600(36),
                       gmt-plus-0630(37),
                       gmt-plus-0700(38),
                       gmt-plus-0730(39),
                       gmt-plus-0800(40),
                       gmt-plus-0830(41),
                       gmt-plus-0900(42),
                       gmt-plus-0930(43),
                       gmt-plus-1000(44),
                       gmt-plus-1030(45),
                       gmt-plus-1100(46),
                       gmt-plus-1130(47),
                       gmt-plus-1200(48),
                       gmt-plus-1230(49),
                       gmt-plus-1300(50)
                     }
      ACCESS read-write
      STATUS mandatory
      DESCRIPTION
             "Time zone offsets from Greenwich Mean Time (GMT)"  
      ::= { sntp 8 } 
 
   daylightSaving OBJECT-TYPE
      SYNTAX INTEGER {
                       disabled(0),
                       enabled(1)
                     }
      ACCESS read-write
      STATUS mandatory
      DESCRIPTION
             "Daylight Saving Time"  
      ::= { sntp 9 } 
 
   -- Reset group attribute definitions 
   --
   systemReset OBJECT-TYPE
      SYNTAX INTEGER {
                       running(0),
                       console-reboot(1)
                     }
      ACCESS read-write
      STATUS mandatory
      DESCRIPTION
             "Setting this attribute to '1' will cause a system 
             reboot. NB: a system reboot will apply any pending new 
             settings held in memory"  
      ::= { reset 1 } 
 
