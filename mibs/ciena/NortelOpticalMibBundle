			DisplayString,
		oChRxMinEthPwr
			DisplayString,
		oChRxMaxEthPwr
			DisplayString,
		maxTransUnit
			INTEGER,
		flowControl
			INTEGER,
		equipment
			INTEGER,
		ethMapping
			INTEGER,
		ethLoopbackType
			LoopbackType,
		ethPrimaryState
			PrimaryState,
		ethSecondaryState
			DisplayString,
		ethAdminState
			AdminState,
		ethAID
			DisplayString
	}

