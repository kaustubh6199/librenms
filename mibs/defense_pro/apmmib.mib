    rsAPMMonitorDataItemNumberOfAlternateClientTcpResponses INTEGER,
    rsAPMMonitorDataItemNumberOfIncompleteAlternateClientTcpResponses INTEGER,
    rsAPMMonitorDataItemNumberOfServerFullSslHandshakeResponses INTEGER,
    rsAPMMonitorDataItemNumberOfIncompleteServerFullSslHandshakeResponses INTEGER,
    rsAPMMonitorDataItemNumberOfServerResumedSslHandshakeResponses INTEGER,
    rsAPMMonitorDataItemNumberOfIncompleteServerResumedSslHandshakeResponses INTEGER,
    rsAPMMonitorDataItemNumberOfServerTransactionResponses INTEGER,
    rsAPMMonitorDataItemNumberOfIncompleteServerTransactionResponses INTEGER,
    rsAPMMonitorDataItemNumberOfServerTransactionCompletedBytes INTEGER,
    rsAPMMonitorDataItemNumberOfNewSessions INTEGER,
    rsAPMMonitorDataItemNumberOfNewTransactions INTEGER,
    rsAPMMonitorDataItemNumberOfConcurrentSessions INTEGER,
    rsAPMMonitorDataItemNumberOfTransactionFailures INTEGER,
    rsAPMMonitorDataItemNumberOfReceivedRequestPackets INTEGER,
    rsAPMMonitorDataItemNumberOfReceivedResponsePackets INTEGER,
    rsAPMMonitorDataItemNumberOfTransmittedRequestPackets INTEGER,
    rsAPMMonitorDataItemNumberOfTransmittedResponsePackets INTEGER,
    rsAPMMonitorDataItemNumberOfReceivedRetransmittedRequestPackets INTEGER,
    rsAPMMonitorDataItemNumberOfReceivedRetransmittedResponsePackets INTEGER,
    rsAPMMonitorDataItemNumberOfNewDelayedBindingSessions INTEGER,
    rsAPMMonitorDataItemNumberOfSkippedSessions INTEGER,
    rsAPMMonitorDataItemNumberOfUnsampledSessions INTEGER,
    rsAPMMonitorDataItemNumberOfSkippedTransactions INTEGER,
    rsAPMMonitorDataItemTotalServerTcpResponseTime Counter64,
    rsAPMMonitorDataItemTotalClientTcpResponseTime Counter64,
    rsAPMMonitorDataItemTotalAlternateClientTcpResponseTime Counter64,
    rsAPMMonitorDataItemTotalServerFullSslHandshakeResponseTime Counter64,
    rsAPMMonitorDataItemTotalServerResumedSslHandshakeResponseTime Counter64,
    rsAPMMonitorDataItemTotalServerTransactionResponseTime Counter64,
    rsAPMMonitorDataItemTotalServerTransactionCompletedResponseTime Counter64,
    rsAPMMonitorDataItemNumberOfReceivedRequestBytes Counter64,
    rsAPMMonitorDataItemNumberOfReceivedResponseBytes Counter64,
    rsAPMMonitorDataItemNumberOfTransmittedRequestBytes Counter64,
    rsAPMMonitorDataItemNumberOfTransmittedResponseBytes Counter64,
    rsAPMMonitorDataItemAverageServerTcpResponseTime Float,
    rsAPMMonitorDataItemAverageEffectiveServerTcpResponseTime Float,
    rsAPMMonitorDataItemAverageClientTcpResponseTime Float,
    rsAPMMonitorDataItemAverageEffectiveClientTcpResponseTime Float,
    rsAPMMonitorDataItemAverageAlternateClientTcpResponseTime Float,
    rsAPMMonitorDataItemAverageEffectiveAlternateClientTcpResponseTime Float,
    rsAPMMonitorDataItemAverageAllClientTcpResponseTime Float,
    rsAPMMonitorDataItemAverageEffectiveAllClientTcpResponseTime Float,
    rsAPMMonitorDataItemAverageServerFullSslHandshakeResponseTime Float,
    rsAPMMonitorDataItemAverageEffectiveServerFullSslHandshakeResponseTime Float,
    rsAPMMonitorDataItemAverageServerResumedSslHandshakeResponseTime Float,
    rsAPMMonitorDataItemAverageEffectiveServerResumedSslHandshakeResponseTime Float,
    rsAPMMonitorDataItemAverageServerTransactionResponseTime Float,
    rsAPMMonitorDataItemAverageEffectiveServerTransactionResponseTime Float,
    rsAPMMonitorDataItemAverageServerTransactionCompletedResponseTimePerByte Float,
    rsAPMMonitorDataItemAverageEffectiveServerTransactionCompletedResponseTimePerByte Float,
    rsAPMMonitorDataItemAverageNumberOfConcurrentSessions Float,
    rsAPMMonitorDataItemIntervalId INTEGER,
    rsAPMMonitorDataItemSamplingAlgorithm SamplingAlgorithm,
    rsAPMMonitorDataItemSamplingRate INTEGER,
    rsAPMMonitorDataItemUsePreciseSampling Bool
        
}


