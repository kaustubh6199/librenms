    rsAPMMonitorDataItemPolicyID INTEGER,
    rsAPMMonitorDataItemFarmMonitorID INTEGER,
    rsAPMMonitorDataItemFarmElementMonitorID INTEGER,
    rsAPMMonitorDataItemSetNumber DataItemSetNumber,
    rsAPMMonitorDataItemFarmID DisplayString,
    rsAPMMonitorDataItemNumberOfServerTcpResponses INTEGER,
    rsAPMMonitorDataItemNumberOfIncompleteServerTcpResponses INTEGER,
    rsAPMMonitorDataItemNumberOfClientTcpResponses INTEGER,
