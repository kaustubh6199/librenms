RsAPMAlertSpecEntry ::= SEQUENCE {
    rsAPMAlertSpecID INTEGER,
    rsAPMAlertSpecRowStatus RowStatus,
    rsAPMAlertSpecEnabledStatus INTEGER,
    rsAPMAlertSpecDescription DisplayString,
    rsAPMAlertSpecExpression DisplayString,
    rsAPMAlertSpecEvaluationInterval INTEGER,
    rsAPMAlertSpecReportedMonitorDataItemList DisplayString
}

rsAPMAlertSpecID OBJECT-TYPE
    SYNTAX INTEGER
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
        "This variable indicates the administrative ID of the alert spec."
    ::=  { rsAPMAlertSpecEntry 1 }

rsAPMAlertSpecRowStatus OBJECT-TYPE
    SYNTAX RowStatus
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
       "This variable indicates the administrative status of this entry.
        Used to delete an entry of this table."
    ::=  { rsAPMAlertSpecEntry 2 }
    
