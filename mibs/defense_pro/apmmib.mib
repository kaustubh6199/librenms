IMPORTS
       NetworkAddress,
       IpAddress,
       Counter,
       Gauge,
       enterprises
              FROM RFC1155-SMI
       Integer32,        
       OBJECT-TYPE,
       Counter64
              FROM SNMPv2-SMI
        
       DisplayString,
       ipAddrEntry
              FROM RFC1213-MIB
       TRAP-TYPE
                    FROM RFC-1215
       rsAPM,
       rndErrorDesc,
       rndErrorSeverity
                    FROM RADWARE-MIB
        MacAddress,
        TEXTUAL-CONVENTION
				    FROM SNMPv2-TC;

    Bool ::= INTEGER {
         true (1),
         false (2)
    }
    RowStatus ::= INTEGER {
        active(1),
        notInService(2),
        notReady(3),
        createAndGo(4),
        createAndWait(5),
        destroy(6)
    }
  
    LogSeverity ::= INTEGER {
      none(1),
      emergency(2),
      alert(3),   
      critical(4),
      error(5),  
      warning(6),
      notice(7),
      info(8), 
      debug(9)
    }  


    SamplingAlgorithm ::= INTEGER {
      deterministic(1),
      nonDeterministic(2)
    }

    L4Protocol ::= INTEGER {
          tcp(1),
          udp(2)
    }

    L7Protocol ::= INTEGER {
          generic(1),
          http(2),
          smtp(3),
          pop3(4),
          imap4(5),
          https(6)
    }


    DataItemSetNumber ::= INTEGER {
        currentInterval (1),
        previousInterval (2),
        peakSubIntervalValuePerCurrentInterval (3),
        peakSubIntervalValuePerPreviousInterval (4),
        currentSubinterval (5),
        previousSubinterval (6)
    }


-- Bits designating which data items need to be collected

 CollectItems ::= BITS {
    numberOfServerTcpResponses(0),
    numberOfIncompleteServerTcpResponses(1),
    numberOfClientTcpResponses(2),
    numberOfIncompleteClientTcpResponses(3),
    numberOfAlternateClientTcpResponses(4),
    numberOfIncompleteAlternateClientTcpResponses(5),
    numberOfServerFullSslHandshakeResponses(6),
    numberOfIncompleteServerFullSslHandshakeResponses(7),
    numberOfServerResumedSslHandshakeResponses(8),
    numberOfIncompleteServerResumedSslHandshakeResponses(9),
    numberOfServerTransactionResponses(10),
    numberOfIncompleteServerTransactionResponses(11),
    numberOfServerTransactionCompletedBytes(12),
    numberOfNewSessions(13),
    numberOfNewTransactions(14),
    numberOfConcurrentSessions(15),
    numberOfTransactionFailures(16),
    numberOfReceivedRequestPackets(17),
    numberOfReceivedResponsePackets(18),
    numberOfTransmittedRequestPackets(19),
    numberOfTransmittedResponsePackets(20),
    numberOfReceivedRetransmittedRequestPackets(21),
    numberOfReceivedRetransmittedResponsePackets(22),
    numberOfNewDelayedBindingSessions(23),
    numberOfSkippedSessions(24),
    numberOfUnsampledSessions(25),
    numberOfSkippedTransactions(26),
    totalServerTcpResponseTime(27),
    totalClientTcpResponseTime(28),
    totalAlternateClientTcpResponseTime(29),
    totalServerFullSslHandshakeResponseTime(30),
    totalServerResumedSslHandshakeResponseTime(31),
    totalServerTransactionResponseTime(32),
    totalServerTransactionCompletedResponseTime(33),
    numberOfReceivedRequestBytes(34),
    numberOfReceivedResponseBytes(35),
    numberOfTransmittedRequestBytes(36),
    numberOfTransmittedResponseBytes(37),
    averageServerTcpResponseTime(38),
    averageEffectiveServerTcpResponseTime(39),
    averageClientTcpResponseTime(40),
    averageEffectiveClientTcpResponseTime(41),
    averageAlternateClientTcpResponseTime(42),
    averageEffectiveAlternateClientTcpResponseTime(43),
    averageAllClientTcpResponseTime(44),
    averageEffectiveAllClientTcpResponseTime(45),
    averageServerFullSslHandshakeResponseTime(46),
    averageEffectiveServerFullSslHandshakeResponseTime(47),
    averageServerResumedSslHandshakeResponseTime(48),
    averageEffectiveServerResumedSslHandshakeResponseTime(49),
    averageServerTransactionResponseTime(50),
    averageEffectiveServerTransactionResponseTime(51),
    averageServerTransactionCompletedResponseTimePerByte(52),
    averageEffectiveServerTransactionCompletedResponseTimePerByte(53),
    averageNumberOfConcurrentSessions(54)

}    

-- Floats are scaled appropriately such that they can be treated as integers
   Float ::= INTEGER

--    NetNumber ::=  OCTET STRING (SIZE(4))

-- Some MIB compilers require that the following 7 lines which define the path
-- to RND MIB are commented out:

-- mib            OBJECT IDENTIFIER ::= { mgmt 1  }
-- directory      OBJECT IDENTIFIER ::= { internet 1  }
-- experimental   OBJECT IDENTIFIER ::= { internet 3  }
-- private        OBJECT IDENTIFIER ::= { internet 4  }
-- enterprises    OBJECT IDENTIFIER ::= { private 1  }

rsAPMPolicyTable OBJECT-TYPE
    SYNTAX SEQUENCE OF RsAPMPolicyEntry
    MAX-ACCESS not-accessible
    STATUS  current
    DESCRIPTION
      "Table containing the list of APM policies."
    ::=  { rsAPM 1 }

rsAPMPolicyEntry OBJECT-TYPE
    SYNTAX RsAPMPolicyEntry
    MAX-ACCESS not-accessible
    STATUS  current
    DESCRIPTION
       "The row definition for the APM policies table."
    INDEX { rsAPMPolicyID }
    ::=  { rsAPMPolicyTable 1 }

RsAPMPolicyEntry ::= SEQUENCE {
    rsAPMPolicyID INTEGER,
    rsAPMPolicyRowStatus RowStatus,
    rsAPMPolicyName DisplayString,
    rsAPMPolicyEnabledStatus INTEGER,
    rsAPMPolicyDescription DisplayString,
    rsAPMPolicyDestination DisplayString,
    rsAPMPolicySource DisplayString,
    rsAPMPolicyFilter DisplayString,
    rsAPMPolicyFilterType INTEGER,
    rsAPMPolicyPhysicalPortGroup DisplayString,
    rsAPMPolicyVLANTagGroup DisplayString,
    rsAPMPolicyL4Protocol L4Protocol,
    rsAPMPolicyL7Protocol L7Protocol,
    rsAPMPolicyReportInterval INTEGER,
    rsAPMPolicyReportSubInterval INTEGER,
    rsAPMPolicyNextReportIntervalID INTEGER,
    rsAPMPolicyRemainingTimeTillNextReportIntervalStart INTEGER,
    rsAPMPolicyReportViaSrp Bool,
    rsAPMPolicyInsiteInfo DisplayString
}

rsAPMPolicyID OBJECT-TYPE
    SYNTAX INTEGER
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
       "This variable indicates the administrative ID of a policy. APM policies
       have no order semantics, so this ID is used for identification purposes only."
    ::=  { rsAPMPolicyEntry 1 }

rsAPMPolicyRowStatus OBJECT-TYPE
    SYNTAX RowStatus
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
       "This variable indicates the administrative status of this entry.
        Used to delete an entry of this table."
    ::=  { rsAPMPolicyEntry 2 }

rsAPMPolicyName OBJECT-TYPE
    SYNTAX DisplayString (SIZE(0..32))
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
      "The name for the APM policy. Must be unique."
    ::=  { rsAPMPolicyEntry 3 }


rsAPMPolicyEnabledStatus OBJECT-TYPE
    SYNTAX INTEGER {
        enabled (1),
        disabled (2)
    }
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
       "This variable indicates if the policy is monitoring traffic at the moment
       or not. Policies in the modified policy table are not monitoring traffic
       by definition."
    ::= { rsAPMPolicyEntry 4 }

rsAPMPolicyDescription OBJECT-TYPE
    SYNTAX DisplayString (SIZE(0..64))
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
      "Description."
    ::=  { rsAPMPolicyEntry 5 }

rsAPMPolicyDestination OBJECT-TYPE
    SYNTAX DisplayString (SIZE(0..32))
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
      "The destination address / range of addresses for monitored sessions. The destination
      is defined as the server side, i.e., the side sending response packets. The
      name is defined in the classifier networks table or can be an IP address."
    ::=  { rsAPMPolicyEntry 6 }

rsAPMPolicySource OBJECT-TYPE
    SYNTAX DisplayString (SIZE(0..32))
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
      "The source address / range of addresses for monitored sessions. The source
      is defined as the client side, i.e., the side sending request packets. The
      name is defined in the classifier networks table or can be an IP address."
    ::=  { rsAPMPolicyEntry 7 }

rsAPMPolicyFilter OBJECT-TYPE
    SYNTAX DisplayString (SIZE(0..32))
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
       "The name of the filter, defined in the classifier filter table, for the APM policy."
    ::=  { rsAPMPolicyEntry 8 }

rsAPMPolicyFilterType OBJECT-TYPE
    SYNTAX INTEGER {
        basic (1),
        advanced (2),
        group (3)
    }
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
       "The Type OF Filter."
    DEFVAL  { basic }
    ::=  { rsAPMPolicyEntry 9 }

rsAPMPolicyPhysicalPortGroup OBJECT-TYPE
    SYNTAX DisplayString (SIZE(0..32))
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
       "The name of the physical port group, defined by the classifier, for the policy."
    ::=  { rsAPMPolicyEntry 10 }

rsAPMPolicyVLANTagGroup OBJECT-TYPE
    SYNTAX DisplayString (SIZE(0..32))
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
        "The name of the VLAN tag group, defined by the classifier, for this rule."
    ::= { rsAPMPolicyEntry 11 }

rsAPMPolicyL4Protocol OBJECT-TYPE
    SYNTAX L4Protocol
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
       "The L4 protocol monitored by the policy."
    ::=  { rsAPMPolicyEntry 12 }

rsAPMPolicyL7Protocol OBJECT-TYPE
    SYNTAX L7Protocol
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
       "The L7 protocol monitored by the policy."
    DEFVAL  { generic }
    ::=  { rsAPMPolicyEntry 13 }

rsAPMPolicyReportInterval OBJECT-TYPE
    SYNTAX INTEGER 
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
       "The reporting interval to Insite. Specified in seconds."
    ::=  { rsAPMPolicyEntry 14 }

rsAPMPolicyReportSubInterval OBJECT-TYPE
    SYNTAX INTEGER 
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
       "The reporting sub interval. Specified in seconds."
    ::=  { rsAPMPolicyEntry 15 }

rsAPMPolicyNextReportIntervalID OBJECT-TYPE
    SYNTAX INTEGER 
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
       "The ID of the next report interval."
    DEFVAL  { 0 }
    ::=  { rsAPMPolicyEntry 16 }

rsAPMPolicyRemainingTimeTillNextReportIntervalStart OBJECT-TYPE
    SYNTAX INTEGER 
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
       "Specifies the remaining time, in msec, until the start of the next report interval."
    ::=  { rsAPMPolicyEntry 17 }

rsAPMPolicyReportViaSrp OBJECT-TYPE
    SYNTAX Bool
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
       "Specifies whether the monitored data items for this policy should
       be reported via SRP or not."
    ::=  { rsAPMPolicyEntry 18 }

rsAPMPolicyInsiteInfo OBJECT-TYPE
    SYNTAX DisplayString (SIZE(0..64))
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
       "64 Bytes that insite can use to store insite related information"
    ::=  { rsAPMPolicyEntry 19 }

rsAPMMonitorTable OBJECT-TYPE
    SYNTAX SEQUENCE OF RsAPMMonitorEntry
    MAX-ACCESS not-accessible
    STATUS  current
    DESCRIPTION
      "Table containing the list of APM monitors."
    ::=  { rsAPM 2 }

rsAPMMonitorEntry OBJECT-TYPE
    SYNTAX RsAPMMonitorEntry
    MAX-ACCESS not-accessible
    STATUS  current
    DESCRIPTION
       "The row definition for the APM policies table."
    INDEX { rsAPMMonitorPolicyID, rsAPMMonitorID }
    ::=  { rsAPMMonitorTable 1 }

RsAPMMonitorEntry ::= SEQUENCE {
    rsAPMMonitorPolicyID INTEGER,
    rsAPMMonitorID INTEGER,
    rsAPMMonitorRowStatus RowStatus,
    rsAPMMonitorType INTEGER,
    rsAPMMonitorFarmID DisplayString,
    rsAPMMonitorDirectionOrientation INTEGER,
    rsAPMMonitorItemsToCollect CollectItems
}

rsAPMMonitorPolicyID OBJECT-TYPE
    SYNTAX INTEGER
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
       "This variable indicates the administrative ID of the APM policy this monitor belongs to."
    ::=  { rsAPMMonitorEntry 1 }

rsAPMMonitorID OBJECT-TYPE
    SYNTAX INTEGER
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
       "This variable indicates the administrative ID of a monitor. This ID is used for identification purposes only."
    ::=  { rsAPMMonitorEntry 2 }

rsAPMMonitorRowStatus OBJECT-TYPE
    SYNTAX RowStatus
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
       "This variable indicates the administrative status of this entry.
        Used to delete an entry of this table."
    ::=  { rsAPMMonitorEntry 3 }

rsAPMMonitorType OBJECT-TYPE
    SYNTAX INTEGER {
        device (1),
        farm (2)
    }
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
       "The monitor type."
    ::= { rsAPMMonitorEntry 4 }

rsAPMMonitorFarmID OBJECT-TYPE
    SYNTAX DisplayString (SIZE(0..32))
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
       "The farm ID of the farm that is being monitored. Not relevant for device monitors."
    ::=  { rsAPMMonitorEntry 5 }

rsAPMMonitorDirectionOrientation OBJECT-TYPE
    SYNTAX INTEGER { 
        source (1),
        destination (2)
    }
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
       "Specifies the direction orientation of the monitor. Monitors with 'source'
       direction orientation will have packets associated to them according to
       the farm from which request packets arrived and the farm to which response packets
       were sent. Monitors with 'destination' direction orientation will have packets
       associated to them according to the farm to which request packets were sent and the
       farm from which response packets arrived."
    ::=  { rsAPMMonitorEntry 6 }

rsAPMMonitorItemsToCollect OBJECT-TYPE
    SYNTAX CollectItems
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
       "Specifies which items need to be collected."
    ::=  { rsAPMMonitorEntry 7 }

rsAPMLegFarmTable OBJECT-TYPE
    SYNTAX SEQUENCE OF RsAPMLegFarmEntry 
    MAX-ACCESS not-accessible
    STATUS  current
    DESCRIPTION
      "Table listing all the leg farms that will be encountered by a policy, per policy.
      Any farm encountered by a policy that is not listed in this table will be considered
      to be an edge farm."
    ::=  { rsAPM 3 }

rsAPMLegFarmEntry OBJECT-TYPE
    SYNTAX RsAPMLegFarmEntry
    MAX-ACCESS not-accessible
    STATUS  current
    DESCRIPTION
       "The row definition for the APM leg farm table."
    INDEX { rsAPMLegFarmPolicyID,rsAPMLegFarmIndex }
    ::=  { rsAPMLegFarmTable 1 }

RsAPMLegFarmEntry ::= SEQUENCE {
    rsAPMLegFarmPolicyID INTEGER,
    rsAPMLegFarmIndex    INTEGER,
    rsAPMLegFarmFarmID DisplayString,
    rsAPMLegFarmRowStatus RowStatus
}

rsAPMLegFarmPolicyID  OBJECT-TYPE
    SYNTAX INTEGER
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
       "This variable indicates the administrative ID of the APM policy this leg farm belongs to."
    ::=  { rsAPMLegFarmEntry 1 }

rsAPMLegFarmIndex  OBJECT-TYPE
    SYNTAX INTEGER
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
       "Index of the leg farm."
    ::=  { rsAPMLegFarmEntry 2 }

    
rsAPMLegFarmFarmID  OBJECT-TYPE
    SYNTAX DisplayString (SIZE(0..32))
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
       "This variable indicates the ID of this leg farm."
    ::=  { rsAPMLegFarmEntry  3 }
    
rsAPMLegFarmRowStatus OBJECT-TYPE
   SYNTAX RowStatus
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
       "This variable indicates the administrative status of this entry.
        Used to delete an entry of this table."
    ::=  { rsAPMLegFarmEntry  4 }


rsAPMMonitorDataItemTable OBJECT-TYPE
    SYNTAX SEQUENCE OF RsAPMMonitorDataItemEntry 
    MAX-ACCESS not-accessible
    STATUS  current
    DESCRIPTION
      "Table providing read-only access to all Monitor data items. Access to
      the Monitor data items is given per policy, farm monitor, farm element
      monitor (if applicable), farm element element monitor (if applicable),
      current or previous data item and Monitor data item type. The available
      types are: interval, subinterval, peak subinterval value per interval. Not
      all these types are available for each Monitor data item. See the APM
      documentation for full details."
    ::=  { rsAPM 4 }

rsAPMMonitorDataItemEntry OBJECT-TYPE
    SYNTAX RsAPMMonitorDataItemEntry
    MAX-ACCESS not-accessible
    STATUS  current
    DESCRIPTION
       "The row definition for the APM Monitor data item table."
    INDEX { rsAPMMonitorDataItemPolicyID,
            rsAPMMonitorDataItemFarmMonitorID,
            rsAPMMonitorDataItemFarmElementMonitorID,
            rsAPMMonitorDataItemSetNumber }
    ::=  { rsAPMMonitorDataItemTable 1 }



