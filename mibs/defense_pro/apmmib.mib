       "The farm ID of the farm that is being monitored. Not relevant for device monitors."
    ::=  { rsAPMCurrentMonitorEntry 4 }

rsAPMCurrentMonitorDirectionOrientation OBJECT-TYPE
    SYNTAX INTEGER { 
        source (1),
        destination (2)
    }
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
       "Specifies the direction orientation of the monitor. Monitors with 'source'
       direction orientation will have packets associated to them according to
       the farm from which request packets arrived and the farm to which response packets
       were sent. Monitors with 'destination' direction orientation will have packets
       associated to them according to the farm to which request packets were sent and the
       farm from which response packets arrived."
    ::=  { rsAPMCurrentMonitorEntry 5 }


rsAPMCurrentMonitorItemsToCollect OBJECT-TYPE
    SYNTAX CollectItems
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
       "The items to collect."
    ::=  { rsAPMCurrentMonitorEntry 6 }


rsAPMCurrentLegFarmTable OBJECT-TYPE
    SYNTAX SEQUENCE OF RsAPMCurrentLegFarmEntry 
    MAX-ACCESS not-accessible
    STATUS  current
    DESCRIPTION
      "Table listing all the leg farms that will be encountered by a policy, per policy.
      Any farm encountered by a policy that is not listed in this table will be considered
      to be an edge farm."
    ::=  { rsAPM 10 }

rsAPMCurrentLegFarmEntry OBJECT-TYPE
    SYNTAX RsAPMCurrentLegFarmEntry
    MAX-ACCESS not-accessible
    STATUS  current
    DESCRIPTION
       "The row definition for the APM leg farm table."
    INDEX { rsAPMCurrentLegFarmPolicyID,rsAPMCurrentLegFarmIndex}
    ::=  { rsAPMCurrentLegFarmTable 1 }

RsAPMCurrentLegFarmEntry ::= SEQUENCE {
    rsAPMCurrentLegFarmPolicyID INTEGER,
    rsAPMCurrentLegFarmIndex    INTEGER,
    rsAPMCurrentLegFarmFarmID DisplayString
}

rsAPMCurrentLegFarmPolicyID  OBJECT-TYPE
    SYNTAX INTEGER
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
       "This variable indicates the administrative ID of the APM policy this leg farm belongs to."
    ::=  { rsAPMCurrentLegFarmEntry 1 }

rsAPMCurrentLegFarmIndex  OBJECT-TYPE
    SYNTAX INTEGER
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
       "Index of the leg farm"
    ::=  { rsAPMCurrentLegFarmEntry 2 }

    
rsAPMCurrentLegFarmFarmID  OBJECT-TYPE
    SYNTAX DisplayString (SIZE(0..32))
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
       "This variable indicates the ID of this leg farm."
    ::=  { rsAPMCurrentLegFarmEntry  3 }

rsAPMEnabledStatus OBJECT-TYPE
    SYNTAX INTEGER {
        enabled (1),
        disabled (2)
    }
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
        "Enable/disable the apm - becomes valid after reboot. Get returns the current runtime value"
    ::=  {rsAPM 11}
    
rsAPMUpdateAllPolicies OBJECT-TYPE
    SYNTAX  INTEGER
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
        "Updates all apm policies (temp becomes active). Any value is valid"
    DEFVAL  { 1 }
    ::=  {rsAPM 12}

rsAPMUpdatePolicy OBJECT-TYPE
    SYNTAX  INTEGER
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
        "Updates a single apm policy (temp becomes active for specified policy)"
    DEFVAL  { 1 }
    ::=  {rsAPM 13}


rsAPMResetApm OBJECT-TYPE
    SYNTAX  INTEGER
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
        "Reset Apm to an empty configuration. Any value is valid"
    DEFVAL  { 1 }
    ::=  {rsAPM 14}

rsAPMResetApmFromDatabase OBJECT-TYPE
    SYNTAX  INTEGER
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
        "Resets the apm configuration from the database. Any value is valid"
    DEFVAL  { 1 }
    ::=  {rsAPM 15}


rsAPMGlobalParameters  OBJECT IDENTIFIER ::= { rsAPM 17 }

rsAPMSamplingAlgorithm OBJECT-TYPE
    SYNTAX SamplingAlgorithm
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
       "The sampling algorithm to be used"
    DEFVAL  { deterministic }
    ::=  { rsAPMGlobalParameters 1 }

rsAPMSamplingRate OBJECT-TYPE
    SYNTAX INTEGER (1..100)
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
       "The rate to be used. Specified in percentage. Valid range is 1-100."
    ::=  { rsAPMGlobalParameters 2 }


rsAPMUsePreciseSampling OBJECT-TYPE
    SYNTAX Bool
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
       "Specifies whether te apm should use precise sampling"
    ::=  { rsAPMGlobalParameters 3 }

rsAPMTimerMode  OBJECT-TYPE
    SYNTAX INTEGER {
        automatic (1),
        manual (2)
    }
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
        "Timer Mode for the APM"
    DEFVAL  { 1 }
    ::=  { rsAPMGlobalParameters 4 }

rsAPMInsiteConfigurationId OBJECT-TYPE
    SYNTAX INTEGER
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
       "Id of the insite configuration - set/read by insite"
    ::=  { rsAPMGlobalParameters 5 }

