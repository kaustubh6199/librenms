  ConnectorTypeTC ::= TEXTUAL-CONVENTION
    STATUS     current
    DESCRIPTION
           "An integer which indicates the type of CONNECTOR_TYPE according SFF-8024."
    SYNTAX     INTEGER {
		eUNKNOWNORUNSPECIFIED         (1),
		eSC                           (2),
		eFIBERCHANNELSTYLE1COPPER     (3),
		eFIBERCHANNELSTYLE2COPPER     (4),
