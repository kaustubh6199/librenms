		eLC                           (8),
		eMTRT                         (9),
		eMU                           (10),
		eSG                           (11),
		eOPTICALPIGTAIL               (12),
		eRESERVED                     (13),
		eHSSDCII                      (14),
		eCOPPERPIGTAIL                (15),
		eVENDORSPECIFIC               (16)
	}
	
