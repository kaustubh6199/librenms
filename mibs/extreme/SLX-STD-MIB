    SYNTAX      Unsigned32
    UNITS       "microseconds"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "This attribute contains the minimum one-way frame delay
        in the forward direction calculated by this MEP for this
        measurement interval.
       "
    REFERENCE
       "[MEF SOAM-PM] R45 and R89"
    ::= { mefSoamDmHistoricStatsEntry 8 }

mefSoamDmHistoricStatsFrameDelayForwardMax OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "microseconds"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "This attribute contains the maximum one-way frame delay
        in the forward direction calculated by this MEP for this
        measurement interval.
       "
    REFERENCE
       "[MEF SOAM-PM] R45 and R89"
    ::= { mefSoamDmHistoricStatsEntry 9 }

mefSoamDmHistoricStatsFrameDelayForwardAvg OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "microseconds"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "This attribute contains the average one-way frame delay
        in the forward direction calculated by this MEP for this
        measurement interval.
       "
    REFERENCE
       "[MEF SOAM-PM] R45 and R89"
    ::= { mefSoamDmHistoricStatsEntry 10 }

