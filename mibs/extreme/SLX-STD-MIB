mefSoamDmHistoricStatsCompletedMeasurements OBJECT-TYPE
    SYNTAX      Gauge32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "This attribute contains the count of the number of measurements 
             initiated in this measurement interval that have completed."
    REFERENCE
       "[MEF SOAM-PM] R44 and R88"
    ::= { mefSoamDmHistoricStatsEntry 24 } 


-- *****************************************************************************
-- Ethernet Delay Measurement Bin History Statistic Table
-- *****************************************************************************

mefSoamDmHistoricStatsBinsTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF MefSoamDmHistoricStatsBinsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
       "This table contains the result bins for the historic Measurement
        Intervals in a SOAM Delay Measurement session.

        Rows of this table are created automatically
        by the SNMP Agent when the Delay Measurement session is running and a
        measurement interval is completed.

        At least 32 completed measurement intervals must be supported. 96
        completed measurement intervals should be supported. The oldest completed
        measurement interval data may be deleted (row deletion) if at least 32 
        completed measurement intervals have already been completed and a new
        measurement interval has just completed.

        Rows in this table pertaining to a given measurement interval are 
        deleted when (and only when) the corresponding row in the 
        mefSoamDmHistoricStatsTable is deleted.

        The objects in this table are ignored if mefSoamDmType is 1DmTx.

        This table applies only if the corresponding bit is set in 
        mefSoamDmCounterEnable.

        The rows and objects in this table must be persistent upon reboot 
        or restart of a device.
       "
    ::= { mefSoamPmDmObjects 6 }

mefSoamDmHistoricStatsBinsEntry OBJECT-TYPE
    SYNTAX      MefSoamDmHistoricStatsBinsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "The conceptual row of mefSoamDmHistoricStatsBinsTable"
    INDEX      {
               dot1agCfmMdIndex,
               dot1agCfmMaIndex, 
               dot1agCfmMepIdentifier, 
               mefSoamDmCfgIndex,
               mefSoamDmHistoricStatsIndex,
               mefSoamDmCfgMeasBinType,
               mefSoamDmCfgMeasBinNumber
               }
    ::= { mefSoamDmHistoricStatsBinsTable 1 }

MefSoamDmHistoricStatsBinsEntry ::= SEQUENCE {
    mefSoamDmHistoricStatsBinsCounter                Gauge32
}

mefSoamDmHistoricStatsBinsCounter OBJECT-TYPE
    SYNTAX      Gauge32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "This attribute contains the count of the number of completed
        measurements initiated in this measurement interval whose value
        falls within the range specified for this bin (that is, greater
        than or equal to the measurement boundary for the bin, and
        (unless the bin is the last bin) less than the measurement
        boundary for the following bin.
       "
    REFERENCE
       "[MEF SOAM-PM] R44, R45, R88 and R89"
    ::= { mefSoamDmHistoricStatsBinsEntry 1 }


-- *****************************************************************************
-- Performance Measurement Delay Threshold Configuration Table
-- *****************************************************************************

mefSoamDmThresholdTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF MefSoamDmThresholdEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
       "This table contains the list of Delay Measurement threshold values 
        for DM Performance Monitoring. Instances of this managed object are 
        created automatically by the MEP when the MEP creates an instance 
        of MefSoamDmCfg.

        The objects in this table must be persistent upon reboot or restart
        of a device. 
       "
    ::= { mefSoamPmDmObjects 7 }

mefSoamDmThresholdEntry OBJECT-TYPE
    SYNTAX      MefSoamDmThresholdEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "The conceptual row of mefSoamDmThresholdTable."
    INDEX       { 
                dot1agCfmMdIndex,
                dot1agCfmMaIndex, 
                dot1agCfmMepIdentifier, 
                mefSoamDmCfgIndex,
                mefSoamDmThresholdIndex
                }
    ::= {mefSoamDmThresholdTable 1 }

MefSoamDmThresholdEntry ::= SEQUENCE {
    mefSoamDmThresholdIndex                             Unsigned32,
    mefSoamDmThresholdEnable                            BITS,
    mefSoamDmThresholdMaxFrameDelayRoundTripThreshold   Unsigned32,
    mefSoamDmThresholdAveFrameDelayRoundTripThreshold   Unsigned32,
    mefSoamDmThresholdMaxIFDVRoundTripThreshold         Unsigned32,
    mefSoamDmThresholdAveIFDVRoundTripThreshold         Unsigned32,
    mefSoamDmThresholdMaxFrameDelayForwardThreshold     Unsigned32,
    mefSoamDmThresholdAveFrameDelayForwardThreshold     Unsigned32,
    mefSoamDmThresholdMaxIFDVForwardThreshold           Unsigned32,
    mefSoamDmThresholdAveIFDVForwardThreshold           Unsigned32,
    mefSoamDmThresholdMaxFrameDelayBackwardThreshold    Unsigned32,
    mefSoamDmThresholdAveFrameDelayBackwardThreshold    Unsigned32,
    mefSoamDmThresholdMaxIFDVBackwardThreshold          Unsigned32,
    mefSoamDmThresholdAveIFDVBackwardThreshold          Unsigned32
}

mefSoamDmThresholdIndex OBJECT-TYPE
    SYNTAX      Unsigned32(1..4294967295)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
       "The index of the threshold number for the specific DM
        threshold entry. 

        An index value of '1' must be supported. Other indexes values
        may be supported.
       "
    ::= { mefSoamDmThresholdEntry 1 }

mefSoamDmThresholdEnable OBJECT-TYPE
    SYNTAX      BITS {
                     bMefSoamDmMaxFrameDelayRoundTripThreshold(0),
                     bMefSoamDmAveFrameDelayRoundTripThreshold(1),
                     bMefSoamDmMaxIFDVRoundTripThreshold(2),
                     bMefSoamDmAveIFDVRoundTripThreshold(3),
                     bMefSoamDmMaxFrameDelayForwardThreshold(4),
                     bMefSoamDmAveFrameDelayForwardThreshold(5),
                     bMefSoamDmMaxIFDVForwardThreshold(6),
                     bMefSoamDmAveIFDVForwardThreshold(7),
                     bMefSoamDmMaxFrameDelayBackwardThreshold(8),
                     bMefSoamDmAveFrameDelayBackwardThreshold(9),
                     bMefSoamDmMaxIFDVBackwardThreshold(10),
                     bMefSoamDmAveIFDVBackwardThreshold(11)
                }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
       "A vector of bits that indicates the type of SOAM DM threshold 
        notifications that are enabled. 

        A bit set to '1' enables the specific SOAM DM threshold notification
        and when the specific counter is enabled and the threshold is crossed a 
        notification is generated. 

        A bit set to '0' disables the specific SOAM DM threshold notification.

        If a particular SOAM DM threshold is not supported the BIT value should
        be set to '0'.

        bMefSoamDmMaxFrameDelayRoundTripThreshold(0) 
                Enables/disables maximum frame round trip delay threshold 
                notification. The notification is sent immediately when the
                counter value is greater than or equal to threshold value in a
                measurement interval.
        bMefSoamDmAveFrameDelayRoundTripThreshold(1)
                Enables/disables average frame round trip delay threshold
                notification. The notification is sent when at the end of a
                measurement interval if the counter value is greater than or 
                equal to the threshold value.
        bMefSoamDmMaxIFDVRoundTripThreshold(2)
                Enables/disables maximum frame IFDV round trip threshold 
                notification. The notification is sent immediately when the
                counter value is greater than or equal to threshold value in a
                measurement interval.
        bMefSoamDmAveIFDVRoundTripThreshold(3)
                Enables/disables average frame IFDV round trip threshold
                notification. The notification is sent when at the end of a
                measurement interval if the counter value is greater than or 
                equal to the threshold value.
        bMefSoamDmMaxFrameDelayForwardThreshold(4)
                Enables/disables maximum forward frame delay threshold
                notification. The notification is sent immediately when the
                counter value is greater than or equal to threshold value in a
                measurement interval.
        bMefSoamDmAveFrameDelayForwardThreshold(5)
                Enables/disables average forward frame delay threshold
                notification. The notification is sent when at the end of a
                measurement interval if the counter value is greater than or 
                equal to the threshold value.
        bMefSoamDmMaxIFDVForwardThreshold(6)
                Enables/disables maximum frame IFDV forward threshold 
                notification. The notification is sent immediately when the
                counter value is greater than or equal to threshold value in a
                measurement interval.
        bMefSoamDmAveIFDVForwardThreshold(7)
                Enables/disables average frame IFDV forward threshold
                notification. The notification is sent when at the end of a
                measurement interval if the counter value is greater than or 
                equal to the threshold value.
        bMefSoamDmMaxFrameDelayBackwardThreshold(8)
                Enables/disables maximum backward frame delay threshold
                notification. The notification is sent immediately when the
                counter value is greater than or equal to threshold value in a
                measurement interval.
        bMefSoamDmAveFrameDelayBackwardThreshold(9)
                Enables/disables average backward frame delay threshold
                notification. The notification is sent when at the end of a
                measurement interval if the counter value is greater than or 
                equal to the threshold value.
        bMefSoamDmMaxIFDVBackwardThreshold(10)
                Enables/disables maximum frame IFDV backward threshold 
                notification. The notification is sent immediately when the
                counter value is greater than or equal to threshold value in a
                measurement interval.
        bMefSoamDmAveIFDVBackwardThreshold(11)
                Enables/disables average frame IFDV backward threshold
                notification. The notification is sent when at the end of a
                measurement interval if the counter value is greater than or 
                equal to the threshold value.
      "
    DEFVAL { { } }
    ::= { mefSoamDmThresholdEntry 2 }

mefSoamDmThresholdMaxFrameDelayRoundTripThreshold OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "microseconds"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
       "This object is used to set the maximum two-way round trip delay threshold
        value that will be used to determine if a threshold notification should
        be generated.
       "
    DEFVAL { 4294967295 }
    ::= { mefSoamDmThresholdEntry 3 }

