      mefSoamLmHistoricStatsForwardAvgFlr,
      mefSoamLmHistoricStatsForwardAvailable,
      mefSoamLmHistoricStatsForwardUnavailable,
      mefSoamLmHistoricStatsBackwardTransmittedFrames,
      mefSoamLmHistoricStatsBackwardReceivedFrames,
      mefSoamLmHistoricStatsBackwardMinFlr,
      mefSoamLmHistoricStatsBackwardMaxFlr,
      mefSoamLmHistoricStatsBackwardAvgFlr,
      mefSoamLmHistoricStatsBackwardAvailable,
      mefSoamLmHistoricStatsBackwardUnavailable,
      mefSoamLmHistoricStatsInitiatedMeasurements,
      mefSoamLmHistoricStatsCompletedMeasurements
}
    STATUS      current
    DESCRIPTION
       "Mandatory objects for the Service OAM LM historical statistics group."
    ::= { mefSoamPmMibGroups 5 }

