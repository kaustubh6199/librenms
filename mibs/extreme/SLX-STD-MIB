-- *****************************************************************************
-- Performance Measurement Loss Threshold Configuration Table
-- *****************************************************************************

mefSoamLmThresholdTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF MefSoamLmThresholdEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
       "This table contains the list of Loss Measurement threshold values 
        for LM Performance Monitoring. Instances of this managed object are 
        created automatically by the MEP when the MEP creates an instance 
        of MefSoamLmCfg.

        The objects in this table must be persistent upon reboot or restart
        of a device. 
       "
    ::= { mefSoamPmLmObjects 5 }

mefSoamLmThresholdEntry OBJECT-TYPE
    SYNTAX      MefSoamLmThresholdEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "The conceptual row of mefSoamLmThresholdTable."
    INDEX       { 
                dot1agCfmMdIndex,
                dot1agCfmMaIndex, 
                dot1agCfmMepIdentifier, 
                mefSoamLmCfgIndex,
                mefSoamLmThresholdIndex
                }
    ::= {mefSoamLmThresholdTable 1 }

MefSoamLmThresholdEntry ::= SEQUENCE {
    mefSoamLmThresholdIndex                             Unsigned32,
    mefSoamLmThresholdEnable                            BITS,
    mefSoamLmThresholdMaxFlrForwardThreshold            Unsigned32,
    mefSoamLmThresholdAveFlrForwardThreshold            Unsigned32,
    mefSoamLmThresholdMaxFlrBackwardThreshold           Unsigned32,
    mefSoamLmThresholdAveFlrBackwardThreshold           Unsigned32,
    mefSoamLmThresholdUnavailForwardThreshold           Unsigned32,
    mefSoamLmThresholdUnavailBackwardThreshold          Unsigned32
}

mefSoamLmThresholdIndex OBJECT-TYPE
    SYNTAX      Unsigned32(1..4294967295)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
       "The index of the threshold number for the specific LM
        threshold entry. 

        An index value of '1' must be supported. Other indexes values
        may be supported.
       "
    ::= { mefSoamLmThresholdEntry 1 }

mefSoamLmThresholdEnable OBJECT-TYPE
    SYNTAX      BITS {
                     bMefSoamLmMaxFlrForwardThreshold(0),
                     bMefSoamLmAveFlrForwardThreshold(1),
                     bMefSoamLmMaxFlrBackwardThreshold(2),
                     bMefSoamLmAveFlrBackwardThreshold(3),
                     bMefSoamLmUnavailForwardThreshold(4),
                     bMefSoamLmUnavailBackwardThreshold(5)
                }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
       "A vector of bits that indicates the type of SOAM LM thresholds
        notifications that are enabled. 

        A bit set to '1' enables the specific SOAM LM threshold notification 
        and when the specific counter is enabled and the threshold is crossed a
        notification is generated. 

        A bit set to '0' disables the specific SOAM LM threshold notification.

        If a particular SOAM LM threshold is not supported the BIT value should be
        set to '0'.

        bMefSoamLmMaxFlrForwardThreshold(0)
                Enables/disables maximum frame loss forward ratio threshold 
                notification. The notification is sent immediately when the
                counter value is greater than or equal to threshold value in a
                measurement interval.
        bMefSoamLmAveFlrForwardThreshold(1)
                Enables/disables average frame loss forward ratio threshold
                notification. The notification is sent when at the end of a
                measurement interval if the counter value is greater than or 
                equal to the threshold value.
        bMefSoamLmMaxFlrBackwardThreshold(2)
                Enables/disables maximum frame loss backward ratio threshold 
                notification. The notification is sent immediately when the
                counter value is greater than or equal to threshold value in a
                measurement interval.
        bMefSoamLmAveFlrBackwardThreshold(3)
                Enables/disables average frame loss backward ratio threshold
                notification. The notification is sent when at the end of a
                measurement interval if the counter value is greater than or 
                equal to the threshold value.
        bMefSoamLmUnavailForwardThreshold(4)
                Enables/disables unavailable forward threshold 
                notification. The notification is sent immediately when the
                counter value is greater than or equal to threshold value in a
                measurement interval.
        bMefSoamLmUnavailBackwardThreshold(5)
                Enables/disables unavailable backward threshold 
                notification. The notification is sent immediately when the
                counter value is greater than or equal to threshold value in a
                measurement interval.
      "
    DEFVAL { { } }
    ::= { mefSoamLmThresholdEntry 2 }

mefSoamLmThresholdMaxFlrForwardThreshold OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "milli-percent"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
       "This object is used to set the maximum forward frame loss ratio
        threshold value that will be used to determine if a threshold 
        notification should be generated.
       "
    DEFVAL { 4294967295 }
    ::= { mefSoamLmThresholdEntry 3 }

mefSoamLmThresholdAveFlrForwardThreshold OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "milli-percent"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
       "This object is used to set the average forward frame loss ratio
        threshold value that will be used to determine if a threshold 
        notification should be generated.
       "
    DEFVAL { 4294967295 }
    ::= { mefSoamLmThresholdEntry 4 }

mefSoamLmThresholdMaxFlrBackwardThreshold OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "milli-percent"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
       "This object is used to set the maximum backward frame loss ratio
        threshold value that will be used to determine if a threshold 
        notification should be generated.
       "
    DEFVAL { 4294967295 }
    ::= { mefSoamLmThresholdEntry 5 }

mefSoamLmThresholdAveFlrBackwardThreshold OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "milli-percent"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
       "This object is used to set the average backward frame loss ratio
        threshold value that will be used to determine if a threshold 
        notification should be generated.
       "
    DEFVAL { 4294967295 }
    ::= { mefSoamLmThresholdEntry 6 }

mefSoamLmThresholdUnavailForwardThreshold OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
       "This object is used to set the forward unavailability 
        threshold value that will be used to determine if a threshold 
        notification should be generated.
       "
    DEFVAL { 4294967295 }
    ::= { mefSoamLmThresholdEntry 7 }

mefSoamLmThresholdUnavailBackwardThreshold OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
       "This object is used to set the backward unavailability 
        threshold value that will be used to determine if a threshold 
        notification should be generated.
       "
    DEFVAL { 4294967295 }
    ::= { mefSoamLmThresholdEntry 8 }


-- *****************************************************************************
-- Notification Configuration Objects
-- *****************************************************************************

mefSoamNotificationDateAndTime OBJECT-TYPE
    SYNTAX      DateAndTime
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
       "This object contains the time and date at the time that the time that 
        the notification event is detected, not the time of the notification 
        generation.  

        This object is used only for notifications. The mechanism to set and keep
        current the date and time is not specified.
       "
    ::= { mefSoamPmNotificationCfg 1 }

mefSoamThresholdNotificationId OBJECT-TYPE
    SYNTAX      OBJECT IDENTIFIER
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
       "The Object Identifier of the object that caused the generation of the
        notification from the mefSoamLmThresholdEntry or mefSoamDmThresholdEntry.

        This object is only used for the notification.
       "
    ::= { mefSoamPmNotificationCfg 2 }

mefSoamThresholdNotificationCfg OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
       "The configured threshold value of the object that caused the generation
        of the notification.

        This object is only used for the notification.
       "
    ::= { mefSoamPmNotificationCfg 3 }

mefSoamThresholdNotificationCount OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
       "The threshold counter of the object that caused the generation of the
        notification for the mefSoamLmCurrentStatsTable or 
        mefSoamDmCurrentStatsTable.

        This object is only used for the notification.
       "
    ::= { mefSoamPmNotificationCfg 4 }

mefSoamThresholdSuspect OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
       "The suspect flag for the current measurement interval in which the
        notification was generated from the mefSoamLmCurrentStatsTable or 
        mefSoamDmCurrentStatsTable.

        This object is only used for the notification.
       "
    ::= { mefSoamPmNotificationCfg 5 }

-- *****************************************************************************
-- NOTIFICATIONS (TRAPS)
-- *****************************************************************************

mefSoamAvailabilityChange NOTIFICATION-TYPE
    OBJECTS     {
                mefSoamLmAvailStatsForwardAvailability,
                mefSoamLmAvailStatsBackwardAvailability,
                mefSoamLmAvailStatsLastTransitionTime,
                mefSoamLmAvailStatsLastTransitionDir,
                mefSoamLmCurrentStatsForwardAvailable,
                mefSoamLmCurrentStatsForwardUnavailable,
                mefSoamLmCurrentStatsBackwardAvailable,
                mefSoamLmCurrentStatsBackwardUnavailable
                }
    STATUS      current
    DESCRIPTION
       "An mefSoamAvailabilityChange notification is sent when the state of the
        availability of the indicated service changes. 

        The management entity that receives the notification can identify
        the system from the network source address of the notification,
        and can identify the PM session reporting the change
        by the indices in the OID mefSoamLmStatsLastTransitionTime, including
        dot1agCfmMdIndex, dot1agCfmMaIndex, dot1agCfmMepIdentifier, and 
        mefSoamLmCfgIndex.

        An agent should not generate more than one mefSoamAvailabilityChange
        'notification-event' in a given time interval per PM session as 
        specified by the mefSoamAlarmInterval.  A 'notification-event' is the 
        transmission of a single notification to a list of notification 
        destinations.

        If additional availability state changes occur within the 
        mefSoamAlarmInterval period, then notification generation for these 
        changes shall be suppressed by the agent until the current alarm 
        interval expires. At the end of an alarm interval period, one 
        notification-event shall be generated if any availability state changes
        occurred since the start of the alarm interval period. In such a case, 
        another alarm interval period is started right away.
       "
    ::= { mefSoamPmNotifications 1 }

mefSoamLmSessionStartStop NOTIFICATION-TYPE
    OBJECTS     {
                mefSoamLmCfgSessionStatus,
                mefSoamNotificationDateAndTime
                }
    STATUS      current
    DESCRIPTION
       "An mefSoamLmSessionStartStop notification is sent when the state of the
        LM session changes. 

        The management entity that receives the notification can identify
        the system from the network source address of the notification,
        and can identify the individual PM session reporting the start/stop
        by the indices in the OID mefSoamLmStatsLastTransitionTime, including
        dot1agCfmMdIndex, dot1agCfmMaIndex, dot1agCfmMepIdentifier, and 
        mefSoamLmCfgIndex.

        An agent should not generate more than one mefSoamLmSessionStartStop
        'notification-event' in a given time interval per LM session as specified
        by the mefSoamAlarmInterval.  A 'notification-event' is the transmission
        of a single notification to a list of notification destinations.

        If additional operational state changes occur within the 
        mefSoamAlarmInterval period, then notification generation for these 
        changes shall be suppressed by the agent until the current alarm 
        interval expires. At the end of an alarm interval period, one 
        notification-event shall be generated if any operational state changes
        occurred since the start of the alarm interval period. In such a case, 
        another alarm interval period is started right away.
       "
    ::= { mefSoamPmNotifications 2 }

mefSoamDmSessionStartStop NOTIFICATION-TYPE
    OBJECTS     {
                mefSoamDmCfgSessionStatus,
                mefSoamNotificationDateAndTime
                }
    STATUS      current
    DESCRIPTION
       "An mefSoamDmSessionStartStop notification is sent when the state of the
        DM session changes. 

        The management entity that receives the notification can identify
        the system from the network source address of the notification,
        and can identify the individual PM session reporting the start/stop
        by the indices in the OID mefSoamDmStatsLastTransitionTime, including
        dot1agCfmMdIndex, dot1agCfmMaIndex, dot1agCfmMepIdentifier, and 
        mefSoamDmCfgIndex.

        An agent should not generate more than one mefSoamDmSessionStartStop
        'notification-event' in a given time interval per DM session as specified
        by  mefSoamAlarmInterval.  A 'notification-event' is the transmission
        of a single notification to a list of notification destinations.

        If additional operational state changes occur within the 
        mefSoamAlarmInterval period, then notification generation for these 
        changes shall be suppressed by the agent until the current alarm 
        interval expires. At the end of an alarm interval period, one 
        notification-event shall be generated if any operational state changes
        occurred since the start of the alarm interval period. In such a case, 
        another alarm interval period is started right away.
       "
    ::= { mefSoamPmNotifications 3 }

mefSoamLmThresholdCrossing NOTIFICATION-TYPE
    OBJECTS     {
                mefSoamThresholdNotificationId,
                mefSoamThresholdNotificationCfg,
                mefSoamThresholdNotificationCount,
                mefSoamThresholdSuspect,
                mefSoamNotificationDateAndTime
                }
    STATUS      current
    DESCRIPTION
       "An mefSoamLmThresholdCrossing notification is sent when the value of
        the crossing object from mefSoamLmThresholdTable as indicated by the
        mefSoamThresholdNotificationId is exceeded during the current 
        measurement interval.

        The management entity that receives the notification can identify
        the system from the network source address of the notification,
        and can identify the LM session reporting the threshold
        crossing by the indices in the mefSoamThresholdNotificationId object,
        including dot1agCfmMdIndex, dot1agCfmMaIndex, dot1agCfmMepIdentifier, 
        and mefSoamLmCfgIndex. 

        An agent must not generate more than one mefSoamLmThresholdCrossing
        'notification-event' of a given type per LM session in a given 
        measurement interval. A 'notification-event' is the transmission of a
        single notification to a list of notification destinations.
       "
    ::= { mefSoamPmNotifications 4 }

mefSoamDmThresholdCrossing NOTIFICATION-TYPE
    OBJECTS     {
                mefSoamThresholdNotificationId,
                mefSoamThresholdNotificationCfg,
                mefSoamThresholdNotificationCount,
                mefSoamThresholdSuspect,
                mefSoamNotificationDateAndTime
                }
    STATUS      current
    DESCRIPTION
       "An mefSoamDmThresholdCrossing notification is sent when the value of
        the crossing object from mefSoamDmThresholdTable as indicated by the
        mefSoamThresholdNotificationId is exceeded during the current 
        measurement interval.

        The management entity that receives the notification can identify
        the system from the network source address of the notification,
        and can identify the DM session reporting the threshold
        crossing by the indices in the mefSoamThresholdNotificationId object,
        including dot1agCfmMdIndex, dot1agCfmMaIndex, dot1agCfmMepIdentifier, 
        and mefSoamDmCfgIndex. 

        An agent must not generate more than one mefSoamDmThresholdCrossing
        'notification-event' of a given type per DM session in a given 
        measurement interval. A 'notification-event' is the transmission of a
        single notification to a list of notification destinations.
       "
    ::= { mefSoamPmNotifications 5 }

