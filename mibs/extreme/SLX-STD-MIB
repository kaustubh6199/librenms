mefSoamPmMibCompliance MODULE-COMPLIANCE
    STATUS       current
    DESCRIPTION "The compliance statement for the Ethernet Service OAM PM MIB."
    MODULE
        MANDATORY-GROUPS { 
            mefSoamPmGlobalGroup,
            mefSoamLmCfgGroup,
            mefSoamLmAvailStatsGroup,
            mefSoamLmCurrentStatsGroup,
            mefSoamLmHistoricStatsGroup,
            mefSoamDmCfgGroup,
            mefSoamDmCfgMeasBinGroup,
            mefSoamDmCurrentStatsGroup,
            mefSoamDmCurrentStatsBinsGroup,
            mefSoamDmHistoricStatsGroup,
            mefSoamDmHistoricStatsBinsGroup,
            mefSoamLmThresholdGroup,
            mefSoamDmThresholdGroup,
            mefSoamPmNotificationsGroup,
            mefSoamPmNotificationCfgGroup
        }
    ::= { mefSoamPmMibCompliances 1 }

END
