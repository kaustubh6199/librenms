      ::= { wriMplsVplsCompliances 2 }

-- Units of conformance.

      wriMplsVplsGroups
        OBJECT IDENTIFIER ::= { wriMplsVplsConformance 2 }


      wriMplsVplsGroup OBJECT-GROUP
          OBJECTS {
              wriMplsVplsConfigName,
              wriMplsVplsConfigDescr,
              wriMplsVplsConfigAdminStatus,
              wriMplsVplsConfigMacLearning,
              wriMplsVplsConfigDiscardUnknownDest,
              wriMplsVplsConfigMacAging,
              wriMplsVplsConfigVpnId,
              wriMplsVplsConfigFwdFullHighWatermark,
              wriMplsVplsConfigFwdFullLowWatermark,
              wriMplsVplsConfigRowStatus,
              wriMplsVplsConfigIndexNext,
              wriMplsVplsConfigMtu,
              wriMplsVplsConfigServiceType,
              wriMplsVplsConfigStorageType,

              wriMplsVplsStatusOperStatus,
