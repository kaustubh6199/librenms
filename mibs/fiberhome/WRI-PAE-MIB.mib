-- ---------------------------------------------------------- --
-- The 802.1x Port Current User Information Table
-- ---------------------------------------------------------- --
	wriPaePortUserTable OBJECT-TYPE
		SYNTAX      SEQUENCE OF WriPaePortUserEntry
		MAX-ACCESS  not-accessible
		STATUS      current
		DESCRIPTION
			"."
		::= { wridot1xPaeSupplicant 3 }
	 
	wriPaePortUserEntry OBJECT-TYPE
		SYNTAX      WriPaePortUserEntry
		MAX-ACCESS  not-accessible
		STATUS      current
		DESCRIPTION
			"."
		INDEX { wridot1xPaePortNumber,wriPaePortUserMac}
		::= { wriPaePortUserTable 1 }
	
	WriPaePortUserEntry ::= SEQUENCE 
	{
		wriPaePortUserMac			MacAddress,
		wriPaePortUserVlan			Unsigned32,
		wriPaePortUserName			OCTET STRING,
		wriPaePortUserState			INTEGER
	}
			
	wriPaePortUserMac OBJECT-TYPE
		SYNTAX      MacAddress
		MAX-ACCESS  read-only
		STATUS      current
		DESCRIPTION
			"User MAC address."
		::= { wriPaePortUserEntry 2 }
		
	wriPaePortUserVlan OBJECT-TYPE
		SYNTAX      Unsigned32
		MAX-ACCESS  read-only
		STATUS      current
		DESCRIPTION
			"User VLAN information."
		::= { wriPaePortUserEntry 3 }
		
	wriPaePortUserName OBJECT-TYPE
		SYNTAX      OCTET STRING
		MAX-ACCESS  read-only
		STATUS      current
		DESCRIPTION
			"User name."
		::= { wriPaePortUserEntry 4 }
		
	wriPaePortUserState OBJECT-TYPE
		SYNTAX      INTEGER{authenticated(1),authenticating(2)}
		MAX-ACCESS	read-write
		STATUS      current
		DESCRIPTION
			"Port User state."
		::= { wriPaePortUserEntry 5 }
