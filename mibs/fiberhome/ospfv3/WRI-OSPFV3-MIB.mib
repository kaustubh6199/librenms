    wriOspfv3AreaLsdbChecksum OBJECT-TYPE
            SYNTAX          Integer32
            MAX-ACCESS      read-only
            STATUS          current
            DESCRIPTION
                "This field is the checksum of the complete
                contents of the advertisement, excepting the
                age field.  The age field is excepted so that
                an advertisement's age can be incremented
                without updating the checksum.  The checksum
                used is the same that is used for ISO
                connectionless datagrams; it is commonly
                referred to as the Fletcher checksum."
            REFERENCE
                "OSPF Version 2, Section 12.1.7, LS checksum"
            ::= { wriOspfv3AreaLsdbEntry 7 }

    wriOspfv3AreaLsdbAdvertisement OBJECT-TYPE
            SYNTAX          OCTET STRING (SIZE (1..65535))
            MAX-ACCESS      read-only
            STATUS          current
            DESCRIPTION
                "The entire link state advertisement, including
                its header."
            ::= { wriOspfv3AreaLsdbEntry 8 }  
            
   wriOspfv3AreaLsdbLen OBJECT-TYPE
	       SYNTAX       INTEGER
	       MAX-ACCESS   read-only
	       STATUS       current
	       DESCRIPTION
	          "The AreaLsa length."
	       ::= { wriOspfv3AreaLsdbEntry 9 }
   
   wriOspfv3AreaLsdbPrefix  OBJECT-TYPE
	       SYNTAX       InetAddress
	       MAX-ACCESS   read-only
	       STATUS       current
	       DESCRIPTION
	          "The AreaLsa prefix."
	       ::= { wriOspfv3AreaLsdbEntry 10 }  
       
    wriOspfv3AreaLsdbTypeKnown OBJECT-TYPE
            SYNTAX          TruthValue
            MAX-ACCESS      read-only
            STATUS          current
            DESCRIPTION
                "The value true (1) indicates that the LSA type is
                recognized by this router."
            ::= { wriOspfv3AreaLsdbEntry 11 }

    -- OSPFv3 Link-Scope Link State Database, for non-virtual interfaces

    wriOspfv3LinkLsdbTable OBJECT-TYPE
            SYNTAX          SEQUENCE OF WriOspfv3LinkLsdbEntry
            MAX-ACCESS      not-accessible
            STATUS          current
            DESCRIPTION
                "The OSPFV3 Process's Link-scope LSDB for non-virtual
                interfaces.  The LSDB contains the Link-scope link
                state advertisements from the interfaces that the
                device is attached to."
            ::= { wriOspfv3Objects 5 }

    wriOspfv3LinkLsdbEntry OBJECT-TYPE
            SYNTAX          WriOspfv3LinkLsdbEntry
            MAX-ACCESS      not-accessible
            STATUS          current
            DESCRIPTION
                "A single Link-scope link state advertisement." 
            INDEX           { 
            				  wriOspfv3ProcessId,
            				  wriOspfv3LinkLsdbIfIndex,
                              wriOspfv3LinkLsdbIfInstId,
                              wriOspfv3LinkLsdbType,
                              wriOspfv3LinkLsdbRouterId,
                              wriOspfv3LinkLsdbLsid }
            ::= { wriOspfv3LinkLsdbTable 1 }

    WriOspfv3LinkLsdbEntry ::= SEQUENCE {
            wriOspfv3LinkLsdbIfIndex
                    InterfaceIndex,
            wriOspfv3LinkLsdbIfInstId
                    WriOspfv3IfInstIdTC,
            wriOspfv3LinkLsdbType
                    Unsigned32,
            wriOspfv3LinkLsdbRouterId
                    WriOspfv3RouterIdTC,
            wriOspfv3LinkLsdbLsid
                    WriOspfv3LsIdTC,
            wriOspfv3LinkLsdbSequence
                    WriOspfv3LsaSequenceTC,
            wriOspfv3LinkLsdbAge
                    WriOspfv3LsaAgeTC,
            wriOspfv3LinkLsdbChecksum
                    Integer32,
            wriOspfv3LinkLsdbAdvertisement
                    OCTET STRING,
	        wriOspfv3LinkLsdbLen
	                INTEGER,
	        wriOspfv3LinkLsdbPrefix
	          	    InetAddress,
            wriOspfv3LinkLsdbTypeKnown
                    TruthValue
            }

    wriOspfv3LinkLsdbIfIndex OBJECT-TYPE
            SYNTAX         InterfaceIndex
            MAX-ACCESS     not-accessible
            STATUS         current
            DESCRIPTION
                "The identifier of the link from which the LSA
                was received."
            ::= { wriOspfv3LinkLsdbEntry 1 }

    wriOspfv3LinkLsdbIfInstId OBJECT-TYPE
            SYNTAX         WriOspfv3IfInstIdTC
            MAX-ACCESS     not-accessible
            STATUS         current
            DESCRIPTION
                "The identifier of the interface instance from
                which the LSA was received."
            ::= { wriOspfv3LinkLsdbEntry 2 }

    wriOspfv3LinkLsdbType OBJECT-TYPE
            SYNTAX          Unsigned32(0..'FFFFFFFF'h)
            MAX-ACCESS      not-accessible
            STATUS          current
            DESCRIPTION
                "The type of the link state advertisement.
                Each link state type has a separate
                advertisement format.  Link-scope LSAs unrecognized
                by the router are also stored in this database."
            ::= { wriOspfv3LinkLsdbEntry 3 }

    wriOspfv3LinkLsdbRouterId OBJECT-TYPE
            SYNTAX          WriOspfv3RouterIdTC
            MAX-ACCESS      not-accessible
            STATUS          current
            DESCRIPTION
                "The 32-bit number that uniquely identifies the
                originating router in the Autonomous System."
            REFERENCE
                "OSPF Version 2, Appendix C.1, Global parameters"
            ::= { wriOspfv3LinkLsdbEntry 4 }

    wriOspfv3LinkLsdbLsid OBJECT-TYPE
            SYNTAX        WriOspfv3LsIdTC
            MAX-ACCESS    not-accessible
            STATUS        current
            DESCRIPTION
                "The Link State ID is an LS type-specific field
                containing a unique identifier;
                it identifies the piece of the routing domain
                that is being described by the advertisement.
                In contrast to OSPFv2, the LSID has no
                addressing semantics.  However, in OSPFv3
                the Link State ID always contains the flooding
                scope of the LSA."
            ::= { wriOspfv3LinkLsdbEntry 5 }

    -- Note that the OSPF sequence number is a 32-bit signed
    -- integer.  It starts with the value '80000001'h
    -- or -'7FFFFFFF'h, and increments until '7FFFFFFF'h.
    -- Thus, a typical sequence number will be very negative.

    wriOspfv3LinkLsdbSequence OBJECT-TYPE
            SYNTAX          WriOspfv3LsaSequenceTC
            MAX-ACCESS      read-only
            STATUS          current
            DESCRIPTION
                "The sequence number field is a signed 32-bit
                integer.  It is used to detect old and duplicate
                link state advertisements.  The space of
                sequence numbers is linearly ordered.  The
                larger the sequence number, the more recent the
                advertisement."
            REFERENCE
                "OSPF Version 2, Section 12.1.6, LS sequence
                number"
            ::= { wriOspfv3LinkLsdbEntry 6 }

    wriOspfv3LinkLsdbAge OBJECT-TYPE
            SYNTAX          WriOspfv3LsaAgeTC
            UNITS           "seconds"
            MAX-ACCESS      read-only
            STATUS          current
            DESCRIPTION
                "This field is the age of the link state
                advertisement in seconds.  The high-order bit
                of the LS age field is considered the DoNotAge
                bit for support of on-demand circuits."
            REFERENCE
                "OSPF Version 2, Section 12.1.1, LS age;
                 Extending OSPF to Support Demand Circuits,
                 Section 2.2, The LS age field."
            ::= { wriOspfv3LinkLsdbEntry 7 }

    wriOspfv3LinkLsdbChecksum OBJECT-TYPE
            SYNTAX          Integer32
            MAX-ACCESS      read-only
            STATUS          current
            DESCRIPTION
                "This field is the checksum of the complete
                contents of the advertisement, excepting the
                age field.  The age field is excepted so that
                an advertisement's age can be incremented
                without updating the checksum.  The checksum
                used is the same that is used for ISO
                connectionless datagrams; it is commonly
                referred to as the Fletcher checksum."
            REFERENCE
                "OSPF Version 2, Section 12.1.7, LS checksum"
            ::= { wriOspfv3LinkLsdbEntry 8 }

    wriOspfv3LinkLsdbAdvertisement OBJECT-TYPE
            SYNTAX          OCTET STRING (SIZE (1..65535))
            MAX-ACCESS      read-only
            STATUS          current
            DESCRIPTION
                "The entire link state advertisement, including
                its header."
            ::= { wriOspfv3LinkLsdbEntry 9 }

   wriOspfv3LinkLsdbLen OBJECT-TYPE
       SYNTAX       INTEGER
       MAX-ACCESS   read-only
       STATUS       current
       DESCRIPTION
          "The LinkLsa length."
       ::= { wriOspfv3LinkLsdbEntry 10 }
   
   wriOspfv3LinkLsdbPrefix  OBJECT-TYPE
       SYNTAX       InetAddress
       MAX-ACCESS   read-only
       STATUS       current
       DESCRIPTION
          "The LinkLsa prefix."
       ::= { wriOspfv3LinkLsdbEntry 11 }    
       
    wriOspfv3LinkLsdbTypeKnown OBJECT-TYPE
            SYNTAX          TruthValue
            MAX-ACCESS      read-only
            STATUS          current
            DESCRIPTION
                "The value true (1) indicates that the LSA type is
                recognized by this router."
            ::= { wriOspfv3LinkLsdbEntry 12 }

    -- OSPF Host Table

    wriOspfv3HostTable OBJECT-TYPE
            SYNTAX          SEQUENCE OF WriOspfv3HostEntry
            MAX-ACCESS      not-accessible
            STATUS          current
            DESCRIPTION
                "The Host/Metric Table indicates what hosts are
                directly attached to the router and their
                corresponding metrics."
            REFERENCE
                "OSPF Version 2, Appendix C.7, Host route
                parameters"
            ::= { wriOspfv3Objects 6 }

    wriOspfv3HostEntry OBJECT-TYPE
            SYNTAX          WriOspfv3HostEntry
            MAX-ACCESS      not-accessible
            STATUS          current
            DESCRIPTION
                "A metric to be advertised when a given host is
                reachable.

                The information in this table is persistent, and
                when written, the entity SHOULD save the change
                to non-volatile storage."
            INDEX           {   wriOspfv3ProcessId,
            					wriOspfv3HostAddressType,
                              wriOspfv3HostAddress }
            ::= { wriOspfv3HostTable 1 }

    WriOspfv3HostEntry ::= SEQUENCE {
            wriOspfv3HostAddressType
                    InetAddressType,
            wriOspfv3HostAddress
                    InetAddress,
            wriOspfv3HostMetric
                    Metric,
            wriOspfv3HostRowStatus
                    RowStatus,
            wriOspfv3HostAreaID
                    WriOspfv3AreaIdTC
            }

    wriOspfv3HostAddressType OBJECT-TYPE
            SYNTAX          InetAddressType
            MAX-ACCESS      not-accessible
            STATUS          current
            DESCRIPTION
                "The address type of wriOspfv3HostAddress.  Only IPv6
                global address type is expected."
            REFERENCE
                "OSPF Version 2, Appendix C.7, Host route
                parameters"
            ::= { wriOspfv3HostEntry 1 }

    wriOspfv3HostAddress OBJECT-TYPE
            SYNTAX          InetAddress
            MAX-ACCESS      not-accessible
            STATUS          current
            DESCRIPTION
                "The IPv6 address of the host.  Must be an
                IPv6 global address."
            REFERENCE
                "OSPF Version 2, Appendix C.7, Host route
                parameters"
            ::= { wriOspfv3HostEntry 2 }

    wriOspfv3HostMetric OBJECT-TYPE
            SYNTAX          Metric
            MAX-ACCESS      read-create
            STATUS          current
            DESCRIPTION
                "The metric to be advertised."
            REFERENCE
                "OSPF Version 2, Appendix C.7, Host route
                parameters"
            ::= { wriOspfv3HostEntry 3 }

    wriOspfv3HostRowStatus OBJECT-TYPE
            SYNTAX          RowStatus
            MAX-ACCESS      read-create
            STATUS          current
            DESCRIPTION
                "This object permits management of the table by
                facilitating actions such as row creation,
                construction, and destruction.

                The value of this object has no effect on
                whether other objects in this conceptual row can be
                modified."
            ::= { wriOspfv3HostEntry 4 }

    wriOspfv3HostAreaID OBJECT-TYPE
            SYNTAX          WriOspfv3AreaIdTC
            MAX-ACCESS      read-create
            STATUS          current
            DESCRIPTION
                "The Area the host entry is to be found within.
                By default, the area for the subsuming OSPFv3
                interface, or Area 0 if there is no subsuming
                interface."
            REFERENCE
                "OSPF Version 2, Appendix C.2, Area parameters"
            ::= { wriOspfv3HostEntry 5 }

    -- OSPFv3 Interface Table

    wriOspfv3IfTable OBJECT-TYPE
            SYNTAX          SEQUENCE OF WriOspfv3IfEntry
            MAX-ACCESS      not-accessible
            STATUS          current
            DESCRIPTION
                "The OSPFV3 Interface Table describes the
                interfaces from the viewpoint of OSPFv3."
            REFERENCE
                "OSPF for IPv6, Appendix C.3, Router Interface
                Parameters"
            ::= { wriOspfv3Objects 7 }

    wriOspfv3IfEntry OBJECT-TYPE
            SYNTAX          WriOspfv3IfEntry
            MAX-ACCESS      not-accessible
            STATUS          current
            DESCRIPTION
                "The OSPFv3 Interface Entry describes one
                interface from the viewpoint of OSPFV3.

                The information in this table is persistent,
                and when written, the entity SHOULD save the
                change to non-volatile storage."
            INDEX           { 
            				  wriOspfv3ProcessId,	
                              wriOspfv3IfIndex,
                              wriOspfv3IfInstId }
            ::= { wriOspfv3IfTable 1 }

    WriOspfv3IfEntry ::= SEQUENCE {
            wriOspfv3IfIndex
                    InterfaceIndex,
            wriOspfv3IfInstId
                    WriOspfv3IfInstIdTC,
            wriOspfv3IfAreaId
                    WriOspfv3AreaIdTC,
            wriOspfv3IfType
                    INTEGER,
            wriOspfv3IfAdminStatus
                    Status,
            wriOspfv3IfRtrPriority
                    DesignatedRouterPriority,
            wriOspfv3IfTransitDelay
                    WriOspfv3UpToRefreshIntervalTC,
            wriOspfv3IfRetransInterval
                    WriOspfv3UpToRefreshIntervalTC,
            wriOspfv3IfHelloInterval
                    HelloRange,
            wriOspfv3IfRtrDeadInterval
                     WriOspfv3DeadIntervalRangeTC,
            wriOspfv3IfPollInterval
                    Unsigned32,
            wriOspfv3IfState
                    INTEGER,
            wriOspfv3IfDesignatedRouter
                    WriOspfv3RouterIdTC,
            wriOspfv3IfBackupDesignatedRouter
                    WriOspfv3RouterIdTC,
            wriOspfv3IfEvents
                    Counter32,
            wriOspfv3IfRowStatus
                    RowStatus,
            wriOspfv3IfDemand
                    TruthValue,
            wriOspfv3IfMetricValue
                    Metric,
            wriOspfv3IfLinkScopeLsaCount
                    Gauge32,
            wriOspfv3IfLinkLsaCksumSum
                    Unsigned32,
            wriOspfv3IfDemandNbrProbe
                    TruthValue,
            wriOspfv3IfDemandNbrProbeRetransLimit
                    Unsigned32,
            wriOspfv3IfDemandNbrProbeInterval
                    Unsigned32,
            wriOspfv3IfTEDisabled
                    TruthValue,
            wriOspfv3IfLinkLSASuppression
                    TruthValue,
            wriOspfv3IfMtu
                     INTEGER,
            wriOspfv3IfBfdState
            		INTEGER,
            wriOspfv3IfMtuIgnore
            		INTEGER,
            wriOspfv3IfPassive
            		INTEGER				                
            }

    wriOspfv3IfIndex OBJECT-TYPE
            SYNTAX          InterfaceIndex
            MAX-ACCESS      not-accessible
            STATUS          current
            DESCRIPTION
                "The interface index of this OSPFV3 interface.
                It corresponds to the interface index of the
                IPv6 interface on which OSPFv3 is configured."
            ::= { wriOspfv3IfEntry 1 }

    wriOspfv3IfInstId OBJECT-TYPE
            SYNTAX          WriOspfv3IfInstIdTC
            MAX-ACCESS      not-accessible
            STATUS          current
            DESCRIPTION
                "Enables multiple interface instances of OSPFv3
                to be run over a single link.  Each interface
                instance would be assigned a separate ID.  This ID
                has local link significance only."
            ::= { wriOspfv3IfEntry 2 }

    wriOspfv3IfAreaId OBJECT-TYPE
            SYNTAX          WriOspfv3AreaIdTC
            MAX-ACCESS      read-create
            STATUS          current
            DESCRIPTION
                "A 32-bit integer uniquely identifying the area
                to which the interface connects.  Area ID
                0 is used for the OSPFv3 backbone."
            DEFVAL          { 0 }
            ::= { wriOspfv3IfEntry 3 }

    wriOspfv3IfType OBJECT-TYPE
            SYNTAX          INTEGER {
                            broadcast(1),
                            nbma(2),
                            pointToPoint(3),
                            pointToMultipoint(5)
                            }
            MAX-ACCESS      read-create
            STATUS          current
            DESCRIPTION
                "The OSPFv3 interface type."
            ::= { wriOspfv3IfEntry 4 }

    wriOspfv3IfAdminStatus OBJECT-TYPE
            SYNTAX          Status
            MAX-ACCESS      read-create
            STATUS          current
            DESCRIPTION
                "The OSPFv3 interface's administrative status.
                The value formed on the interface; the interface
                will be advertised as an internal route to some
                area.  The value 'disabled' denotes that the
                interface is external to OSPFv3.
                Note that a value of 'disabled' for the object
                ospfv3AdminStatus will override a value of
                'enabled' for the interface."
            DEFVAL          { enabled }
            ::= { wriOspfv3IfEntry 5 }

    wriOspfv3IfRtrPriority OBJECT-TYPE
            SYNTAX          DesignatedRouterPriority
            MAX-ACCESS      read-create
            STATUS          current
            DESCRIPTION
                "The priority of this interface.  Used in
                multi-access networks, this field is used in
                the designated-router election algorithm.  The
                value 0 signifies that the router is not
                eligible to become the Designated Router on this
                particular network.  In the event of a tie in
                this value, routers will use their Router ID as
                a tie breaker."
            DEFVAL          { 1 }
            ::= { wriOspfv3IfEntry 6 }

    wriOspfv3IfTransitDelay OBJECT-TYPE
            SYNTAX          WriOspfv3UpToRefreshIntervalTC
            UNITS           "seconds"
            MAX-ACCESS      read-create
            STATUS          current
            DESCRIPTION
                "The estimated number of seconds it takes to transmit
                a Link State Update packet over this interface.  LSAs
                contained in the update packet must have their age
                incremented by this amount before transmission.  This
                value should take into account the transmission and
                propagation delays of the interface."
            REFERENCE
                "OSPF for IPv6, Appendix C.3, Router Interface
                Parameters."
            DEFVAL          { 1 }
            ::= { wriOspfv3IfEntry 7 }

    wriOspfv3IfRetransInterval OBJECT-TYPE
            SYNTAX          WriOspfv3UpToRefreshIntervalTC
            UNITS           "seconds"
            MAX-ACCESS      read-create
            STATUS          current
            DESCRIPTION
                "The number of seconds between link state
                advertisement retransmissions for adjacencies
                belonging to this interface.  This value is
                also used when retransmitting database
                description and Link State Request packets."
            DEFVAL          { 5 }
            ::= { wriOspfv3IfEntry 8 }

    wriOspfv3IfHelloInterval OBJECT-TYPE
            SYNTAX          HelloRange
            UNITS           "seconds"
            MAX-ACCESS      read-create
            STATUS          current
            DESCRIPTION
                "The length of time, in seconds, between the
                Hello packets that the router sends on the
                interface.  This value must be the same for all
                routers attached to a common network."
            DEFVAL          { 10 }
            ::= { wriOspfv3IfEntry 9 }

    wriOspfv3IfRtrDeadInterval OBJECT-TYPE
            SYNTAX          WriOspfv3DeadIntervalRangeTC
            UNITS           "seconds"
            MAX-ACCESS      read-create
            STATUS          current
            DESCRIPTION
                "The number of seconds that a router's Hello
                packets have not been seen before its
                neighbors declare the router down on the interface.
                This should be some multiple of the Hello interval.
                This value must be the same for all routers attached
                to a common network."
            DEFVAL          { 40 }
            ::= { wriOspfv3IfEntry 10 }

    wriOspfv3IfPollInterval OBJECT-TYPE
            SYNTAX          Unsigned32
            UNITS           "seconds"
            MAX-ACCESS      read-create
            STATUS          current
            DESCRIPTION
                "The larger time interval, in seconds, between
                the Hello packets sent to an inactive,
                non-broadcast multi-access neighbor."
            DEFVAL          { 120 }
            ::= { wriOspfv3IfEntry 11 }

    wriOspfv3IfState OBJECT-TYPE
            SYNTAX          INTEGER {
                            down(1),
                            loopback(2),
                            waiting(3),
                            pointToPoint(4),
                            designatedRouter(5),
                            backupDesignatedRouter(6),
                            otherDesignatedRouter(7),
                            standby(8)
                            }
            MAX-ACCESS      read-only
            STATUS          current
            DESCRIPTION
                "The OSPFv3 interface state.  An interface may be
                in standby state if there are multiple interfaces
                on the link and another interface is active.  The
                interface may be in Down state if the underlying
                IPv6 interface is down or if the admin status is
                'disabled' either globally or for the interface."
            ::= { wriOspfv3IfEntry 12 }

    wriOspfv3IfDesignatedRouter OBJECT-TYPE
            SYNTAX          WriOspfv3RouterIdTC
            MAX-ACCESS      read-only
            STATUS          current
            DESCRIPTION
                "The Router ID of the Designated Router."
            ::= { wriOspfv3IfEntry 13 }

    wriOspfv3IfBackupDesignatedRouter OBJECT-TYPE
            SYNTAX          WriOspfv3RouterIdTC
            MAX-ACCESS      read-only
            STATUS          current
            DESCRIPTION
                "The Router ID of the Backup Designated
                Router."
            ::= { wriOspfv3IfEntry 14 }

    wriOspfv3IfEvents OBJECT-TYPE
            SYNTAX          Counter32
            MAX-ACCESS      read-only
            STATUS          current
            DESCRIPTION
                "The number of times this OSPFv3 interface has
                changed its state or an error has occurred.
                Discontinuities in the value of this counter
                can occur at re-initialization of the management
                system and at other times as indicated by the
                value of ospfv3DiscontinuityTime."
            ::= { wriOspfv3IfEntry 15 }

     wriOspfv3IfRowStatus OBJECT-TYPE
            SYNTAX          RowStatus
            MAX-ACCESS      read-create
            STATUS          current
            DESCRIPTION
                "This object permits management of the table by
                facilitating actions such as row creation,
                construction, and destruction.

                The value of this object has no effect on
                whether other objects in this conceptual row can be
                modified."
            ::= { wriOspfv3IfEntry 16 }

    wriOspfv3IfDemand OBJECT-TYPE
            SYNTAX          TruthValue
            MAX-ACCESS      read-create
            STATUS          current
            DESCRIPTION
                "Indicates whether Demand OSPFv3 procedures
                (Hello suppression to FULL neighbors and
                setting the DoNotAge flag on propagated LSAs)
                should be performed on this interface."
            DEFVAL { false }
            ::= { wriOspfv3IfEntry 17 }

    wriOspfv3IfMetricValue OBJECT-TYPE
            SYNTAX          Metric
            MAX-ACCESS      read-create
            STATUS          current
            DESCRIPTION
                "The metric assigned to this interface.
                 The default value of the metric is
                 'Reference Bandwidth / ifSpeed'.  The value
                 of the reference bandwidth can be set
                 in the ospfv3ReferenceBandwidth object."
            ::= { wriOspfv3IfEntry 18 }

     wriOspfv3IfLinkScopeLsaCount OBJECT-TYPE
            SYNTAX          Gauge32
            MAX-ACCESS      read-only
            STATUS          current
            DESCRIPTION
                "The total number of Link-scope link state
                advertisements in this link's link state
                database."
            ::= { wriOspfv3IfEntry 19 }

     wriOspfv3IfLinkLsaCksumSum OBJECT-TYPE
            SYNTAX          Unsigned32
            MAX-ACCESS      read-only
            STATUS          current
            DESCRIPTION
                "The 32-bit unsigned sum of the Link-scope link state
                advertisements' LS checksums contained in this
                link's link state database.  The sum can be used
                to determine if there has been a change in a
                router's link state database or to compare the
                link state database of two routers."
            ::= { wriOspfv3IfEntry 20 }

    wriOspfv3IfDemandNbrProbe OBJECT-TYPE
            SYNTAX          TruthValue
            MAX-ACCESS      read-create
            STATUS          current
            DESCRIPTION
                   "Indicates whether or not neighbor probing is
                   enabled to determine whether or not the neighbor
                   is inactive.  Neighbor probing is disabled by
                   default."
            DEFVAL { false }
            ::= { wriOspfv3IfEntry 21 }

   wriOspfv3IfDemandNbrProbeRetransLimit OBJECT-TYPE
           SYNTAX       Unsigned32
           MAX-ACCESS   read-create
           STATUS       current
           DESCRIPTION
              "The number of consecutive LSA retransmissions before
              the neighbor is deemed inactive and the neighbor
              adjacency is brought down."
           DEFVAL          { 10 }
           ::= { wriOspfv3IfEntry 22}

   wriOspfv3IfDemandNbrProbeInterval OBJECT-TYPE
           SYNTAX       Unsigned32
           UNITS        "seconds"
           MAX-ACCESS   read-create
           STATUS       current
           DESCRIPTION
              "Defines how often the neighbor will be probed."
           DEFVAL          { 120 }
           ::= { wriOspfv3IfEntry 23 }

    wriOspfv3IfTEDisabled OBJECT-TYPE
           SYNTAX          TruthValue
           MAX-ACCESS      read-create
           STATUS          current
           DESCRIPTION
              "Indicates whether or not traffic engineering
              is disabled on the interface when traffic
              engineering is enabled in the area where the
              interface is attached.  The object is set
              to the value true (1) to disable traffic engineering
              on the interface.  Traffic engineering is enabled
              by default on the interface when traffic engineering
              is enabled in the area where the interface is
              attached."
           DEFVAL { false }
           ::= { wriOspfv3IfEntry 24 }

    wriOspfv3IfLinkLSASuppression OBJECT-TYPE
           SYNTAX          TruthValue
           MAX-ACCESS      read-create
           STATUS          current
           DESCRIPTION
              "Specifies whether or not link LSA origination is
              suppressed for broadcast or NBMA interface types.
              The object is set to value true (1) to suppress
              the origination."
           REFERENCE
                "OSPF for IPv6, Appendix C.3, Router Interface
                    Parameters"
           DEFVAL { false }
           ::= { wriOspfv3IfEntry 25 } 
           
    wriOspfv3IfMtu   OBJECT-TYPE
           SYNTAX          INTEGER(572..2000)
           MAX-ACCESS      read-write
           STATUS          current
           DESCRIPTION
              "OSPFv3 interface mtu."      
          ::= { wriOspfv3IfEntry 26 }      
          
      wriOspfv3IfBfdState  OBJECT-TYPE
           SYNTAX          INTEGER{enable(1),disable(2)}
           MAX-ACCESS      read-write
           STATUS          current
           DESCRIPTION
              "OSPFv3 bfd state."      
