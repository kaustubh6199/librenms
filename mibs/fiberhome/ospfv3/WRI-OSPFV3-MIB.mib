    -- Notification Definitions

    -- The notifications need to be throttled so as to not overwhelm the
    -- management agent in case of rapid changes to the OSPFv3 module.

   wriOspfv3VirtIfStateChange NOTIFICATION-TYPE
        OBJECTS { wriOspfv3RouterId,  -- The originator of the notification
                  wriOspfv3VirtIfState  -- The new state
           }
        STATUS       current
        DESCRIPTION
            "An ospfv3VirtIfStateChange notification signifies that
            there has been a change in the state of an OSPFv3 virtual
            interface.

            This notification should be generated when the interface
            state regresses (e.g., goes from Point-to-Point to Down)
            or progresses to a terminal state (i.e., Point-to-Point)."
        ::= { wriOspfv3Notifications 1 }

   wriOspfv3NbrStateChange NOTIFICATION-TYPE
        OBJECTS { wriOspfv3RouterId, -- The originator of the notification
                  wriOspfv3NbrState  -- The new state

           }
        STATUS       current
        DESCRIPTION
            "An ospfv3NbrStateChange notification signifies that
            there has been a change in the state of a
            non-virtual OSPFv3 neighbor.  This notification should be
            generated when the neighbor state regresses
            (e.g., goes from Attempt or Full to 1-Way or
            Down) or progresses to a terminal state (e.g.,
            2-Way or Full).  When a neighbor transitions
            from or to Full on non-broadcast multi-access
            and broadcast networks, the notification should be
            generated by the Designated Router.  A Designated
            Router transitioning to Down will be noted by
            ospfIfStateChange."
        ::= { wriOspfv3Notifications 2 }

   wriOspfv3VirtNbrStateChange NOTIFICATION-TYPE
        OBJECTS { wriOspfv3RouterId, -- The originator of the notification
                  wriOspfv3VirtNbrState  -- The new state
           }
        STATUS       current
        DESCRIPTION
            "An wriOspfv3VirtNbrStateChange notification signifies
            that there has been a change in the state of an OSPFv3
            virtual neighbor.  This notification should be generated
            when the neighbor state regresses (e.g., goes
            from Attempt or Full to 1-Way or Down) or
            progresses to a terminal state (e.g., Full)."
        ::= { wriOspfv3Notifications 3 }

   wriOspfv3IfConfigError NOTIFICATION-TYPE
        OBJECTS { wriOspfv3RouterId, -- The originator of the notification
           wriOspfv3IfState,         -- State of the interface
           wriOspfv3PacketSrc,       -- IPv6 address of source
           wriOspfv3ConfigErrorType, -- Type of error
           wriOspfv3PacketType       -- Type of packet
           }
        STATUS       current
        DESCRIPTION
            "An wriOspfv3IfConfigError notification signifies that a
            packet has been received on a non-virtual
            interface from a router whose configuration
            parameters conflict with this router's
            configuration parameters.  Note that the event
            optionMismatch should cause a notification only if it
            prevents an adjacency from forming."
        ::= { wriOspfv3Notifications 4 }


   wriOspfv3VirtIfConfigError NOTIFICATION-TYPE
        OBJECTS { wriOspfv3RouterId, -- The originator of the notification
           wriOspfv3VirtIfState,     -- State of the interface
           wriOspfv3ConfigErrorType, -- Type of error
           wriOspfv3PacketType
           }
        STATUS       current
        DESCRIPTION
            "An wriOspfv3VirtIfConfigError notification signifies that a
            packet has been received on a virtual interface
            from a router whose configuration parameters
            conflict with this router's configuration
            parameters.  Note that the event optionMismatch
            should cause a notification only if it prevents an
            adjacency from forming."
        ::= { wriOspfv3Notifications 5 }

   wriOspfv3IfRxBadPacket NOTIFICATION-TYPE
        OBJECTS { wriOspfv3RouterId, -- The originator of the notification
           wriOspfv3IfState,         -- State of the interface
           wriOspfv3PacketSrc,       -- The source IPv6 address
           wriOspfv3PacketType       -- Type of packet
           }
        STATUS       current
        DESCRIPTION
            "An wriOspfv3IfRxBadPacket notification signifies that an
            OSPFV3 packet that cannot be parsed has been received on a
            non-virtual interface."
        ::= { wriOspfv3Notifications 6 }

   wriOspfv3VirtIfRxBadPacket NOTIFICATION-TYPE
        OBJECTS { wriOspfv3RouterId, -- The originator of the notification
          wriOspfv3VirtIfState,      -- State of the interface
          wriOspfv3PacketType        -- Type of packet
          }
        STATUS       current
        DESCRIPTION
            "An wriOspfv3VirtIfRxBadPacket notification signifies
            that an OSPFv3 packet that cannot be parsed has been
            received on a virtual interface."
        ::= { wriOspfv3Notifications 7 }

   wriOspfv3LsdbOverflow NOTIFICATION-TYPE
        OBJECTS { wriOspfv3RouterId, -- The originator of the notification
           wriOspfv3ExtAreaLsdbLimit -- Limit on External LSAs
           }
        STATUS       current
        DESCRIPTION
            "An wriOspfv3LsdbOverflow notification signifies that the
            number of LSAs in the router's link state
            database has exceeded wriOspfv3ExtAreaLsdbLimit."
        ::= { wriOspfv3Notifications 8 }

   wriOspfv3LsdbApproachingOverflow NOTIFICATION-TYPE
        OBJECTS { wriOspfv3RouterId, -- The originator of the notification
           wriOspfv3ExtAreaLsdbLimit
           }
        STATUS       current
        DESCRIPTION
            "An wriOspfv3LsdbApproachingOverflow notification signifies
            that the number of LSAs in the router's
            link state database has exceeded ninety percent of
            wriOspfv3ExtAreaLsdbLimit."
        ::= { wriOspfv3Notifications 9 }

   wriOspfv3IfStateChange NOTIFICATION-TYPE
        OBJECTS { wriOspfv3RouterId, -- The originator of the notification
           wriOspfv3IfState   -- The new state
           }
        STATUS       current
        DESCRIPTION
            "An wriOspfv3IfStateChange notification signifies that there
            has been a change in the state of a non-virtual
            OSPFv3 interface.  This notification should be generated
            when the interface state regresses (e.g., goes
            from DR to Down) or progresses to a terminal
            state (i.e., Point-to-Point, DR Other, DR, or
            Backup)."
        ::= { wriOspfv3Notifications 10 }

   wriOspfv3NssaTranslatorStatusChange NOTIFICATION-TYPE
        OBJECTS { wriOspfv3RouterId, -- The originator of the notification
           wriOspfv3AreaNssaTranslatorState  -- new state
           }
        STATUS       current
        DESCRIPTION
            "An wriOspfv3NssaTranslatorStatusChange notification
            indicates that there has been a change in the router's
            ability to translate OSPFv3 NSSA LSAs into OSPFv3 External
            LSAs.  This notification should be generated when the
            Translator Status transitions from or to any defined
            status on a per-area basis."
        ::= { wriOspfv3Notifications 11 }

   wriOspfv3RestartStatusChange NOTIFICATION-TYPE
        OBJECTS { wriOspfv3RouterId, -- The originator of the notification
                  wriOspfv3RestartStatus,  -- new status
                  wriOspfv3RestartInterval,
                  wriOspfv3RestartExitReason
                }
        STATUS       current
        DESCRIPTION
            "An wriOspfv3RestartStatusChange notification signifies that
            there has been a change in the graceful restart
            state for the router.  This notification should be
            generated when the router restart status
            changes."
        ::= { wriOspfv3Notifications 12 }

   wriOspfv3NbrRestartHelperStatusChange NOTIFICATION-TYPE
        OBJECTS { wriOspfv3RouterId, -- The originator of the notification
                  wriOspfv3NbrRestartHelperStatus,  -- new status
                  wriOspfv3NbrRestartHelperAge,
                  wriOspfv3NbrRestartHelperExitReason
                }
        STATUS       current
        DESCRIPTION
            "An wriOspfv3NbrRestartHelperStatusChange notification
            signifies that there has been a change in the
            graceful restart helper state for the neighbor.
            This notification should be generated when the
            neighbor restart helper status transitions for a neighbor."
        ::= { wriOspfv3Notifications 13 }

   wriOspfv3VirtNbrRestartHelperStatusChange NOTIFICATION-TYPE
        OBJECTS { wriOspfv3RouterId, -- The originator of the notification
                  wriOspfv3VirtNbrRestartHelperStatus,  -- new status
                  wriOspfv3VirtNbrRestartHelperAge,
                  wriOspfv3VirtNbrRestartHelperExitReason
                }
        STATUS       current
        DESCRIPTION
            "An wriOspfv3VirtNbrRestartHelperStatusChange
            notification signifies that there has been a
            change in the graceful restart helper state for
            the virtual neighbor.  This notification should be
            generated when the virtual neighbor restart helper status
            transitions for a virtual neighbor."
        ::= { wriOspfv3Notifications 14 }

