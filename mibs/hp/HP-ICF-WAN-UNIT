       HP-ICF-WAN-UNIT DEFINITIONS ::= BEGIN

       IMPORTS
           Integer32, Gauge32, OBJECT-TYPE, MODULE-IDENTITY
               FROM SNMPv2-SMI
           DisplayString, RowStatus, TDomain, TAddress
               FROM SNMPv2-TC
           MODULE-COMPLIANCE, OBJECT-GROUP
               FROM SNMPv2-CONF
	   hpWANRouters
               FROM HP-ICF-OID;

-- ========================================================================
-- Textual Conventions for this MIB

     Utf8String ::= TEXTUAL-CONVENTION
         DISPLAY-HINT "255a"
         STATUS  current
         DESCRIPTION
              "To facilitate internationalization, this TC
               represents information taken from the ISO/IEC IS
               10646-1 character set, encoded as an octet string
               using the UTF-8 character encoding scheme described
               in RFC 2044 [10].  For strings in 7-bit US-ASCII,
               there is no impact since the UTF-8 representation is
               identical to the US-ASCII encoding."
         SYNTAX  OCTET STRING (SIZE (0..255))

-- ========================================================================

       hpWANRouterUnitMib MODULE-IDENTITY
        LAST-UPDATED "200410190000Z"  -- October 19, 2004
	ORGANIZATION "Hewlett Packard Company,
                      Network Infrastructure Solutions"
        CONTACT-INFO
              	     "Hewlett Packard Company
                     8000 Foothills Blvd.
                     Roseville, CA 95747"

            DESCRIPTION
                    	"This MIB contains device information, contact information, and
			overall system health information."

            REVISION    "200410190000Z"  -- October 19, 2004
            DESCRIPTION
                    "Initial version of this MIB module."
           ::= { hpWANRouters 10 }

     hpWANRoutersUnit             OBJECT IDENTIFIER ::= {  hpWANRouters 10 }

	hpWANRoutersBootRevision  OBJECT-TYPE
	   SYNTAX      DisplayString (SIZE(3..8))
	   MAX-ACCESS  read-only
	   STATUS      current
	   DESCRIPTION
	       "Unit boot PROM revision."
	   ::= { hpWANRoutersUnit  1 }

	hpWANRoutersCurrentImage  OBJECT-TYPE
	   SYNTAX      DisplayString (SIZE(64))
	   MAX-ACCESS  read-only
	   STATUS      current
	   DESCRIPTION
	       "Filename for current firmware image."
	   ::= { hpWANRoutersUnit  2 }

	hpWANRoutersRunConfigChecksum  OBJECT-TYPE
	   SYNTAX      DisplayString (SIZE(32))
	   MAX-ACCESS  read-only
	   STATUS      current
	   DESCRIPTION
	       "MD5 Digest of the running configuration"
	   ::= { hpWANRoutersUnit  3 }

	hpWANRoutersStartConfigChecksum  OBJECT-TYPE
	   SYNTAX      DisplayString (SIZE(32))
	   MAX-ACCESS  read-only
	   STATUS      current
	   DESCRIPTION
	       "MD5 Digest of the startup configuration"
	   ::= { hpWANRoutersUnit  4 }

	hpWANRoutersDeviceIndex OBJECT-TYPE
	    SYNTAX INTEGER (0..1000)
	    MAX-ACCESS read-only
	    STATUS current
	    DESCRIPTION
	        "The unique device identification within the
	         instance of this Mib."
	    ::= { hpWANRoutersUnit  5 }

	hpWANRoutersDeviceGlobalUniqueID OBJECT-TYPE
	    SYNTAX Utf8String
	    MAX-ACCESS read-only
	    STATUS current
	    DESCRIPTION
	        "This represents a globally unique ID for the device.
	         This MUST be the ordered combination of the Manufacturer,
	         product name, AND any other text that is necessary to guarantee
	         global uniqueness. This value may not be null.  Could use serial number.
	         e.g. AD123456789"
	    ::= { hpWANRoutersUnit  6 }

	hpWANRoutersDeviceHealth OBJECT-TYPE
	    SYNTAX INTEGER {
	           unknown        (1),
	           unused         (2),
	           ok             (3), -- available for meaningful work
	           warning        (4), -- Failure not impacting data flow
	           critical       (5), -- Indicates data flow failure
	           nonrecoverable (6)
	    }
	    MAX-ACCESS read-only
	    STATUS current
	    DESCRIPTION
	        "Overall health of the device. The goal of this object
	         is to be the single poll point to check the status of the
	         device."
	    ::= { hpWANRoutersUnit  7 }

	hpWANRoutersDeviceSysObjID OBJECT-TYPE
	    SYNTAX Utf8String
	    MAX-ACCESS read-only
	    STATUS current
	    DESCRIPTION
	        "System Object ID for this Device entity. This should be an
	         ASCII integer format. (i.e. 1.3.6.1.4.1.11.2.36.1.1)
	         The value may not be null."
	        --
	        -- NOTE: For single device agents this field contains a value
	        --       similar to the MIB II/System/sysObjID
	        --
	    ::= { hpWANRoutersUnit  8 }

	hpWANRoutersDeviceManagementURL OBJECT-TYPE
	    SYNTAX Utf8String
	    MAX-ACCESS read-write
	    STATUS current
	    DESCRIPTION
	        "This object contains the URL for the device's management
	         software.  If it does not exist the value is empty string.
		   If write is not supported, then return invalid. This value is
	         retained across boots."
	    ::= { hpWANRoutersUnit  9 }

	hpWANRoutersDeviceManagementURLLabel OBJECT-TYPE
	    SYNTAX Utf8String
	    MAX-ACCESS read-write
	    STATUS current
	    DESCRIPTION
	        "The label that a management application should use for the
	         hyperlink to the entity's URL."
	    ::= { hpWANRoutersUnit  10 }

	hpWANRoutersDeviceManufacturer OBJECT-TYPE
	    SYNTAX Utf8String
	    MAX-ACCESS read-only
	    STATUS current
	    DESCRIPTION
	        "The device's manufacturer name. Will return 'HP' "
	    ::= { hpWANRoutersUnit  11 }

	hpWANRoutersDeviceProductName OBJECT-TYPE
	    SYNTAX Utf8String
	    MAX-ACCESS read-only
	    STATUS current
	    DESCRIPTION
	        "The device's product name. Null is NOT a valid value.
	         (i.e. D1234A)"
	    ::= { hpWANRoutersUnit  12 }

	hpWANRoutersDeviceSerialNumber OBJECT-TYPE
	    SYNTAX Utf8String
	    MAX-ACCESS read-only
	    STATUS current
	    DESCRIPTION
	        "The serial number for the device. This can return
	         a NULL string."
	    ::= { hpWANRoutersUnit  13 }

	hpWANRoutersDeviceVersion OBJECT-TYPE
	    SYNTAX Utf8String (SIZE(0..32))
	    MAX-ACCESS read-only
	    STATUS current
	    DESCRIPTION
	        "Version number for this device."
	    ::= { hpWANRoutersUnit  14 }

	hpWANRoutersDeviceHWVersion OBJECT-TYPE
	    SYNTAX Utf8String (SIZE(0..32))
	    MAX-ACCESS read-only
	    STATUS current
	    DESCRIPTION
	        "Version number for this device's hardware."
	    ::= { hpWANRoutersUnit  15 }

	hpWANRoutersDeviceContactPerson OBJECT-TYPE
	    SYNTAX Utf8String (SIZE(0..32))
	    MAX-ACCESS read-write
	    STATUS current
	    DESCRIPTION
	        "Identifies the name of the person responsible for the
	         operation of this device. If write is not
	         supported then return invalid."
	        --
	        -- NOTE: For single device agents this field contains a value
	        --       similar to the MIB II/System/sysContact
	        --
	    ::= { hpWANRoutersUnit  16 }

	hpWANRoutersDeviceContactPhone OBJECT-TYPE
	    SYNTAX Utf8String (SIZE(0..32))
	    MAX-ACCESS read-write
	    STATUS current
	    DESCRIPTION
	        "Phone number of the contact person for this device."
	    ::= { hpWANRoutersUnit  17 }

	hpWANRoutersDeviceContactEmail OBJECT-TYPE
	    SYNTAX Utf8String
	    MAX-ACCESS read-write
	    STATUS current
	    DESCRIPTION
	        "e-mail address of the contact person for this device."
	    ::= { hpWANRoutersUnit  18 }

	hpWANRoutersDeviceContactPagerNumber OBJECT-TYPE
	    SYNTAX Utf8String (SIZE(0..32))
	    MAX-ACCESS read-write
	    STATUS current
	    DESCRIPTION
	        "Pager number of the contact person for this device."
	    ::= { hpWANRoutersUnit  19 }

	hpWANRoutersDeviceLocation OBJECT-TYPE
	    SYNTAX Utf8String
	    MAX-ACCESS read-write
	    STATUS current
	    DESCRIPTION
	        "Identifies the location for the this device. If
	         write is not supported then return invalid."
	        --
	        -- NOTE: For single device agents this field contains a value
	        --       similar to the MIB II/System/sysLocation
	        --
	    ::= { hpWANRoutersUnit  20 }

	hpWANRoutersSaveConfig OBJECT-TYPE
    		SYNTAX      INTEGER {
                   saveconfig (1)
                }
    		MAX-ACCESS  read-write
    		STATUS      current
    		DESCRIPTION
        		"Set value to 1 to copy running config to startup config."
    		::= { hpWANRoutersUnit  21 }


	hpWANRoutersReloadSystem  OBJECT-TYPE
    		SYNTAX      Integer32 (0..2147483647)
    		MAX-ACCESS  read-write
    		STATUS      current
    		DESCRIPTION
        		"Set to 0 to initiate immediate system reload.
        		Any non-zero value is the number of seconds
        		until reload"
    		DEFVAL      { 0 }
    		::= { hpWANRoutersUnit  22 }


	hpWANRoutersCancelReload  OBJECT-TYPE
    		SYNTAX      INTEGER {
		    cancelreload (1)
		}
    		MAX-ACCESS  read-write
    		STATUS      current
    		DESCRIPTION
        	"Writing any value to this element will cancel a scheduled relaod"
		::= { hpWANRoutersUnit  23 }




         -- conformance information

  	hpWANRoutersUnitConformance
           OBJECT IDENTIFIER ::= { hpWANRoutersUnit 99 }

        hpWANRoutersUnitCompliances
           OBJECT IDENTIFIER ::= { hpWANRoutersUnitConformance 1 }
	hpWANRoutersUnitGroups
           OBJECT IDENTIFIER ::= { hpWANRoutersUnitConformance 2 }




	  hpWANRoutersCompliance MODULE-COMPLIANCE
	    STATUS  current
	    DESCRIPTION
	        "The compliance statement for SNMPv2 entities which
	        implement the HP Unit MIB."

	    MODULE
	    MANDATORY-GROUPS {
	        hpWANRoutersUnitGroup
	       	    }


	    ::= { hpWANRoutersUnitCompliances 1 }

       -- compliance statements


       -- units of conformance
	hpWANRoutersUnitGroup OBJECT-GROUP
	    OBJECTS {
	    hpWANRoutersBootRevision,
	    hpWANRoutersCurrentImage,
	    hpWANRoutersRunConfigChecksum,
	    hpWANRoutersStartConfigChecksum,
	    hpWANRoutersDeviceIndex,
	    hpWANRoutersDeviceGlobalUniqueID,
	    hpWANRoutersDeviceHealth,
	    hpWANRoutersDeviceSysObjID,
	    hpWANRoutersDeviceManagementURL,
	    hpWANRoutersDeviceManufacturer,
	    hpWANRoutersDeviceProductName,
	    hpWANRoutersDeviceSerialNumber,
	    hpWANRoutersDeviceVersion,
	    hpWANRoutersDeviceHWVersion,
	    hpWANRoutersDeviceContactPerson,
	    hpWANRoutersDeviceContactPhone,
	    hpWANRoutersDeviceContactEmail,
	    hpWANRoutersDeviceContactPagerNumber,
	    hpWANRoutersDeviceLocation,
	    hpWANRoutersSaveConfig,
	    hpWANRoutersReloadSystem,
	    hpWANRoutersCancelReload

	    }
	STATUS  current
    	DESCRIPTION
        "The Unit SNMP Config Group."
    	::= { hpWANRoutersUnitGroups 1 }



       END




