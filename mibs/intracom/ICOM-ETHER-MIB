icomEthPerf1MinIntervalTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IcomEthPerf1MinIntervalEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
       "This table provides performance statistics for historical
        (completed) 1-minute intervals.

        This table will have a maximum of 60 entries of completed 1-minute
        intervals, to provide historical statistics for one hour.
       "
    ::= { icomEthMIBPerfObjects 3 }

icomEthPerf1MinIntervalEntry OBJECT-TYPE
    SYNTAX      IcomEthPerf1MinIntervalEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
       "An entry (conceptual row) in the icomEthPerf1MinIntervalTable.
       "
    INDEX       { ifIndex,
                  icomEthPerf1MinIntervalNumber
                }
    ::= { icomEthPerf1MinIntervalTable 1 }

IcomEthPerf1MinIntervalEntry ::= SEQUENCE {
    icomEthPerf1MinIntervalNumber               Unsigned32,
    icomEthPerf1MinIntervalEndTime              IcomDateTimeType,
    icomEthPerf1MinIntervalElapsedTime          TimeInterval,
    icomEthPerf1MinIntervalSuspect              TruthValue,
    icomEthPerf1MinIntervalTxPkts               Counter64,
    icomEthPerf1MinIntervalRxPkts               Counter64,
    icomEthPerf1MinIntervalTxOctets             Counter64,
    icomEthPerf1MinIntervalRxOctets             Counter64,
    icomEthPerf1MinIntervalMinTxBitRate         Gauge32,
    icomEthPerf1MinIntervalMaxTxBitRate         Gauge32,
    icomEthPerf1MinIntervalMinRxBitRate         Gauge32,
    icomEthPerf1MinIntervalMaxRxBitRate         Gauge32,
    icomEthPerf1MinIntervalCRCAlignErrors       Counter64,
    icomEthPerf1MinIntervalFragments            Counter64,
    icomEthPerf1MinIntervalJabbers              Counter64,
    icomEthPerf1MinIntervalMinTxBwUtil          IcomBwUtilType,
    icomEthPerf1MinIntervalMaxTxBwUtil          IcomBwUtilType,
    icomEthPerf1MinIntervalAvgTxBwUtil          IcomBwUtilType,
    icomEthPerf1MinIntervalMinRxBwUtil          IcomBwUtilType,
    icomEthPerf1MinIntervalMaxRxBwUtil          IcomBwUtilType,
    icomEthPerf1MinIntervalAvgRxBwUtil          IcomBwUtilType
}

icomEthPerf1MinIntervalNumber OBJECT-TYPE
    SYNTAX      Unsigned32 (1..60)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
       "A number that identifies the interval for which the set of
        statistics is available. The interval identified by 1 is the
        most recently completed 1-minute interval, and the interval
        identified by N is the interval immediately preceding the one
        identified by N-1.
       "
    ::= { icomEthPerf1MinIntervalEntry 1 }

