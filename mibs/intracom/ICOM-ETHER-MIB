   --
   --  The icomEthMIBNotifObjects group
   --
icomEthMIBNotifObjects   OBJECT IDENTIFIER ::= { icomEthMIB 0 }

icomEthLoopTrap NOTIFICATION-TYPE
    OBJECTS {
        icomEthCfgLoopback
    }
    STATUS current
    DESCRIPTION
        "This trap reports a change in the loopback status of an Ethernet
         interface.
        "
    ::= { icomEthMIBNotifObjects 1 }

icomEthSFPExistsTrap NOTIFICATION-TYPE
    OBJECTS {
        icomEthStatusSFPExists
    }
    STATUS current
    DESCRIPTION
        "This trap reports a change in the existence of an SFP.
        "
    ::= { icomEthMIBNotifObjects 2 }

icomEthSFPTxFaultTrap NOTIFICATION-TYPE
    OBJECTS {
        icomEthStatusSFPTxFault
    }
    STATUS current
    DESCRIPTION
        "This trap reports a change in the Tx fault indication status
         of an SFP.
        "
    ::= { icomEthMIBNotifObjects 3 }

icomEthSFPRxLOSTrap NOTIFICATION-TYPE
    OBJECTS {
        icomEthStatusSFPRxLOS
    }
    STATUS current
    DESCRIPTION
        "This trap reports a change in the Rx loss of signal status of
         an SFP.
        "
    ::= { icomEthMIBNotifObjects 4 }

icomEthLlfTrap NOTIFICATION-TYPE
    OBJECTS {
        icomEthLlfStatusState
    }
    STATUS current
    DESCRIPTION
        "This trap reports a change in the LLF operational status of an
         Ethernet interface.
        "
    ::= { icomEthMIBNotifObjects 5 }

   --
   -- Conformance information
   --

icomEthMIBConformance OBJECT IDENTIFIER ::= { icomEthMIB 3 }

icomEthMIBCompliances OBJECT IDENTIFIER ::= { icomEthMIBConformance 1 }

icomEthMIBGroups      OBJECT IDENTIFIER ::= { icomEthMIBConformance 2 }

   --
   -- Compliance statements
   --
icomEthMIBCompliance  MODULE-COMPLIANCE
    STATUS            current
    DESCRIPTION
       "The compliance statement for icomEthMIB.
       "
    MODULE -- This Module
    MANDATORY-GROUPS {
        icomEthMIBGroup,
        icomEthMIBPerfGroup,
        icomEthMIBNotifGroup
    }
    ::= { icomEthMIBCompliances 1 }

icomEthMIBGroup OBJECT-GROUP
    OBJECTS {
        icomEthCfgAutoNegotiation,
        icomEthCfgSpeed,
        icomEthCfgDuplex,
        icomEthCfgLoopback,
        icomEthCfgSFPType,

        icomEthStatusModuleType,
        icomEthStatusSFPExists,
        icomEthStatusSFPTxFault,
        icomEthStatusSFPRxLOS,

        icomEthStatsReset,
        icomEthStatsTxBitRate,
        icomEthStatsRxBitRate,
        icomEthStatsTxPktRate,
        icomEthStatsRxPktRate,
        icomEthStatsHCPkts1519to1522Octets,
        icomEthStatsHCPkts1523toMRUOctets,
        icomEthStatsRxIwPkts,
        icomEthStatsTxMacCtrlPkts,
        icomEthStatsRxMacCtrlPkts,
        icomEthStatsTxUndersizePkts,
        icomEthStatsRxUndersizePkts,
        icomEthStatsTxOversizePkts,
        icomEthStatsRxOversizePkts,
        icomEthStatsTxBwUtil,
        icomEthStatsRxBwUtil,

        icomEthLoopSrcAddress,
        icomEthLoopDestAddress,
        icomEthLoopVlanList,
        icomEthLoopTimeout,
        icomEthLoopTimer,
        icomEthLoopRowStatus,

        icomEthLlfCfgEnable,
        icomEthLlfCfgModemList,
        icomEthLlfCfgModemLogic,
        icomEthLlfCfgHoldoffTime,
        icomEthLlfCfgWtrTime,

        icomEthLlfStatusState,
        icomEthLlfStatusHoldoffTimer,
        icomEthLlfStatusWtrTimer
    }
    STATUS      current
    DESCRIPTION
        "A collection of objects for icomEthMIB.
        "
    ::= { icomEthMIBGroups 1 }

