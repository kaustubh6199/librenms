-- -------------------------------------------------------------
-- icomEthPerf1DayIntervalTable
-- -------------------------------------------------------------

icomEthPerf1DayIntervalTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IcomEthPerf1DayIntervalEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
       "This table provides performance statistics for historical
        (completed) 1-day intervals.

        This table will have a maximum of 30 entries of completed 1-day
        intervals, to provide historical statistics for one month.
       "
    ::= { icomEthMIBPerfObjects 5 }

icomEthPerf1DayIntervalEntry OBJECT-TYPE
    SYNTAX      IcomEthPerf1DayIntervalEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
       "An entry (conceptual row) in the icomEthPerf1DayIntervalTable.
       "
    INDEX       { ifIndex,
                  icomEthPerf1DayIntervalNumber
                }
    ::= { icomEthPerf1DayIntervalTable 1 }

IcomEthPerf1DayIntervalEntry ::= SEQUENCE {
    icomEthPerf1DayIntervalNumber               Unsigned32,
    icomEthPerf1DayIntervalEndTime              IcomDateTimeType,
    icomEthPerf1DayIntervalElapsedTime          TimeInterval,
    icomEthPerf1DayIntervalSuspect              TruthValue,
    icomEthPerf1DayIntervalTxPkts               Counter64,
    icomEthPerf1DayIntervalRxPkts               Counter64,
    icomEthPerf1DayIntervalTxOctets             Counter64,
    icomEthPerf1DayIntervalRxOctets             Counter64,
    icomEthPerf1DayIntervalMinTxBitRate         Gauge32,
    icomEthPerf1DayIntervalMaxTxBitRate         Gauge32,
    icomEthPerf1DayIntervalMinRxBitRate         Gauge32,
    icomEthPerf1DayIntervalMaxRxBitRate         Gauge32,
    icomEthPerf1DayIntervalCRCAlignErrors       Counter64,
    icomEthPerf1DayIntervalFragments            Counter64,
    icomEthPerf1DayIntervalJabbers              Counter64,
    icomEthPerf1DayIntervalMinTxBwUtil          IcomBwUtilType,
    icomEthPerf1DayIntervalMaxTxBwUtil          IcomBwUtilType,
    icomEthPerf1DayIntervalAvgTxBwUtil          IcomBwUtilType,
    icomEthPerf1DayIntervalMinRxBwUtil          IcomBwUtilType,
    icomEthPerf1DayIntervalMaxRxBwUtil          IcomBwUtilType,
    icomEthPerf1DayIntervalAvgRxBwUtil          IcomBwUtilType
}

icomEthPerf1DayIntervalNumber OBJECT-TYPE
    SYNTAX      Unsigned32 (1..30)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
       "A number that identifies the interval for which the set of
        statistics is available. The interval identified by 1 is the
        most recently completed 1-day interval, and the interval
        identified by N is the interval immediately preceding the one
        identified by N-1.
       "
    ::= { icomEthPerf1DayIntervalEntry 1 }

icomEthPerf1DayIntervalEndTime OBJECT-TYPE
    SYNTAX      IcomDateTimeType
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "The time that this measurement interval ended.
       "
    REFERENCE
       "ITU-T G.7710/Y.1701 clause 10.1.6.1
       "
    ::= { icomEthPerf1DayIntervalEntry 2 }

icomEthPerf1DayIntervalElapsedTime OBJECT-TYPE
    SYNTAX      TimeInterval
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "The length of time that this measurement interval ran for,
        in centi-seconds.
       "
    REFERENCE
       "ITU-T G.7710/Y.1701 clause 10.1.6.1
       "
    ::= { icomEthPerf1DayIntervalEntry 3 }

icomEthPerf1DayIntervalSuspect OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "This variable indicates if the data for this interval have been
        marked as suspect.

        Value 'true(1)' indicates that the related performance data have
        been marked as suspect.

        Value 'false(2)' indicates that the related performance data
        have not been marked as suspect.
       "
    REFERENCE
       "ITU-T G.7710/Y.1701 clause 10.1.6.1
       "
    ::= { icomEthPerf1DayIntervalEntry 4 }

icomEthPerf1DayIntervalTxPkts OBJECT-TYPE
    SYNTAX      Counter64
    UNITS       "Packets"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "This object is used to provide the total number of packets
        transmitted by the current Ethernet interface in this completed
        1-day interval.
       "
    ::= { icomEthPerf1DayIntervalEntry 5 }

icomEthPerf1DayIntervalRxPkts OBJECT-TYPE
    SYNTAX      Counter64
    UNITS       "Packets"
    MAX-ACCESS  read-only
