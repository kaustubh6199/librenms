icomEthMIBNotifGroup NOTIFICATION-GROUP
    NOTIFICATIONS {
        icomEthLoopTrap,
        icomEthSFPExistsTrap,
        icomEthSFPTxFaultTrap,
        icomEthSFPRxLOSTrap,
        icomEthLlfTrap
    }
    STATUS      current
    DESCRIPTION
        "A collection of notifications for icomEthMIB.
        "
    ::= { icomEthMIBGroups 3 }

