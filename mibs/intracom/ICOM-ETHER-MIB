    icomEthPerf15MinIntervalTxPkts              Counter64,
    icomEthPerf15MinIntervalRxPkts              Counter64,
    icomEthPerf15MinIntervalTxOctets            Counter64,
    icomEthPerf15MinIntervalRxOctets            Counter64,
    icomEthPerf15MinIntervalMinTxBitRate        Gauge32,
    icomEthPerf15MinIntervalMaxTxBitRate        Gauge32,
    icomEthPerf15MinIntervalMinRxBitRate        Gauge32,
    icomEthPerf15MinIntervalMaxRxBitRate        Gauge32,
    icomEthPerf15MinIntervalCRCAlignErrors      Counter64,
    icomEthPerf15MinIntervalFragments           Counter64,
    icomEthPerf15MinIntervalJabbers             Counter64,
    icomEthPerf15MinIntervalMinTxBwUtil         IcomBwUtilType,
    icomEthPerf15MinIntervalMaxTxBwUtil         IcomBwUtilType,
    icomEthPerf15MinIntervalAvgTxBwUtil         IcomBwUtilType,
    icomEthPerf15MinIntervalMinRxBwUtil         IcomBwUtilType,
    icomEthPerf15MinIntervalMaxRxBwUtil         IcomBwUtilType,
