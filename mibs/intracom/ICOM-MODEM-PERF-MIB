-- icomModemPerfRfu15MinIntervalTable
-- -------------------------------------------------------------

icomModemPerfRfu15MinIntervalTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IcomModemPerfRfu15MinIntervalEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
       "This table provides RFU performance statistics for historical
        (completed) 15-minute intervals.

        This table will have a maximum of 96 entries of completed 15-minute
        intervals, to provide historical statistics for one day.
       "
    ::= { icomModemPerfMIBObjects 21 }

icomModemPerfRfu15MinIntervalEntry OBJECT-TYPE
    SYNTAX      IcomModemPerfRfu15MinIntervalEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
       "An entry (conceptual row) in the icomModemPerfRfu15MinIntervalTable.
       "
    INDEX       { icomModemPerfRfuId,
                  icomModemPerfRfu15MinIntervalNumber
                }
    ::= { icomModemPerfRfu15MinIntervalTable 1 }

IcomModemPerfRfu15MinIntervalEntry ::= SEQUENCE {
    icomModemPerfRfu15MinIntervalNumber         Unsigned32,
    icomModemPerfRfu15MinIntervalEndTime        IcomDateTimeType,
    icomModemPerfRfu15MinIntervalSuspect        TruthValue,
    icomModemPerfRfu15MinIntervalMinTxPower     IcomModemPerfDbmType,
    icomModemPerfRfu15MinIntervalMaxTxPower     IcomModemPerfDbmType,
    icomModemPerfRfu15MinIntervalAvgTxPower     IcomModemPerfDbmType,
    icomModemPerfRfu15MinIntervalMinRssi        IcomModemPerfDbmType,
    icomModemPerfRfu15MinIntervalMaxRssi        IcomModemPerfDbmType,
    icomModemPerfRfu15MinIntervalAvgRssi        IcomModemPerfDbmType
}

icomModemPerfRfu15MinIntervalNumber OBJECT-TYPE
    SYNTAX      Unsigned32 (1..96)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
       "A number that identifies the interval for which the set of
        statistics is available. The interval identified by 1 is the
        most recently completed 15-minute interval, and the interval
        identified by N is the interval immediately preceding the one
        identified by N-1.
       "
    ::= { icomModemPerfRfu15MinIntervalEntry 1 }

icomModemPerfRfu15MinIntervalEndTime OBJECT-TYPE
    SYNTAX      IcomDateTimeType
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "The time that this measurement interval ended.
       "
    REFERENCE
       "ITU-T G.7710/Y.1701 clause 10.1.6.1
       "
    ::= { icomModemPerfRfu15MinIntervalEntry 2 }

icomModemPerfRfu15MinIntervalSuspect OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "This variable indicates if the data for this interval have been
        marked as suspect.

        Value 'true(1)' indicates that the related performance data have
        been marked as suspect.

        Value 'false(2)' indicates that the related performance data
        have not been marked as suspect.
       "
    REFERENCE
       "ITU-T G.7710/Y.1701 clause 10.1.6.1
       "
    ::= { icomModemPerfRfu15MinIntervalEntry 3 }

