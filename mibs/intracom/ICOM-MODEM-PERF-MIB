-- -------------------------------------------------------------
-- icomModemPerfStatus1MinIntervalTable
-- -------------------------------------------------------------

icomModemPerfStatus1MinIntervalTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IcomModemPerfStatus1MinIntervalEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
       "This table provides fine-grain modem performance statistics
        for historical (completed) 1-minute intervals.

        This table will have a maximum of 60 entries of completed 1-minute
        intervals, to provide historical statistics for one hour.
       "
    ::= { icomModemPerfMIBObjects 3 }

icomModemPerfStatus1MinIntervalEntry OBJECT-TYPE
    SYNTAX      IcomModemPerfStatus1MinIntervalEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
       "An entry (conceptual row) in the icomModemPerfStatus1MinIntervalTable.
       "
    INDEX       { icomModemPerfModemId,
                  icomModemPerfStatus1MinIntervalNumber
                }
    ::= { icomModemPerfStatus1MinIntervalTable 1 }

IcomModemPerfStatus1MinIntervalEntry ::= SEQUENCE {
    icomModemPerfStatus1MinIntervalNumber         Unsigned32,
    icomModemPerfStatus1MinIntervalEndTime        IcomDateTimeType,
    icomModemPerfStatus1MinIntervalElapsedTime    TimeInterval,
    icomModemPerfStatus1MinIntervalSuspect        TruthValue,
    icomModemPerfStatus1MinIntervalMinRxSnr       IcomModemPerfDbType,
    icomModemPerfStatus1MinIntervalMaxRxSnr       IcomModemPerfDbType,
    icomModemPerfStatus1MinIntervalAvgRxSnr       IcomModemPerfDbType,
    icomModemPerfStatus1MinIntervalMinTxPhyMod    IcomModemPerfModulationType,
    icomModemPerfStatus1MinIntervalMaxTxPhyMod    IcomModemPerfModulationType,
    icomModemPerfStatus1MinIntervalModTxPhyMod    IcomModemPerfModulationType,
    icomModemPerfStatus1MinIntervalMinRxPhyMod    IcomModemPerfModulationType,
    icomModemPerfStatus1MinIntervalMaxRxPhyMod    IcomModemPerfModulationType,
    icomModemPerfStatus1MinIntervalModRxPhyMod    IcomModemPerfModulationType,
    icomModemPerfStatus1MinIntervalMinLpdcStress  Unsigned32,
    icomModemPerfStatus1MinIntervalMaxLpdcStress  Unsigned32,
    icomModemPerfStatus1MinIntervalAvgLpdcStress  Unsigned32,
    icomModemPerfStatus1MinIntervalMinTxBwUtil    IcomModemPerfBwUtilType,
    icomModemPerfStatus1MinIntervalMaxTxBwUtil    IcomModemPerfBwUtilType,
    icomModemPerfStatus1MinIntervalAvgTxBwUtil    IcomModemPerfBwUtilType,
    icomModemPerfStatus1MinIntervalMinRxBwUtil    IcomModemPerfBwUtilType,
    icomModemPerfStatus1MinIntervalMaxRxBwUtil    IcomModemPerfBwUtilType,
    icomModemPerfStatus1MinIntervalAvgRxBwUtil    IcomModemPerfBwUtilType,
    icomModemPerfStatus1MinIntervalFecBlocks      Counter64,
    icomModemPerfStatus1MinIntervalFecUncorBlocks Counter64,
    icomModemPerfStatus1MinIntervalLossOfLink     Counter64
}

icomModemPerfStatus1MinIntervalNumber OBJECT-TYPE
    SYNTAX      Unsigned32 (1..60)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
       "A number that identifies the interval for which the set of
        statistics is available. The interval identified by 1 is the
        most recently completed 1-minute interval, and the interval
        identified by N is the interval immediately preceding the one
        identified by N-1.
       "
    ::= { icomModemPerfStatus1MinIntervalEntry 1 }

icomModemPerfStatus1MinIntervalEndTime OBJECT-TYPE
    SYNTAX      IcomDateTimeType
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "The time that this measurement interval ended.
       "
    REFERENCE
       "ITU-T G.7710/Y.1701 clause 10.1.6.1
       "
    ::= { icomModemPerfStatus1MinIntervalEntry 2 }

icomModemPerfStatus1MinIntervalElapsedTime OBJECT-TYPE
    SYNTAX      TimeInterval
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "The length of time that this measurement interval ran for,
        in centi-seconds.
       "
    REFERENCE
       "ITU-T G.7710/Y.1701 clause 10.1.6.1
       "
    ::= { icomModemPerfStatus1MinIntervalEntry 3 }

icomModemPerfStatus1MinIntervalSuspect OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "This variable indicates if the data for this interval have been
        marked as suspect.

        Value 'true(1)' indicates that the related performance data have
        been marked as suspect.

        Value 'false(2)' indicates that the related performance data
        have not been marked as suspect.
       "
    REFERENCE
       "ITU-T G.7710/Y.1701 clause 10.1.6.1
       "
    ::= { icomModemPerfStatus1MinIntervalEntry 4 }

