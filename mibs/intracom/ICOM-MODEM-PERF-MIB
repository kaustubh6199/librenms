-- -------------------------------------------------------------
-- icomModemPerfStatus15MinIntervalTable
-- -------------------------------------------------------------

icomModemPerfStatus15MinIntervalTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IcomModemPerfStatus15MinIntervalEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
       "This table provides fine-grain modem performance statistics
        for historical (completed) 15-minute intervals.

        This table will have a maximum of 96 entries of completed 15-minute
        intervals, to provide historical statistics for one day.
       "
    ::= { icomModemPerfMIBObjects 4 }

icomModemPerfStatus15MinIntervalEntry OBJECT-TYPE
    SYNTAX      IcomModemPerfStatus15MinIntervalEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
       "An entry (conceptual row) in the icomModemPerfStatus15MinIntervalTable.
       "
    INDEX       { icomModemPerfModemId,
                  icomModemPerfStatus15MinIntervalNumber
                }
    ::= { icomModemPerfStatus15MinIntervalTable 1 }

IcomModemPerfStatus15MinIntervalEntry ::= SEQUENCE {
    icomModemPerfStatus15MinIntervalNumber         Unsigned32,
    icomModemPerfStatus15MinIntervalEndTime        IcomDateTimeType,
    icomModemPerfStatus15MinIntervalElapsedTime    TimeInterval,
    icomModemPerfStatus15MinIntervalSuspect        TruthValue,
    icomModemPerfStatus15MinIntervalMinRxSnr       IcomModemPerfDbType,
    icomModemPerfStatus15MinIntervalMaxRxSnr       IcomModemPerfDbType,
    icomModemPerfStatus15MinIntervalAvgRxSnr       IcomModemPerfDbType,
    icomModemPerfStatus15MinIntervalMinTxPhyMod    IcomModemPerfModulationType,
    icomModemPerfStatus15MinIntervalMaxTxPhyMod    IcomModemPerfModulationType,
    icomModemPerfStatus15MinIntervalModTxPhyMod    IcomModemPerfModulationType,
    icomModemPerfStatus15MinIntervalMinRxPhyMod    IcomModemPerfModulationType,
    icomModemPerfStatus15MinIntervalMaxRxPhyMod    IcomModemPerfModulationType,
    icomModemPerfStatus15MinIntervalModRxPhyMod    IcomModemPerfModulationType,
    icomModemPerfStatus15MinIntervalMinLpdcStress  Unsigned32,
    icomModemPerfStatus15MinIntervalMaxLpdcStress  Unsigned32,
    icomModemPerfStatus15MinIntervalAvgLpdcStress  Unsigned32,
    icomModemPerfStatus15MinIntervalMinTxBwUtil    IcomModemPerfBwUtilType,
    icomModemPerfStatus15MinIntervalMaxTxBwUtil    IcomModemPerfBwUtilType,
    icomModemPerfStatus15MinIntervalAvgTxBwUtil    IcomModemPerfBwUtilType,
    icomModemPerfStatus15MinIntervalMinRxBwUtil    IcomModemPerfBwUtilType,
    icomModemPerfStatus15MinIntervalMaxRxBwUtil    IcomModemPerfBwUtilType,
    icomModemPerfStatus15MinIntervalAvgRxBwUtil    IcomModemPerfBwUtilType,
    icomModemPerfStatus15MinIntervalFecBlocks      Counter64,
    icomModemPerfStatus15MinIntervalFecUncorBlocks Counter64,
    icomModemPerfStatus15MinIntervalLossOfLink     Counter64
}

icomModemPerfStatus15MinIntervalNumber OBJECT-TYPE
    SYNTAX      Unsigned32 (1..96)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
       "A number that identifies the interval for which the set of
        statistics is available. The interval identified by 1 is the
        most recently completed 15-minute interval, and the interval
        identified by N is the interval immediately preceding the one
        identified by N-1.
       "
    ::= { icomModemPerfStatus15MinIntervalEntry 1 }

icomModemPerfStatus15MinIntervalEndTime OBJECT-TYPE
    SYNTAX      IcomDateTimeType
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "The time that this measurement interval ended.
       "
    REFERENCE
       "ITU-T G.7710/Y.1701 clause 10.1.6.1
       "
    ::= { icomModemPerfStatus15MinIntervalEntry 2 }

icomModemPerfStatus15MinIntervalElapsedTime OBJECT-TYPE
    SYNTAX      TimeInterval
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "The length of time that this measurement interval ran for,
        in centi-seconds.
       "
    REFERENCE
       "ITU-T G.7710/Y.1701 clause 10.1.6.1
       "
    ::= { icomModemPerfStatus15MinIntervalEntry 3 }

icomModemPerfStatus15MinIntervalSuspect OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "This variable indicates if the data for this interval have been
        marked as suspect.

        Value 'true(1)' indicates that the related performance data have
        been marked as suspect.

        Value 'false(2)' indicates that the related performance data
        have not been marked as suspect.
       "
    REFERENCE
       "ITU-T G.7710/Y.1701 clause 10.1.6.1
       "
    ::= { icomModemPerfStatus15MinIntervalEntry 4 }

