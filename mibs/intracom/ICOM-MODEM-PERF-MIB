-- -------------------------------------------------------------
-- icomModemPerfStatsEnable
-- -------------------------------------------------------------

icomModemPerfStatsEnable OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
       "This object enables/disables the collection of coarse-grain
        historical performance statistics for the modem interfaces with
        a 15-minute granularity.

        If value is set to 'true(1)', then collection of historical
        performance statistics is enabled.

        If value is set to 'false(2)', then collection of historical
        performance statistics is disabled. Any existing historical
        performance statistics will be discarded.

        This object is persistent, and its value must be retained across
        reinitializations of the management system.
       "
    DEFVAL { true }
    ::= { icomModemPerfMIBObjects 6 }

-- -------------------------------------------------------------
-- icomModemPerfStatsCurrentTable
-- -------------------------------------------------------------

icomModemPerfStatsCurrentTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IcomModemPerfStatsCurrentEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
       "This table provides coarse-grain modem performance statistics for
        the current 15-minute interval. All table statistics are reset to
        zero after the completion of each 15-minute measurement interval.
       "
    ::= { icomModemPerfMIBObjects 7 }

icomModemPerfStatsCurrentEntry OBJECT-TYPE
    SYNTAX      IcomModemPerfStatsCurrentEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
       "An entry (conceptual row) in the icomModemPerfStatsCurrentTable.
       "
    INDEX       { icomModemPerfModemId }
    ::= { icomModemPerfStatsCurrentTable 1 }

IcomModemPerfStatsCurrentEntry ::= SEQUENCE {
    icomModemPerfStatsCurrentElapsedTime         TimeInterval,
    icomModemPerfStatsCurrentSuspect             TruthValue,
    icomModemPerfStatsCurrentInService           Counter64,
    icomModemPerfStatsCurrentOutService          Counter64
}

icomModemPerfStatsCurrentElapsedTime OBJECT-TYPE
    SYNTAX      TimeInterval
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "The length of time that the current measurement interval has
        been running for, in centi-seconds.
       "
    REFERENCE
       "ITU-T G.7710/Y.1701 clause 10.1.6.1
       "
    ::= { icomModemPerfStatsCurrentEntry 1 }

icomModemPerfStatsCurrentSuspect OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "This variable indicates if the data for this interval have been
        marked as suspect.

        Value 'true(1)' indicates that the related performance data have
        been marked as suspect.

        Value 'false(2)' indicates that the related performance data
        have not been marked as suspect.
       "
    REFERENCE
       "ITU-T G.7710/Y.1701 clause 10.1.6.1
       "
    ::= { icomModemPerfStatsCurrentEntry 2 }

icomModemPerfStatsCurrentInService OBJECT-TYPE
    SYNTAX      Counter64
    UNITS       "seconds"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "This object is used to provide the current time in service (in
        seconds) of the indexed modem interface in the current 15-minute
        interval.
       "
    ::= { icomModemPerfStatsCurrentEntry 3 }

icomModemPerfStatsCurrentOutService OBJECT-TYPE
    SYNTAX      Counter64
    UNITS       "seconds"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "This object is used to provide the current time out of service
        (in seconds) of the indexed modem interface in the current
        15-minute interval.
       "
    ::= { icomModemPerfStatsCurrentEntry 4 }

-- -------------------------------------------------------------
-- icomModemPerfStats15MinIntervalTable
-- -------------------------------------------------------------

icomModemPerfStats15MinIntervalTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IcomModemPerfStats15MinIntervalEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
       "This table provides coarse-grain modem performance statistics
        for historical (completed) 15-minute intervals.

        This table will have a maximum of 96 entries of completed 15-minute
        intervals, to provide historical statistics for one day.
       "
    ::= { icomModemPerfMIBObjects 8 }

icomModemPerfStats15MinIntervalEntry OBJECT-TYPE
    SYNTAX      IcomModemPerfStats15MinIntervalEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
       "An entry (conceptual row) in the icomModemPerfStats15MinIntervalTable.
       "
    INDEX       { icomModemPerfModemId,
                  icomModemPerfStats15MinIntervalNumber
                }
    ::= { icomModemPerfStats15MinIntervalTable 1 }

IcomModemPerfStats15MinIntervalEntry ::= SEQUENCE {
    icomModemPerfStats15MinIntervalNumber         Unsigned32,
    icomModemPerfStats15MinIntervalEndTime        IcomDateTimeType,
    icomModemPerfStats15MinIntervalElapsedTime    TimeInterval,
    icomModemPerfStats15MinIntervalSuspect        TruthValue,
    icomModemPerfStats15MinIntervalInService      Counter64,
    icomModemPerfStats15MinIntervalOutService     Counter64
}

icomModemPerfStats15MinIntervalNumber OBJECT-TYPE
    SYNTAX      Unsigned32 (1..96)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
       "A number that identifies the interval for which the set of
        statistics is available. The interval identified by 1 is the
        most recently completed 15-minute interval, and the interval
        identified by N is the interval immediately preceding the one
        identified by N-1.
       "
    ::= { icomModemPerfStats15MinIntervalEntry 1 }

icomModemPerfStats15MinIntervalEndTime OBJECT-TYPE
    SYNTAX      IcomDateTimeType
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "The time that this measurement interval ended.
       "
    REFERENCE
       "ITU-T G.7710/Y.1701 clause 10.1.6.1
       "
    ::= { icomModemPerfStats15MinIntervalEntry 2 }

icomModemPerfStats15MinIntervalElapsedTime OBJECT-TYPE
    SYNTAX      TimeInterval
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "The length of time that this measurement interval ran for,
        in centi-seconds.
       "
    REFERENCE
       "ITU-T G.7710/Y.1701 clause 10.1.6.1
       "
    ::= { icomModemPerfStats15MinIntervalEntry 3 }

icomModemPerfStats15MinIntervalSuspect OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "This variable indicates if the data for this interval have been
        marked as suspect.

        Value 'true(1)' indicates that the related performance data have
        been marked as suspect.

        Value 'false(2)' indicates that the related performance data
        have not been marked as suspect.
       "
    REFERENCE
       "ITU-T G.7710/Y.1701 clause 10.1.6.1
       "
    ::= { icomModemPerfStats15MinIntervalEntry 4 }

icomModemPerfStats15MinIntervalInService OBJECT-TYPE
    SYNTAX      Counter64
    UNITS       "seconds"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "This object is used to provide the time in service (in seconds) of
        the indexed modem interface in this completed 15-minute interval.
       "
    ::= { icomModemPerfStats15MinIntervalEntry 5 }

