    icomModemPerfG8261DayIntervalBlockErrors    Counter64,
    icomModemPerfG8261DayIntervalBgBlockErrors  Counter64,
    icomModemPerfG8261DayIntervalESs            Counter64,
    icomModemPerfG8261DayIntervalSESs           Counter64,
    icomModemPerfG8261DayIntervalASs            Counter64,
    icomModemPerfG8261DayIntervalUASs           Counter64
}

icomModemPerfG8261DayIntervalNumber OBJECT-TYPE
    SYNTAX      Unsigned32 (1..30)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
       "A number that identifies the interval for which the set of
        statistics is available. The interval identified by 1 is the
        most recently completed 1-day interval, and the interval
        identified by N is the interval immediately preceding the one
        identified by N-1.
       "
    ::= { icomModemPerfG8261DayIntervalEntry 1 }

