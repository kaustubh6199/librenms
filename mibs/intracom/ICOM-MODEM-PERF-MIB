-- -------------------------------------------------------------
-- icomModemPerfPhyEnable
-- -------------------------------------------------------------

icomModemPerfPhyEnable OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
       "This object enables/disables the collection of historical
        performance statistics for the Rx PHY modes.

        If value is set to 'true(1)', then collection of historical
        performance statistics is enabled.

        If value is set to 'false(2)', then collection of historical
        performance statistics is disabled. Any existing historical
        performance statistics will be discarded.

        This object is persistent, and its value must be retained across
        reinitializations of the management system.
       "
    DEFVAL { true }
    ::= { icomModemPerfMIBObjects 14 }

-- -------------------------------------------------------------
-- icomModemPerfPhyCurrentTable
-- -------------------------------------------------------------

icomModemPerfPhyCurrentTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IcomModemPerfPhyCurrentEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
       "This table provides Rx PHY performance statistics for the
        current 15-minute interval. All table statistics are reset to
        zero after the completion of each 15-minute measurement interval.
       "
    ::= { icomModemPerfMIBObjects 15 }

icomModemPerfPhyCurrentEntry OBJECT-TYPE
    SYNTAX      IcomModemPerfPhyCurrentEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
       "An entry (conceptual row) in the icomModemPerfPhyCurrentTable.
       "
    INDEX       { icomModemPerfModemId,
                  icomModemPerfPhyMode
                }
    ::= { icomModemPerfPhyCurrentTable 1 }

IcomModemPerfPhyCurrentEntry ::= SEQUENCE {
    icomModemPerfPhyMode                       IcomModemPerfModulationType,
    icomModemPerfPhyCurrentElapsedTime         TimeInterval,
    icomModemPerfPhyCurrentSuspect             TruthValue,
    icomModemPerfPhyCurrentInPhyTime           Counter64,
    icomModemPerfPhyCurrentInPhyChanges        Counter64
}

