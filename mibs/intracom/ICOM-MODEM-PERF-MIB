ICOM-MODEM-PERF-MIB DEFINITIONS ::= BEGIN
IMPORTS
        MODULE-IDENTITY, OBJECT-TYPE,
        Integer32, Unsigned32, Counter64
                FROM SNMPv2-SMI
        TEXTUAL-CONVENTION, TruthValue, TimeInterval
                FROM SNMPv2-TC
        MODULE-COMPLIANCE, OBJECT-GROUP
                FROM SNMPv2-CONF
        IcomDateTimeType
                FROM ICOM-PERFHIST-TC-MIB
        intracom
                FROM ICOM-SMI;

icomModemPerfMIB MODULE-IDENTITY
    LAST-UPDATED        "201804110000Z"     -- 11 April 2018
    ORGANIZATION        "Intracom S.A."
    CONTACT-INFO
       "EMail:
        Phone:
        Co-editor:
        EMail:
        Phone:
       "
    DESCRIPTION
       "This module provides proprietary Intracom objects to support
        historical statistics of modem interfaces on point-to-point
        systems. The MIB includes the following proprietary objects:

        1.  icomModemPerfStatusEnable
        2.  icomModemPerfStatusCurrentTable
        3.  icomModemPerfStatus1MinIntervalTable
        4.  icomModemPerfStatus15MinIntervalTable
        5.  icomModemPerfStatus1DayIntervalTable
        6.  icomModemPerfStatsEnable
        7.  icomModemPerfStatsCurrentTable
        8.  icomModemPerfStats15MinIntervalTable
        9.  icomModemPerfStats1DayIntervalTable
        10. icomModemPerfG826Enable
        11. icomModemPerfG826CurrentTable
        12. icomModemPerfG82615MinIntervalTable
        13. icomModemPerfG8261DayIntervalTable
        14. icomModemPerfPhyEnable
        15. icomModemPerfPhyCurrentTable
        16. icomModemPerfPhy15MinIntervalTable
        17. icomModemPerfPhy1DayIntervalTable
        18. icomModemPerfRfuEnable
        19. icomModemPerfRfuCurrentTable
        20. icomModemPerfRfu1MinIntervalTable
        21. icomModemPerfRfu15MinIntervalTable
        22. icomModemPerfRfu1DayIntervalTable
       "

-- Revision History

    REVISION            "201804110000Z"     -- 11 April 2018
    DESCRIPTION
       "Refactor fine-grain modem performance statistics:
        - compute min/max/avg for Rx SNR, LDPC decoder stress and Tx/Rx
        bandwidth utilization each minute
        - compute min/max/mod for Tx/Rx PHY modulation each minute
        - move total FEC blocks & uncorrected FEC blocks to fine-grain
        modem performance statistics
        - add link out of lock (LoL) events
        - add ElapsedTime MOs, due to the introduction of counter-type
        modem performance statistics

        Refactor RFU performance statistics:
        - compute min/max/avg Tx power and RSSI each minute
       "
    REVISION            "201512030000Z"     -- 03 December 2015
    DESCRIPTION
       "
        This is the initial version of this MIB.
       "

::= { intracom 71 }

-- -------------------------------------------------------------
-- Textual Conventions
-- -------------------------------------------------------------

IcomModemPerfDbType ::= TEXTUAL-CONVENTION
    DISPLAY-HINT "d-1"
    STATUS      current
    DESCRIPTION
        "Normalized dB value. To obtain the actual value in dB, divide
         the IcomModemPerfDbType value by 10.
        "
    SYNTAX      Integer32

IcomModemPerfDbmType ::= TEXTUAL-CONVENTION
    DISPLAY-HINT "d-1"
    STATUS      current
    DESCRIPTION
        "Normalized dBm value. To obtain the actual value in dBm, divide the
         IcomModemPerfDbmType value by 10.
        "
    SYNTAX      Integer32

IcomModemPerfModulationType ::= TEXTUAL-CONVENTION
    DISPLAY-HINT "d"
    STATUS      current
    DESCRIPTION
        "Modem modulation type.
        "
    SYNTAX      Unsigned32

IcomModemPerfBwUtilType ::= TEXTUAL-CONVENTION
    DISPLAY-HINT "d-2"
    STATUS      current
    DESCRIPTION
        "Hundredths of bandwidth utilization percentage (0..10000). To
         obtain the actual percentage in the 0.00..100.00 range, divide
         the IcomModemPerfBwUtilType value by 100.
        "
    SYNTAX      Unsigned32

--
--  The icomModemPerfMIBObjects group
--
icomModemPerfMIBObjects   OBJECT IDENTIFIER ::= { icomModemPerfMIB 1 }

-- -------------------------------------------------------------
-- icomModemPerfStatusEnable
-- -------------------------------------------------------------

icomModemPerfStatusEnable OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
       "This object enables/disables the collection of fine-grain
        historical performance statistics for the modem interfaces with
        a 1-minute granularity.

        If value is set to 'true(1)', then collection of historical
        performance statistics is enabled.

        If value is set to 'false(2)', then collection of historical
        performance statistics is disabled. Any existing historical
        performance statistics will be discarded.

        This object is persistent, and its value must be retained across
        reinitializations of the management system.
       "
    DEFVAL { true }
    ::= { icomModemPerfMIBObjects 1 }

-- -------------------------------------------------------------
-- icomModemPerfStatusCurrentTable
-- -------------------------------------------------------------

icomModemPerfStatusCurrentTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IcomModemPerfStatusCurrentEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
       "This table provides fine-grain modem performance statistics for
        the current 1-minute interval. All table statistics are reset to
        zero after the completion of each 1-minute measurement interval.
       "
    ::= { icomModemPerfMIBObjects 2 }

icomModemPerfStatusCurrentEntry OBJECT-TYPE
    SYNTAX      IcomModemPerfStatusCurrentEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
       "An entry (conceptual row) in the icomModemPerfStatusCurrentTable.
       "
    INDEX       { icomModemPerfModemId }
    ::= { icomModemPerfStatusCurrentTable 1 }

IcomModemPerfStatusCurrentEntry ::= SEQUENCE {
    icomModemPerfModemId                          Unsigned32,
    icomModemPerfStatusCurrentElapsedTime         TimeInterval,
    icomModemPerfStatusCurrentSuspect             TruthValue,
    icomModemPerfStatusCurrentMinRxSnr            IcomModemPerfDbType,
    icomModemPerfStatusCurrentMaxRxSnr            IcomModemPerfDbType,
    icomModemPerfStatusCurrentAvgRxSnr            IcomModemPerfDbType,
    icomModemPerfStatusCurrentMinTxPhyMod         IcomModemPerfModulationType,
    icomModemPerfStatusCurrentMaxTxPhyMod         IcomModemPerfModulationType,
    icomModemPerfStatusCurrentModTxPhyMod         IcomModemPerfModulationType,
    icomModemPerfStatusCurrentMinRxPhyMod         IcomModemPerfModulationType,
    icomModemPerfStatusCurrentMaxRxPhyMod         IcomModemPerfModulationType,
    icomModemPerfStatusCurrentModRxPhyMod         IcomModemPerfModulationType,
    icomModemPerfStatusCurrentMinLpdcStress       Unsigned32,
    icomModemPerfStatusCurrentMaxLpdcStress       Unsigned32,
    icomModemPerfStatusCurrentAvgLpdcStress       Unsigned32,
    icomModemPerfStatusCurrentMinTxBwUtil         IcomModemPerfBwUtilType,
    icomModemPerfStatusCurrentMaxTxBwUtil         IcomModemPerfBwUtilType,
    icomModemPerfStatusCurrentAvgTxBwUtil         IcomModemPerfBwUtilType,
    icomModemPerfStatusCurrentMinRxBwUtil         IcomModemPerfBwUtilType,
    icomModemPerfStatusCurrentMaxRxBwUtil         IcomModemPerfBwUtilType,
    icomModemPerfStatusCurrentAvgRxBwUtil         IcomModemPerfBwUtilType,
    icomModemPerfStatusCurrentFecBlocks           Counter64,
    icomModemPerfStatusCurrentFecUncorBlocks      Counter64,
    icomModemPerfStatusCurrentLossOfLink          Counter64
}

icomModemPerfModemId OBJECT-TYPE
    SYNTAX      Unsigned32 (1..2147483647)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
       "The modem identifier.
       "
    ::= { icomModemPerfStatusCurrentEntry 1 }

icomModemPerfStatusCurrentElapsedTime OBJECT-TYPE
    SYNTAX      TimeInterval
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "The length of time that the current measurement interval has
        been running for, in centi-seconds.
       "
    REFERENCE
       "ITU-T G.7710/Y.1701 clause 10.1.6.1
       "
    ::= { icomModemPerfStatusCurrentEntry 2 }

icomModemPerfStatusCurrentSuspect OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "This variable indicates if the data for this interval have been
        marked as suspect.

        Value 'true(1)' indicates that the related performance data have
        been marked as suspect.

        Value 'false(2)' indicates that the related performance data
        have not been marked as suspect.
       "
    REFERENCE
       "ITU-T G.7710/Y.1701 clause 10.1.6.1
       "
    ::= { icomModemPerfStatusCurrentEntry 3 }

icomModemPerfStatusCurrentMinRxSnr OBJECT-TYPE
    SYNTAX      IcomModemPerfDbType
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "This object is used to provide the minimum received SNR (in 0.1
        dB steps) of the indexed modem interface in the current 1-minute
        interval.
       "
    ::= { icomModemPerfStatusCurrentEntry 4 }

icomModemPerfStatusCurrentMaxRxSnr OBJECT-TYPE
    SYNTAX      IcomModemPerfDbType
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "This object is used to provide the maximum received SNR (in 0.1
        dB steps) of the indexed modem interface in the current 1-minute
        interval.
       "
    ::= { icomModemPerfStatusCurrentEntry 5 }

icomModemPerfStatusCurrentAvgRxSnr OBJECT-TYPE
    SYNTAX      IcomModemPerfDbType
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "This object is used to provide the average received SNR (in 0.1
        dB steps) of the indexed modem interface in the current 1-minute
        interval.
       "
    ::= { icomModemPerfStatusCurrentEntry 6 }

icomModemPerfStatusCurrentMinTxPhyMod OBJECT-TYPE
    SYNTAX      IcomModemPerfModulationType
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "This object is used to provide the minimum Tx PHY modulation of
        the indexed modem interface in the current 1-minute interval.
       "
    ::= { icomModemPerfStatusCurrentEntry 7 }

icomModemPerfStatusCurrentMaxTxPhyMod OBJECT-TYPE
    SYNTAX      IcomModemPerfModulationType
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "This object is used to provide the maximum Tx PHY modulation of
        the indexed modem interface in the current 1-minute interval.
       "
    ::= { icomModemPerfStatusCurrentEntry 8 }

icomModemPerfStatusCurrentModTxPhyMod OBJECT-TYPE
    SYNTAX      IcomModemPerfModulationType
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "This object is used to provide the most frequent (i.e.,
        statistical mode) Tx PHY modulation of the indexed modem interface
        in the current 1-minute interval.
       "
    ::= { icomModemPerfStatusCurrentEntry 9 }

icomModemPerfStatusCurrentMinRxPhyMod OBJECT-TYPE
    SYNTAX      IcomModemPerfModulationType
