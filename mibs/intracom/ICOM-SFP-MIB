    REFERENCE   "SFP-8472 Table 3.17."
	::= { icomSfpDiagEntry 22 }

icomSfpDiagSupplyVoltage OBJECT-TYPE
    SYNTAX               Unsigned32 (0..65535)
    MAX-ACCESS           read-only
    STATUS               current
    DESCRIPTION
		"Internally measured supply voltage in transceiver. The value 0 means 0 Volts.
		An increment on one is equivalent to 100 uVolt,
		yielding a total range of +6.55 Volts.
		Accuracy is vendor specific but must be better than +/-3% of the
		manufacturer's nominal value over specified operating temperature and voltage."
    REFERENCE   "SFP-8472 Table 3.17."
	::= { icomSfpDiagEntry 23 }

icomSfpDiagTxBias  OBJECT-TYPE
    SYNTAX         Unsigned32 (0..65535)
    MAX-ACCESS     read-only
    STATUS         current
    DESCRIPTION
		"Internally measured TX Bias Current. The value 0
		means 0 Volt. An increment on one is equivalent to 2 uA,
		yielding a total range of 0 to 131 mA.
		Accuracy is vendor specific but must be better than +/-3dB over specified
		temperature and voltage. Data is not valid when the transmitter is disabled."
    REFERENCE   "SFP-8472 Table 3.17."
	::= { icomSfpDiagEntry 24 }

icomSfpDiagTxOutputPower OBJECT-TYPE
    SYNTAX               Unsigned32 (0..65535)
    MAX-ACCESS           read-only
    STATUS               current
    DESCRIPTION
		"Measured TX output power. The value 0
		means 0 Watts. An increment on one is equivalent to 0.1 uW,
		yielding a total range of 0 to 6.5535 mW.
		For the vendor specified wavelength, accuracy shall be better than +/-3dB
		over specified temperature and voltage."
    REFERENCE   "SFP-8472 Table 3.17."
	::= { icomSfpDiagEntry 25 }

icomSfpDiagRxInputPower OBJECT-TYPE
    SYNTAX              Unsigned32 (0..65535)
    MAX-ACCESS          read-only
    STATUS              current
    DESCRIPTION
		"Measured RX input power. The value 0
		means 0 Watts. An increment on one is equivalent to 0.1 uW,
		yielding a total range of 0 to 6.5535 mW.
		For the vendor specified wavelength, accuracy shall be better than +/-3dB
		over specified temperature and voltage."
    REFERENCE   "SFP-8472 Table 3.17."
	::= { icomSfpDiagEntry 26 }

icomSfpDiagRxLOSPinOut OBJECT-TYPE
    SYNTAX             TruthValue
    MAX-ACCESS         read-only
    STATUS             current
    DESCRIPTION
        "This contains the True / False state of the SFP Rx LOS (output) pin. It is
         True if the SFP has detected Loss-Of-Signal. located at I2c
         address 0xa2, address: 110, mask 0x02"
    ::= { icomSfpDiagEntry 27 }

icomSfpDiagTxFaultPinOut OBJECT-TYPE
    SYNTAX               TruthValue
    MAX-ACCESS           read-only
    STATUS               current
    DESCRIPTION
        "This contains the True / False state of the SFP Tx Fault (output) pin.
         It is True if the SFP has detected a Tx fault condition.
         located at I2C address 0xa2, Byte 110, mask 0x04"
    ::= { icomSfpDiagEntry 28 }

icomSfpDiagRateSelectPinIn OBJECT-TYPE
    SYNTAX                 TruthValue
    MAX-ACCESS             read-only
    STATUS                 current
    DESCRIPTION
        "This contains the True / False state of the SFP RX Rate Select (input)
         pin. This function is not implemented on Finisar SFPs. located
         at I2C address 0xa2, Byte 110, mask 0x10"
    ::= { icomSfpDiagEntry 29 }

icomSfpDiagTxDisablePinIn OBJECT-TYPE
    SYNTAX                TruthValue
    MAX-ACCESS            read-only
    STATUS                current
    DESCRIPTION
        "This contains the True / False state of the SFP TX Disable (input) pin.
         located at I2C address 0xa2, Byte 110, mask 0x80"
    ::= { icomSfpDiagEntry 30 }

icomSfpDiagAlarms OBJECT-TYPE
    SYNTAX        BITS { sfpAlarmTempHigh(0),
                         sfpAlarmTempLow(1),
                         sfpAlarmVoltageHigh(2),
                         sfpAlarmVoltageLow(3),
                         sfpAlarmTxBiasHigh(4),
                         sfpAlarmTxBiasLow(5),
                         sfpAlarmTxPowerHigh(6),
                         sfpAlarmTxPowerLow(7),
                         sfpAlarmRxPowerHigh(8),
                         sfpAlarmRxPowerLow(9)
                       }
	MAX-ACCESS    read-only
	STATUS        current
	DESCRIPTION
		"This object indicates the Alarms of the SFP module.
		The icomSfpDiagAlarms is a bit map represented as a sum,  therefore,
		it can represent multiple failures (alarms) simultaneously.
	    The various bit positions are:
		  Bit 0:  Temp High Alarm Set when internal temperature exceeds high alarm threshold.
		  Bit 1:  Temp Low Alarm Set when internal temperature is below low alarm threshold.
		  Bit 2:  Vcc High Alarm Set when internal supply voltage exceeds high alarm threshold.
		  Bit 3:  Vcc Low Alarm Set when internal supply voltage is below low alarm threshold.
		  Bit 4:  TX Bias High Alarm Set when TX Bias current exceeds high alarm threshold.
		  Bit 5:  TX Bias Low Alarm Set when TX Bias current is below low alarm threshold.
		  Bit 6:  TX Power High Alarm Set when TX output power exceeds high alarm threshold.
		  Bit 7:  TX Power Low Alarm Set when TX output power is below low alarm threshold.
		  Bit 8:  RX Power High Alarm Set when Received Power exceeds high alarm threshold.
		  Bit 9:  RX Power Low Alarm Set when Received Power is below low alarm threshold."
    REFERENCE   "SFP-8472 Table 3.18."
     ::= { icomSfpDiagEntry 31 }

icomSfpDiagAlarmsMask OBJECT-TYPE
    SYNTAX        BITS { sfpAlarmMaskTempHigh(0),
                         sfpAlarmMaskTempLow(1),
                         sfpAlarmMaskVoltageHigh(2),
                         sfpAlarmMaskVoltageLow(3),
                         sfpAlarmMaskTxBiasHigh(4),
                         sfpAlarmMaskTxBiasLow(5),
                         sfpAlarmMaskTxPowerHigh(6),
                         sfpAlarmMaskTxPowerLow(7),
                         sfpAlarmMaskRxPowerHigh(8),
                         sfpAlarmMaskRxPowerLow(9)
                       }
	MAX-ACCESS   read-write
	STATUS       current
	DESCRIPTION
		"This object indicates the Masking of the Alarms of the SFP module.
		The icomSfpDiagAlarmsMask is a bit map. Any of the alarm and warning flags can
		be masked by writing a 1 to the corresponding bits.
	    The various bit positions are:
		  Bit 0:  Temp High Alarm Mask Set.
		  Bit 1:  Temp Low Alarm Mask Set.
		  Bit 2:  Vcc High Alarm Mask Set.
		  Bit 3:  Vcc Low Alarm Mask Set.
		  Bit 4:  TX Bias High Alarm Mask Set.
		  Bit 5:  TX Bias Low Alarm Mask Set.
		  Bit 6:  TX Power High Alarm Mask Set.
		  Bit 7:  TX Power Low Alarm Mask Set.
		  Bit 8:  RX Power High Alarm Mask Set.
		  Bit 9:  RX Power Low Alarm Mask Set."
    REFERENCE   "SFP-8472 Table 3.18."
     ::= { icomSfpDiagEntry 32 }

icomSfpDiagWarnings OBJECT-TYPE
    SYNTAX        BITS { sfpWarningTempHigh(0),
                         sfpWarningTempLow(1),
                         sfpWarningVoltageHigh(2),
                         sfpWarningVoltageLow(3),
                         sfpWarningTxBiasHigh(4),
                         sfpWarningTxBiasLow(5),
                         sfpWarningTxPowerHigh(6),
                         sfpWarningTxPowerLow(7),
                         sfpWarningRxPowerHigh(8),
                         sfpWarningRxPowerLow(9)
                       }
	MAX-ACCESS    read-only
	STATUS        current
	DESCRIPTION
		"This object indicates the Warnings of the SFP module.
		The icomSfpDiagWarnings is a bit map represented as a sum,  therefore,
		it can represent multiple failures (Warnings) simultaneously.
	    The various bit positions are:
		  Bit 0:  Temp High Warning Set when internal temperature exceeds high warning threshold.
		  Bit 1:  Temp Low Warning Set when internal temperature is below low warning threshold.
		  Bit 2:  Vcc High Warning Set when internal supply voltage exceeds high warning threshold.
		  Bit 3:  Vcc Low Warning Set when internal supply voltage is below low warning threshold.
		  Bit 4:  TX Bias High Warning Set when TX Bias current exceeds high warning threshold.
		  Bit 5:  TX Bias Low Warning Set when TX Bias current is below low warning threshold.
		  Bit 6:  TX Power High Warning Set when TX output power exceeds high warning threshold.
		  Bit 7:  TX Power Low Warning Set when TX output power is below low warning threshold.
		  Bit 8:  RX Power High Warning Set when Received Power exceeds high warning threshold.
		  Bit 9:  RX Power Low Warning Set when Received Power is below low warning threshold."
    REFERENCE   "SFP-8472 Table 3.18."
     ::= { icomSfpDiagEntry 33 }

icomSfpDiagWarningsMask OBJECT-TYPE
    SYNTAX        BITS { sfpWarningMaskTempHigh(0),
                         sfpWarningMaskTempLow(1),
                         sfpWarningMaskVoltageHigh(2),
                         sfpWarningMaskVoltageLow(3),
                         sfpWarningMaskTxBiasHigh(4),
                         sfpWarningMaskTxBiasLow(5),
                         sfpWarningMaskTxPowerHigh(6),
                         sfpWarningMaskTxPowerLow(7),
                         sfpWarningMaskRxPowerHigh(8),
                         sfpWarningMaskRxPowerLow(9)
                       }
	MAX-ACCESS    read-write
	STATUS        current
	DESCRIPTION
		"This object indicates the Masking of the Warnings of the SFP module.
		The icomSfpDiagWarningsMask is a bit map. Any of the alarm and warning flags can
		be masked by writing a 1 to the corresponding bits.
	    The various bit positions are:
		  Bit 0:  Temp High Warning Mask Set.
		  Bit 1:  Temp Low Warning Mask Set.
		  Bit 2:  Vcc High Warning Mask Set.
		  Bit 3:  Vcc Low Warning Mask Set.
		  Bit 4:  TX Bias High Warning Mask Set.
		  Bit 5:  TX Bias Low Warning Mask Set.
		  Bit 6:  TX Power High Warning Mask Set.
		  Bit 7:  TX Power Low Warning Mask Set.
		  Bit 8:  RX Power High Warning Mask Set.
		  Bit 9:  RX Power Low Warning Mask Set."
    REFERENCE   "SFP-8472 Table 3.18."
     ::= { icomSfpDiagEntry 34 }

-- -------------------------------------------------------------
-- The SFP ALS Configuration Table
-- -------------------------------------------------------------

icomSfpAlsCfgTable	 OBJECT-TYPE
    SYNTAX       SEQUENCE OF IcomSfpAlsCfgEntry
    MAX-ACCESS   not-accessible
    STATUS  	 current
    DESCRIPTION
	    "The SFP ALS Configuration table to
	     configure the attributes of Automatic Laser Shutdown
         feature."
    ::= { icomSfpAls 1 }

icomSfpAlsCfgEntry OBJECT-TYPE
    SYNTAX      IcomSfpAlsCfgEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
	    "An entry in the SFP ALS Configuration table.
        The index to this table is the same as the ifIndex of the SFP.
        The SFP ALS Configuration Table contain an entry for each module
        of a type SFP."
    INDEX   { icomSfpAlsCfgInterface }
    ::= { icomSfpAlsCfgTable 1 }

IcomSfpAlsCfgEntry ::=
    SEQUENCE {
        icomSfpAlsCfgInterface	        InterfaceIndex,
        icomSfpAlsCfgRestartMode	        INTEGER,
        icomSfpAlsCfgPulseInterval              Integer32,
        icomSfpAlsCfgPulseWidth                 Integer32
	}

icomSfpAlsCfgInterface OBJECT-TYPE
    SYNTAX  	    InterfaceIndex
    MAX-ACCESS      not-accessible
    STATUS  	    current
    DESCRIPTION
	    "The Interface Index of the corresponding system port that uses
		Optical Transceiver module."
    ::= { icomSfpAlsCfgEntry 1 }

icomSfpAlsCfgRestartMode    OBJECT-TYPE
    SYNTAX      INTEGER { disable(1),
                          auto   (2),
                          manual (3)
	                    }
    MAX-ACCESS  read-write
    STATUS  	current
    DESCRIPTION
	    " o disable: Disabled mode. If mode is disabled, ALS is disabled.
	        LOS will not cause laser shutdown.
          o manual: Manual restart mode. The laser is turned off when the ALS agent
            detects an LOS for 500 ms. After ALS is engaged, a manual command is issued
            that turns on the laser for the time period of the pulse width.
            The laser is turned on when the LOS has been cleared for 100 ms.
          o auto: Automatic restart mode. ?The laser is shut down for the time period
           of pulse interval after the ALS agent detects a LOS for 500 ms.
           Then, the laser automatically turns on for the time period of the
           selected pulse width. If an LOS still exists at that time,
           the laser is shut down again. This pattern continues until
           the LOS is cleared for 100 ms; Then, the laser will stay on."
    DEFVAL { disable }
    ::= { icomSfpAlsCfgEntry 2 }

icomSfpAlsCfgPulseInterval    OBJECT-TYPE
    SYNTAX        Integer32 (100..20000)
    UNITS         "seconds"
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "The pulse interval indicates the time period of the ALS restart
        when the laser transmitter is off at the time between two
        consecutive laser transmissions and
        applies to the automatic restart mode only."
    DEFVAL { 100 }
    ::= { icomSfpAlsCfgEntry 3 }

icomSfpAlsCfgPulseWidth    OBJECT-TYPE
    SYNTAX        Integer32 (2..200)
    UNITS         "seconds"
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "The pulse width indicates the time period of the ALS restart
         that the laser becomes on and applies to the automatic restart mode
         and manual restart mode."
    DEFVAL { 2 }
    ::= { icomSfpAlsCfgEntry 4 }

-- -------------------------------------------------------------
-- The SFP ALS Status Table
-- -------------------------------------------------------------

icomSfpAlsStatusTable	 OBJECT-TYPE
    SYNTAX       SEQUENCE OF IcomSfpAlsStatusEntry
    MAX-ACCESS   not-accessible
    STATUS  	 current
    DESCRIPTION
	    "The SFP ALS Status table to
	     get status of ALS feature."
    ::= { icomSfpAls 2 }

icomSfpAlsStatusEntry OBJECT-TYPE
    SYNTAX      IcomSfpAlsStatusEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
	    "An entry in the SFP ALS Status table.
        The index to this table is the same as the ifIndex of the SFP.
        The SFP ALS Action Table contain an entry for each module
        of a type SFP."
    INDEX   { icomSfpAlsCfgInterface }
    ::= { icomSfpAlsStatusTable 1 }

IcomSfpAlsStatusEntry ::=
    SEQUENCE {
        icomSfpAlsStatusOperStatus  INTEGER
	}

icomSfpAlsStatusOperStatus   OBJECT-TYPE
    SYNTAX      INTEGER { disable(1),
                          auto   (2),
                          manual (3),
                          idle   (4)
	                    }
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "Actual Status of the ALS process."
    ::= { icomSfpAlsStatusEntry 1 }

-- -------------------------------------------------------------
-- The SFP ALS Action Table
-- -------------------------------------------------------------

icomSfpAlsActTable	 OBJECT-TYPE
    SYNTAX       SEQUENCE OF IcomSfpAlsActEntry
    MAX-ACCESS   not-accessible
    STATUS  	 current
    DESCRIPTION
	    "The SFP ALS Action table to
	     trigger actions relevant Automatic Laser Shutdown
         feature."
    ::= { icomSfpAls 3 }

icomSfpAlsActEntry OBJECT-TYPE
    SYNTAX      IcomSfpAlsActEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
	    "An entry in the SFP ALS Action table.
        The index to this table is the same as the ifIndex of the SFP.
        The SFP ALS Action Table contain an entry for each module
        of a type SFP."
    INDEX   { icomSfpAlsCfgInterface }
    ::= { icomSfpAlsActTable 1 }

IcomSfpAlsActEntry ::=
    SEQUENCE {
        icomSfpAlsActManualRestart              TruthValue
	}

icomSfpAlsActManualRestart   OBJECT-TYPE
    SYNTAX        TruthValue
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Action to trigger the Manual Restart of ALS.
         Setting to true triggers the ALS Manual Restart."
    DEFVAL { false }
    ::= { icomSfpAlsActEntry 1 }

