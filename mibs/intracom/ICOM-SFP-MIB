        icomSfpDiagHighRxPowerWrnThreshold,
        icomSfpDiagLowRxPowerWrnThreshold,
        icomSfpDiagModuleTemperature,
        icomSfpDiagSupplyVoltage,
        icomSfpDiagTxBias,
        icomSfpDiagTxOutputPower,
        icomSfpDiagRxInputPower,
        icomSfpDiagRxLOSPinOut,
        icomSfpDiagTxFaultPinOut,
        icomSfpDiagRateSelectPinIn,
        icomSfpDiagTxDisablePinIn,
        icomSfpDiagAlarms,
        icomSfpDiagAlarmsMask,
        icomSfpDiagWarnings,
        icomSfpDiagWarningsMask,

        icomSfpAlsCfgRestartMode,
        icomSfpAlsCfgPulseInterval,
        icomSfpAlsCfgPulseWidth,

        icomSfpAlsStatusOperStatus,

        icomSfpAlsActManualRestart
    }
    STATUS      current
    DESCRIPTION
        "A collection of objects for icomSfpMib."
    ::= { icomSfpMibGroups 1 }

icomSfpNotificationsGroup NOTIFICATION-GROUP
    NOTIFICATIONS {
        icomSfpAlarmsTrap,
        icomSfpWarningsTrap
    }
    STATUS	current
    DESCRIPTION
        "A collection of SFP Diagnostics Notifications."
    ::= { icomSfpMibGroups 2 }


