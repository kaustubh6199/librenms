           "
             ODU Duplexer Status
           "
      ::= { oduStatusEntry 20 }

  --=========================================================                                      
   -- ODU ANALOGUE MONITOR TABLE
   --=========================================================
   oduAnalogueMonitorTable OBJECT-TYPE
      SYNTAX       SEQUENCE OF   OduAnalogueMonitorEntry
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  " 
                     This table models the ODU Analogue Monitor information
                   "    
      ::= { oduConfig  4 }

   oduAnalogueMonitorEntry OBJECT-TYPE 
      SYNTAX       OduAnalogueMonitorEntry
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  " "
      INDEX  {oduAnalogueMonitorEntryIndex} 
      ::={oduAnalogueMonitorTable 1 }

    OduAnalogueMonitorEntry ::= SEQUENCE {                   
      
        oduAnalogueMonitorEntryIndex  INTEGER,
        oduTemperature        INTEGER,
        oduRSSI     INTEGER,
        oduPif INTEGER,
        oduPoutVdet INTEGER,
        oduMonitorPlus5V INTEGER,
        oduMonitorPlus7V INTEGER,
        oduMonitorMinus5V INTEGER,
        oduMonitorPlus6point5V INTEGER,
        oduMonitorMinus48V INTEGER
   }   
   
   oduAnalogueMonitorEntryIndex   OBJECT-TYPE 
      SYNTAX       INTEGER (1)
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  "
                     Index of the OduAnalogueMonitorEntry. 
                     It is restricted to 1 instance.
                   "
      ::= { oduAnalogueMonitorEntry 1 }

   oduTemperature OBJECT-TYPE 
      SYNTAX       INTEGER(-127..127)
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  " 
                     Odu temperature
                     Value Range -127..127
                     The value is calculated   
                     in 0.1 step.
                   "
      ::= { oduAnalogueMonitorEntry 2 }   
      
   oduRSSI OBJECT-TYPE 
      SYNTAX       INTEGER(0..65535)
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  "  
                      Odu input power from the remote Radio Unit (RSSI)
                      The value is calculated     
                      in -0.1dBm step. 
                    "
      ::= { oduAnalogueMonitorEntry 3 } 

    oduPif OBJECT-TYPE 
      SYNTAX       INTEGER(-32767..32767)
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  "  ODU Radio power in cable before Air (Pif).
                       The value is calculated     
                       in 0.1dBm step. 
                        Value Range -32767..32767
    
                       In case of RFU Odu, OID oduPif is not used .
                       "
      ::= { oduAnalogueMonitorEntry 4 }     

    oduPoutVdet OBJECT-TYPE 
      SYNTAX       INTEGER(-32767..32767)
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  " 
                   Odu power output volt detector.
                   The value is calculated   
                   in 0.1dBm step.   
                   Value Range -32767..32767

                   In case of RFU Odu, OID oduPoutVdet is not used .
    "
      ::= { oduAnalogueMonitorEntry 5 }  
    
    oduMonitorPlus5V OBJECT-TYPE 
      SYNTAX       INTEGER
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  " 
             Odu monitor +5 volt.
             The value is calculated   
             in 0.1V step.        
                                                   
               oduMonitorPlus5V  shows +5V.
                                                   
             - When OID oduModelType(b7-b6) is unknown oduMonitorPlus5V is not used.             

             In case of RFU Odu, OID oduMonitorPlus5V is not used .

    "
      ::= { oduAnalogueMonitorEntry 6 }

    oduMonitorPlus7V OBJECT-TYPE 
      SYNTAX       INTEGER
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  " 
             Odu monitor +7 volt.
             The value is calculated   
             in 0.1V step.   
               
             - This OID shows +12V when               
               the OID oduModelType(b7-b6) is Intralink ODU.
                              
             - When OID oduModelType(b7-b6) is FLEX ODU or SODU 
                          oduMonitorPlus7V shows V_PA with the following volts:                   
                            -- When oduModelType(b5-b0) is 0x07 or 0x08 
                               oduMonitorPlus7V shows V_PA +12V                     
                            -- When oduModelType(b5-b0) is 0x0D or 0x0F or 0x26 
                               oduMonitorPlus7V shows V_PA +9.5V                     
                            -- When oduModelType(b5-b0) is 0x12 or 0x17 or 0x1A  
                                oduMonitorPlus7V shows V_PA +7.6V             
             - When OID oduModelType(b7-b6) is unknown oduMonitorPlus7V is not used.
     
                     In case of RFU Odu, OID oduMonitorPlus7V is not used .

    "
      ::= { oduAnalogueMonitorEntry 7 }

    oduMonitorMinus5V OBJECT-TYPE 
      SYNTAX       INTEGER
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  " 
             
             The value is calculated   
             in 0.1V step.          
             
             - When OID oduModelType(b7-b6) is Intralink ODU.            
               oduMonitorMinus5V  shows -12V.

             - When OID oduModelType(b7-b6) is FLEX ODU or SODU 
               oduMonitorMinus5V  shows -5V.        

             - When OID oduModelType(b7-b6) is unknown oduMonitorMinus5V is not used.             

         In case of RFU Odu, OID oduMonitorMinus5V is not used .

    "
      ::= { oduAnalogueMonitorEntry 8 }

    oduMonitorPlus6point5V OBJECT-TYPE 
      SYNTAX       INTEGER
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  " 
             Odu monitor +6.5 volt.
             The value is calculated   
             in 0.1V step.        
             
             - When OID oduModelType(b7-b6) is Intralink ODU.            
               oduMonitorPlus6point5V  shows +6.5.

             - When OID oduModelType(b7-b6) is FLEX ODU or SODU 
               oduMonitorPlus6point5V  shows +6.5V.

             - When OID oduModelType(b7-b6) is unknown oduMonitorPlus6point5V is not used.             

         In case of RFU Odu, OID oduMonitorPlus6point5V is not used .

    "
      ::= { oduAnalogueMonitorEntry 9 }
      
    oduMonitorMinus48V OBJECT-TYPE 
      SYNTAX       INTEGER
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  " 
             Odu monitor -48 volt.
             In order to obtain the 
             absolute value of the measured 
             voltage, the returned of the 
             OID must be divided by 100.
            
             - When OID oduModelType(b7-b6) is Intralink ODU.            
               oduMonitorMinus48V is not used

             - When OID oduModelType(b7-b6) is FLEX ODU 
               oduMonitorMinus48V  shows -48V.

             - When OID oduModelType(b7-b6) is unknown oduMonitorMinus48V is not used.             

         In case of RFU Odu, OID oduMonitorMinus48V is not used .
    "
      ::= { oduAnalogueMonitorEntry 10 }

  --=========================================================                                      
   -- ODU VERSION TABLE
   --========================================================
   oduVersionTable OBJECT-TYPE
      SYNTAX       SEQUENCE OF   OduVersionEntry
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  " 
                     This table models the ODU Version information
                     This table is not used in case of RFU ODU
                   "    
      ::= { oduConfig  5 }

   oduVersionEntry OBJECT-TYPE 
      SYNTAX       OduVersionEntry
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  " "
      INDEX  {oduVersionEntryIndex} 
      ::={oduVersionTable 1 }

    OduVersionEntry ::= SEQUENCE {                   
        oduVersionEntryIndex       INTEGER,
        oduModelType               OCTET STRING,
        oduSerialNumber            OCTET STRING,
        oduID                      OCTET STRING,
        oduVersion                 OCTET STRING,
        oduSysSWver                OCTET STRING,
        oduProductCode             OCTET STRING,
        oduModelSubType            OCTET STRING,
        oduBootLoaderVersion       INTEGER,
        oduRadioModel              Unsigned32,
        oduStringBootLoaderVersion OCTET STRING
   }   
   
   oduVersionEntryIndex   OBJECT-TYPE 
      SYNTAX       INTEGER (1)
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  "
                    Index of the oduVersionEntry. 
                    It is restricted to 1 instance.
                   "
      ::= { oduVersionEntry 1 }

   oduModelType OBJECT-TYPE 
      SYNTAX       OCTET STRING
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  " 
                      The format of oduModelType is 
                      two characters which are in range
                      of '0..9' or 'A..F'    
                      
                      The value encoding is shown below:                 
                      Odu Model Type
                         - When oduModelType(b7-b6) is 00b
                           Model Type is FLEX ODU

                         - When oduModelType(b7-b6) is 01b
                           Model Type is  Intralink ODU.           
                           
                         - When oduModelType(b7-b6) is 10b
                           Model Type is  SODU ODU
                         
                         - When OID oduModelType(b7-b6) is 11b
                           and bits b5-b0 are not equal to zero 
                           Model Type is CFs.

                         - When OID oduModelType(b7-b6) is 11b
                           and bits b5-b0 are 000000b
                           Model Type is Unknown ODU.

                   "
      ::= { oduVersionEntry 2 }   
      
   oduSerialNumber OBJECT-TYPE 
      SYNTAX       OCTET STRING (SIZE (0..6))
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  "  Odu Serial Number
                      N/A for SODU ODUs"
      ::= { oduVersionEntry 3 } 

   oduID OBJECT-TYPE 
      SYNTAX       OCTET STRING (SIZE (0..4))
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  "  ODU ID 
                      N/A for SODU ODUs"
      ::= { oduVersionEntry 4 }     
    
   oduVersion OBJECT-TYPE 
      SYNTAX       OCTET STRING (SIZE (0..2))
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  "  ODU software version"
      ::= { oduVersionEntry 5 }     
      
   oduSysSWver OBJECT-TYPE 
      SYNTAX       OCTET STRING (SIZE (0..2))
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  "  ODU update software version"
      ::= { oduVersionEntry 6 }  
      
  oduProductCode OBJECT-TYPE
      SYNTAX       OCTET STRING (SIZE (0..18))
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  "  ODU Product Code
                      N/A for SODU ODUs"
      ::= { oduVersionEntry 7 } 

   oduModelSubType OBJECT-TYPE
      SYNTAX       OCTET STRING (SIZE (0..1))
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION
          " The format of oduModelSubType is two characters which are in range
             of '0..9' or 'A..F'

             The value encoding is shown below:
                      Odu Model SubType:
                         - When bit0 is 0: CFe Version-1 with RFU service channel
                         - When bit0 is 1: CFe Version-2
                          
                           bit1-bit7: Not used
          "
      ::= { oduVersionEntry 8 }   

   oduBootLoaderVersion OBJECT-TYPE
      SYNTAX       INTEGER(0..255)
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  
          "Odu Boot Loader Version Number. Valid for SODU and SF ODUs for all 
          system types except OmniBAS-XR.
          Value 255 means not available ODU."
      ::= { oduVersionEntry 9 }  

  oduRadioModel OBJECT-TYPE 
      SYNTAX       Unsigned32
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  
          "Radio Model Type. Valid only for PTP_FODU systems."
      ::= { oduVersionEntry 10 }  

   oduStringBootLoaderVersion OBJECT-TYPE
      SYNTAX       OCTET STRING
      MAX-ACCESS   read-only
