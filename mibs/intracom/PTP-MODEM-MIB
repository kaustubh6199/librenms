   oduConfig OBJECT IDENTIFIER ::= {    modemConfigMibObjects 2 }

   oduConfigTable OBJECT-TYPE
      SYNTAX       SEQUENCE OF   OduConfigEntry
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  " 
                     This table models the ODU Configuration general parameters 
                   "    
      ::= { oduConfig  1 }

   oduConfigEntry OBJECT-TYPE 
      SYNTAX       OduConfigEntry
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  " "
      INDEX  {oduConfigEntryIndex} 
      ::={oduConfigTable 1 }

    OduConfigEntry ::= SEQUENCE {                   
      
        oduConfigEntryIndex   INTEGER,
        oduBandSelection      PtpModemChannelBWType,
        oduSetTxFrequency     INTEGER,
        oduSetTxPower INTEGER,
        oduMute PtpOperationState,
        oduLoop PtpOperationState,
        oduRestart PtpRestartMode,
        oduConfigDownLoadValues INTEGER,
        oduConfigPower PtpOnOffOper,
        oduAutoUpdate  INTEGER,
        oduUpdateNow   INTEGER,
        oduTestType     INTEGER,
        oduTestTimeout  Integer32,
        oduTestTimer    Integer32,
        oduCfgDuplexSpacing INTEGER,
        oduCfgAntennaAlignMode INTEGER
   }   
   
   oduConfigEntryIndex   OBJECT-TYPE 
      SYNTAX       INTEGER (1)
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  "
                    Index of the OduConfigEntry. 
                    It is restricted to 1 instance.
                   "
      ::= { oduConfigEntry 1 }

   oduBandSelection OBJECT-TYPE 
      SYNTAX       PtpModemChannelBWType
      MAX-ACCESS   read-write
      STATUS       current
      DESCRIPTION  " 
                     PTP ODU bandwidth selection 
                       Value Range for non-V/E-BAND systems:
                           ptp7MhzBand(0) - default value 
                           ptp14MhzBand(1),   
                           ptp28MhzBand(2),   
                           ptp56MhzBand(3),
                           ptp40MhzBand(4),
                           ptp20MhzBand(5),
                           ptp14MhzNarrowBand(6),
                           ptp10MhzBand(9),
                           ptp30MhzBand(10),
                           ptp50MhzBand(11),
                           ptp112MhzBand(12)
                       Value Range (and default value) for V-BAND systems:
                           ptp250MhzBand(7),
					             Value Range E-BAND systems:
					                 ptp250MhzBand(7), - default value
						               ptp500MhzBand(8)
                       Values for E-BAND-GX systems:    
                           ptp750MhzBand (13),
                           ptp1000MhzBand (14),
                           ptp1500MhzBand (15)
                       Value for OmniBAS-BX systems
                           ptp100MhzBand (16)
                   " 
      DEFVAL { 0 }
      ::= { oduConfigEntry 2 }   
      
   oduSetTxFrequency OBJECT-TYPE 
      SYNTAX       INTEGER
      MAX-ACCESS   read-write
      STATUS       current
      DESCRIPTION  "  
                    Odu Set Tx Frequency
					non V/E-BAND systems: in KHz
					V/E-BAND systems: in MHz
                   " 
      DEFVAL {0}
      ::= { oduConfigEntry 3 } 

    oduSetTxPower OBJECT-TYPE 
      SYNTAX       INTEGER(-32767..32767)
      MAX-ACCESS   read-write
      STATUS       current
      DESCRIPTION  " 
                      ODU set Tx Power.
                      The value is calculated 
                      in 0.5dbm for products with
                      type PTP_R and
                      in 0.1dBm step for the 
                      rest products

                   "
      ::= { oduConfigEntry 4 }     

    oduMute OBJECT-TYPE 
      SYNTAX       PtpOperationState
      MAX-ACCESS   read-write
      STATUS       current
      DESCRIPTION  "  
                      ODU Mute enable/disable action  
                      Value Range  operationStateOFF(0),
                                   operationStateON(1)
                   " 
      DEFVAL {operationStateON}
      ::= { oduConfigEntry 5 }  
      
    oduLoop OBJECT-TYPE 
      SYNTAX       PtpOperationState
      MAX-ACCESS   read-write
      STATUS       current
      DESCRIPTION  " 
                     ODU Loop enable/disable action 
                      Value Range  operationStateOFF(0),
                                   operationStateON(1)

                      This OID is not used in case of 
                      ODU RFU
                   " 
      DEFVAL {operationStateOFF}

      ::= { oduConfigEntry 6 }     
   
   oduRestart OBJECT-TYPE 
      SYNTAX       PtpRestartMode
      MAX-ACCESS   read-write
      STATUS       current
      DESCRIPTION  " 
                     Action Restart ODU
                     Value Range none(0),   
                                 restart(1) 
                   " 
      DEFVAL {none}
      ::= { oduConfigEntry 7 }     
      
   oduConfigDownLoadValues OBJECT-TYPE 
       SYNTAX       INTEGER {notDownloadValue(1),
                             downloadValue(2)}
       MAX-ACCESS   read-write
       STATUS       current
       DESCRIPTION
           "     
                When set to value 2, the manager 
                 downloads the stored values of the 
                 OIDs oduBandSelection, oduSetTxFrequency,
                         oduSetTxPower.
           "
       ::= { oduConfigEntry 8 }  

    oduConfigPower OBJECT-TYPE 
      SYNTAX       PtpOnOffOper
      MAX-ACCESS   read-write
      STATUS       current
