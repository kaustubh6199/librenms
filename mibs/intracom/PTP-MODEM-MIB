   --=====================================================================
   modemConfigMibObjects  OBJECT-IDENTITY
   STATUS   current
   DESCRIPTION    " 7 objects for ptp modem MIB"
      ::= { ptpModemMIB 1 }      


   ---=========================================================
   -------------- Textual Conventions 
   ---=========================================================
  -- Definition of textual conventions                
   
   PtpPHYProfileType ::= TEXTUAL-CONVENTION

   STATUS current
   DESCRIPTION 
         " Ptp PHY profile type"
   SYNTAX INTEGER {   
      profOptimumGain(0),
      profileNormal(1),
      profileOptimumCapacity(2),
      profileExtraCapacity(3),
      profileOptimumLatency(4),
      profileEnhancedACM(5)
      }

   PtpACMode ::= TEXTUAL-CONVENTION

   STATUS current
   DESCRIPTION 
         " Ptp ACM Mode"
   SYNTAX INTEGER {   
      manual(0),
      auto(1)
      }

   PtpRestartMode ::= TEXTUAL-CONVENTION

   STATUS current
   DESCRIPTION 
         " Ptp Restart Mode"
   SYNTAX INTEGER {   
      none(0),
      restart(1)
      }

   PtpErrorStatus ::= TEXTUAL-CONVENTION

   STATUS current
   DESCRIPTION 
         " Ptp Error Status"
   SYNTAX INTEGER {   
      noErrorStatus(0),
      errorStatus(1)
      }

   PtpLinkStatus ::= TEXTUAL-CONVENTION

   STATUS current
   DESCRIPTION 
         " Ptp Link Status"
   SYNTAX INTEGER {   
      waitLock(0),
      lock(1),
      failed(2),
      waitAlign(3),
      waitLinkId(4),
      waitLinkIdAuto(5),
      sleep(6)         
      }


   PtpLinkErrorState ::= TEXTUAL-CONVENTION

   STATUS current
   DESCRIPTION 
         " Ptp Modem Error State"
   SYNTAX INTEGER {   
      success(0),
      failAtAGC(1),
      failAtTiming(2),
      failAtFreqSweep(3),
      errorInMSE(4),
      bitError(5),
      serviceError(6),
      failAtBLIND(7),
      failOnTimeOut(10),
      stopped(11),
      fatalError(12)
      }

   PtpCounterOperation ::= TEXTUAL-CONVENTION

   STATUS current
   DESCRIPTION 
         " Ptp Operation action"
   SYNTAX INTEGER {   
      stopCounter(0),
      startCounter(1),
      resetCounter(2)
      }

  PtpOperationState ::= TEXTUAL-CONVENTION

   STATUS current
   DESCRIPTION 
         " Ptp Operation State"
   SYNTAX INTEGER {   
      operationStateOFF(0),
      operationStateON(1)
      }

  PtpReferenceLevel ::= TEXTUAL-CONVENTION

   STATUS current
   DESCRIPTION 
         " Ptp Reference level "
   SYNTAX INTEGER {   
      levelHigh(0),
      levelLow(1)
      }

