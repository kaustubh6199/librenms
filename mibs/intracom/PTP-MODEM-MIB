   --===================================================================
   -- oduInterferenceScanConfigTable
   --===================================================================  
   oduInterferenceScanConfigTable OBJECT-TYPE
      SYNTAX       SEQUENCE OF   OduInterferenceScanConfigEntry
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  
          "This table models the ODU PTP Interference Scan Configuration parameters.
          Valid for STRN V-Band."    
      ::= { oduConfig  12 }

   oduInterferenceScanConfigEntry OBJECT-TYPE 
      SYNTAX       OduInterferenceScanConfigEntry
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  " "
      INDEX  { oduInterferenceScanConfigIndex  } 
      ::={ oduInterferenceScanConfigTable 1 }

    OduInterferenceScanConfigEntry ::= SEQUENCE {
        oduInterferenceScanConfigIndex      Integer32,
        oduInterferenceScanStep             Integer32
   }   
   
   oduInterferenceScanConfigIndex   OBJECT-TYPE 
      SYNTAX       Integer32 (1)
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  
          "Index of the oduInterferenceScanConfigEntry.
           It is restricted to 1 instance."
      ::= { oduInterferenceScanConfigEntry 1 }
	 
   oduInterferenceScanStep   OBJECT-TYPE
      SYNTAX        Integer32
      MAX-ACCESS    read-write
      STATUS        current
      DESCRIPTION
          "It specifies the Frequency step used to perform
          the Interference Scanning. The units are as indicated
          by the value of oduInterferenceScanStepScale."
      DEFVAL { 125 }
      ::= { oduInterferenceScanConfigEntry 2 }

   oduInterferenceScanStepScale   OBJECT-TYPE
      SYNTAX        INTEGER
                    { hz (1),
                      khz(2),
                      mhz(3),
                      ghz(4) }
      MAX-ACCESS    read-write
      STATUS        current
      DESCRIPTION
          "It specifies the Scale of the Frequency step in used to perform 
          the Interference Scanning.
          Applicable at ULFX80-CN."
      DEFVAL { mhz }
      ::= { oduInterferenceScanConfigEntry 3 }

