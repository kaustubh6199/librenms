        SYNTAX      INTEGER 
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "0..450db. Values in tenths of dB. This value should be divided by 10 (/10) in
             order to get the actual value e.g. a value of 173 stands for 173/10 = 17,3db."
        ::= { strnAntennaCurrentStatusEntry  5 }
        
   strnAntennaCurrentStatusModemLock OBJECT-TYPE
        SYNTAX      INTEGER  {lock(0),unlock(1)}
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            " Modem Lock Status."
        ::= { strnAntennaCurrentStatusEntry  6 }
	
   strnAntennaCurrentStatusAlignmentState    OBJECT-TYPE 
      SYNTAX        INTEGER {notAligned(0),
                             validatingAlignment(1),
                             aligning(2),
                             fineTuning(3),
                             aligned(4),
                             monitoring(5),
                             motorFailure(6)}
      MAX-ACCESS    read-only
      STATUS        current
      DESCRIPTION  
          "NOT_ALIGNED: The antenna has not been aligned yet, or the alignment has failed.
           VALIDATING_ALIGNMENT: The antenna is validating the saved peak (i.e. align saved)		
           ALIGNING: The antenna is currently executing the alignment procedure, including a possible validation scan (i.e. align normal).
           FINE_TUNING: The antenna is currently executing the fine-tune procedure.
           ALIGNED: The antenna has been successfully aligned (and possibly fine-tuned).
           MONITORING: The antenna has been successfully aligned, and is monitoring the signal to detect degradation and automatically fine-tune.
           MOTOR_FAILURE: mechanical issue with the antenna."
      ::= { strnAntennaCurrentStatusEntry  7 }

   strnAntennaCurrentStatusNumMeasurementElements    OBJECT-TYPE 
      SYNTAX        INTEGER
      MAX-ACCESS    read-only
      STATUS        current
      DESCRIPTION  
