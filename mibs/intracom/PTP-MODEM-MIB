      ::={oduRFUVersionTable 1 }

    OduRFUVersionEntry ::= SEQUENCE {                   
      
        oduRFUVersionIndex  INTEGER,
        oduRFUModelType     OCTET STRING,    
        oduRFUModelNumber   OCTET STRING,  
        oduRFUSerialNumber  OCTET STRING,         
        oduRFUOEM OCTET STRING, 
        oduRFUCLIE OCTET STRING,                     
        oduSWVersion OCTET STRING,                  
        oduUpdateSWVersion OCTET STRING,
        oduRfuProductCode OCTET STRING                  
    }

   oduRFUVersionIndex   OBJECT-TYPE 
      SYNTAX       INTEGER (1)
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  "
                    Index of the oduRFUVersionEntry. 
                    It is restricted to 1 instance.
                   "
      ::= { oduRFUVersionEntry 1 }

   oduRFUModelType OBJECT-TYPE 
      SYNTAX       OCTET STRING
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  " 
                      The format of oduRFUModelType is 
                      two characters which are in range
                      of '0..9' or 'A..F'    
                      
                      The value encoding is shown below:                 
                      Odu RFU Model Type
                        0x00: legacy
	                    0x01: RBW1
	                    0x02: RBW0
	                    0x03: RBW2
	                    0x04: RBW3/4	
	                    0x20: CTT-Lite ETSI
	                    0x21: CTT-Lite ANSI
	                    0x30: CTT-S     ETSI
	                    0x31: CTT-S     ANSI
	                    0x40: CTT-H    ETSI
	                    0x41: CTT-H    ANSI

                      The valid oduRFUModelType values 
                      are 0x00..0xFF and the output format 
                      will be XxXX: RFU (e.g '0x04: RFU')
                      The 0xC0 value indicates  Unknown Model type and
                      the output format will be '0xC0:  Unknown'
                        
                   "
      ::= { oduRFUVersionEntry 2 }   

      oduRFUModelNumber OBJECT-TYPE 
           SYNTAX       OCTET STRING
           MAX-ACCESS   read-only
