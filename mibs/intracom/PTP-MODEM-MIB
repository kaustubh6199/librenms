        modemSecondsInPhyChanges5  Counter32,
        modemSecondsInPhyChanges6  Counter32,
        modemSecondsInPhyChanges7  Counter32,
        modemSecondsInPhyChanges8  Counter32,
        modemSecondsInPhyChanges9  Counter32,
        modemSecondsInPhyChanges10 Counter32,
        modemSecondsInPhyChanges11 Counter32,
        modemSecondsInPhyChanges12 Counter32,
        modemSecondsInPhyChanges13 Counter32,
        modemSecondsInPhyChanges14 Counter32,
        modemSecondsInPhyChanges15 Counter32,
        modemSecondsInPhyChanges16 Counter32,
        modemCountersControl PtpCounterOperation,
        modemChangesInPHYMode1     Counter32,
        modemChangesInPHYMode2     Counter32,
        modemChangesInPHYMode3     Counter32,
        modemChangesInPHYMode4     Counter32,
        modemChangesInPHYMode5     Counter32,
        modemChangesInPHYMode6     Counter32,
        modemChangesInPHYMode7     Counter32,
        modemChangesInPHYMode8     Counter32,
        modemChangesInPHYMode9     Counter32,
        modemChangesInPHYMode10    Counter32,
        modemChangesInPHYMode11    Counter32,
        modemChangesInPHYMode12    Counter32,
        modemChangesInPHYMode13    Counter32,
        modemChangesInPHYMode14    Counter32,
        modemChangesInPHYMode15    Counter32,
        modemChangesInPHYMode16    Counter32
   }   
  
