   oduInterferenceScanStatus OBJECT-TYPE
        SYNTAX      INTEGER  { notStarted(1),
                               inProgress(2),
                               scanOK(3),
                               scanFailed(4)
                             }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            " Interference Last Scan Status."
        ::= { oduInterferenceScanStatusEntry  2 }
 
   oduInterferenceScanStatusDateTime OBJECT-TYPE
        SYNTAX         DateAndTime
        MAX-ACCESS     read-only
        STATUS    current
        DESCRIPTION
             "The date and time at which the last Interference Scan 
             Measurement has started."
        ::= { oduInterferenceScanStatusEntry 3 }   
        
   oduInterferenceScanStatusAzimuth    OBJECT-TYPE 
      SYNTAX        Integer32 (-900..900)
      MAX-ACCESS    read-only
      STATUS        current
      DESCRIPTION  
          "Antenna Azimuth Position (-900 up to 900) in tenths of degrees
           at the last interference frequency scan."
      ::= { oduInterferenceScanStatusEntry 4 }
          
   oduInterferenceScanStatusElevation    OBJECT-TYPE 
      SYNTAX        Integer32 (-150..150)
      MAX-ACCESS    read-only
      STATUS        current
      DESCRIPTION  
          "Antenna Elevation position (-150 up to 150) in tenths of degrees
           at last interference frequency scan."
      ::= { oduInterferenceScanStatusEntry 5 }

   oduInterferenceScanStatusLastStep   OBJECT-TYPE 
      SYNTAX        Integer32
      MAX-ACCESS    read-only
      STATUS        current
      DESCRIPTION  
          "It specifies the Frequency step used to perform 
          the Last Interference Scanning. The units are as indicated
          by the value of oduInterferenceScanStepScale."
      ::= { oduInterferenceScanStatusEntry 6 }

   oduInterferenceScanStatusLastNoOfFreqs   OBJECT-TYPE 
      SYNTAX        Integer32
      MAX-ACCESS    read-only
      STATUS        current
      DESCRIPTION  
          "It specifies the number of scanned frequncies in the last 
          performed interference scanning. If the scan is in-progress 
          then 0 is specified as object value."
      ::= { oduInterferenceScanStatusEntry 7 }

   oduInterferenceScanStatusMode OBJECT-TYPE
      SYNTAX      INTEGER  { local(1),
                             link(2)}
      MAX-ACCESS  read-only
      STATUS      current
      DESCRIPTION
          "Interference Last Scan Status."
      ::= { oduInterferenceScanStatusEntry  8 }

   oduInterferenceScanStatusStartFreq OBJECT-TYPE
       SYNTAX      Integer32 
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
           "First Scanned and recorded frequency.
            The units are as indicated by the value of oduInterferenceScanStepScale."
       ::= { oduInterferenceScanStatusEntry 9 }

   oduInterferenceScanStatusEndFreq OBJECT-TYPE
       SYNTAX      Integer32 
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
           "Last Scanned and recorded frequency. 
            The units are as indicated by the value of oduInterferenceScanStepScale."
       ::= { oduInterferenceScanStatusEntry 10 }

   oduInterferenceScanStatusTotalNoOfFreqs OBJECT-TYPE
       SYNTAX      Integer32
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
           "It specifies the total number of frequencies that will be scanned
            at the seesion of the scanning measurement."
       ::= { oduInterferenceScanStatusEntry 11 }

   --===================================================================
   -- oduInterferenceScanMeasureTable
   --===================================================================  
   oduInterferenceScanMeasureTable OBJECT-TYPE   
       SYNTAX      SEQUENCE OF OduInterferenceScanMeasureEntry
       MAX-ACCESS  not-accessible
       STATUS      current
       DESCRIPTION
           "This table contains the Last Scan Interference Measurements. The object
           oduInterferenceScanStatusLastNoOfFreqs includes the number of measurements
           i.e. the number of the entries of this table."
        ::= { oduConfig 14 }
        
   oduInterferenceScanMeasureEntry OBJECT-TYPE
       SYNTAX      OduInterferenceScanMeasureEntry
       MAX-ACCESS  not-accessible
       STATUS      current
       DESCRIPTION     
           "An entry in the oduInterferenceScanMeasureEntry."
       INDEX { oduInterferenceScanMeasureIndex }
       ::= { oduInterferenceScanMeasureTable 1 }

    OduInterferenceScanMeasureEntry ::= SEQUENCE {
        oduInterferenceScanMeasureIndex       	Integer32,
        oduInterferenceScanMeasureCenterFreq   	Integer32,
        oduInterferenceScanMeasureRSSI 	        Integer32,
        oduInterferenceScanMeasureSPAlarm       TruthValue
    }    

   oduInterferenceScanMeasureIndex OBJECT-TYPE
       SYNTAX      Integer32(1..32768)
       MAX-ACCESS  not-accessible
       STATUS      current
       DESCRIPTION 
           "Measurement entries."
       ::= { oduInterferenceScanMeasureEntry 1 }

   oduInterferenceScanMeasureCenterFreq OBJECT-TYPE
       SYNTAX      Integer32 
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
           "Scanned and recorded center frequency. 
            The units are as indicated by the value of oduInterferenceScanStepScale."
       ::= { oduInterferenceScanMeasureEntry 2 }   

   oduInterferenceScanMeasureRSSI OBJECT-TYPE
       SYNTAX      Integer32 
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
           "-1000..300. Values in tenths of dBm. This value should 
           be divided by 10 (/10) in order to get the actual value 
           e.g. a value of 173 stands for 173/10 = 17,3db."
       ::= { oduInterferenceScanMeasureEntry 3 }

   oduInterferenceScanMeasureSPAlarm OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "If false then no Signal Presence Alarm exists at the Radio Link
        otherwise Signal Presence Alarm exists.
       " 
    ::= { oduInterferenceScanMeasureEntry 4 }

   --===================================================================
   -- oduInterferenceScanAction (SCALAR)
   --===================================================================  
   oduInterferenceScanAction OBJECT-TYPE
       SYNTAX      INTEGER { noAction (1), 
                             startScan (2),
                             stopScan(3)
                           }
       MAX-ACCESS  read-write
       STATUS      current
       DESCRIPTION 
           "On demand action related to Interference Scanning. 
            When startScan is ordered then the local unit starts 
            Interference Scanning.
            The object is not persistent at system restart."
       ::= { oduConfig  15 }

   --===================================================================
