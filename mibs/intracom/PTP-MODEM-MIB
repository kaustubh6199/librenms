 ---=========================================================

   --=========================================================                                      
   -- MODEM MIB CONTROL OBJECTS
   --=========================================================

   --=========================================================                                      
   -- MODEM CONFIGURATION TABLE
   --=========================================================
   
   modemConfig OBJECT IDENTIFIER ::= {    modemConfigMibObjects 1 }

   
   modemConfigTable OBJECT-TYPE
      SYNTAX       SEQUENCE OF   ModemConfigEntry
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  " 
          This table models the Modem Configuration general parameters.
                   "    
      ::= { modemConfig  1 }

   modemConfigEntry OBJECT-TYPE 
      SYNTAX       ModemConfigEntry
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  " "
      INDEX  {modemConfigEntryIndex} 
      ::={modemConfigTable 1 }

    ModemConfigEntry ::= SEQUENCE {                   
      
        modemConfigEntryIndex  INTEGER,
        modemPHYprofile         PtpPHYProfileType,
        modemACMmode     PtpACMode,
        modemPHYmodulationVal      INTEGER,
        modemRestart   PtpRestartMode,
        modemConfigDownLoadValues INTEGER,
        modemLinkId  INTEGER,
        modemLinkIdCheck  INTEGER,
        modemRxSpectralInversion INTEGER,
        modemTddRole INTEGER,
        modemCfgTddTxRatio TddTxRatio,
        modemCfgAirEncryptAdminState CfgAirEncryptAdminState,
        modemCfgForceCfgSet CfgForceCfgSet
   }   
   
   modemConfigEntryIndex   OBJECT-TYPE 
      SYNTAX       INTEGER (1)
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  "Index of the ModemConfigEntry. 
                    It is restricted to 1 instance."
      ::= { modemConfigEntry 1 }

   modemPHYprofile OBJECT-TYPE 
      SYNTAX       PtpPHYProfileType
      MAX-ACCESS   read-write
      STATUS       current
      DESCRIPTION  " Modem PHY Profile 
                     Value Range  profOptimumGain(0),       
                                  profileNormal(1),         
                                  profileOptimumCapacity(2),
                                  profileExtraCapacity(3),
                                  profileOptimumLatency(4),
                                  profileEnhancedACM(5) 
      " 
      DEFVAL {profOptimumGain}
      ::= { modemConfigEntry 2 }   
      
   modemACMmode OBJECT-TYPE 
      SYNTAX       PtpACMode
      MAX-ACCESS   read-write
      STATUS       current
      DESCRIPTION  "  Modem ACM mode
                      Value Range manual(0),   
                                  auto(1)
      " 
      DEFVAL {manual}
      ::= { modemConfigEntry 3 } 

    modemPHYmodulationVal OBJECT-TYPE 
      SYNTAX       INTEGER(1..16)
      MAX-ACCESS   read-write
      STATUS       current
      DESCRIPTION  " 
                    Modem configuration physical Mode value.
                    Value Range 1..16
                   " 
      DEFVAL {1}
      ::= { modemConfigEntry 4 }     
   
   modemRestart OBJECT-TYPE 
      SYNTAX       PtpRestartMode
      MAX-ACCESS   read-write
      STATUS       current
      DESCRIPTION  " Action Restart Modem
                     Value Range none(0),      
                                 restart(1)    
      " 
      DEFVAL {none}
      ::= { modemConfigEntry 5 }  
      
   modemConfigDownLoadValues OBJECT-TYPE 
       SYNTAX       INTEGER {notDownloadValue(1),
                             downloadValue(2)}
       MAX-ACCESS   read-write
       STATUS       current
       DESCRIPTION
           "     
                When set to value 2, the manager 
                 downloads the stored values of the 
                 OIDs modemPHYprofile, modemACMmode,
                      modemPHYmodulationVal.
           "
       ::= { modemConfigEntry 6 }  

   modemLinkId OBJECT-TYPE 
       SYNTAX       INTEGER(1..65535)
       MAX-ACCESS   read-write
       STATUS       current
       DESCRIPTION  
           " This is a configuration unique number which must be identical at both sides 
             of the link. Using this number the system identifies that has link with the right 
             remote system. This object is applicable only when the object modemLinkIdCheck is 
             set as used otherwise it includes only a preconfigured value but that LinkId value 
             is neither transmitted not used to check the link validity. The object is applicable 
             only for Antenna Alignment Handling. " 
      DEFVAL { 1 }
      ::= { modemConfigEntry 7 }  

   modemLinkIdCheck OBJECT-TYPE 
       SYNTAX       INTEGER{notUsed(0),used(1)}
       MAX-ACCESS   read-write
       STATUS       current
       DESCRIPTION  
           " When modemLinkIdCheck is used(1) then modemLinkId is valid to be send to the peer 
            and used for the link validity check. The object is applicable only for Antenna Alignment 
            Handling. " 
      DEFVAL { 0 }
      ::= { modemConfigEntry 8 }  

   modemRxSpectralInversion OBJECT-TYPE
       SYNTAX           INTEGER{disable(0), enable(1)}
       MAX-ACCESS       read-write
       STATUS           current
       DESCRIPTION 
           "When modemRxSpectralInversion is enable(1) then spectral inversion is applied
             to the received signal. The object is applicable only for OmniBAS-8W/OmniBAS-2WCX."
      DEFVAL { 0 }
      ::= { modemConfigEntry 9 }
   
  modemTddRole OBJECT-TYPE
    SYNTAX INTEGER{master (1),
    slave (2)}
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    " This object indicates the role of the modem in the TDD communication in relation to its PTP peer. "
    DEFVAL { master }
    ::= { modemConfigEntry 10 }

  modemCfgTddTxRatio OBJECT-TYPE
    SYNTAX TddTxRatio
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
      "This object specifies the configured Tx-Ratio between Master and Slave at TDD 
              communication. The first value refers to the Tx Ratio of Master unit.
              The object can be configured in either Master or Slave unit but it actually applies
              in a unit that has TDD Master role."
    DEFVAL { one2one }
    ::= { modemConfigEntry 11 }

  modemCfgAirEncryptAdminState OBJECT-TYPE
    SYNTAX CfgAirEncryptAdminState
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
      "The modemCfgAirEncryptAdminState controls, on a per
            modem basis,  whether Air Data Encryption is enabled or disabled. 
            The value airEncryptionOn(1) means that Air Data Encryption is
            enabled; the value airEncryptionOff(2) means that it is not." 
    DEFVAL { airEncryptionOff }
    ::= { modemConfigEntry 12 }

  modemCfgForceCfgSet OBJECT-TYPE
    SYNTAX CfgForceCfgSet
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
      "The modemCfgForceCfgSet is used to forcefully update modem configuration.
              The value forceSetCfgTrue(1) forces the update of modem configuration.
              Currently used to force the new AES encryption key.
              The value forceSetCfgFalse(2) does nothing." 
    DEFVAL { forceSetCfgFalse }
    ::= { modemConfigEntry 13 }
   
   --=========================================================                                      
   -- MODEM PHY MODULATION TABLE
   --=========================================================
   
   modemPHYModulationTable OBJECT-TYPE
      SYNTAX       SEQUENCE OF   ModemPHYModulationEntry
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  " 
                    This table models the modem PHY Modulation parameters.
                   "    
      ::= { modemConfig  2 }

   modemPHYModulationEntry OBJECT-TYPE 
      SYNTAX       ModemPHYModulationEntry
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  " "
      INDEX  {modemPHYModulationEntryIndex} 
      ::={modemPHYModulationTable 1 }

    ModemPHYModulationEntry ::= SEQUENCE {                       
        modemPHYModulationEntryIndex  INTEGER,
        modemPHYDescription           OCTET STRING
   }   
  
   modemPHYModulationEntryIndex   OBJECT-TYPE 
      SYNTAX       INTEGER(1..16)
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  "Index of the modemPHYModulationEntry. 
                    It is restricted to 16 instances."
      ::= { modemPHYModulationEntry 1 }

   modemPHYDescription OBJECT-TYPE 
      SYNTAX    OCTET STRING (SIZE (0..80))
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  " String providing a textual description of attributes of this phymode value." 
      ::= { modemPHYModulationEntry 2 } 
 
 
   --=========================================================                                      
   -- MODEM STATUS TABLE
   --=========================================================

   modemStatusTable OBJECT-TYPE
      SYNTAX       SEQUENCE OF   ModemStatusEntry
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  " 
                     This table models the table with modem Status information. 
                   "    
      ::= { modemConfig  3 }

   modemStatusEntry OBJECT-TYPE 
      SYNTAX       ModemStatusEntry
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  " "
      INDEX  {modemStatusEntryIndex} 
      ::={modemStatusTable 1 }

    ModemStatusEntry ::= SEQUENCE {                   
      
        modemStatusEntryIndex  INTEGER,
        modemStatTemperature     INTEGER,
        modemStatFail         INTEGER,
        modemStatLinkStatus  PtpLinkStatus,
        modemStatLinkError  PtpLinkErrorState,
        modemStatLinkIndicator   INTEGER,
        modemStatPHYModValue   INTEGER,
        modemStatTxSymbolRate   Unsigned32,
        modemStatRxNormalizedMSE   INTEGER,
        modemStatRxPHYModValue  INTEGER,
        rxLdpcDecodersStress Unsigned32,
        modemprotectionMode PtpModemProtMode,
        modemPowerStatus INTEGER,
        modemAlarmThreshStatus INTEGER, 
        modemTxAirthroughput  Unsigned32,
        modemRxAirthroughput  Unsigned32,
        modemStatusSuppCapTxPhymodId   Unsigned32,
        modemStatusSuppCapRxPhymodId   Unsigned32,
        modemStatusSuppCapId  Unsigned32,
        modemActiveTddTxRatio Unsigned32,
        modemActiveTddRxRatio  Unsigned32,
        modemStatusAirLinkDistance  Unsigned32,
        modemStatusAirEncryptOperState INTEGER,
        modemActualMaxATPCTxPower INTEGER,
        modemActualMinATPCTxPower INTEGER
   }   
   
   modemStatusEntryIndex   OBJECT-TYPE 
      SYNTAX      INTEGER (1)
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  "Index of the modemStatusEntry. 
                    It is restricted to 1 instance."
      ::= { modemStatusEntry 1 }

   modemStatTemperature OBJECT-TYPE 
      SYNTAX      INTEGER(-255..255)
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  " Modem temperature.
                     The actual value is calculated with 10 " 
      ::= { modemStatusEntry 2 }   
      
   modemStatFail OBJECT-TYPE 
      SYNTAX       INTEGER
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  " Modem failure information
                     The information is bitfield.
                     Bit value meaning:

                     bit0: Clock PLL info               
                      - 0: PLL locked   (Alarm ceased)               
                      - 1: PLL UnLocked (Alarm raised)                 
                     bit1: Receiver's PLL info          
                      - 0: PLL Locked    (Alarm ceased)              
                      - 1: PLL Unlocked  (Alarm raised) 
                     bit2: Transmiter's PLL info             
                      - 0: PLL Locked     (Alarm ceased)            
                      - 1: PLL Unlocked   (Alarm raised) 
                     bit3: Modem Chip Failure              
                     - 0: Modem Chip OK        (Alarm ceased)       
                     - 1: Modem Chip Failure   (Alarm raised) 
                    bit4: Modem Chip Lock  
                     - 0: Modem Chip Locked    (Alarm ceased)
                     - 1: Modem Chip Unlocked (Alarm raised)                     
                     bit5..bit31: not used          

      " 
      ::= { modemStatusEntry 3 } 

    modemStatLinkStatus OBJECT-TYPE 
      SYNTAX       PtpLinkStatus
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  "  Link Status information
                      Value Range waitLock(0),       
                                  lock(1),          
                                  failed(2),
    				              waitAlign(3),
                                  waitLinkId(4),
                                  waitLinkIdAuto(5),
                                  sleep(6)         
         
      " 
      ::= { modemStatusEntry 4 } 
      
    modemStatLinkError OBJECT-TYPE 
      SYNTAX       PtpLinkErrorState
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  " Link Error information
                     Value Range success(0),             
                                 failAtAGC(1),           
                                 failAtTiming(2),        
                                 failAtFreqSweep(3),     
                                 errorInMSE(4),          
                                 bitError(5),            
                                 serviceError(6),        
                                 failAtBLIND(7),         
                                 failOnTimeOut(10),      
                                 stopped(11),            
                                 fatalError(12)
      " 
      ::= { modemStatusEntry 5 } 

   modemStatLinkIndicator OBJECT-TYPE 
      SYNTAX       INTEGER
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  "
        Lock indicators.
        The information is bitfield.
        In the following lines there is the bit value meaning:
            bit0: Timing  
                - 0: Unlocked 
                - 1: Locked
                    
            bit1..2: Preample
                - 0: Unlocked 
                - 1: Verified 
                - 2: Locked  
                - rest valus unused  
    
            bit3: AGC Lock
                - 0: UnLocked    
                - 1: Locked  
                Valid for NEPTUNE modem only. Not supported in VBAND system

            bit4: IMC (Internal Management Communication) Failure:
                - 0: False
                - 1: True   
                               
            bit5: LDPC Unlock
                - 0: Locked   
                - 1: Unlocked 

            bit6: LDPC corrected BER
                - 0: Off    
                - 1: On  
                Valid for NEPTUNE modem only

            bit7: LDPC uncorrected BER
                - 0: Off    
                - 1: On  
                Valid for NEPTUNE modem only

            bit8: GFP BER
                - 0: Off    
                - 1: On  
                Valid for NEPTUNE modem only. Not supported in VBAND system

            bit9: CRS Init
                - 0: Off    
                - 1: On  
                Valid for NEPTUNE modem only. Not supported in VBAND system

            bit10: Superframe Lock
                - 0: Unlocked
                - 1: Locked
                Valid for NEPTUNE modem only

            bit11: GFP Lock
                - 0: UnLocked    
                - 1: Locked
                Valid for NEPTUNE modem only

            bit12: L1 RAI Received
                - 0: Off    
                - 1: On  
                Valid for NEPTUNE modem only

            bit13: MSE Lock
                - 0: UnLocked    
                - 1: Locked
                Valid for NEPTUNE modem only

            bit14: Yellow Alarm
                - 0: ceased    
                - 1: raised
                When raised check the bits 0,3,6,7,8,9,13 of modemStatLinkIndicator
                Valid for NEPTUNE modem only

            bit15: Red Alarm
                - 0: ceased    
                - 1: raised
                When raised check the bits 1,2,5,10,11 of modemStatLinkIndicator
                Valid for NEPTUNE modem only

            bit16: MSE_minPhymode Lock
                -0:UnLocked
                -1:Locked
                Valid for NEPTUNE modem only

            bit17: Min Phymode Lock
                -0:UnLocked
                -1:Locked
                Valid for NEPTUNE modem only     

            bit18: XPIC Active Status
                -0:Inactive
                -1:Active
                Valid for NEPTUNE XPIC modem only

            bit19: XPIC Lock
                -0:UnLocked
                -1:Locked
                Valid for NEPTUNE XPIC modem only

            bit20: XPIC_PAIR Comm Lock
                -0:UnLocked
                -1:Locked
                Valid for NEPTUNE XPIC modem only

            bit21: XPIC_Sweeper Inactive Status
                -0:Active
                -1:Inactive
                
            bit22..bit31: not used
        "
      ::= { modemStatusEntry 6 }   
      
    modemStatPHYModValue OBJECT-TYPE 
      SYNTAX       INTEGER(1..16)
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  "  Current Tx PHY Modulation value.
                      Value Range 1..16.
                      This value is used as index in the modemPHYModulationEntry.
                   " 
      ::= { modemStatusEntry 7 } 
  
    modemStatTxSymbolRate OBJECT-TYPE 
      SYNTAX      Unsigned32
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  " Transimit Symbol rate information for incoming flow " 
      ::= { modemStatusEntry 8 } 

    modemStatRxNormalizedMSE OBJECT-TYPE 
      SYNTAX      INTEGER(-255..255)
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  "  Information about received normalized MSE
                      This info is counted in 0.1 dB steps." 
      ::= { modemStatusEntry 9 } 

    modemStatRxPHYModValue OBJECT-TYPE 
      SYNTAX       INTEGER(1..16)
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  "  Current Rx PHY Modulation value.
                      Value Range 1..16
                      This value is used as index in the modemPHYModulationEntry.
                                             Value 16 means not valid  profile.                                            
                   "
      ::= { modemStatusEntry 10 } 
     
    rxLdpcDecodersStress OBJECT-TYPE 
      SYNTAX      Unsigned32
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  " Receive LDPC Decodes Stress 
                   "
      ::= { modemStatusEntry 11 } 
  
    modemprotectionMode OBJECT-TYPE 
      SYNTAX      PtpModemProtMode
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  " Modem Protection Mode 
                     Value Range none(0),      
                                 working(1),   
                                 protect(2)
      "
      ::= { modemStatusEntry 12 } 

      
   modemPowerStatus OBJECT-TYPE 
      SYNTAX       INTEGER
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  "
        Modem Power Power status
        The information is bitfield.
        In the following lines there is the value meaning of each bit:
            bit0: Power Fault
            bit1: Fault 48V ODU
            bit2: Fault 48V
            bit3: Power Fault of 1.2V
            bit4: Power Fault of 1.8V
            bit5: Power Fault of 2.5V
            bit6: Power Fault of 10V and 5.1V
            bit7..bit31: not used               
        "
      ::= { modemStatusEntry 13 }   

   modemAlarmThreshStatus OBJECT-TYPE 
      SYNTAX       INTEGER
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  "
        Modem threshhold status Alarm.
        The information is bitfield.
        In the following lines there is the value meaning of each bit:
            bit0: Rx NormalizedMSE Alarm (SNR)
                  - 0: Alarm ceased  
                  - 1: Alarm raised  
            bit1: Rx Ldpc Decoders Stress Alarm
                  - 0: Alarm ceased
                  - 1: Alarm raised
            bit2: Modem PHY modulation configuration is not supported.                                                    
                  The Alarm is raised, when the modem Physical Modulation value stored in
                  the configuration, is not supported by the inserted modem in the system.                                            
                  - 0: Alarm ceased
                  - 1: Alarm raised
            bit3: The Alarm is raised when modemStatTemperature exceeds an internal
                  High temperature threshold.
                  The Alarm is ceased when modemStatTemperature falls lower than an
                  internal Low temperature threshold.
                  - 0: Alarm ceased
                  - 1: Alarm raised
            bit4..bit31: not used
        "
      ::= { modemStatusEntry 14 }
      
   modemTxAirthroughput OBJECT-TYPE 
      SYNTAX    Unsigned32
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  " 
                     Max Tx throughput on the air 
                     Applicable only for the NEPTUNE modems
                   " 
      ::= { modemStatusEntry 15 }   
      
   modemRxAirthroughput OBJECT-TYPE 
      SYNTAX    Unsigned32
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  " 
                     Max Rx throughput on the air  
                     Applicable only for the NEPTUNE modems
                   " 
      ::= { modemStatusEntry 16 }   

    modemStatusSuppCapTxPhymodId   OBJECT-TYPE
        SYNTAX              Unsigned32 (1..16)
        MAX-ACCESS          read-only
        STATUS              current
        DESCRIPTION
           " Modem Tx PHYmode Index 1 to 16, i.e. the modemSuppCapPhymodIndex 
            for the Tx PHYmode of the specific Modem Capabilities combination indicated 
            by modemSuppCapabilitiesIndex. The Indexing from 1 to 16 corrresponds to 
            indexing of phymodes from lower air capacity to higher air capacity."
        ::= { modemStatusEntry 17 }

    modemStatusSuppCapRxPhymodId   OBJECT-TYPE
        SYNTAX              Unsigned32 (1..16)
        MAX-ACCESS          read-only
        STATUS              current
        DESCRIPTION
           " Modem Rx PHYmode Index 1 to 16, i.e. the modemSuppCapPhymodIndex 
            for the Rx PHYmode of the specific Modem Capabilities combination indicated 
            by modemStatusSuppCapId. The Indexing from 1 to 16 corrresponds to 
            indexing of phymodes from lower air capacity to higher air capacity."
        ::= { modemStatusEntry 18 }

   modemStatusSuppCapId   OBJECT-TYPE
       SYNTAX              Unsigned32
       MAX-ACCESS          read-only
       STATUS              current
       DESCRIPTION
          " The index of current supported Modem Capabilities combination, 
            in the ModemSuppCapabilitiesTable. "
       ::= { modemStatusEntry 19 }
	   
   modemActiveTddTxRatio OBJECT-TYPE
       SYNTAX           Unsigned32 (1..1000)
       MAX-ACCESS       read-only
       STATUS           current
       DESCRIPTION 
           "This object specifies the parts per thousand of the current
            Active Tx rate in relation to the maximum aggregate rate."
      ::= { modemStatusEntry 20 }

   modemActiveTddRxRatio OBJECT-TYPE
       SYNTAX           Unsigned32 (1..1000)
       MAX-ACCESS       read-only
       STATUS           current
       DESCRIPTION
           "This object specifies the parts per thousand of the current
            Active Rx rate in relation to the maximum aggregate rate."
      ::= { modemStatusEntry 21 }

   modemStatusAirLinkDistance OBJECT-TYPE
       SYNTAX        Unsigned32
       UNITS         "centimeters"
       MAX-ACCESS    read-only
       STATUS        current
       DESCRIPTION
           "The TDD measured air link distance. Valid only after the link
            has been established."
       ::= { modemStatusEntry 22 }

   modemStatusAirEncryptOperState   OBJECT-TYPE
       SYNTAX INTEGER {
                        working (1),
                        notWorking (2)
                      }
       MAX-ACCESS          read-only
       STATUS              current
       DESCRIPTION
          " The object indicates the operational state of the Air Encryption.
            Not Working reasons can be due to Air Encryption being disabled at the
            peer modem or because the peer mode has a different Encryption key. "
       ::= { modemStatusEntry 23 }

   modemActualMaxATPCTxPower   OBJECT-TYPE 
      SYNTAX       INTEGER (-32767..32767)
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  
          "Modem Actual Maximum ATPC Tx Power value in tenths of dBms" 
      ::= { modemStatusEntry 24 }

   modemActualMinATPCTxPower   OBJECT-TYPE 
      SYNTAX       INTEGER (-32767..32767)
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  
          "Modem Actual Minimum ATPC Tx Power value in tenths of dBms" 
      ::= { modemStatusEntry 25 }

