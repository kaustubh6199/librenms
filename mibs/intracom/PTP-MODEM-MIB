      ::= { oduVersionEntry 11 }

  --=========================================================                                      
   -- ODU ALARM CONFIGURATION TABLE
   --========================================================

   oduAlarmConfigurationTable OBJECT-TYPE
      SYNTAX       SEQUENCE OF   OduAlarmConfigurationEntry
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  " 
                     This table models the ODU Alarm Configuration information 
                   "    
      ::= { oduConfig  6 }

   oduAlarmConfigurationEntry OBJECT-TYPE 
      SYNTAX       OduAlarmConfigurationEntry
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  " "
      INDEX  {oduAlarmConfigurationEntryIndex} 
      ::={oduAlarmConfigurationTable 1 }

    OduAlarmConfigurationEntry ::= SEQUENCE {                   
      
        oduAlarmConfigurationEntryIndex  INTEGER,
        oduPinRSSIThres  INTEGER,
        oduTempThres     INTEGER,
        oduAlarmConfDownLoadValues INTEGER,
        oduAlarmConfTempLoThresh INTEGER,
        oduAlarmConfLinkQualityAdminState TruthValue,
        oduAlarmConfSupprState BITS
   }   
   
   oduAlarmConfigurationEntryIndex   OBJECT-TYPE 
      SYNTAX       INTEGER (1)
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  "
                    Index of the OduAlarmConfigurationEntry. 
                    It is restricted to 1 instance.
                   "
      ::= { oduAlarmConfigurationEntry 1 }

   oduPinRSSIThres OBJECT-TYPE 
      SYNTAX       INTEGER(0..255)
      MAX-ACCESS   read-write
      STATUS       current
      DESCRIPTION  " 
                     Odu RSSI Threshold
                     Value range 0..255
                     The value is multiplied
                      with -1dbm 
                     "
      DEFVAL {0}
      ::= { oduAlarmConfigurationEntry 2 }   
      
   oduTempThres OBJECT-TYPE 
      SYNTAX       INTEGER(-128..127)
      MAX-ACCESS   read-write
      STATUS       current
      DESCRIPTION  "  
                     Odu High temperature threshold
                     Value range -128..127
                   "
      DEFVAL {65}
      ::= { oduAlarmConfigurationEntry 3 } 

   oduAlarmConfDownLoadValues OBJECT-TYPE 
       SYNTAX       INTEGER {notDownloadValue(1),
                             downloadValue(2)}
       MAX-ACCESS   read-write
       STATUS       current
       DESCRIPTION
           "     
                When set to value 2, the manager 
                 downloads the stored values of the 
                 OIDs oduPinRSSIThres, oduTempThres.
           "
       ::= { oduAlarmConfigurationEntry 4 }  

   oduAlarmConfTempLoThresh OBJECT-TYPE 
      SYNTAX       INTEGER(-128..127)
      MAX-ACCESS   read-write
      STATUS       current
      DESCRIPTION  "  
                     Odu Low temperature threshold
                     Value range -128..127
                   "
      DEFVAL {0}
      ::= { oduAlarmConfigurationEntry 5 } 

   oduAlarmConfLinkQualityAdminState OBJECT-TYPE 
      SYNTAX       TruthValue
      MAX-ACCESS   read-write
      STATUS       current
      DESCRIPTION  "  
                     When true, the ODU Link Quality Alarms Notifications, oduSlowFadingDetection,
                     oduFastFadingDetection, oduCoChannelInterfereDetection, oduStrongAdjChannelPresence,
                     are enabled to be spawned otherwise no ODU Link Quality Alarm is issued.
                     Similarly when false, the ODU link Quality Alarm Statuses, oduSlowFadingStatus,
                     oduFastFadingStatus, oduCoChannelInterfereDetectStatus, oduStrongAdjChannelPresenceStatus,
                     oduStatusBRAL, are not available.
                   " 
      DEFVAL { false }
      ::= { oduAlarmConfigurationEntry 6 }

   oduAlarmConfSupprState OBJECT-TYPE
       SYNTAX      BITS {
                     sfd(0),
                     ffd(1),
                     coChannelInterfereDetect(2),
                     strongAdjChannelPresence(3) }
       MAX-ACCESS  read-write
       STATUS      current
       DESCRIPTION
          "The object sets the suppression state of the Radio Link Quality Alarms.  The
           values mean:

             sfd
                When bit 0 is set, the Radio Link Quality Alarms Notification, oduSlowFadingDetection,
                is suppressed and not spawned when oduAlarmConfLinkQualityAdminState is enabled.

             ffd
                When bit 1 is set, the Radio Link Quality Alarms Notification, oduFastFadingDetection,
                is suppressed and not spawned when oduAlarmConfLinkQualityAdminState is enabled.

             CoChannelInterfereDetect
                When bit 2 is set, the Radio Link Quality Alarms Notification, oduCoChannelInterfereDetection,
                is suppressed and not spawned when oduAlarmConfLinkQualityAdminState is enabled.

             StrongAdjChannelPresence
                When bit 3 is set, the Radio Link Quality Alarms Notification, oduStrongAdjChannelPresence,
                is suppressed and not spawned when oduAlarmConfLinkQualityAdminState is enabled." 
       DEFVAL { {} }
       ::= { oduAlarmConfigurationEntry 7 }

