   --=========================================================
   --  MODEM PHC TABLE
   --=========================================================

   ptpFlexModemPHCTable OBJECT-TYPE
      SYNTAX       SEQUENCE OF  PtpFlexModemPHCEntry
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  "Table that models Packet Header Compression Header configuration information."
      ::= { modemConfig  24}

   ptpFlexModemPHCEntry OBJECT-TYPE
      SYNTAX       PtpFlexModemPHCEntry
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  ""
      INDEX {ptpFlexModemPHCIndex}
      ::= { ptpFlexModemPHCTable 1 }

   PtpFlexModemPHCEntry ::= SEQUENCE {
      ptpFlexModemPHCIndex  Unsigned32,
      ptpFlexModemPHCState  TruthValue,
      ptpFlexModemPHCAging Unsigned32,
      ptpFlexModemPHCRobustness TruthValue,
      ptpFlexModemPHCMode  BITS,
      ptpFlexModemPHCAutoResetTimer Unsigned32
   }

   ptpFlexModemPHCIndex     OBJECT-TYPE
      SYNTAX       Unsigned32
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION
         "The modem index."
      ::= { ptpFlexModemPHCEntry 1 }

   ptpFlexModemPHCState OBJECT-TYPE
       SYNTAX       TruthValue
       MAX-ACCESS   read-write
       STATUS       current
       DESCRIPTION
           " Enable (true) or Disable (false) the Packet Header Compression."
       DEFVAL {false}
       ::= { ptpFlexModemPHCEntry 2 }

   ptpFlexModemPHCAging OBJECT-TYPE
       SYNTAX       Unsigned32 (1..511)
       UNITS        "seconds"
       MAX-ACCESS   read-write
       STATUS       current
       DESCRIPTION
           " Aging Timeout for each compressed Packet Header learnt by the system
             Value range    1..511
             All other values are not used.
             Default value 10 seconds."
       DEFVAL {10}
       ::= { ptpFlexModemPHCEntry 3 }

   ptpFlexModemPHCRobustness OBJECT-TYPE
       SYNTAX       TruthValue
       MAX-ACCESS   read-write
       STATUS       current
       DESCRIPTION
           " When value is set to true, a more efficient application of MAC Header
             Compression feature is utilized in traffic cases with unstable Radio Link
             (due to fading, interference etc)."
       DEFVAL {true}
       ::= { ptpFlexModemPHCEntry 4 }

   ptpFlexModemPHCMode OBJECT-TYPE
       SYNTAX       BITS { l2eth(0),
                           mpls(1),
                           ipv4(2),
                           l4port(3)
      }
       MAX-ACCESS   read-write
       STATUS       current
       DESCRIPTION
           " Specify the Mode of Packet Header Compression, the fields
           that will be compressed. Various combinations of fields compressions
           are supported.

            The bit l2eth (0) indicates that the Layer 2 Ethernet Header will be compressed.

            The bit mpls (1) indicates that MPLS label(s) will be compressed.

            The bit ipv4 (2) indicates that IPv4 Header will be compressed.

            The bit l4port (3) indicates L4 Port ids will be compressed.

            The default value for ptpFlexModemPHCMode object is
            empty set, which means no enumerated values are set."
       DEFVAL { { } }
       ::= { ptpFlexModemPHCEntry 5 }

   ptpFlexModemPHCAutoResetTimer OBJECT-TYPE
       SYNTAX       Unsigned32 (0..4095)
       UNITS        "seconds"
       MAX-ACCESS   read-write
       STATUS       current
       DESCRIPTION
           "Auto Reset Timeout of all learnt Packet Header Compression for modem.
            The value is counted in seconds
            Value range    0..4095 sec
            All other values are not used.
            Default value = 30, i.e. no auto-reset."
       DEFVAL { 30 }
       ::= { ptpFlexModemPHCEntry 6 }


   --================================================================ 
   -- PTP NOTIFICATIONS OBJECTS
   --================================================================
   
   ptpNotificationObjects OBJECT IDENTIFIER 
   ::= { modemConfig  7}     --- traps
   
   ptpModemFailureChange NOTIFICATION-TYPE
      OBJECTS {modemStatFail}
      STATUS current
      DESCRIPTION "
                    A trap indicating modem connection failure status                                   
                    [OmniBAS-2W, OmniBAS-4W  implementation]
                        The NMS forwards the following information to the operator:
                      
                        varbind[0].oid = oid_sysUpTime.0 = 1.3.6.1.2.1.1.3.0
                        varbind[0].val = NMS agent time (in centiseconds)
                   
                        varbind[1].oid = oid_snmpTrapOID.0 = 1.3.6.1.6.3.1.1.4.1.0
                        varbind[1].val = ptpModemFailureChange
                                       = 1.3.6.1.4.1.1807.55.1.1.7.1
                        varbind[2].oid = OidmodemStatFail
                                       = 1.3.6.1.4.1.1807.55.1.1.3.1.3
                        varbind[2].val = modemStatFail
                    
                      where
                      -- modemStatFail is the driver received status information
                          Modem failure information
                          The information is bitfield.
                          Bit value meaning:
                      
                          bit0: Clock PLL info               
                           - 0: PLL locked   (Alarm ceased)               
                           - 1: PLL UnLocked (Alarm raised)                 
                          bit1: Receiver's PLL info          
                           - 0: PLL Locked    (Alarm ceased)              
                           - 1: PLL Unlocked  (Alarm raised) 
                          bit2: Transmiter's PLL info             
                           - 0: PLL Locked     (Alarm ceased)            
                           - 1: PLL Unlocked   (Alarm raised) 
                          bit3: Modem Chip Failure              
                          - 0: Modem Chip OK        (Alarm ceased)       
                          - 1: Modem Chip Failure   (Alarm raised) 
                          bit4: Modem Chip Lock  
                           - 0: Modem Chip Locked    (Alarm ceased)
                           - 1: Modem Chip Unlocked (Alarm raised)                   
                          bit5..bit31: not used 
                  "
         ::= { ptpNotificationObjects 1}
   
   ptpmodemStatLinkStatusChange NOTIFICATION-TYPE
      OBJECTS {modemStatLinkStatus}
      STATUS current
      DESCRIPTION "
                    The trap signals the changes in Link status 
                    [OmniBAS-2W, OmniBAS-4W  implementation]
                    The NMS forwards the following information to the operator:
                     
                       varbind[0].oid = oid_sysUpTime.0 = 1.3.6.1.2.1.1.3.0
                       varbind[0].val = NMS agent time (in centiseconds)
                   
                       varbind[1].oid = oid_snmpTrapOID.0 = 1.3.6.1.6.3.1.1.4.1.0
                       varbind[1].val = ptpModemFailureChange
                                      = 1.3.6.1.4.1.1807.55.1.1.7.2
                       varbind[2].oid = OidModemStatLinkStatus
                                      = 1.3.6.1.4.1.1807.55.1.1.3.1.4
                       varbind[2].val = modemStatLinkStatus
                    
                     where
                     -- modemStatLinkStatus is the driver received link status information
                        (waitLock(0), lock(1), failed(2))
                  "
         ::= { ptpNotificationObjects 2}

   ptpRemoteModemStatLinkStatusChange NOTIFICATION-TYPE
      OBJECTS {remoteModemLinkStatus}
      STATUS current
      DESCRIPTION "
                    The trap signals the changes in remote Link status.  
                    [OmniBAS-2W, OmniBAS-4W  implementation]                  
                    The NMS forwards the following information to the operator:
                   
                     varbind[0].oid = oid_sysUpTime.0 = 1.3.6.1.2.1.1.3.0
                     varbind[0].val = NMS agent time (in centiseconds)
                   
                     varbind[1].oid = oid_snmpTrapOID.0 = 1.3.6.1.6.3.1.1.4.1.0
                     varbind[1].val = ptpRemoteModemStatLinkStatusChange
                                    = 1.3.6.1.4.1.1807.55.1.1.7.3
                     varbind[2].oid = OidRemoteModemLinkStatus
                                    = 1.3.6.1.4.1.1807.55.1.1.9.1.2
                     varbind[2].val = remoteModemLinkStatus
                   
                   where
                   -- remoteModemLinkStatus is the driver received remote link status information
                      (unLocked(0), locked(1), offLine(2), linkStatusUnknown(3) )
                  "
         ::= { ptpNotificationObjects 3}

   ptpXpicModeAlarm NOTIFICATION-TYPE
      OBJECTS {modemXpicConfEnabled}
      STATUS current
      DESCRIPTION "
                    The trap signals the changes in xpic modem configuration mode.  
                    [OmniBAS-4W  implementation]                  
                    The NMS forwards the following information to the operator:
                   
                     varbind[0].oid = oid_sysUpTime.0 = 1.3.6.1.2.1.1.3.0
                     varbind[0].val = NMS agent time (in centiseconds)
                   
                     varbind[1].oid = oid_snmpTrapOID.0 = 1.3.6.1.6.3.1.1.4.1.0
                     varbind[1].val = ptpXpicModeAlarm
                                    = 1.3.6.1.4.1.1807.55.1.1.7.4
                     varbind[2].oid = oid_modemXpicConfEnabled
                                    = 1.3.6.1.4.1.1807.55.1.1.14.1.2
                     varbind[2].val = modemXpicConfEnabled
                   
                   where
                   -- modemXpicConfEnabled is the driver received xpic modem mode information
                      (xpicenabled(1), xpicdisabled(2))
            
                  "
         ::= { ptpNotificationObjects 4}

   ptpmodemAlarmThreshStatusAlarm NOTIFICATION-TYPE
      OBJECTS {modemAlarmThreshStatus}
      STATUS current
      DESCRIPTION "
                    The trap signals the changes in modem threshhold status.  
                    [OmniBAS-2W, OmniBAS-4W  implementation]                  
                    The NMS forwards the following information to the operator:
                   
                     varbind[0].oid = oid_sysUpTime.0 = 1.3.6.1.2.1.1.3.0
                     varbind[0].val = NMS agent time (in centiseconds)
                   
                     varbind[1].oid = oid_snmpTrapOID.0 = 1.3.6.1.6.3.1.1.4.1.0
                     varbind[1].val = ptpmodemAlarmThreshStatusAlarm
                                    = 1.3.6.1.4.1.1807.55.1.1.7.5
                     varbind[2].oid = oid_modemAlarmThreshStatus
                                    = 1.3.6.1.4.1.1807.55.1.1.3.1.14
                     varbind[2].val = modemAlarmThreshStatus
                   
                   where
                   -- modemAlarmThreshStatus is the driver received modem threshhold information.
                                 bit0: Rx NormalizedMSE Alarm (SNR)
                                       - 0: Alarm ceased  
                                       - 1: Alarm raised  
                                 bit1: Rx Ldpc Decoders Stress Alarm
                                       - 0: Alarm ceased
                                       - 1: Alarm raised
                                 bit2: Modem PHY modulation configuration is not supported.                                                    
                                       The Alarm is raised, when the modem Physical Modulation value stored in
                                       the configuration, is not supported by the inserted modem in the system.                                            
                                       - 0: Alarm ceased
                                       - 1: Alarm raised
                                 bit3: The Alarm is raised when modemStatTemperature exceeds an internal
                                       High temperature threshold.
                                       The Alarm is ceased when modemStatTemperature falls lower than an
                                       internal Low temperature threshold.
                                       - 0: Alarm ceased
                                       - 1: Alarm raised
                                 bit4..bit31: not used
                  "
         ::= { ptpNotificationObjects 5}

   ptpModemTxPHYModulationChange NOTIFICATION-TYPE
      OBJECTS {modemPHYDescription}
      STATUS current
      DESCRIPTION "
                    The trap signals the changes in PHY Modulation Description for Tx direction.  
                    [OmniBAS-2W, OmniBAS-4W  implementation]                  
