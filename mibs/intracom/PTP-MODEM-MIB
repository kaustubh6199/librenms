                   " 
      ::= { g826CountersEntry 6 } 

  unavailableSecondsCounter OBJECT-TYPE 
      SYNTAX    Counter32
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  " 
                     The OID represents the 
                    the unavailable seconds counter.
                   " 
      ::= { g826CountersEntry 7 } 
      
    g826CounterControl OBJECT-TYPE 
      SYNTAX    INTEGER{ none(0),
                         reset(1)
                         }
      MAX-ACCESS   read-write
      STATUS       current
      DESCRIPTION  " 
                    The OID used to reset the g826 counters. 
   
                   " 
      DEFVAL {none}
      ::= { g826CountersEntry 8 }


 --==========================================================
   ptpFlexModemMHSTable OBJECT-TYPE 
      SYNTAX       SEQUENCE OF  PtpFlexModemMHSEntry
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  " Table that models Modem MAC SuppressionHeader configuration information of an OMNIBAS system,4wHE or 2w16E1 or XR"
      ::= { modemConfig  17}

   ptpFlexModemMHSEntry OBJECT-TYPE 
      SYNTAX       PtpFlexModemMHSEntry
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  ""
      INDEX {ptpFlexModemMHSIndex}
      ::= { ptpFlexModemMHSTable 1 }    

   PtpFlexModemMHSEntry ::= SEQUENCE { 

   ptpFlexModemMHSIndex     INTEGER,
   ptpFlexModemMHSSupprState  INTEGER,
   ptpFlexModemMHSAging INTEGER,
   ptpFlexModemMHSRobustness INTEGER,
   ptpFlexModemMHSAutoResetTimer INTEGER
   }
   
   ptpFlexModemMHSIndex     OBJECT-TYPE 
      SYNTAX       INTEGER (1..8)
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  
         "The number of modems in an OmniBas system
	   The value range of the index is system depended 
	   as shown below:
           OB-4W-HE the range is 1..4
	   OB-2w16E1 the range is 1..2
           OB-XR the range is 1
	 "
      ::= { ptpFlexModemMHSEntry 1 }  

ptpFlexModemMHSSupprState OBJECT-TYPE 
       SYNTAX       INTEGER { disable (0), enable (1)}
       MAX-ACCESS   read-write
       STATUS       current
       DESCRIPTION
           " Enable or Disable the MAC Address Suppression
              Value range 
	      FlexModemMHSSupprState 0 disabled
	      FlexModemMHSSupprState 1 enabled
	      All other values are not used.
	      default value disable(0)
	   "
       DEFVAL {disable}    
       ::= { ptpFlexModemMHSEntry  2}  
	
ptpFlexModemMHSAging OBJECT-TYPE 
       SYNTAX       INTEGER (1..511)
       MAX-ACCESS   read-write
       STATUS       current
       DESCRIPTION
           " Aging Timeout of the suppressed MAC Addresses learnt by the system
	     Value range    1..511
	     All other values are not used.
	     Default value 10 
	   "
       DEFVAL {10}    
       ::= { ptpFlexModemMHSEntry 3}  

ptpFlexModemMHSRobustness OBJECT-TYPE 
       SYNTAX       INTEGER { on (7), off (3)}
       MAX-ACCESS   read-write
       STATUS       current
       DESCRIPTION
           "              
             When value is set to on, a more efficient application of MAC Header S
             uppression feature is utilized in traffic cases with unstable Radio Link 
             (due to fading, interference etc).

              Value Range: {on | off}
              Default value: on
	          All other values are not used. 
	   "
       DEFVAL {on}    
       ::= { ptpFlexModemMHSEntry 4}  

ptpFlexModemMHSAutoResetTimer OBJECT-TYPE 
       SYNTAX       INTEGER (0..4095)
       MAX-ACCESS   read-write
       STATUS       current
       DESCRIPTION
           " 
                CAM Auto Reset Timeout of the MAC header suppressor for modem 
                The value is counted in seconds
	            Value range    0..4095 sec
	            All other values are not used.
	            Default value = 0, i.e. no CAM auto-reset.
	   "
       DEFVAL {0}    
       ::= { ptpFlexModemMHSEntry 5} 

