      INDEX  {g826CounterIndex} 
      ::={g826CountersTable 1 }

    G826CountersEntry ::= SEQUENCE {
        g826CounterIndex  INTEGER,
        errorBlockCounter   Counter64,
        backGroundBlockErrorsCounter  Counter64,
        errorSecondsCounter Counter32,
        severelyErroredSecondsCounter Counter32,
        availlableSecCounter Counter32,
        unavailableSecondsCounter Counter32,
        g826CounterControl INTEGER
     }   
  
   g826CounterIndex   OBJECT-TYPE 
      SYNTAX       INTEGER (1)
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  "Index of the G826CountersEntry. 
                   It is restricted to 1 instance.
                   "
      ::= { g826CountersEntry 1 }

  errorBlockCounter OBJECT-TYPE 
      SYNTAX    Counter64
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  " 
                     The OID represents the 
                    the error block counter.
                   " 
      ::= { g826CountersEntry 2 } 

  backGroundBlockErrorsCounter OBJECT-TYPE 
      SYNTAX    Counter64
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  " 
                     The OID represents the 
                    the background block error 
                    counter.
                   " 
      ::= { g826CountersEntry 3 } 

  errorSecondsCounter OBJECT-TYPE 
      SYNTAX    Counter32
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  " 
                     The OID represents the 
                    the error seconds counter.
                   " 
      ::= { g826CountersEntry 4 } 

  severelyErroredSecondsCounter OBJECT-TYPE 
      SYNTAX    Counter32
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  " 
                     The OID represents the 
                     the severely errored seconds
                     counter.
                   " 
      ::= { g826CountersEntry 5 } 

     availlableSecCounter OBJECT-TYPE 
      SYNTAX    Counter32
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  " 
                     The OID represents the 
