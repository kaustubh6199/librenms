   oduServChanStatTable OBJECT-TYPE
      SYNTAX       SEQUENCE OF   OduServChanStatEntry
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  " 
                     This table models the ODU 
                     service channel statistics
                   "    
      ::= { oduConfig  10 }

   oduServChanStatEntry OBJECT-TYPE 
      SYNTAX       OduServChanStatEntry
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  " "
      INDEX  {oduServChanStatIndex} 
      ::={oduServChanStatTable 1 }

    OduServChanStatEntry ::= SEQUENCE {                   
      
        oduServChanStatIndex  INTEGER,
        oduCrcErrors         Counter32,    
        oduTotalCrcErrors   Counter32,  
        iduCrcErrors  Counter32,         
        oduDisconnects Counter32, 
        oduResetCmds Counter32,                     
        oduCountCtrl INTEGER,
        oduSfdAlarmsCounter Counter64,
        oduFfdAlarmsCounter Counter64,
        oduLinkQualityMonUpTime TimeTicks,
        oduCoChannelInterfereDetectAlarmsCounter Counter64,
        oduStrongAdjChannelPresenceAlarmsCounter Counter64
    }

   oduServChanStatIndex   OBJECT-TYPE 
      SYNTAX       INTEGER (1)
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  "
                    Index of the oduServChanStatEntry. 
                    It is restricted to 1 instance.
                   "
      ::= { oduServChanStatEntry 1 }


    oduCrcErrors OBJECT-TYPE 
      SYNTAX       Counter32
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  " 
            ODU CRC Errors	 : The CRC errors of the ODU
            Not affected by Counters Control
			and reset only with ODU reset.

    "
      ::= { oduServChanStatEntry 2 }      

    oduTotalCrcErrors OBJECT-TYPE 
      SYNTAX       Counter32
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  " 

        The ODU CRC errors between ODU resets. 
        The IDU stores and reports total ODU
	    CRC errors between previous ODU resets . 
        Cleared by Counters Control and it starts 
        counting ODU CRC errors after counter reset.
            
    "
      ::= { oduServChanStatEntry 3 }

    iduCrcErrors OBJECT-TYPE 
      SYNTAX       Counter32
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  " 

            The IDU CRC Errors.  
            Cleared by Counters Control

    "
      ::= { oduServChanStatEntry 4 }

    oduDisconnects OBJECT-TYPE 
      SYNTAX       Counter32
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  " 

            The ODU disconnects, which are reported by IDU.
            Cleared by Counters Control

    "
      ::= { oduServChanStatEntry 5 }


    oduResetCmds OBJECT-TYPE 
      SYNTAX       Counter32
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  " 

            Counter of reset command that IDU sends to ODU.  
            Cleared by Counters Control.
    "
      ::= { oduServChanStatEntry 6 }

    oduCountCtrl OBJECT-TYPE 
      SYNTAX       INTEGER {none(0),
                             resetCnt(1)}

      MAX-ACCESS   read-write
      STATUS       current
      DESCRIPTION  " 
            Counter reset command.
    "
      ::= { oduServChanStatEntry 7 }

    oduSfdAlarmsCounter OBJECT-TYPE 
      SYNTAX       Counter64
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  " 

            Counter of Raised Slow Fading Detection Alarms.  
            Cleared by Counters Control.
    " 
      ::= { oduServChanStatEntry 8 }

    oduFfdAlarmsCounter OBJECT-TYPE 
      SYNTAX       Counter64
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  " 

            Counter of Raised Fast Fading Detection Alarms.  
            Cleared by Counters Control.
    " 
      ::= { oduServChanStatEntry 9 }

    oduLinkQualityMonUpTime OBJECT-TYPE 
      SYNTAX       TimeTicks
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION  " 

            Time in hundredths of seconds that the Radio Link Monitoring is active.
            Cleared by Counters Control.
    " 
      ::= { oduServChanStatEntry 10 }

    oduCoChannelInterfereDetectAlarmsCounter OBJECT-TYPE 
