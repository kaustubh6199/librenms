   --========================================================
   --========================================================
   --  XPIC Modem Configuration Table
   --========================================================
   
      modemXpicGroupConfTable OBJECT-TYPE 
         SYNTAX       SEQUENCE OF  ModemXpicGroupConfEntry
         MAX-ACCESS   not-accessible
         STATUS       current
         DESCRIPTION  "
              [Omnibas-4W implementation]
               This table refers only to XPIC modems,
               so it is is applicable only in OmniBAS-4W.

               Table of configuration settings regarding Xpic modem 
               functionality. 
               
               There are automatically created rows for each possible Xpic modem.
           "
         ::= { modemConfig 14 }   
         
      modemXpicGroupConfEntry OBJECT-TYPE 
         SYNTAX       ModemXpicGroupConfEntry
         MAX-ACCESS   not-accessible
         STATUS       current
         DESCRIPTION  "XPIC Modem."
         INDEX {modemXpicConfIndex}
         ::= { modemXpicGroupConfTable 1 }       
      
      ModemXpicGroupConfEntry ::= SEQUENCE {    
          modemXpicConfIndex         INTEGER,
          modemXpicConfEnabled       INTEGER,
          modemXpicConfPreambleType  INTEGER
      }
      
      modemXpicConfIndex     OBJECT-TYPE 
         SYNTAX       INTEGER (1)
         MAX-ACCESS   not-accessible
         STATUS       current
         DESCRIPTION  "

           The index is 1 for all modems.
           "
         ::= { modemXpicGroupConfEntry 1 }     
      
      modemXpicConfEnabled    OBJECT-TYPE 
         SYNTAX       INTEGER {xpicenabled(1),
                               xpicdisabled(2)}
         MAX-ACCESS   read-write
         STATUS       current
         DESCRIPTION  "
               Xpic Modem Enable/ ptp Modem enable.
               Valid values are: xpicenabled(1), xpicdisabled(2)."
         DEFVAL {xpicdisabled}
         ::= { modemXpicGroupConfEntry 2 } 

      modemXpicConfPreambleType    OBJECT-TYPE 
         SYNTAX       INTEGER {lockOddModem(1),
                               lockEvenModem(2)}
         MAX-ACCESS   read-write
         STATUS       current
         DESCRIPTION  " 
               Xpic Preamble Pattern Indicator. 
               The preamble pattern type is used to differentiate 
               the two streams of an XPIC link. Orthogonal preambles are assigned at both ends, 
               so that the corresponding modems do not lock to the wrong signal. 
               The name of the preamble patterns is based on their use in the systems, where
               they are fixed and not provisioned by the user.
               The pattern lockOddModem is the one that is used by OmniBAS Split systems at the modems 
               at odd system slots. 
               The orthogonal to that pattern lockEvenModem is the one that is used by OmniBAS Split systems 
               at the modems at even system slots."  
         DEFVAL { lockOddModem}
         ::= { modemXpicGroupConfEntry 3 }

