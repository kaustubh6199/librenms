   --===================================================================
   -- strnAntennaStartAction (SCALAR)
   --===================================================================  
   strnAntennaStartAction OBJECT-TYPE
       SYNTAX      INTEGER {noAction (0), 
                            startAlign (1), 
                            startFineTuning (2),
                            init(3),
                            park(4),
                            calibrate(5)}
       MAX-ACCESS  read-write
       STATUS      current
       DESCRIPTION 
           "On demand action related to STRN Antenna Alignment. 
            When init is ordered then move the antenna to the central position.
            When startAlign is ordered then antenna starts auto alignment.
            When startFineTuning is ordered then antenna starts Fine Tuning.
            When park is ordered then antenna parks to a specified position.
            When calibrate is ordered then antenna performs self-calibration, 
                for optimum antenna position tracking. This action may also 
                be performed automatically when required by the device.
            When an action is ordered to run then other running actions are cleared.
            The object is not persistent at system restart."
       ::= { strnAntennaConfig  8 }

   --================================================================ 
   -- STRN ANTENNA NOTIFICATIONS OBJECTS
   --================================================================
   strnAntennaNotificationObjects OBJECT IDENTIFIER 
   ::= { strnAntennaConfig  9}     
   
   strnAntennaFailureAlarm NOTIFICATION-TYPE
      OBJECTS {strnAntennaCurrentAntennaFailureAlarmStatus}
      STATUS current
      DESCRIPTION 
          " A trap indicating STRN Antenna Failure Alarm Status
            The NMS forwards the following information to the operator:
               varbind[0].oid = oid_sysUpTime.0 = 1.3.6.1.2.1.1.3.0
               varbind[0].val = NMS agent time (in centiseconds)
             
               varbind[1].oid = oid_snmpTrapOID.0 = 1.3.6.1.6.3.1.1.4.1.0
               varbind[1].val = oid_stnAntennaFailureAlarm
                              = 1.3.6.1.4.1.1807.55.1.3.9.1
               varbind[2].oid = oid_strnAntennaCurrentAntennaFailureAlarmStatus
                              = 1.3.6.1.4.1.1807.55.1.3.5.1.10
               varbind[2].val = strnAntennaCurrentAntennaFailureAlarmStatus
         "
     ::= { strnAntennaNotificationObjects 1}


