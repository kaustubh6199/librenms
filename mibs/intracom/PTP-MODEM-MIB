      DESCRIPTION  " Table that models clock source configuration per modem slot for the case of Neptune Modem"
      ::= { modemConfig  18 }

  modemClockSourceEntry OBJECT-TYPE
      SYNTAX       ModemClockSourceEntry
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  ""
      INDEX { modemClockSourceIndex }
      ::= { modemClockSourceTable 1 }

  ModemClockSourceEntry::= SEQUENCE {
      modemClockSourceIndex     INTEGER,
      modemClockSource          INTEGER
  }

  modemClockSourceIndex   OBJECT-TYPE
      SYNTAX              INTEGER(1)
      MAX-ACCESS          not-accessible
      STATUS              current
      DESCRIPTION
         " The index is 1 for all modems "
      ::= { modemClockSourceEntry 1 }

  modemClockSource OBJECT-TYPE
      SYNTAX       INTEGER { air (0),
                             external(1)
                           }
      MAX-ACCESS   read-write
      STATUS       current
      DESCRIPTION
          " External means in practice that the modem gets the system clock source.
            Air means that the air-received clock is used by the modem.
	        Default value air(0)."
      DEFVAL { air }
      ::= { modemClockSourceEntry 2 }

--==========================================================                                      
-- SUPPORTED MODEM CAPABILITIES TABLE
--==========================================================
  modemSuppCapabilitiesTable OBJECT-TYPE
      SYNTAX       SEQUENCE OF  ModemSuppCapabilitiesEntry
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  " Table that models supported Modem Capabilities combinations
                     for the modem types that can be used in the system."
      ::= { modemConfig  19 }

  modemSuppCapabilitiesEntry OBJECT-TYPE
      SYNTAX       ModemSuppCapabilitiesEntry
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  ""
      INDEX { modemSuppCapabilitiesIndex }
      ::= { modemSuppCapabilitiesTable 1 }

  ModemSuppCapabilitiesEntry::= SEQUENCE {
      modemSuppCapabilitiesIndex       Unsigned32,
      modemSuppCapModemType            INTEGER,
      modemSuppCapBitMap               BITS,
      modemSuppCapChannelBW            PtpModemChannelBWType,
      modemSuppCapPHYProfile           PtpPHYProfileType, 
      modemSuppCapPHYprofileVersion    SnmpAdminString,
      modemSuppPhymodeBitMap           BITS,
      modemSuppTDDTxRatioBitMap        BITS
  }

  modemSuppCapabilitiesIndex   OBJECT-TYPE
      SYNTAX              Unsigned32
      MAX-ACCESS          not-accessible
      STATUS              current
      DESCRIPTION
         " A sequential index of supported Modem Capabilities combination, 
           presented by the ModemSuppCapabilitiesEntry. "
      ::= { modemSuppCapabilitiesEntry 1 }

  modemSuppCapModemType   OBJECT-TYPE
      SYNTAX              INTEGER{   
                                  npt(1),
                                  pvg(2)
      }
      MAX-ACCESS          read-only
      STATUS              current
      DESCRIPTION
         " The modem type. "
      ::= { modemSuppCapabilitiesEntry 2 }

  modemSuppCapBitMap  OBJECT-TYPE
      SYNTAX              BITS{   
                                  xpicEnabled       (0),
                                  atpcEnabled       (1),
                                  jumboUnfragmented (2)
      }
      MAX-ACCESS          read-only
      STATUS              current
      DESCRIPTION
         " The modem Capabilities combination bitmap. 
           Bit-0 set to 1 means that XPIC is supported and enabled and 0 that XPIC is not supported
           or disabled. 
           Bit-1 equal to 0 implies no ATPC applied (Adaptive mode, Manual and Auto / Manual Power).
           Bit-1 set to 1 means Auto, i.e. ATPC enabled (adaptive mode, Auto / Optimum and Auto / Max Power).  
           Bit-2 set to 1 means that Unfragmented Jumbo handling is supported 
           otherwise Fragmented Jumbo handling is supported. "
           
      ::= { modemSuppCapabilitiesEntry 3 }

  modemSuppCapChannelBW   OBJECT-TYPE
      SYNTAX              PtpModemChannelBWType
      MAX-ACCESS          read-only
      STATUS              current
      DESCRIPTION
         " The modem supported Channel Bandwidth value for this Modem Capabilities combination."
      ::= { modemSuppCapabilitiesEntry 4 }


  modemSuppCapPHYProfile   OBJECT-TYPE
      SYNTAX              PtpPHYProfileType
      MAX-ACCESS          read-only
      STATUS              current
      DESCRIPTION
         " The modem supported PHY Profile value for this Modem Capabilities combination."
      ::= { modemSuppCapabilitiesEntry 5 }

  modemSuppCapPHYprofileVersion       OBJECT-TYPE
      SYNTAX       SnmpAdminString
      MAX-ACCESS   read-only
      STATUS       current
      DESCRIPTION
          "Modem profile version.
          "
      ::= { modemSuppCapabilitiesEntry 6 }

  modemSuppPhymodeBitMap   OBJECT-TYPE
      SYNTAX              BITS {   
                                  phymodeId1  (0),
                                  phymodeId2  (1),
                                  phymodeId3  (2),
                                  phymodeId4  (3),
                                  phymodeId5  (4),
                                  phymodeId6  (5),
                                  phymodeId7  (6),
                                  phymodeId8  (7),
                                  phymodeId9  (8),
                                  phymodeId10  (9),
                                  phymodeId11  (10),
                                  phymodeId12  (11),
                                  phymodeId13  (12),
                                  phymodeId14  (13),
                                  phymodeId15  (14),
                                  phymodeId16  (15)
      }
      MAX-ACCESS          read-only
      STATUS              current
      DESCRIPTION
         " The modem supported PHYmodes for this Modem Capabilities combination. 
         Each bit set to 1, indicates one of the PHYmodes supported from 1 to 16, 
         i.e. bit-0 corresponds to phymodeId = 1, bit-15 to phmodeId = 16, 
         where the phymodeId is the modemSuppCapPhymodIndex of the modemSuppCapPhymodCapacityTable.
         For invalid modemSuppCapabilites Entries, the modemSuppPhymodeBitMap is 0."
      ::= { modemSuppCapabilitiesEntry 7 }

  modemSuppTDDTxRatioBitMap   OBJECT-TYPE
      SYNTAX BITS {
                    one2one (1),
                    two2one (2),
                    three2one (3),
                    five2one (4),
                    seven2one (5),
                    eleven2one (6),
                    one2two (7),
                    one2three (8),
                    one2five (9),
                    one2seven (10),
                    one2eleven (11)
      }
      MAX-ACCESS          read-only
      STATUS              current
      DESCRIPTION
         " The modem supported TDD Tx Ratios for this Modem Capabilities combination.
         Each bit set to 1, indicates one of the TDD Tx Ratios supported.
         Not supported TDD Tx Ratios have their corresponding bit set to 0."
      ::= { modemSuppCapabilitiesEntry 8 }

   --=========================================================
   -- SUPPORTED MODEM PHY MODULATION CAPACITIES TABLE
   --=========================================================
   
   modemSuppCapPhymodCapacityTable OBJECT-TYPE
      SYNTAX       SEQUENCE OF   ModemSuppCapPhymodCapacityEntry
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  " 
                    This table models the modem supported phymod capacities per supported
                    modem phymode, i.e Modulation and FEC Rate types combinations and
                    supported Modem Capabilities combination.
                   "    
      ::= { modemConfig  20 }

   modemSuppCapPhymodCapacityEntry OBJECT-TYPE 
      SYNTAX       ModemSuppCapPhymodCapacityEntry
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  " "
      INDEX  {modemSuppCapabilitiesIndex, modemSuppCapPhymodIndex} 
      ::={modemSuppCapPhymodCapacityTable 1 }

    ModemSuppCapPhymodCapacityEntry ::= SEQUENCE {
        modemSuppCapPhymodIndex               Unsigned32,
        modemSuppCapPhymodName                SnmpAdminString,      
        modemSuppCapPhymodAirCapacity         Unsigned32,
        modemSuppCapPhymodCfgId               Unsigned32          
    }   

    modemSuppCapPhymodIndex   OBJECT-TYPE
        SYNTAX              Unsigned32 (1..16)
        MAX-ACCESS          not-accessible
        STATUS              current
        DESCRIPTION
           " Modem PHYmode Index 1 to 16, i.e. the supported PHYmodes of the specific Modem Capabilities combination
            indicated by modemSuppCapabilitiesIndex. Valid modemSuppCapPhymodIndex values are indicated in the bits
            set to 1 of the modemSuppPhymodeBitMap. The Indexing from 1 to 16 corrresponds to indexing of phymodes 
            from lower air capacity to higher air capacity."
        ::= { modemSuppCapPhymodCapacityEntry 1 }


    modemSuppCapPhymodName   OBJECT-TYPE
        SYNTAX              SnmpAdminString
        MAX-ACCESS          read-only
        STATUS              current
        DESCRIPTION
           " Modem Phymode (Modulation plus FEC type)Name that can be used as phymode identifier."
        ::= { modemSuppCapPhymodCapacityEntry 2 }


    modemSuppCapPhymodAirCapacity         OBJECT-TYPE
        SYNTAX       Unsigned32
        UNITS        "Kbits/sec"
        MAX-ACCESS   read-only
        STATUS       current
        DESCRIPTION
            "Maximum air capacity for this phymode (modulation and fec combination) in Kbits/sec.
            "
        ::= { modemSuppCapPhymodCapacityEntry 3 }

    modemSuppCapPhymodCfgId   OBJECT-TYPE
        SYNTAX              Unsigned32 (1..16)
        MAX-ACCESS          read-only
        STATUS              current
        DESCRIPTION
           "The configuration Phymode Id that is used to configure this phymode."
        ::= { modemSuppCapPhymodCapacityEntry 4 }

      
   --=========================================================                                      
