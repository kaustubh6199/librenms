        modemEthernetCountersEntryIndex  INTEGER,
        modemEthTxPacketCounter    Counter32,
        modemEthRxPacketCounter    Counter32,
        modemEthRxCrcErrors    Counter32,
        modemEthCountersControl PtpCounterOperation,
        modemEthRxTruncatedFrames Counter32,
        modemEthRxLongEvents      Counter32,
        modemEthRxVlanTagsDetected Counter32,
        modemEthRxUnsupportedOpCodes Counter32,
        modemEthRxpauseFrames    Counter32,
        modemEthRxControlFrames  Counter32,
        modemEthRxDribbleNibbles Counter32,
        modemEthRxBroadCasts     Counter32,
        modemEthRxMultiCasts     Counter32,
        modemEthRxOutOfRangeErrors Counter32,
        modemEthRxLengthCheckErrors Counter32,
        modemEthRxCodeErrors   Counter32,
        modemEthRxFalseCarrierErrors Counter32,
        modemEthRxDvEvent Counter32,
        modemEthRxPreviousPacketDropped Counter32,
        modemEthRxByteCounterLow Counter32,
        modemEthRxByteCounterHigh Counter32,
        modemEthRxBitRate Counter32,
        modemEthTxVlanTags Counter32,
        modemEthTxBackPressuresEvents Counter32,
        modemEthTxPauseFrames  Counter32,
        modemEthTxControlFrames  Counter32,
        modemEthTxWireByteCounterLow  Counter32,
        modemEthTxWireByteCounterHigh Counter32,
        modemEthTxUnderRuns Counter32,
        modemEthTxGiants Counter32,
        modemEthTxLateCollisions Counter32,
        modemEthTxMaxCollisions Counter32,
        modemEthTxExcessiveDefers Counter32,
        modemEthTxNonExcessiveDefers Counter32,
        modemEthTxBroadCasts Counter32,
        modemEthTxMulticasts Counter32,
        modemEthTxReserved Counter32,
        modemEthTxLengthCheckErrors Counter32,
        modemEthTxCrcErrors Counter32,
        modemEthTxCollisions Counter32,
        modemEthTxByteCounterLow Counter32,
        modemEthTxByteCounterHigh Counter32,
        modemEthTxBitRate Counter32,
        modemEthCommulativeTime Unsigned32,
        modemEthRateTime INTEGER,
        modemEthRxBitRateHigh Counter32,
        modemEthTxBitRateHigh Counter32,
        modemEthRxShortEvents Counter32
   }   
  
