   -- STRN Antenna Objects
   strnAntennaConfig OBJECT IDENTIFIER ::= { modemConfigMibObjects 3 } 

   --===================================================================
   -- strnAntennaGenConfigTable
   --===================================================================  
   strnAntennaGenConfigTable OBJECT-TYPE
      SYNTAX       SEQUENCE OF   StrnAntennaGenConfigEntry
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  
          "This table models the STRN-PTP Antenna Configuration general parameters.
          N/A for the OSDRS (same holds for every strnAntennaConfig object).
		  N/A for the EBAND (same holds for every strnAntennaConfig object)"    
      ::= { strnAntennaConfig  1 }

   strnAntennaGenConfigEntry OBJECT-TYPE 
      SYNTAX       StrnAntennaGenConfigEntry
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  " "
      INDEX  { strnAntennaGenConfigEntryIndex  } 
      ::={ strnAntennaGenConfigTable 1 }

    StrnAntennaGenConfigEntry ::= SEQUENCE {                   
        strnAntennaGenConfigEntryIndex      INTEGER,
        strnAntennaAlignConf                INTEGER,
        strnAntennaAzimuthRefPos            INTEGER
   }   
   
   strnAntennaGenConfigEntryIndex   OBJECT-TYPE 
      SYNTAX       INTEGER (1)
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  
          "Index of the StrnAntennaGenConfigEntry. It is restricted to 1 instance."
      ::= { strnAntennaGenConfigEntry 1 }
	 
   strnAntennaAlignConf    OBJECT-TYPE 
      SYNTAX        INTEGER {manual(0),auto(1)}
      MAX-ACCESS    read-write
      STATUS        current
      DESCRIPTION  
          "It specifies the way that the Antenna Alignment is performed. Manually 
           or Automatically."
      DEFVAL { 0 }
      ::= { strnAntennaGenConfigEntry 2 }
	
    strnAntennaAzimuthRefPos    OBJECT-TYPE 
      SYNTAX        INTEGER (0..3599)
      MAX-ACCESS    read-write
      STATUS        current
      DESCRIPTION  
          "Reference Position to start measuring the Azimuth angle of the STRN Unit. 
           North is considered as the Reference Position."
      DEFVAL { 0 }
      ::= { strnAntennaGenConfigEntry 3 }

   --===================================================================
   -- strnAntennaAutoConfigTable
   --===================================================================  
   strnAntennaAutoConfigTable OBJECT-TYPE
      SYNTAX       SEQUENCE OF StrnAntennaAutoConfigEntry
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  
          "This table models the STRN-PTP Antenna Auto Configuration  parameters."    
      ::= { strnAntennaConfig  2 }

   strnAntennaAutoConfigEntry OBJECT-TYPE 
      SYNTAX       StrnAntennaAutoConfigEntry
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  " "
      INDEX  { strnAntennaAutoConfigEntryIndex } 
      ::={ strnAntennaAutoConfigTable 1 }

   StrnAntennaAutoConfigEntry ::= SEQUENCE {                   
        strnAntennaAutoConfigEntryIndex  INTEGER,
        strnAntennaVerificationScan      INTEGER,
        strnAntennaSavedPeakDegrThresh   INTEGER,
        strnAntennaSavedPeakTestTimeout  INTEGER        
   }   
   
   strnAntennaAutoConfigEntryIndex   OBJECT-TYPE 
      SYNTAX       INTEGER (1)
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  "Index of the StrnAntennaAutoConfigEntry. 
                    It is restricted to 1 instance."
      ::= { strnAntennaAutoConfigEntry 1 }
	
   strnAntennaVerificationScan    OBJECT-TYPE 
      SYNTAX        INTEGER {enable(0),disable(1)}
      MAX-ACCESS    read-write
      STATUS        current
      DESCRIPTION  
          "Enables / disables an evaluation of the peaks found at the initial scan.
           Valid (applicable) only when strnAntennaAlignConf = auto."
      DEFVAL { 1 }
      ::= { strnAntennaAutoConfigEntry 2 }
      
     strnAntennaSavedPeakDegrThresh    OBJECT-TYPE 
      SYNTAX        INTEGER (0..100)
      MAX-ACCESS    read-write
      STATUS        current
      DESCRIPTION  
          "If stored Antenna alignment parameter exists then the STRN tries to use them 
           before attempting Antenna initialization and Initial Scan. It checks then if the 
           RSSI / SNR results of this evaluation have a difference not bigger than the value 
           indicated by savedpeakdegrthesh then these stored parameters can be used and antenna 
           is considered as aligned. Valid (applicable) only when strnAntennaAlignConf = auto.
           Values in Tenths of  dB e.g. a value of 173 stands for 173/10 = 17,3dB."
      DEFVAL { 30 }
      ::= { strnAntennaAutoConfigEntry 3 }
      
   strnAntennaSavedPeakTestTimeout    OBJECT-TYPE 
      SYNTAX        INTEGER (100..20000)
      MAX-ACCESS    read-write
      STATUS        current
      DESCRIPTION  
          "Timeout used to evaluate the stored antenna alignment parameters to evaluate the 
           RSSI / SNR results of this evaluation have a difference not bigger than the value 
           indicated by savedpeakdegrthesh. Valid (applicable) only when strnAntennaAlignConf = auto.
           Values in ms 173."
      DEFVAL { 10000 }
      ::= { strnAntennaAutoConfigEntry 4 }

   --===================================================================
   -- strnAntennaAutoFineConfigTable
   --===================================================================  
   strnAntennaAutoFineConfigTable OBJECT-TYPE
      SYNTAX       SEQUENCE OF   StrnAntennaAutoFineConfigEntry
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  
          "This table models the STRN-PTP Antenna Auto Fine Tuning Configuration  parameters."    
      ::= { strnAntennaConfig  3 }

   strnAntennaAutoFineConfigEntry OBJECT-TYPE 
      SYNTAX       StrnAntennaAutoFineConfigEntry
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  " "
      INDEX  { strnAntennaAutoFineConfigEntryIndex   } 
      ::={ strnAntennaAutoFineConfigTable 1 }

   StrnAntennaAutoFineConfigEntry ::= SEQUENCE {                   
        strnAntennaAutoFineConfigEntryIndex       INTEGER,
        strnAntennaAutoFineTune                   INTEGER,
        strnAntennaMaxFineTunes                   INTEGER,
        strnAntennaAutoFineTuneDegrThresh         INTEGER,
        strnAntennaAutoFineTuneTimeout            INTEGER
   }   
   
   strnAntennaAutoFineConfigEntryIndex   OBJECT-TYPE 
      SYNTAX       INTEGER (1)
      MAX-ACCESS   not-accessible
      STATUS       current
      DESCRIPTION  
          "Index of the StrnAntennaAutoFineConfigEntry. It is restricted to 1 instance."
      ::= { strnAntennaAutoFineConfigEntry 1 }
      
   strnAntennaAutoFineTune    OBJECT-TYPE 
      SYNTAX        INTEGER {enable(0),disable(1)}
      MAX-ACCESS    read-write
      STATUS        current
      DESCRIPTION  
          "Automatic continues fine tuning status"
      DEFVAL { 1 }
      ::= { strnAntennaAutoFineConfigEntry 2 }

   strnAntennaMaxFineTunes    OBJECT-TYPE 
      SYNTAX        INTEGER (1..10)
      MAX-ACCESS    read-write
      STATUS        current
      DESCRIPTION  
         "Maximum Number of Fine Tunes performed to find better RSSI/SNR."
      DEFVAL { 3 }
      ::= { strnAntennaAutoFineConfigEntry 3 }

   strnAntennaAutoFineTuneDegrThresh    OBJECT-TYPE 
      SYNTAX        INTEGER (0..100)
      MAX-ACCESS    read-write
      STATUS        current
      DESCRIPTION  
          "At fine tuning, the STRN checks if the RSSI / SNR results of the position found 
           by fine tuning are better that the values of the previous found peak. If no better 
           peak is found then the last peak is verified that has a difference not bigger than 
           the value indicated by autofinedegrthesh. If verified then antenna is considered as 
           aligned. Valid (applicable) only when alignconf = auto and autofinetune = enable. 
           Values in Tenths of dB e.g. a value of 173 stands for 173/10 = 17,3db. "
      DEFVAL { 30 }
      ::= { strnAntennaAutoFineConfigEntry 4 }
      
   strnAntennaAutoFineTuneTimeout    OBJECT-TYPE 
      SYNTAX        INTEGER (100..20000)
      MAX-ACCESS    read-write
      STATUS        current
      DESCRIPTION  
          "Automatic Fine Tuning Timeout. At timeout the degradation of current peak is checked 
           and if not within the RSSI/SNR limits then a new fine tuning is performed. Valid 
           (applicable) only when alignconf = auto and autofinetune = enable. Values in ms 173 
           stands for 173ms. "
      DEFVAL { 10000 }
      ::= { strnAntennaAutoFineConfigEntry 5 }

