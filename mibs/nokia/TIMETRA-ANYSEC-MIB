         performance of the Specification."

    REVISION    "202303010000Z"
    DESCRIPTION
        "Rev 23.0                 1 Mar 2023 00:00
         Initial version of the TIMETRA-ANYSEC-MIB."

    ::= { timetraSRMIBModules 131 }

TmnxAnySecRoutingProtocol        ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "A value of type TmnxAnySecRoutingProtocol specifies the routing
         protocol used by a security termination policy or ANYsec remote peer."
    SYNTAX      INTEGER {
        srOspf  (0),
        srOspf3 (1),
        srIsis  (2)
    }

tmnxAnySecObjs                   OBJECT IDENTIFIER ::= { tmnxSRObjs 131 }

tmnxAnySecCfgObjs                OBJECT IDENTIFIER ::= { tmnxAnySecObjs 1 }

tmnxAnySecCfgGlobalObjs          OBJECT IDENTIFIER ::= { tmnxAnySecCfgObjs 1 }

tmnxAnySecReservedLabelBlock     OBJECT-TYPE
    SYNTAX      TLNamedItemOrEmpty
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The value of tmnxAnySecReservedLabelBlock specifies the name of the
         label block reserved for ANYsec.

         tmnxAnySecReservedLabelBlock must contain an entry specified by
         TIMETRA-MPLS-MIB::vRtrMplsReservedLblBlkName."
    DEFVAL      { ''H }
    ::= { tmnxAnySecCfgGlobalObjs 1 }

tmnxAnySecMkaUdpPort             OBJECT-TYPE
    SYNTAX      TmnxUdpPort (0 | 1024..49151)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The value of tmnxAnySecMkaUdpPort specifies the UDP port on which MKA
         packets must be extracted to the CPM for ANYsec MKA over IP."
    DEFVAL      { 0 }
    ::= { tmnxAnySecCfgGlobalObjs 2 }

tmnxAnySecCfgTableLastChgObjs    OBJECT IDENTIFIER ::= { tmnxAnySecCfgObjs 2 }

tmnxAnySecCfgTableObjs           OBJECT IDENTIFIER ::= { tmnxAnySecCfgObjs 3 }

tmnxAnySecTermPolicyTable        OBJECT-TYPE
    SYNTAX      SEQUENCE OF TmnxAnySecTermPolicyEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "tmnxAnySecTermPolicyTable contains configuration information for
         ANYsec security termination policies."
    ::= { tmnxAnySecCfgTableObjs 1 }

tmnxAnySecTermPolicyEntry        OBJECT-TYPE
    SYNTAX      TmnxAnySecTermPolicyEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "tmnxAnySecTermPolicyEntry contains configuration information for one
         ANYsec security termination policy.

         A security termination policy specifies ANYsec attributes for
         decrypting ingress on a locally terminating tunnel. A security
         termination policy is used by an encryption group, as specified in
         tmnxAnySecEncryptGrpTable.

         Rows are created or destroyed using tmnxAnySecTermPolicyRowStatus."
    INDEX       { tmnxAnySecTermPolicyName }
    ::= { tmnxAnySecTermPolicyTable 1 }

TmnxAnySecTermPolicyEntry        ::= SEQUENCE
{
    tmnxAnySecTermPolicyName         TLNamedItem,
    tmnxAnySecTermPolicyRowStatus    RowStatus,
    tmnxAnySecTermPolicyLastChanged  TimeStamp,
    tmnxAnySecTermPolicyProtocol     TmnxAnySecRoutingProtocol,
    tmnxAnySecTermPolicyIgpInstn     TmnxIgpInstance,
    tmnxAnySecTermPolicyAlgoId       TmnxFlexAlgoIdOrZero,
    tmnxAnySecTermPolicyLoclAddrType InetAddressType,
    tmnxAnySecTermPolicyLoclAddr     InetAddress,
    tmnxAnySecTermPolicyRxMustEcrptd TruthValue,
    tmnxAnySecTermPolicyAdminState   TmnxEnabledDisabledAdminState
}

tmnxAnySecTermPolicyName         OBJECT-TYPE
    SYNTAX      TLNamedItem
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The value of tmnxAnySecTermPolicyName specifies the name that uniquely
         identifies this security termination policy."
    ::= { tmnxAnySecTermPolicyEntry 1 }

tmnxAnySecTermPolicyRowStatus    OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxAnySecTermPolicyRowStatus specifies the status of the
         configuration row for this security termination policy."
    ::= { tmnxAnySecTermPolicyEntry 2 }

tmnxAnySecTermPolicyLastChanged  OBJECT-TYPE
    SYNTAX      TimeStamp
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The value of tmnxAnySecTermPolicyLastChanged specifies the most recent
         time the value of a settable object in this row was changed."
    ::= { tmnxAnySecTermPolicyEntry 3 }

tmnxAnySecTermPolicyProtocol     OBJECT-TYPE
    SYNTAX      TmnxAnySecRoutingProtocol
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxAnySecTermPolicyProtocol specifies the name that
         uniquely identifies this security termination policy."
    DEFVAL      { srIsis }
    ::= { tmnxAnySecTermPolicyEntry 4 }

tmnxAnySecTermPolicyIgpInstn     OBJECT-TYPE
    SYNTAX      TmnxIgpInstance
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxAnySecTermPolicyIgpInstn specifies the identifier of
         the IGP instance used by this security termination policy."
    DEFVAL      { 0 }
    ::= { tmnxAnySecTermPolicyEntry 5 }

tmnxAnySecTermPolicyAlgoId       OBJECT-TYPE
    SYNTAX      TmnxFlexAlgoIdOrZero
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxAnySecTermPolicyAlgoId specifies the identifier of
         the Flexible Algorithm used by this security termination policy."
    DEFVAL      { 0 }
    ::= { tmnxAnySecTermPolicyEntry 6 }

tmnxAnySecTermPolicyLoclAddrType OBJECT-TYPE
    SYNTAX      InetAddressType
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxAnySecTermPolicyLoclAddrType specifies the address
         type of tmnxAnySecTermPolicyLoclAddr."
    DEFVAL      { unknown }
    ::= { tmnxAnySecTermPolicyEntry 7 }

tmnxAnySecTermPolicyLoclAddr     OBJECT-TYPE
    SYNTAX      InetAddress (SIZE (0|4|16))
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxAnySecTermPolicyLoclAddr specifies the local system
         or loopback IPv4 or IPv6 address that will receive ANYsec encrypted
         packets for this security termination policy."
    DEFVAL      { ''H }
    ::= { tmnxAnySecTermPolicyEntry 8 }

tmnxAnySecTermPolicyRxMustEcrptd OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxAnySecTermPolicyRxMustEcrptd specifies whether only
         encrypted traffic will be considered valid for this security
         termination policy. If true, all packets received that are not
         encrypted will be dropped."
    DEFVAL      { false }
    ::= { tmnxAnySecTermPolicyEntry 9 }

tmnxAnySecTermPolicyAdminState   OBJECT-TYPE
    SYNTAX      TmnxEnabledDisabledAdminState
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxAnySecTermPolicyAdminState specifies the
         administrative state of this security termination policy."
    DEFVAL      { disabled }
    ::= { tmnxAnySecTermPolicyEntry 10 }

tmnxAnySecEncryptGrpTable        OBJECT-TYPE
    SYNTAX      SEQUENCE OF TmnxAnySecEncryptGrpEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "tmnxAnySecEncryptGrpTable contains configuration information for
         ANYsec encryption groups."
    ::= { tmnxAnySecCfgTableObjs 2 }

tmnxAnySecEncryptGrpEntry        OBJECT-TYPE
    SYNTAX      TmnxAnySecEncryptGrpEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "tmnxAnySecEncryptGrpEntry contains configuration information for one
         ANYsec encryption group.

         An encryption group is a set of encrypted LSP tunnels using the same
         Connectivity Association (CA) for ANYsec encryption.

         The encryption group's peer tunnel attributes specify attributes
         (tmnxAnySecEncryptGrpPeerProtocol, tmnxAnySecEncryptGrpPeerIgpInstn,
         tmnxAnySecEncryptGrpPeerAlgoId) for encrypting egress.

         Rows are created or destroyed using tmnxAnySecEncryptGrpRowStatus."
    INDEX       { tmnxAnySecEncryptGrpName }
    ::= { tmnxAnySecEncryptGrpTable 1 }

TmnxAnySecEncryptGrpEntry        ::= SEQUENCE
{
    tmnxAnySecEncryptGrpName         TNamedItem,
    tmnxAnySecEncryptGrpRowStatus    RowStatus,
    tmnxAnySecEncryptGrpLastChanged  TimeStamp,
    tmnxAnySecEncryptGrpSecTermPolcy TLNamedItemOrEmpty,
    tmnxAnySecEncryptGrpCAName       TNamedItemOrEmpty,
    tmnxAnySecEncryptGrpEncryptLabel TmnxMplsLabelOrZero,
    tmnxAnySecEncryptGrpPeerProtocol TmnxAnySecRoutingProtocol,
    tmnxAnySecEncryptGrpPeerIgpInstn TmnxIgpInstance,
    tmnxAnySecEncryptGrpPeerAlgoId   TmnxFlexAlgoIdOrZero,
    tmnxAnySecEncryptGrpAdminState   TmnxEnabledDisabledAdminState
}

tmnxAnySecEncryptGrpName         OBJECT-TYPE
    SYNTAX      TNamedItem
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The value of tmnxAnySecEncryptGrpName specifies the name that uniquely
         identifies this encryption group."
    ::= { tmnxAnySecEncryptGrpEntry 1 }

tmnxAnySecEncryptGrpRowStatus    OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxAnySecEncryptGrpRowStatus specifies the status of the
         configuration row for this encryption group."
    ::= { tmnxAnySecEncryptGrpEntry 2 }

tmnxAnySecEncryptGrpLastChanged  OBJECT-TYPE
    SYNTAX      TimeStamp
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The value of tmnxAnySecEncryptGrpLastChanged specifies specifies the
         most recent time the value of a settable object in this row was
         changed."
    ::= { tmnxAnySecEncryptGrpEntry 3 }

tmnxAnySecEncryptGrpSecTermPolcy OBJECT-TYPE
    SYNTAX      TLNamedItemOrEmpty
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxAnySecEncryptGrpSecTermPolcy specifies the security
         termination policy used by this encryption group."
    DEFVAL      { ''H }
    ::= { tmnxAnySecEncryptGrpEntry 4 }

tmnxAnySecEncryptGrpCAName       OBJECT-TYPE
    SYNTAX      TNamedItemOrEmpty
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxAnySecEncryptGrpCAName specifies the connectivity
         association used for this encryption group and its MKA (MACSec Key
         Agreement).

         tmnxAnySecEncryptGrpCAName must contain an entry specified by
         TIMETRA-MACSEC-MIB::tmnxMacsecConnAssocName or be empty."
    DEFVAL      { ''H }
    ::= { tmnxAnySecEncryptGrpEntry 5 }

tmnxAnySecEncryptGrpEncryptLabel OBJECT-TYPE
    SYNTAX      TmnxMplsLabelOrZero
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxAnySecEncryptGrpEncryptLabel specifies the MPLS label
         that uniquely identifies this encryption group. Packets sent from this
         encryption group will have this label included so that peers can
         identify this group as the encrypter and decrypt accordingly.

         This encryption label must be within the range specified by the label
         block name configured by tmnxAnySecReservedLabelBlock."
    DEFVAL      { 0 }
    ::= { tmnxAnySecEncryptGrpEntry 6 }

tmnxAnySecEncryptGrpPeerProtocol OBJECT-TYPE
    SYNTAX      TmnxAnySecRoutingProtocol
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxAnySecEncryptGrpPeerProtocol specifies the routing
         protocol being used by this encryption group's remote peer."
    DEFVAL      { srIsis }
    ::= { tmnxAnySecEncryptGrpEntry 7 }

tmnxAnySecEncryptGrpPeerIgpInstn OBJECT-TYPE
    SYNTAX      TmnxIgpInstance
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxAnySecEncryptGrpPeerIgpInstn specifies the identifier
         of the IGP instance used by this encryption group's remote peer."
    DEFVAL      { 0 }
    ::= { tmnxAnySecEncryptGrpEntry 8 }

tmnxAnySecEncryptGrpPeerAlgoId   OBJECT-TYPE
    SYNTAX      TmnxFlexAlgoIdOrZero
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxAnySecEncryptGrpPeerAlgoId specifies the identifier
         of the Flexible Algorithm used by this encryption group's remote peer."
    DEFVAL      { 0 }
    ::= { tmnxAnySecEncryptGrpEntry 9 }

tmnxAnySecEncryptGrpAdminState   OBJECT-TYPE
    SYNTAX      TmnxEnabledDisabledAdminState
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxAnySecEncryptGrpAdminState specifies the
         administrative state of the encryption group and all of it's peers.
         Transmission to a peer will only occur when this object and the peer's
         tmnxAnySecPeerAdminState are both enabled."
    DEFVAL      { disabled }
    ::= { tmnxAnySecEncryptGrpEntry 10 }

tmnxAnySecPeerTable              OBJECT-TYPE
    SYNTAX      SEQUENCE OF TmnxAnySecPeerEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "tmnxAnySecPeerTable contains configuration information for ANYsec
         encryption group peers."
    ::= { tmnxAnySecCfgTableObjs 3 }

tmnxAnySecPeerEntry              OBJECT-TYPE
    SYNTAX      TmnxAnySecPeerEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "tmnxAnySecPeerEntry contains configuration information for one ANYsec
         encryption group peer.

         A peer of an encryption group identifies a remote LSP tunnel sending
         ANYsec encrypted traffic.

         Rows are created or destroyed using tmnxAnySecPeerRowStatus."
    INDEX       {
        tmnxAnySecEncryptGrpName,
        tmnxAnySecPeerAddrType,
        tmnxAnySecPeerAddr
    }
    ::= { tmnxAnySecPeerTable 1 }

TmnxAnySecPeerEntry              ::= SEQUENCE
{
    tmnxAnySecPeerAddrType           InetAddressType,
    tmnxAnySecPeerAddr               InetAddress,
    tmnxAnySecPeerRowStatus          RowStatus,
    tmnxAnySecPeerLastChanged        TimeStamp,
    tmnxAnySecPeerEncryptLabel       TmnxMplsLabelOrZero,
    tmnxAnySecPeerAdminState         TmnxEnabledDisabledAdminState
}

tmnxAnySecPeerAddrType           OBJECT-TYPE
    SYNTAX      InetAddressType
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The value of tmnxAnySecPeerAddrType specifies the address type of
         tmnxAnySecPeerAddr."
    ::= { tmnxAnySecPeerEntry 1 }

tmnxAnySecPeerAddr               OBJECT-TYPE
    SYNTAX      InetAddress (SIZE (4|16))
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The value of tmnxAnySecPeerAddr specifies the IPv4 or IPv6 address
         that uniquely identifies this peer of the encryption group."
    ::= { tmnxAnySecPeerEntry 2 }

tmnxAnySecPeerRowStatus          OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxAnySecPeerRowStatus specifies the status of the
         configuration row for this peer of the encryption group."
    ::= { tmnxAnySecPeerEntry 3 }

tmnxAnySecPeerLastChanged        OBJECT-TYPE
    SYNTAX      TimeStamp
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The value of tmnxAnySecPeerLastChanged specifies the most recent time
         the value of a settable object in this row was changed."
    ::= { tmnxAnySecPeerEntry 4 }

tmnxAnySecPeerEncryptLabel       OBJECT-TYPE
    SYNTAX      TmnxMplsLabelOrZero
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxAnySecPeerEncryptLabel specifies the MPLS label that
         uniquely identifies the peer encryption group. This label allows for
         egress packets to be decrypted according to the MKA associated with
         this peer."
    DEFVAL      { 0 }
    ::= { tmnxAnySecPeerEntry 5 }

tmnxAnySecPeerAdminState         OBJECT-TYPE
    SYNTAX      TmnxEnabledDisabledAdminState
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxAnySecPeerAdminState specifies the administrative
         state of traffic going to this peer of the encryption group."
    DEFVAL      { disabled }
    ::= { tmnxAnySecPeerEntry 6 }

tmnxAnySecStatsObjs              OBJECT IDENTIFIER ::= { tmnxAnySecObjs 2 }

tmnxAnySecStatsGlobalObjs        OBJECT IDENTIFIER ::= { tmnxAnySecStatsObjs 1 }

tmnxAnySecMkaUdpPortOperState    OBJECT-TYPE
    SYNTAX      TmnxOperState
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The value of tmnxAnySecMkaUdpPortOperState indicates the operational
         state of the MKA UDP port configured by tmnxAnySecMkaUdpPort."
    ::= { tmnxAnySecStatsGlobalObjs 1 }

tmnxAnySecStatsTableObjs         OBJECT IDENTIFIER ::= { tmnxAnySecStatsObjs 2 }

tmnxAnySecMkaStatsTable          OBJECT-TYPE
    SYNTAX      SEQUENCE OF TmnxAnySecMkaStatsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The tmnxAnySecMkaStatsTable contains stats information for MKA over IP
         differentiated by an encryption group associated with a peer."
    ::= { tmnxAnySecStatsTableObjs 1 }

tmnxAnySecMkaStatsEntry          OBJECT-TYPE
    SYNTAX      TmnxAnySecMkaStatsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each tmnxAnySecMkaStatsEntry represents MKA over IP for an encryption
         group associated with a peer in the system and contains MKA
         statistical information for that MKA."
    INDEX       {
        tmnxAnySecEncryptGrpName,
        tmnxAnySecPeerAddrType,
        tmnxAnySecPeerAddr,
        tmnxAnySecCkn
    }
    ::= { tmnxAnySecMkaStatsTable 1 }

TmnxAnySecMkaStatsEntry          ::= SEQUENCE
{
    tmnxAnySecCkn                    OCTET STRING,
    tmnxAnySecMkaStatsMemberId       OCTET STRING,
    tmnxAnySecMkaStatsOutboundSci    OCTET STRING,
    tmnxAnySecMkaStatsMessageNumber  Unsigned32,
    tmnxAnySecMkaStatsKeyNumber      Unsigned32,
    tmnxAnySecMkaStatsKeyServer      TruthValue,
    tmnxAnySecMkaStatsLatestSakAn    Unsigned32,
    tmnxAnySecMkaStatsLatestSakKi    OCTET STRING,
    tmnxAnySecMkaStatsPreviousSakAn  Unsigned32,
    tmnxAnySecMkaStatsPreviousSakKi  OCTET STRING,
    tmnxAnySecMkaStatsPeerRemTimeout Counter64,
    tmnxAnySecMkaStatsCknNotFound    Counter64,
    tmnxAnySecMkaStatsNewLivePeer    Counter64,
    tmnxAnySecMkaStatsSakGenerated   Counter64,
    tmnxAnySecMkaStatsSakInstalledTx Counter64,
    tmnxAnySecMkaStatsSakInstalledRx Counter64,
    tmnxAnySecMkaStatsPduTooSmall    Counter64,
    tmnxAnySecMkaStatsPduTooBig      Counter64,
    tmnxAnySecMkaStatsPduNotQuadSize Counter64,
    tmnxAnySecMkaStatsPduInvalidNum  Counter64,
    tmnxAnySecMkaStatsParamSzInvalid Counter64,
    tmnxAnySecMkaStatsLvnessChckFail Counter64,
    tmnxAnySecMkaStatsParamNotQuadSz Counter64,
    tmnxAnySecMkaStatsUnsupportedAgi Counter64,
    tmnxAnySecMkaStatsInvldCknLength Counter64,
    tmnxAnySecMkaStatsIcvCheckFailed Counter64,
    tmnxAnySecMkaStatsPeerSameMid    Counter64,
    tmnxAnySecMkaStatsSakNonLivePeer Counter64,
    tmnxAnySecMkaStatsSakNoKeyServer Counter64,
    tmnxAnySecMkaStatsSakDecryptFail Counter64,
    tmnxAnySecMkaStatsSakEncryptFail Counter64,
    tmnxAnySecMkaStatsKeyNumInvalid  Counter64,
    tmnxAnySecMkaStatsSakInstallFail Counter64,
    tmnxAnySecMkaStatsCakInfoMissing Counter64,
    tmnxAnySecMkaStatsMxPeersSetZero Counter64,
    tmnxAnySecMkaStatsOperState      TmnxOperState,
    tmnxAnySecMkaStatsOperOffset     Unsigned32,
    tmnxAnySecMkaStatsOperCipher     INTEGER,
    tmnxAnySecMkaStatsLatestSakLpn   Counter64,
    tmnxAnySecMkaStatsPreviousSakLpn Counter64
}

tmnxAnySecCkn                    OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE (1..32))
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The value of tmnxAnySecCkn specifies the CKN information for this MKA."
    REFERENCE
        "IEEE 802.1X Clause 9.16, Figure 12-3"
    ::= { tmnxAnySecMkaStatsEntry 1 }

tmnxAnySecMkaStatsMemberId       OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE (12))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The value of tmnxAnySecMkaStatsMemberId indicates the Member
         Identifier (MI) for the MKA instance."
    REFERENCE
        "IEEE 802.1X Clause 9.4.2"
    ::= { tmnxAnySecMkaStatsEntry 2 }

tmnxAnySecMkaStatsOutboundSci    OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE (8))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The value of tmnxAnySecMkaStatsOutboundSci indicates the Secure
         Channel Identifier (SCI) information for transmitting ANYsec packets."
    REFERENCE
        "IEEE 802.1AE Clause 7.1.2, 10.7.1 and figure 7.7"
    ::= { tmnxAnySecMkaStatsEntry 3 }

tmnxAnySecMkaStatsMessageNumber  OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The value of tmnxAnySecMkaStatsMessageNumber indicates the current
         count of MKA messages that is attached to MKA PDUs."
    ::= { tmnxAnySecMkaStatsEntry 4 }

tmnxAnySecMkaStatsKeyNumber      OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The value of tmnxAnySecMkaStatsKeyNumber indicates the number of the
         currently assigned CAK. When a new CAK is generated, this number is
         incremented."
    ::= { tmnxAnySecMkaStatsEntry 5 }

tmnxAnySecMkaStatsKeyServer      OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The value of tmnxAnySecMkaStatsKeyServer indicates whether this server
         is the highest priority server in the peer group."
    ::= { tmnxAnySecMkaStatsEntry 6 }

tmnxAnySecMkaStatsLatestSakAn    OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The value of tmnxAnySecMkaStatsLatestSakAn indicates the Association
         Number (AN) of the latest Secure Association Key (SAK). This number is
         concatenated with an SCI to identify a Secure Association (SA)."
    ::= { tmnxAnySecMkaStatsEntry 7 }

tmnxAnySecMkaStatsLatestSakKi    OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE (16))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The value of tmnxAnySecMkaStatsLatestSakKi indicates the Key
         Identifier (KI) of the latest SAK. This number is derived from the MI
         of the key server and the key number."
    ::= { tmnxAnySecMkaStatsEntry 8 }

tmnxAnySecMkaStatsPreviousSakAn  OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The value of tmnxAnySecMkaStatsPreviousSakAn indicates the AN of the
         previous SAK. This number is concatenated with an SCI to identify an
         SA."
    ::= { tmnxAnySecMkaStatsEntry 9 }

tmnxAnySecMkaStatsPreviousSakKi  OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE (16))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The value of tmnxAnySecMkaStatsPreviousSakKi indicates the KI of the
         previous SAK. This number is derived from the MI of the key server and
         the key number."
    ::= { tmnxAnySecMkaStatsEntry 10 }

tmnxAnySecMkaStatsPeerRemTimeout OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
