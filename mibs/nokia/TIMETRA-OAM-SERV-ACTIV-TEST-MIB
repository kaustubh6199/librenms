tmnxOamSathCfgSvcStrmEntry       OBJECT-TYPE
    SYNTAX      TmnxOamSathCfgSvcStrmEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "tmnxOamSathCfgSvcStrmEntry contains the configuration information for
         one service stream.

         Rows are created or destroyed using tmnxOamSathCfgSvcStrmRowStatus."
    INDEX       {
        tmnxOamSathCfgSvcTestName,
        tmnxOamSathCfgSvcStrmNum
    }
    ::= { tmnxOamSathCfgSvcStrmTable 1 }

TmnxOamSathCfgSvcStrmEntry       ::= SEQUENCE
{
    tmnxOamSathCfgSvcStrmNum         Unsigned32,
    tmnxOamSathCfgSvcStrmRowStatus   RowStatus,
    tmnxOamSathCfgSvcStrmLastChanged TimeStamp,
    tmnxOamSathCfgSvcStrmAdminState  TmnxEnabledDisabledAdminState,
    tmnxOamSathCfgSvcStrmDescription TItemDescription,
    tmnxOamSathCfgSvcStrmCIR         TruthValue,
    tmnxOamSathCfgSvcStrmCirPir      TruthValue,
    tmnxOamSathCfgSvcStrmPolicing    TruthValue,
    tmnxOamSathCfgSvcStrmPerformance TruthValue,
    tmnxOamSathCfgSvcStrmTxCIR       Unsigned32,
    tmnxOamSathCfgSvcStrmTxPIR       Unsigned32,
    tmnxOamSathCfgSvcStrmDestMacAddr MacAddress,
    tmnxOamSathCfgSvcStrmSrcMepId    Dot1agCfmMepIdOrZero,
    tmnxOamSathCfgSvcStrmSrcMdIndex  Unsigned32,
    tmnxOamSathCfgSvcStrmSrcMaIndex  Unsigned32,
    tmnxOamSathCfgSvcStrmCustTagPrio Unsigned32,
    tmnxOamSathCfgSvcStrmCustTagDEI  INTEGER,
    tmnxOamSathCfgSvcStrmSvcTagPrio  Unsigned32,
    tmnxOamSathCfgSvcStrmSvcTagDEI   INTEGER,
    tmnxOamSathCfgSvcStrmPadPattern  Unsigned32,
    tmnxOamSathCfgSvcStrmFrmSizeSeq  DisplayString,
    tmnxOamSathCfgSvcStrmFrmSizeTmpl TLNamedItemOrEmpty,
    tmnxOamSathCfgSvcStrmAcpCritTmpl TLNamedItemOrEmpty,
    tmnxOamSathCfgSvcStrmComplNotif  TmnxEnabledDisabled
}

tmnxOamSathCfgSvcStrmNum         OBJECT-TYPE
    SYNTAX      Unsigned32 (1..8)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The value of tmnxOamSathCfgSvcStrmNum specifies the number of the
         service stream to be configured or read.  The service stream
         number influences the execution order of tests belonging to the
         parent service test."
    ::= { tmnxOamSathCfgSvcStrmEntry 1 }

