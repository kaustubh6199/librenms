tmnxOamSathCfgSvcStrmDestMacAddr OBJECT-TYPE
    SYNTAX      MacAddress
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxOamSathCfgSvcStrmDestMacAddr specifies the
         destination MAC address for this service stream.

         00:00:00:00:00:00 indicates a destination MAC address is not
         available."
    DEFVAL      { '000000000000'H }
    ::= { tmnxOamSathCfgSvcStrmEntry 12 }

tmnxOamSathCfgSvcStrmSrcMepId    OBJECT-TYPE
    SYNTAX      Dot1agCfmMepIdOrZero
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxOamSathCfgSvcStrmSrcMepId specifies the source
         Ethernet Maintenance association End Point (MEP) identifier for this
         service stream.

         The MEP/domain/association triple
             tmnxOamSathCfgSvcStrmSrcMepId
             tmnxOamSathCfgSvcStrmSrcMdIndex
             tmnxOamSathCfgSvcStrmSrcMaIndex
         must refer to an Eth-CFM endpoint on a SAP on an EPIPE or VPLS
         service.

         Zero indicates a MEP is not available."
    DEFVAL      { 0 }
    ::= { tmnxOamSathCfgSvcStrmEntry 13 }

tmnxOamSathCfgSvcStrmSrcMdIndex  OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxOamSathCfgSvcStrmSrcMdIndex specifies the source
         Eth-CFM domain for this service stream.

         Zero indicates a domain is not available."
    DEFVAL      { 0 }
    ::= { tmnxOamSathCfgSvcStrmEntry 14 }

tmnxOamSathCfgSvcStrmSrcMaIndex  OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxOamSathCfgSvcStrmSrcMaIndex specifies the source
         Eth-CFM association for this service stream.

         Zero indicates an association is not available."
    DEFVAL      { 0 }
    ::= { tmnxOamSathCfgSvcStrmEntry 15 }

tmnxOamSathCfgSvcStrmCustTagPrio OBJECT-TYPE
    SYNTAX      Unsigned32 (0..7)
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxOamSathCfgSvcStrmCustTagPrio specifies the Priority
         Code Point (PCP) to be copied into the IEEE 802.1q Customer Tag
         (C-Tag) header sent for this service stream."
    DEFVAL      { 7 }
    ::= { tmnxOamSathCfgSvcStrmEntry 16 }

tmnxOamSathCfgSvcStrmCustTagDEI  OBJECT-TYPE
    SYNTAX      INTEGER {
        true  (1),
        false (2)
    }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxOamSathCfgSvcStrmCustTagDEI specifies the Drop
         Eligible Indicator (DEI) to be formatted into the IEEE 802.1q Customer
         Tag (C-Tag) header sent for this service stream."
    DEFVAL      { false }
    ::= { tmnxOamSathCfgSvcStrmEntry 17 }

tmnxOamSathCfgSvcStrmSvcTagPrio  OBJECT-TYPE
    SYNTAX      Unsigned32 (0..7)
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxOamSathCfgSvcStrmSvcTagPrio specifies the Priority
         Code Point (PCP) to be copied into the IEEE 802.1q Service Tag (S-Tag)
         header sent for this service stream."
    DEFVAL      { 7 }
    ::= { tmnxOamSathCfgSvcStrmEntry 18 }

tmnxOamSathCfgSvcStrmSvcTagDEI   OBJECT-TYPE
    SYNTAX      INTEGER {
        true  (1),
        false (2)
    }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxOamSathCfgSvcStrmSvcTagDEI specifies the Drop
         Eligible Indicator (DEI) to be formatted into the IEEE 802.1q Service
         Tag (S-Tag) header sent for this service stream."
    DEFVAL      { false }
    ::= { tmnxOamSathCfgSvcStrmEntry 19 }

tmnxOamSathCfgSvcStrmPadPattern  OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxOamSathCfgSvcStrmPadPattern specifies the four byte
         pattern used (repeatedly) to pad each packet sent by the tests
         belonging to this service stream."
    DEFVAL      { 0 }
    ::= { tmnxOamSathCfgSvcStrmEntry 20 }

tmnxOamSathCfgSvcStrmFrmSizeSeq  OBJECT-TYPE
    SYNTAX      DisplayString (SIZE (1..16))
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxOamSathCfgSvcStrmFrmSizeSeq specifies the sequence of
         frame sizes to be sent by the tests belonging to this service stream.

         For example, 'aua' specifies the frames sent must cycle
         through the sizes
             tmnxOamSathCfgFrmSizeA,
             tmnxOamSathCfgFrmSizeU,
             tmnxOamSathCfgFrmSizeA.

         In this release, only a single letter (e.g. 'u') is supported."
    DEFVAL      { "a" }
    ::= { tmnxOamSathCfgSvcStrmEntry 21 }

tmnxOamSathCfgSvcStrmFrmSizeTmpl OBJECT-TYPE
    SYNTAX      TLNamedItemOrEmpty
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxOamSathCfgSvcStrmFrmSizeTmpl specifies the frame size
         template used by this service stream.

         The value must match the INDEX of a row in tmnxOamSathCfgFrmSizeTable
         (i.e. tmnxOamSathCfgFrmSizeName)."
    DEFVAL      { "default" }
    ::= { tmnxOamSathCfgSvcStrmEntry 22 }

tmnxOamSathCfgSvcStrmAcpCritTmpl OBJECT-TYPE
    SYNTAX      TLNamedItemOrEmpty
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxOamSathCfgSvcStrmAcpCritTmpl specifies the acceptance
         criteria template used by this service stream.

         The value must match the INDEX of a row in tmnxOamSathCfgAcpCritTable
         (i.e. tmnxOamSathCfgAcpCritName)."
    DEFVAL      { "default" }
    ::= { tmnxOamSathCfgSvcStrmEntry 23 }

tmnxOamSathCfgSvcStrmComplNotif  OBJECT-TYPE
    SYNTAX      TmnxEnabledDisabled
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxOamSathCfgSvcStrmComplNotif specifies whether or not
         to raise a tmnxOamSathSvcStrmCompleted trap when the service stream
         completes."
    DEFVAL      { disabled }
    ::= { tmnxOamSathCfgSvcStrmEntry 24 }

tmnxOamSathCfgAcpCritTable       OBJECT-TYPE
    SYNTAX      SEQUENCE OF TmnxOamSathCfgAcpCritEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "tmnxOamSathCfgAcpCritTable contains the configuration information for
         Y.1564 acceptance criteria templates."
    REFERENCE
        "ITU-T Y.1564, 'Ethernet Service Activation Test Methodology', 02/2016"
    ::= { tmnxOamSathCfgTableObjs 3 }

tmnxOamSathCfgAcpCritEntry       OBJECT-TYPE
    SYNTAX      TmnxOamSathCfgAcpCritEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "tmnxOamSathCfgAcpCritEntry contains the configuration information for
         one Y.1564 acceptance criteria template.

         Rows are created or destroyed using tmnxOamSathCfgAcpCritRowStatus.

         At CPM start up, SROS creates a row for the acceptance criteria
         template 'default'.  The row cannot be deleted or changed.

         If a row is referenced by an administratively enabled service stream,
         objects in the row cannot be changed."
    INDEX       { tmnxOamSathCfgAcpCritName }
    ::= { tmnxOamSathCfgAcpCritTable 1 }

TmnxOamSathCfgAcpCritEntry       ::= SEQUENCE
{
    tmnxOamSathCfgAcpCritName        TLNamedItem,
    tmnxOamSathCfgAcpCritRowStatus   RowStatus,
    tmnxOamSathCfgAcpCritLastChanged TimeStamp,
    tmnxOamSathCfgAcpCritDescription TItemDescription,
    tmnxOamSathCfgAcpCritThldCIR     Unsigned32,
    tmnxOamSathCfgAcpCritThldPIR     Unsigned32,
    tmnxOamSathCfgAcpCritThldFLR     Integer32,
    tmnxOamSathCfgAcpCritThldFlrPol  Integer32,
    tmnxOamSathCfgAcpCritThldDelay   Unsigned32,
    tmnxOamSathCfgAcpCritThldDelayVr Unsigned32,
    tmnxOamSathCfgAcpCritMFactor     Unsigned32
}

tmnxOamSathCfgAcpCritName        OBJECT-TYPE
    SYNTAX      TLNamedItem
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The value of tmnxOamSathCfgAcpCritName specifies the name of the
         acceptance criteria template to be configured or read."
    ::= { tmnxOamSathCfgAcpCritEntry 1 }

tmnxOamSathCfgAcpCritRowStatus   OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxOamSathCfgAcpCritRowStatus specifies the status of
         the configuration row for the acceptance criteria template."
    ::= { tmnxOamSathCfgAcpCritEntry 2 }

tmnxOamSathCfgAcpCritLastChanged OBJECT-TYPE
    SYNTAX      TimeStamp
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The value of tmnxOamSathCfgAcpCritLastChanged indicates the most
         recent time the value of a settable object in this row was changed."
    ::= { tmnxOamSathCfgAcpCritEntry 3 }

tmnxOamSathCfgAcpCritDescription OBJECT-TYPE
    SYNTAX      TItemDescription
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxOamSathCfgAcpCritDescription specifies the
         descriptive text for the acceptance criteria template."
    DEFVAL      { "" }
    ::= { tmnxOamSathCfgAcpCritEntry 4 }

tmnxOamSathCfgAcpCritThldCIR     OBJECT-TYPE
    SYNTAX      Unsigned32 (0 | 1..400000000)
    UNITS       "kilobps"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxOamSathCfgAcpCritThldCIR specifies the threshold for
         accepting the measured CIR (mCIR) for a test.

         If mCIR >= tmnxOamSathCfgAcpCritThldCIR, the test passed the CIR
         criteria.

         Example:  tmnxOamSathCfgAcpCritThldCIR = 1 specifies the
         test will pass the CIR criteria if mCIR >= 1 kilobps.

         Zero specifies no CIR threshold is in effect."
    DEFVAL      { 0 }
    ::= { tmnxOamSathCfgAcpCritEntry 5 }

tmnxOamSathCfgAcpCritThldPIR     OBJECT-TYPE
    SYNTAX      Unsigned32 (0 | 1..400000000)
    UNITS       "kilobps"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxOamSathCfgAcpCritThldPIR specifies the threshold for
         accepting the measured PIR (mPIR) for a test.

         If mPIR >= tmnxOamSathCfgAcpCritThldPIR, the test passed the PIR
         criteria.

         Example:  tmnxOamSathCfgAcpCritThldPIR = 1 specifies the
         test will pass the PIR criteria if mPIR >= 1 kilobps.

         Zero specifies no PIR threshold is in effect."
    DEFVAL      { 0 }
    ::= { tmnxOamSathCfgAcpCritEntry 6 }

tmnxOamSathCfgAcpCritThldFLR     OBJECT-TYPE
    SYNTAX      Integer32 (-1 | 0..1000000)
    UNITS       "ten-thousandth of a percent"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxOamSathCfgAcpCritThldFLR specifies the
         threshold for accepting the measured Frame Loss Ratio
         (mFLR) for the following test types:
             'cir        (1)'
             'cirPir     (2)'
             'performance(4)'.

         If mFLR <= tmnxOamSathCfgAcpCritThldFLR, the test passed the FLR
         criteria.

         Example:  tmnxOamSathCfgAcpCritThldFLR = 0 (i.e. 0.0000%)
         requires the test to run with zero frame loss.

         Negative one specifies no FLR threshold is in effect.

         tmnxOamSathCfgAcpCritThldFlrPol has the same role for a 'policing(3)'
         test."
    DEFVAL      { -1 }
    ::= { tmnxOamSathCfgAcpCritEntry 7 }

tmnxOamSathCfgAcpCritThldFlrPol  OBJECT-TYPE
    SYNTAX      Integer32 (-1 | 0..1000000)
    UNITS       "ten-thousandth of a percent"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxOamSathCfgAcpCritThldFlrPol specifies the threshold
         for accepting the measured Frame Loss Ratio (mFLR) for a 'policing(3)'
         test.

         If mFLR <= tmnxOamSathCfgAcpCritThldFlrPol, the test passed the FLR
         policing criteria.

         Example:  tmnxOamSathCfgAcpCritThldFlrPol = 250000 (i.e. 25.0000%)
         requires the test to run with (at most) the 25% frame loss
         expected in a policing test.

         Negative one specifies no FLR policing threshold is in effect.

         tmnxOamSathCfgAcpCritThldFLR has the same role for the other test
         types."
    DEFVAL      { -1 }
    ::= { tmnxOamSathCfgAcpCritEntry 8 }

tmnxOamSathCfgAcpCritThldDelay   OBJECT-TYPE
    SYNTAX      Unsigned32 (0 | 1..100000000)
    UNITS       "microseconds"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxOamSathCfgAcpCritThldDelay specifies the threshold
         for accepting the measured average delay (mDELAY) for a test.

         If mDELAY <= tmnxOamSathCfgAcpCritThldDelay, the test passed the delay
         criteria.

         Example:  tmnxOamSathCfgAcpCritThldDelay = 1 requires the test
         to run with mDELAY <= 1 microsecond.

         Zero specifies no delay threshold is in effect."
    DEFVAL      { 0 }
    ::= { tmnxOamSathCfgAcpCritEntry 9 }

tmnxOamSathCfgAcpCritThldDelayVr OBJECT-TYPE
    SYNTAX      Unsigned32 (0 | 1..100000000)
    UNITS       "microseconds"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxOamSathCfgAcpCritThldDelayVr specifies the threshold
         for accepting the measured average delay variation (mDV) for a test.

         If mDV <= tmnxOamSathCfgAcpCritThldDelayVr, the test passed the delay
         variation criteria.

         Example:  tmnxOamSathCfgAcpCritThldDelayVr = 1 requires the test
         to run with mDV <= 1 microsecond.

         Zero specifies no delay variation threshold is in effect."
    DEFVAL      { 0 }
    ::= { tmnxOamSathCfgAcpCritEntry 10 }

tmnxOamSathCfgAcpCritMFactor     OBJECT-TYPE
    SYNTAX      Unsigned32 (0 | 1..400000000)
    UNITS       "kilobps"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxOamSathCfgAcpCritMFactor specifies the throughput
         margin for the acceptance criteria template.

         Configuring a non-zero margin allows a test to pass when the measured
         throughput is less than the expected throughput.

         For example, a CIR test will pass under the following conditions:
             tmnxOamSathCfgAcpCritThldCIR     :  3000 kilobps
             tmnxOamSathCfgAcpCritMFactor     :   500 kilobps
             tmnxOamSathStsTestMeasThroughput :  2500 kilobps"
    DEFVAL      { 0 }
    ::= { tmnxOamSathCfgAcpCritEntry 11 }

tmnxOamSathCfgFrmSizeTable       OBJECT-TYPE
    SYNTAX      SEQUENCE OF TmnxOamSathCfgFrmSizeEntry
