TIMETRA-OAM-SERV-ACTIV-TEST-MIB DEFINITIONS ::= BEGIN

IMPORTS
    Dot1agCfmMepId, Dot1agCfmMepIdOrZero
                                                         FROM IEEE8021-CFM-MIB
    MODULE-COMPLIANCE, NOTIFICATION-GROUP,
    OBJECT-GROUP
                                                         FROM SNMPv2-CONF
    Counter64, Integer32, MODULE-IDENTITY,
    NOTIFICATION-TYPE, OBJECT-TYPE,
    Unsigned32
                                                         FROM SNMPv2-SMI
    DateAndTime, DisplayString, MacAddress,
    RowStatus, TEXTUAL-CONVENTION,
    TimeStamp, TruthValue
                                                         FROM SNMPv2-TC
    timetraSRMIBModules, tmnxSRConfs,
    tmnxSRNotifyPrefix, tmnxSRObjs
                                                         FROM TIMETRA-GLOBAL-MIB
    TItemDescription, TLNamedItem,
    TLNamedItemOrEmpty, TNamedItemOrEmpty,
    TmnxEnabledDisabled,
    TmnxEnabledDisabledAdminState,
    TmnxStrSapId
                                                         FROM TIMETRA-TC-MIB
    ;

timetraOamServActivTestMIBModule MODULE-IDENTITY
    LAST-UPDATED "202011160000Z"
    ORGANIZATION "Nokia"
    CONTACT-INFO
        "Nokia SROS Support
         Web: http://www.nokia.com"
    DESCRIPTION
        "This document is the SNMP MIB module for the Nokia SROS implementation
         of Y.1564 Service Activation Tests.

         Copyright 2020 Nokia. All rights reserved. Reproduction of this
         document is authorized on the condition that the foregoing copyright
         notice is included.

         This SNMP MIB module (Specification) embodies Nokia's
         proprietary intellectual property.  Nokia retains all title
         and ownership in the Specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use and
         distribute an unmodified copy of this Specification in connection with
         management of Nokia products, and without fee, provided this copyright
         notice and license appear on all copies.

         This Specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or
         performance of the Specification.

         Terminology Used in this MIB
         ----------------------------

         A 'service test' owns 0..8 'service streams'.

         A service stream owns 0..4 'tests' (e.g. a CIR test, a CIR-PIR test, a
         policing test, and a performance test).

         A service test can be started to create a 'run', which generates
         results that are visible using SNMP or in an accounting policy file.

         'Sath' is used to abbreviate 'Service Activation Test Head'."

    REVISION    "202011160000Z"
    DESCRIPTION
        "Rev 21.0                16 Nov 2020 00:00
         TIMETRA-OAM-SERV-ACTIV-TEST-MIB for SROS Release 21."

    ::= { timetraSRMIBModules 127 }

TmnxOamSathSvcTestStrRunOrder    ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "An object of type TmnxOamSathSvcTestStrRunOrder indicates, for
         example, whether or not all the CIR tests belonging to a
         service test run in parallel.  Similarly, the other test
         types."
    SYNTAX      INTEGER {
        sequential (1),
        parallel   (2)
    }

TmnxOamSathSvcTestRunOprState    ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "An object of type TmnxOamSathSvcTestRunOprState indicates the
         operational state of a run for a service test, service stream, or
         test.

         'stoppedByFault(6)' indicates the service-test was stopped because
         a fatal error occurred.
         For example:
         1.  A CPM activity switch occurred after the test started.
         2.  The IOM hosting one of the service-test's service streams
             was extracted."
    SYNTAX      INTEGER {
        none           (1),
        running        (2),
        stoppedByUser  (3),
        passed         (4),
        failed         (5),
        stoppedByFault (6)
    }

TmnxOamSathTestType              ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "An object of type TmnxOamSathTestType specifies the type of a
         particular test belonging to a service stream."
    SYNTAX      INTEGER {
        cir         (1),
        cirPir      (2),
        policing    (3),
        performance (4)
    }

TmnxOamSathTestOprState          ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "An object of type TmnxOamSathTestOprState indicates the operational
         state of a test with respect to a particular acceptance criteria."
    SYNTAX      INTEGER {
        unknown (1),
        pass    (2),
        fail    (3)
    }

