tmnxOamSathStsSvcStrmTable       OBJECT-TYPE
    SYNTAX      SEQUENCE OF TmnxOamSathStsSvcStrmEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "tmnxOamSathStsSvcStrmTable contains the status information for service
         stream runs."
    REFERENCE
        "ITU-T Y.1564, 'Ethernet Service Activation Test Methodology', 02/2016"
    ::= { tmnxOamSathStatsTableObjs 2 }

tmnxOamSathStsSvcStrmEntry       OBJECT-TYPE
    SYNTAX      TmnxOamSathStsSvcStrmEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "tmnxOamSathStsSvcStrmEntry contains the status information for one
         service stream run."
    INDEX       {
        tmnxOamSathCfgSvcTestName,
        tmnxOamSathStsSvcTestRun,
        tmnxOamSathCfgSvcStrmNum
    }
    ::= { tmnxOamSathStsSvcStrmTable 1 }

TmnxOamSathStsSvcStrmEntry       ::= SEQUENCE
{
    tmnxOamSathStsSvcStrmOprState    TmnxOamSathSvcTestRunOprState,
    tmnxOamSathStsSvcStrmSrcSAP      TmnxStrSapId,
    tmnxOamSathStsSvcStrmDescription TItemDescription,
    tmnxOamSathStsSvcStrmCIR         TruthValue,
    tmnxOamSathStsSvcStrmCirPir      TruthValue,
    tmnxOamSathStsSvcStrmPolicing    TruthValue,
    tmnxOamSathStsSvcStrmPerformance TruthValue,
    tmnxOamSathStsSvcStrmTxCIR       Unsigned32,
    tmnxOamSathStsSvcStrmTxPIR       Unsigned32,
    tmnxOamSathStsSvcStrmSrcMacAddr  MacAddress,
    tmnxOamSathStsSvcStrmDestMacAddr MacAddress,
    tmnxOamSathStsSvcStrmSrcMepId    Dot1agCfmMepId,
    tmnxOamSathStsSvcStrmSrcMdIndex  Unsigned32,
    tmnxOamSathStsSvcStrmSrcMaIndex  Unsigned32,
    tmnxOamSathStsSvcStrmCustTagPrio Unsigned32,
    tmnxOamSathStsSvcStrmCustTagDEI  TruthValue,
    tmnxOamSathStsSvcStrmSvcTagPrio  Unsigned32,
    tmnxOamSathStsSvcStrmSvcTagDEI   TruthValue,
    tmnxOamSathStsSvcStrmPadPattern  Unsigned32,
    tmnxOamSathStsSvcStrmFrmSizeSeq  DisplayString,
    tmnxOamSathStsSvcStrmFrmSizeTmpl TLNamedItem,
    tmnxOamSathStsSvcStrmAcpCritTmpl TLNamedItem,
    tmnxOamSathStsSvcStrmComplNotif  TmnxEnabledDisabled,
    tmnxOamSathStsSvcStrmAcThldCIR   Unsigned32,
    tmnxOamSathStsSvcStrmAcThldPIR   Unsigned32,
    tmnxOamSathStsSvcStrmAcThldFLR   Integer32,
    tmnxOamSathStsSvcStrmAcThldFlrPl Integer32,
    tmnxOamSathStsSvcStrmAcThldDelay Unsigned32,
    tmnxOamSathStsSvcStrmAcThldDlyVr Unsigned32,
    tmnxOamSathStsSvcStrmAcMFactor   Unsigned32,
    tmnxOamSathStsSvcStrmFrmSizeA    Unsigned32,
    tmnxOamSathStsSvcStrmFrmSizeB    Unsigned32,
    tmnxOamSathStsSvcStrmFrmSizeC    Unsigned32,
    tmnxOamSathStsSvcStrmFrmSizeD    Unsigned32,
    tmnxOamSathStsSvcStrmFrmSizeE    Unsigned32,
    tmnxOamSathStsSvcStrmFrmSizeF    Unsigned32,
    tmnxOamSathStsSvcStrmFrmSizeG    Unsigned32,
    tmnxOamSathStsSvcStrmFrmSizeH    Unsigned32,
    tmnxOamSathStsSvcStrmFrmSizeU    Unsigned32,
    tmnxOamSathStsSvcStrmSrcRtrInst  TNamedItemOrEmpty,
    tmnxOamSathStsSvcStrmSrcRtrIf    TNamedItemOrEmpty,
    tmnxOamSathStsSvcStrmSrcRtrPort  TNamedItemOrEmpty
}

