tmnxOamSathCfgObjs               OBJECT IDENTIFIER ::= { tmnxOamSathObjs 1 }

tmnxOamSathCfgScalarObjs         OBJECT IDENTIFIER ::= { tmnxOamSathCfgObjs 1 }

tmnxOamSathCfgReportRetentionCnt OBJECT-TYPE
    SYNTAX      Unsigned32 (0..500)
    MAX-ACCESS  read-write
    STATUS      obsolete
    DESCRIPTION
        "tmnxOamSathCfgReportRetentionCnt was marked obsolete in
         SROS Release 22.2.  There is no replacement object.  The
         maximum number of Service Activation test runs retained
         is now hard-coded."
    DEFVAL      { 500 }
    ::= { tmnxOamSathCfgScalarObjs 1 }

tmnxOamSathTableLastChgObjs      OBJECT IDENTIFIER ::= { tmnxOamSathCfgObjs 2 }

tmnxOamSathCfgTableObjs          OBJECT IDENTIFIER ::= { tmnxOamSathCfgObjs 3 }

tmnxOamSathCfgSvcTestTable       OBJECT-TYPE
    SYNTAX      SEQUENCE OF TmnxOamSathCfgSvcTestEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "tmnxOamSathCfgSvcTestTable contains the configuration information for
         service tests."
    REFERENCE
        "ITU-T Y.1564, 'Ethernet Service Activation Test Methodology', 02/2016"
    ::= { tmnxOamSathCfgTableObjs 1 }

tmnxOamSathCfgSvcTestEntry       OBJECT-TYPE
    SYNTAX      TmnxOamSathCfgSvcTestEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "tmnxOamSathCfgSvcTestEntry contains the configuration information for
         one service test.

         Rows are created or destroyed using tmnxOamSathCfgSvcTestRowStatus."
    INDEX       { tmnxOamSathCfgSvcTestName }
    ::= { tmnxOamSathCfgSvcTestTable 1 }

TmnxOamSathCfgSvcTestEntry       ::= SEQUENCE
{
    tmnxOamSathCfgSvcTestName        TLNamedItem,
    tmnxOamSathCfgSvcTestRowStatus   RowStatus,
    tmnxOamSathCfgSvcTestLastChanged TimeStamp,
    tmnxOamSathCfgSvcTestAdminState  TmnxEnabledDisabledAdminState,
    tmnxOamSathCfgSvcTestRunOprState INTEGER,
    tmnxOamSathCfgSvcTestDescription TItemDescription,
    tmnxOamSathCfgSvcTestAccntPolicy Unsigned32,
    tmnxOamSathCfgSvcTestComplNotif  TmnxEnabledDisabled,
    tmnxOamSathCfgSvcTestStrRunOrder TmnxOamSathSvcTestStrRunOrder,
    tmnxOamSathCfgSvcTestDrCirMin    Unsigned32,
    tmnxOamSathCfgSvcTestDrCirSec    Unsigned32,
    tmnxOamSathCfgSvcTestDrCirPirMin Unsigned32,
    tmnxOamSathCfgSvcTestDrCirPirSec Unsigned32,
    tmnxOamSathCfgSvcTestDrPoliceMin Unsigned32,
    tmnxOamSathCfgSvcTestDrPoliceSec Unsigned32,
    tmnxOamSathCfgSvcTestDrPerfHours Unsigned32,
    tmnxOamSathCfgSvcTestDrPerfMin   Unsigned32,
    tmnxOamSathCfgSvcTestDrPerfSec   Unsigned32
}

tmnxOamSathCfgSvcTestName        OBJECT-TYPE
    SYNTAX      TLNamedItem
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The value of tmnxOamSathCfgSvcTestName specifies the name of the
         service test to be configured or read."
    ::= { tmnxOamSathCfgSvcTestEntry 1 }

tmnxOamSathCfgSvcTestRowStatus   OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxOamSathCfgSvcTestRowStatus specifies the status of
         the configuration row for the service test."
    ::= { tmnxOamSathCfgSvcTestEntry 2 }

tmnxOamSathCfgSvcTestLastChanged OBJECT-TYPE
    SYNTAX      TimeStamp
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The value of tmnxOamSathCfgSvcTestLastChanged indicates the most
         recent time the value of a settable object in this row was changed."
    ::= { tmnxOamSathCfgSvcTestEntry 3 }

tmnxOamSathCfgSvcTestAdminState  OBJECT-TYPE
    SYNTAX      TmnxEnabledDisabledAdminState
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxOamSathCfgSvcTestAdminState specifies the
         administrative state of the service test.

         When this value is 'enabled(1)', only the following objects in the row
         can be changed:
             tmnxOamSathCfgSvcTestAdminState,
             tmnxOamSathCfgSvcTestDescription,
             tmnxOamSathCfgSvcTestRunOprState.

         Setting this value to 'disabled(2)' has an operational side effect:
         the service-test's active run (if any) is stopped."
    DEFVAL      { disabled }
    ::= { tmnxOamSathCfgSvcTestEntry 4 }

tmnxOamSathCfgSvcTestRunOprState OBJECT-TYPE
    SYNTAX      INTEGER {
        none    (1),
        running (2),
        start   (3),
        stop    (4)
    }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxOamSathCfgSvcTestRunOprState specifies the
         operational state of the service test.

         'none(1)' indicates there is no currently active run for this service
         test.

         'running(2)' indicates there is a currently active run for this
         service test.

         'start(3)' requests a run be started for this service test.  A
         service test can have at most one active run at a time.

         'stop(4)' requests the currently active run for this service test be
         stopped.

         The following table specifies:
         a) if the code point can be returned by snmpget, and
         b) if snmpset can be used to change the value to the
            specified code point.

         Code Point     snmpget   snmpset
         ------------   -------   -------
         'none   (1)'      Y         N
         'running(2)'      Y         N
         'start  (3)'      N         Y
         'stop   (4)'      N         Y "
    DEFVAL      { none }
    ::= { tmnxOamSathCfgSvcTestEntry 5 }

tmnxOamSathCfgSvcTestDescription OBJECT-TYPE
    SYNTAX      TItemDescription
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxOamSathCfgSvcTestDescription specifies the
         descriptive text for the service test."
    DEFVAL      { "" }
    ::= { tmnxOamSathCfgSvcTestEntry 6 }

tmnxOamSathCfgSvcTestAccntPolicy OBJECT-TYPE
    SYNTAX      Unsigned32 (0 | 1..99)
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxOamSathCfgSvcTestAccntPolicy specifies the
         accounting policy for the service test.  Zero specifies that
         no accounting policy has been assigned.

         The assigned accounting policy must have
         TIMETRA-LOG-MIB::tmnxLogApRecord = 'completeSvcActivTestHead(76)'.

         An accounting policy is used to record the results of the service test
         in a file."
    DEFVAL      { 0 }
    ::= { tmnxOamSathCfgSvcTestEntry 7 }

tmnxOamSathCfgSvcTestComplNotif  OBJECT-TYPE
    SYNTAX      TmnxEnabledDisabled
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of tmnxOamSathCfgSvcTestComplNotif specifies whether or not
         to raise a tmnxOamSathSvcTestCompleted trap when the service test
         completes."
    DEFVAL      { disabled }
    ::= { tmnxOamSathCfgSvcTestEntry 8 }

tmnxOamSathCfgSvcTestStrRunOrder OBJECT-TYPE
