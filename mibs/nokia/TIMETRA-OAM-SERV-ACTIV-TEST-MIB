tmnxOamSathStatsObjs             OBJECT IDENTIFIER ::= { tmnxOamSathObjs 2 }

tmnxOamSathStatsScalarObjs       OBJECT IDENTIFIER ::= { tmnxOamSathStatsObjs 1 }

tmnxOamSathStsSvcTestLimit       OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The value of tmnxOamSathStsSvcTestLimit indicates the maximum number
         of configured service tests.

         See the tmnxOamSathStsSvcTestCount DESCRIPTION clause for more
         information."
    ::= { tmnxOamSathStatsScalarObjs 1 }

tmnxOamSathStsSvcTestCount       OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The value of tmnxOamSathStsSvcTestCount indicates the current number
         of configured service tests.

         The count is incremented when a service test is created (using
         tmnxOamSathCfgSvcTestRowStatus).

         The count is decremented when a service test is destroyed.

         Creation of a service test fails when tmnxOamSathStsSvcTestCount
         equals tmnxOamSathStsSvcTestLimit."
    ::= { tmnxOamSathStatsScalarObjs 2 }

tmnxOamSathStatsTableObjs        OBJECT IDENTIFIER ::= { tmnxOamSathStatsObjs 2 }

tmnxOamSathStsSvcTestTable       OBJECT-TYPE
    SYNTAX      SEQUENCE OF TmnxOamSathStsSvcTestEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "tmnxOamSathStsSvcTestTable contains the status information for service
         test runs."
    REFERENCE
        "ITU-T Y.1564, 'Ethernet Service Activation Test Methodology', 02/2016"
    ::= { tmnxOamSathStatsTableObjs 1 }

tmnxOamSathStsSvcTestEntry       OBJECT-TYPE
    SYNTAX      TmnxOamSathStsSvcTestEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "tmnxOamSathStsSvcTestEntry contains the status information for one
         service test run."
    INDEX       {
        tmnxOamSathCfgSvcTestName,
        tmnxOamSathStsSvcTestRun
    }
    ::= { tmnxOamSathStsSvcTestTable 1 }

TmnxOamSathStsSvcTestEntry       ::= SEQUENCE
{
    tmnxOamSathStsSvcTestRun         Unsigned32,
    tmnxOamSathStsSvcTestDescription TItemDescription,
    tmnxOamSathStsSvcTestOprState    TmnxOamSathSvcTestRunOprState,
    tmnxOamSathStsSvcTestTimeStart   DateAndTime,
    tmnxOamSathStsSvcTestTimeEnd     DateAndTime,
    tmnxOamSathStsSvcTestAccntPolicy Unsigned32,
    tmnxOamSathStsSvcTestAccntOprSta INTEGER,
    tmnxOamSathStsSvcTestComplNotif  TmnxEnabledDisabled,
    tmnxOamSathStsSvcTestStrRunOrder TmnxOamSathSvcTestStrRunOrder,
    tmnxOamSathStsSvcTestDrCirMin    Unsigned32,
    tmnxOamSathStsSvcTestDrCirSec    Unsigned32,
    tmnxOamSathStsSvcTestDrCirPirMin Unsigned32,
    tmnxOamSathStsSvcTestDrCirPirSec Unsigned32,
    tmnxOamSathStsSvcTestDrPoliceMin Unsigned32,
    tmnxOamSathStsSvcTestDrPoliceSec Unsigned32,
    tmnxOamSathStsSvcTestDrPerfHours Unsigned32,
    tmnxOamSathStsSvcTestDrPerfMin   Unsigned32,
    tmnxOamSathStsSvcTestDrPerfSec   Unsigned32
}

tmnxOamSathStsSvcTestRun         OBJECT-TYPE
    SYNTAX      Unsigned32 (1..4294967295)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The value of tmnxOamSathStsSvcTestRun specifies the run number of the
         service test run to be read."
    ::= { tmnxOamSathStsSvcTestEntry 1 }

tmnxOamSathStsSvcTestDescription OBJECT-TYPE
    SYNTAX      TItemDescription
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The value of tmnxOamSathStsSvcTestDescription indicates the value of
         tmnxOamSathCfgSvcTestDescription when the run started."
    ::= { tmnxOamSathStsSvcTestEntry 2 }

tmnxOamSathStsSvcTestOprState    OBJECT-TYPE
    SYNTAX      TmnxOamSathSvcTestRunOprState
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The value of tmnxOamSathStsSvcTestOprState indicates the operational
         state of the service test run.

         'none(1)' will not be returned."
    ::= { tmnxOamSathStsSvcTestEntry 3 }

tmnxOamSathStsSvcTestTimeStart   OBJECT-TYPE
    SYNTAX      DateAndTime (SIZE (8))
    UNITS       "UTC"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The value of tmnxOamSathStsSvcTestTimeStart indicates the start time
         of the service test run.

         1970/01/01 00:00:00.0 is returned if a start time is not available."
    ::= { tmnxOamSathStsSvcTestEntry 4 }

tmnxOamSathStsSvcTestTimeEnd     OBJECT-TYPE
    SYNTAX      DateAndTime (SIZE (8))
    UNITS       "UTC"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The value of tmnxOamSathStsSvcTestTimeEnd indicates the end time of
         the service test run.

         1970/01/01 00:00:00.0 is returned if an end time is not available."
    ::= { tmnxOamSathStsSvcTestEntry 5 }

