    DEFVAL      { 0 }
    ::= { vRtrSrv6LocatorEntry 5 }

vRtrSrv6LocatorFuncLength        OBJECT-TYPE
    SYNTAX      InetAddressPrefixLength (16 | 20..96)
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of vRtrSrv6LocatorFuncLength specifies the value of the
         function prefix length."
    DEFVAL      { 20 }
    ::= { vRtrSrv6LocatorEntry 6 }

vRtrSrv6LocatorTermFpe           OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
