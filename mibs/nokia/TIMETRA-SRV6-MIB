vRtrSrv6Configurations           OBJECT IDENTIFIER ::= { vRtrSrv6Objs 2 }

vRtrSrv6Table                    OBJECT-TYPE
    SYNTAX      SEQUENCE OF VRtrSrv6Entry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The vRtrSrv6Table has an entry for segment routing v6 configured for a
         virtual router in the system."
    ::= { vRtrSrv6Configurations 1 }

vRtrSrv6Entry                    OBJECT-TYPE
    SYNTAX      VRtrSrv6Entry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each row entry represents a configuration for  segment routing v6 for
         a virtual router in the system.

         Entries can be created and deleted via SNMP SET operations."
    INDEX       { vRtrID }
    ::= { vRtrSrv6Table 1 }

VRtrSrv6Entry                    ::= SEQUENCE
{
    vRtrSrv6RowStatus                RowStatus,
    vRtrSrv6LastChanged              TimeStamp,
    vRtrSrv6SourceAddrType           InetAddressType,
    vRtrSrv6SourceAddr               InetAddress,
    vRtrSrv6OriginationFpe           Unsigned32
}

vRtrSrv6RowStatus                OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of vRtrSrv6RowStatus controls the creation and deletion of
         rows in the table."
    ::= { vRtrSrv6Entry 1 }

vRtrSrv6LastChanged              OBJECT-TYPE
    SYNTAX      TimeStamp
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The value of vRtrSrv6LastChanged indicates the sysUpTime when this row
         was last modified."
    ::= { vRtrSrv6Entry 2 }

vRtrSrv6SourceAddrType           OBJECT-TYPE
    SYNTAX      InetAddressType
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of vRtrSrv6SourceAddrType specifies the type of
         vRtrSrv6SourceAddr."
    DEFVAL      { unknown }
    ::= { vRtrSrv6Entry 3 }

vRtrSrv6SourceAddr               OBJECT-TYPE
    SYNTAX      InetAddress (SIZE (0|16))
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of vRtrSrv6SourceAddr specifies the source address."
    DEFVAL      { ''H }
    ::= { vRtrSrv6Entry 4 }

vRtrSrv6OriginationFpe           OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of vRtrSrv6OriginationFpe configures the fpe for srv6
         locator."
    DEFVAL      { 0 }
    ::= { vRtrSrv6Entry 5 }

vRtrSrv6LocatorTable             OBJECT-TYPE
    SYNTAX      SEQUENCE OF VRtrSrv6LocatorEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The vRtrSrv6LocatorTable has an entry for each segment routing locator
         configured for a virtual router in the system."
    ::= { vRtrSrv6Configurations 2 }

vRtrSrv6LocatorEntry             OBJECT-TYPE
    SYNTAX      VRtrSrv6LocatorEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each row entry represents a configuration of a ipv6 locator  in the
         segment routing for a virtual router in the system.

         Entries can be created and deleted via SNMP SET operations."
    INDEX       {
        vRtrID,
        vRtrSrv6LocatorName
    }
    ::= { vRtrSrv6LocatorTable 1 }

VRtrSrv6LocatorEntry             ::= SEQUENCE
{
    vRtrSrv6LocatorName              TLNamedItem,
    vRtrSrv6LocatorRowStatus         RowStatus,
    vRtrSrv6LocatorLastChanged       TimeStamp,
    vRtrSrv6LocatorAdminState        TmnxAdminState,
    vRtrSrv6LocatorBlockLength       InetAddressPrefixLength,
    vRtrSrv6LocatorFuncLength        InetAddressPrefixLength,
    vRtrSrv6LocatorTermFpe           Unsigned32,
    vRtrSrv6LocatorPrefixAddrType    InetAddressType,
    vRtrSrv6LocatorPrefixAddr        InetAddress,
    vRtrSrv6LocatorPrefixLength      InetAddressPrefixLength,
    vRtrSrv6LocatorMaxEntries        Unsigned32,
    vRtrSrv6LocatorLabelBlock        TLNamedItemOrEmpty,
    vRtrSrv6LocatorAlgorithm         TmnxAlgorithmId,
    vRtrSrv6LocatorUnifiedLabelBlock TLNamedItemOrEmpty,
    vRtrSrv6LocatorOperState         TmnxOperState,
    vRtrSrv6LocatorOperErrorCode     TSrv6FunctionErrorCode
}

