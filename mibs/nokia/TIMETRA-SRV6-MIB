        vRtrSrv6MSFuncUaDynSidAddr,
        vRtrSrv6MSFuncUaDynSrhMode,
        vRtrSrv6MSFuncUaDynProtected,
        vRtrSrv6MSFuncUaDynIfName,
        vRtrSrv6MSFuncUaDynIsisSidLevel,
        vRtrSrv6MSFuncUaDynIsisMacAddr,
        vRtrSrv6MSFuncUaDynIsisNbrSysID,
        vRtrSrv6MSFuncUaDynInstId,
        vRtrSrv6MSLocalSidFunctionType,
        vRtrSrv6MSLocalSidFunctionValue,
        vRtrSrv6MSLocalSidLocatorName,
        vRtrSrv6MSLocalSidContextType,
        vRtrSrv6MSLocalSidSvcId
    }
    STATUS      current
    DESCRIPTION
        "The group of objects supporting segment routing v6 micro segment
         feature on Nokia SROS series systems."
    ::= { vRtrSrv6Groups 6 }

vRtrSrv6LocatorOperStateGroup    OBJECT-GROUP
    OBJECTS     {
        vRtrSrv6LocatorOperState,
        vRtrSrv6LocatorOperErrorCode
    }
    STATUS      current
    DESCRIPTION
        "The group of objects supporting segment-routing-v6 locator operational
         state on Nokia SROS series systems."
    ::= { vRtrSrv6Groups 7 }

vRtrSrv6NotifyObjsV22Group       OBJECT-GROUP
    OBJECTS     {
        vRtrSrv6FunctionValue
    }
    STATUS      current
    DESCRIPTION
        "The group of objects supporting SRv6 resource exhaustion notifications
          on virtual routers for Nokia SROS series systems."
    ::= { vRtrSrv6Groups 8 }

vRtrSrv6LocResNotificationGroup  NOTIFICATION-GROUP
    NOTIFICATIONS {
        vRtrSrv6LocatorResExhaustion
    }
    STATUS      current
    DESCRIPTION
        "The group of objects supporting SRv6 locator resource exhaustion
         notification on virtual routers for Nokia SROS series systems."
    ::= { vRtrSrv6Groups 9 }

tmnxSrv6NotifyPrefix             OBJECT IDENTIFIER ::= { tmnxSRNotifyPrefix 129 }

tmnxSrv6Notifications            OBJECT IDENTIFIER ::= { tmnxSrv6NotifyPrefix 0 }

vRtrSrv6FunctionExhaustion       NOTIFICATION-TYPE
    OBJECTS     {
        vRtrID,
        vRtrSrv6LocName,
        vRtrSrv6FunctionType,
        vRtrSrv6FunctionValue
    }
    STATUS      current
    DESCRIPTION
        "[CAUSE] The vRtrSrv6FunctionExhaustion notification is  generated when
         the function or label allocation fails.

         [EFFECT] A log entry is generated.

         [RECOVERY] if another entity or local config change  returns
         resources, then it will automatically allocated."
    ::= { tmnxSrv6Notifications 1 }

vRtrSrv6SvcSidIndex              NOTIFICATION-TYPE
    OBJECTS     {
        vRtrSrv6SidIndex,
        vRtrSrv6SidIndexLevel
    }
    STATUS      current
    DESCRIPTION
        "[CAUSE] The vRtrSrv6SvcSidIndex notification is  generated when the
         service SID index is above or below  vRtrSrv6SidIndex.

         [EFFECT] A log entry is generated.

         [RECOVERY] A config or a policy change to reduce the usage."
    ::= { tmnxSrv6Notifications 2 }

vRtrSrv6LocatorResExhaustion     NOTIFICATION-TYPE
    OBJECTS     {
        vRtrID,
        vRtrSrv6LocName
    }
    STATUS      current
    DESCRIPTION
        "[CAUSE] The vRtrSrv6LocatorResExhaustion notification is  generated
         when the allocation of locator resources fails.

         [EFFECT] A log entry is generated.

         [RECOVERY] if another entity or local config change  returns
         resources, then it will automatically allocated."
    ::= { tmnxSrv6Notifications 3 }

END
