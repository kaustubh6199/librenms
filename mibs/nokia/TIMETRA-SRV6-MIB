    SYNTAX      InetAddress (SIZE (0|16))
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of vRtrSrv6LocatorPrefixAddr specifies the prefix address."
    DEFVAL      { ''H }
    ::= { vRtrSrv6LocatorEntry 11 }

vRtrSrv6LocatorPrefixLength      OBJECT-TYPE
    SYNTAX      InetAddressPrefixLength (0..96)
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of vRtrSrv6LocatorPrefixLength specifies the value of the
         locator prefix length."
    DEFVAL      { 0 }
    ::= { vRtrSrv6LocatorEntry 12 }

vRtrSrv6LocatorMaxEntries        OBJECT-TYPE
    SYNTAX      Unsigned32 (1..1048575)
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of vRtrSrv6LocatorMaxEntries specifies the total static
         label entries."
    DEFVAL      { 1 }
    ::= { vRtrSrv6LocatorEntry 13 }

vRtrSrv6LocatorLabelBlock        OBJECT-TYPE
    SYNTAX      TLNamedItemOrEmpty
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of vRtrSrv6LocatorLabelBlock specifies the reserved static
         label block for this locator. The reserved label block name is defined
         by vRtrMplsReservedLblBlkName and is the index for
         TIMETRA-MPLS-MIB::vRtrMplsReservedLblBlkTable."
    DEFVAL      { ''H }
    ::= { vRtrSrv6LocatorEntry 14 }

vRtrSrv6LocatorAlgorithm         OBJECT-TYPE
    SYNTAX      TmnxAlgorithmId (0 | 128..255)
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of the object vRtrSrv6LocatorAlgorithm specifies the
         algorithm related to the SPF statistics.

         The value can specify a defined IGP Algorithm (value 0 is SPF) or a
         Flexible Algorithm (range 128..255)."
    DEFVAL      { 0 }
    ::= { vRtrSrv6LocatorEntry 15 }

vRtrSrv6LocatorUnifiedLabelBlock OBJECT-TYPE
    SYNTAX      TLNamedItemOrEmpty
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of vRtrSrv6LocatorUnifiedLabelBlock specifies the reserved
         label block for this locator. It allows to allocate static and dynamic
         label from configured reserved label block. The reserved label block
         name is defined by vRtrMplsReservedLblBlkName and is the index for
         TIMETRA-MPLS-MIB::vRtrMplsReservedLblBlkTable."
    DEFVAL      { ''H }
    ::= { vRtrSrv6LocatorEntry 16 }

vRtrSrv6LocatorOperState         OBJECT-TYPE
    SYNTAX      TmnxOperState
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The value of vRtrSrv6LocatorOperState indicates the locator
         operational state."
    ::= { vRtrSrv6LocatorEntry 17 }

vRtrSrv6LocatorOperErrorCode     OBJECT-TYPE
    SYNTAX      TSrv6FunctionErrorCode
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The object vRtrSrv6LocatorOperErrorCode specifies the  error code
         (reason) why the locator is operationally down."
    ::= { vRtrSrv6LocatorEntry 18 }

vRtrSrv6FunctionTable            OBJECT-TYPE
    SYNTAX      SEQUENCE OF VRtrSrv6FunctionEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The vRtrSrv6FunctionTable has an entry for each segment routing dt
         function configured for a virtual router in the system."
    ::= { vRtrSrv6Configurations 3 }

vRtrSrv6FunctionEntry            OBJECT-TYPE
    SYNTAX      VRtrSrv6FunctionEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each row entry represents a configuration of a srv6 function  in the
         segment routing for a virtual router in the system.

         Entries can be created and deleted via SNMP SET operations."
    INDEX       {
        vRtrID,
        vRtrSrv6LocatorName
    }
    ::= { vRtrSrv6FunctionTable 1 }

VRtrSrv6FunctionEntry            ::= SEQUENCE
{
    vRtrSrv6FuncRowStatus            RowStatus,
    vRtrSrv6FuncLastChanged          TimeStamp,
    vRtrSrv6FuncDt4                  TSrv6FunctionType,
    vRtrSrv6FuncDt4Value             TSrv6FunctionValue,
    vRtrSrv6FuncDt4OperValue         TSrv6FunctionValue,
    vRtrSrv6FuncDt4OperState         TmnxOperState,
    vRtrSrv6FuncDt4OperErrorCode     TSrv6FunctionErrorCode,
    vRtrSrv6FuncDt4OperSidAddrType   InetAddressType,
    vRtrSrv6FuncDt4OperSidAddr       InetAddress,
    vRtrSrv6FuncDt6                  TSrv6FunctionType,
    vRtrSrv6FuncDt6Value             TSrv6FunctionValue,
    vRtrSrv6FuncDt6OperValue         TSrv6FunctionValue,
    vRtrSrv6FuncDt6OperState         TmnxOperState,
    vRtrSrv6FuncDt6OperErrorCode     TSrv6FunctionErrorCode,
    vRtrSrv6FuncDt6OperSidAddrType   InetAddressType,
    vRtrSrv6FuncDt6OperSidAddr       InetAddress,
    vRtrSrv6FuncDt46                 TSrv6FunctionType,
    vRtrSrv6FuncDt46Value            TSrv6FunctionValue,
    vRtrSrv6FuncDt46OperValue        TSrv6FunctionValue,
    vRtrSrv6FuncDt46OperState        TmnxOperState,
    vRtrSrv6FuncDt46OpErrorCode      TSrv6FunctionErrorCode,
    vRtrSrv6FuncDt46OpSidAddrType    InetAddressType,
    vRtrSrv6FuncDt46OpSidAddr        InetAddress
}

