        tmnxWlanNetworkWpaPassphrase,
        tmnxWlanAPBroadcastSSID,
        tmnxWlanAPClientLimit,
        tmnxWlanAPClientTimeout,
        tmnxWlanAPDot1xRadiusPlcy,
        tmnxWlanAPDot1xReauthPeriod,
        tmnxWlanAPDhcpAdminState,
        tmnxWlanRadioType,
