    STATUS      current
    DESCRIPTION
        "The compliance statement for the management of the WLAN feature-set
         for Release 15.0 on Nokia SROS series systems."
    MODULE
        MANDATORY-GROUPS {
            tmnxWlanConfigGroup,
            tmnxWlanOperGroup
        }
    ::= { tmnxWlanCompliances 1 }

