	SYNTAX		Integer32
	UNITS		"minutes"
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"Turn on period, minutes.
	Megatec only"
	::= { erd4ups 52 }

erd4upsStartShutdownTurnOn		OBJECT-TYPE
	SYNTAX		Integer32
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"Shutdown after shutdownTimeUPS 
	and turn on after turnOnTimeUPS for Megatec. 
	Shutdown after shutdownTimeUPS for Smart. 
	Send 1 to start, 0 to stop"
	::= { erd4ups 53 }

erd4upsStartTurnOn		OBJECT-TYPE
	SYNTAX		Integer32
	MAX-ACCESS	read-write
	STATUS		current
