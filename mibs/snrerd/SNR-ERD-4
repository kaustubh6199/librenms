SNR-ERD-4-MIB DEFINITIONS ::= BEGIN

IMPORTS
	MODULE-IDENTITY, OBJECT-TYPE, NOTIFICATION-TYPE, enterprises, 
	Integer32, Unsigned32, IpAddress, TimeTicks
		FROM SNMPv2-SMI
	DisplayString, TEXTUAL-CONVENTION
        FROM SNMPv2-TC
	MODULE-COMPLIANCE, NOTIFICATION-GROUP, OBJECT-GROUP
		FROM SNMPv2-CONF
	; 

snrErd4 MODULE-IDENTITY
		LAST-UPDATED	"202102151200Z"
		ORGANIZATION	"NAG"
		CONTACT-INFO	"erd@nag.ru"
		DESCRIPTION		"The MIB module for SNR-ERD-4"
		REVISION		"202102151200Z"
		DESCRIPTION		"Initial revision." 
		::= { snrErd 6 }
		
snr					OBJECT IDENTIFIER	::= { enterprises 40418 }
snrErd				OBJECT IDENTIFIER	::= { snr 2 }

SensorID ::= TEXTUAL-CONVENTION
   DISPLAY-HINT "d"
   STATUS       current
   DESCRIPTION
		   "This data type is a non-negative value."
   SYNTAX       Unsigned32 (0..4294967295)


erd4traps			OBJECT IDENTIFIER	::= { snrErd4 0 }
erd4measurements	OBJECT IDENTIFIER	::= { snrErd4 1 }
erd4dio				OBJECT IDENTIFIER	::= { snrErd4 2 }
erd4serial			OBJECT IDENTIFIER	::= { snrErd4 4 }
erd4modules			OBJECT IDENTIFIER	::= { snrErd4 5 }
erd4options			OBJECT IDENTIFIER	::= { snrErd4 10 }
erd4diffparams		OBJECT IDENTIFIER	::= { snrErd4 11 }
erd4Conformance		OBJECT IDENTIFIER	::= { snrErd4 100 }

erd4oneWire			OBJECT IDENTIFIER	::= { erd4measurements 1 }
erd4sp				OBJECT IDENTIFIER	::= { erd4measurements 30 }
erd4sensorsTemplate	OBJECT IDENTIFIER	::= { erd4measurements 50 }

erd4rs485			OBJECT IDENTIFIER	::= { erd4serial 1 }
erd4rs232			OBJECT IDENTIFIER	::= { erd4serial 2 }

erd4gsm				OBJECT IDENTIFIER	::= { erd4modules 1 }

erd4ups				OBJECT IDENTIFIER	::= { erd4diffparams 1 }
erd4pid				OBJECT IDENTIFIER	::= { erd4diffparams 10 }

erd4crit			OBJECT IDENTIFIER	::= { erd4options 10 }

erd4Groups			OBJECT IDENTIFIER	::= { erd4Conformance 1 }
erd4Compliances		OBJECT IDENTIFIER	::= { erd4Conformance 2 }


-- measurements

erd4adcSensor		OBJECT-TYPE
	SYNTAX		Integer32
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION	"ADC sensor value"
	::= { erd4measurements 2 }

erd4temperatureDHT		OBJECT-TYPE
	SYNTAX		Integer32
	UNITS		"degrees Centigrade"
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION	"DHT temperature value"
	::= { erd4measurements 8 }

erd4humidityDHT		OBJECT-TYPE
	SYNTAX		Integer32
	UNITS		"persents"
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION	"DHT humidity value"
	::= { erd4measurements 9 }


-- dtsTable

erd4dtsTable OBJECT-TYPE
	SYNTAX      SEQUENCE OF Erd4DtsEntry
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION	"A list of temperature sensors values"
	::= { erd4oneWire 1 }
	
erd4dtsEntry OBJECT-TYPE
	SYNTAX      Erd4DtsEntry
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION	"An entry (conceptual row) in the dtsTable"
	INDEX   { erd4dtsId } 
	::= { erd4dtsTable 1 }

Erd4DtsEntry ::= SEQUENCE {
        erd4dtsId           		SensorID,
		erd4dtsName					DisplayString,
        erd4dtsTemp            		Integer32,
		erd4dtsTempCritNotif		Integer32,
		erd4dtsTempCritMin			Integer32,
		erd4dtsTempCritMax			Integer32
    }
	
erd4dtsId 		OBJECT-TYPE
	SYNTAX      SensorID
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION	"Temperature sensor id"
	::= { erd4dtsEntry 1 }

erd4dtsName 	OBJECT-TYPE
	SYNTAX      DisplayString
	MAX-ACCESS  read-write
	STATUS      current
	DESCRIPTION	"Name (max 20)"
	::= { erd4dtsEntry 2 }

erd4dtsTemp 	OBJECT-TYPE
	SYNTAX		Integer32
	UNITS		"degrees Centigrade"
	MAX-ACCESS	read-only
	STATUS      current
	DESCRIPTION	"Temperature value"
	::= { erd4dtsEntry 3 }
	
erd4dtsTempCritNotif	OBJECT-TYPE
	SYNTAX		INTEGER { no(0), yes(1) }
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"Enable notifications on the critical temperature"
	::= { erd4dtsEntry 10 }
	
erd4dtsTempCritMin		OBJECT-TYPE
	SYNTAX		Integer32
	UNITS		"degrees Centigrade"
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"Minimal critical temperature. 
		Up to -55 degrees Celsius"
	::= { erd4dtsEntry 15 }

erd4dtsTempCritMax	OBJECT-TYPE
	SYNTAX		Integer32
	UNITS		"degrees Centigrade"
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"Maximum critical temperature. 
		Up to +125 degrees Celsius"
	::= { erd4dtsEntry 16 }


-- shuntTable

erd4shuntTable OBJECT-TYPE
	SYNTAX      SEQUENCE OF Erd4ShuntEntry
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION
			"A list of SNR-SHUNT values"
	::= { erd4oneWire 2 }

erd4shuntEntry OBJECT-TYPE
	SYNTAX      Erd4ShuntEntry
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION
			"An entry (conceptual row) in the shuntTable"
	INDEX   { erd4shuntId }
	::= { erd4shuntTable 1 }

Erd4ShuntEntry ::= SEQUENCE {
	erd4shuntId      	SensorID,
	erd4shuntName		DisplayString,
	erd4shuntTemp       Integer32,
	erd4shuntVolt       Integer32,
	erd4shuntCurrent	Integer32,
	erd4shuntChrgLvl	Integer32,
	erd4shuntChrgCap	Integer32,
	erd4shuntDischCap	Integer32,
	erd4shuntEndOfChrg	DisplayString,
	erd4shuntDiscn		DisplayString,
	erd4shuntBatLive	TimeTicks,
	erd4shuntDischTime	TimeTicks
    }

erd4shuntId 	OBJECT-TYPE
	SYNTAX      SensorID
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION
			"SNR-SHUNT id"
	::= { erd4shuntEntry 1 }

erd4shuntName OBJECT-TYPE
	SYNTAX      DisplayString
	MAX-ACCESS  read-write
	STATUS      current
	DESCRIPTION
			"Name (max 20)"
	::= { erd4shuntEntry 2 }

erd4shuntTemp OBJECT-TYPE
	SYNTAX      Integer32
	UNITS		"degrees Centigrade"
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
			"Temperature value"
	::= { erd4shuntEntry 3 }

erd4shuntVolt OBJECT-TYPE
	SYNTAX      Integer32
	UNITS		"Volt DC"
	MAX-ACCESS  read-write
	STATUS      current
	DESCRIPTION
			"Voltage"
	::= { erd4shuntEntry 4 }

erd4shuntCurrent OBJECT-TYPE
	SYNTAX      Integer32
	UNITS      "Amp DC"
	MAX-ACCESS  read-write
	STATUS      current
	DESCRIPTION
			"Current"
	::= { erd4shuntEntry 5 }

erd4shuntChrgLvl OBJECT-TYPE
	SYNTAX      Integer32
	UNITS      "percent"
	MAX-ACCESS  read-write
	STATUS      current
	DESCRIPTION
			"Battery charge level (0..100%)"
	::= { erd4shuntEntry 6 }

erd4shuntChrgCap 	OBJECT-TYPE
	SYNTAX      Integer32
	UNITS      "Amp / hour"
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
			"Charge capacity"
	::= { erd4shuntEntry 7 }

erd4shuntDischCap OBJECT-TYPE
	SYNTAX      Integer32
	UNITS      "Amp / hour"
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
			"Discharge capacity"
	::= { erd4shuntEntry 8 }

erd4shuntEndOfChrg OBJECT-TYPE
	SYNTAX      DisplayString
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
			"End of charge time"
	::= { erd4shuntEntry 9 }

erd4shuntDiscn OBJECT-TYPE
	SYNTAX      DisplayString
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
			"Disconnect time"
	::= { erd4shuntEntry 10 }

erd4shuntBatLive OBJECT-TYPE
	SYNTAX      TimeTicks
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
			"Battery using period"
	::= { erd4shuntEntry 11 }

erd4shuntDischTime OBJECT-TYPE
	SYNTAX      TimeTicks
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
			"Time to full discharge"
	::= { erd4shuntEntry 12 }


