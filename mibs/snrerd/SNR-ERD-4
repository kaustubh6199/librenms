    STATUS  current
    DESCRIPTION
            "A collection of objects providing SNR-ERD-4 options"
    ::= { erd4Groups 6 }
	
erd4diffparamsGroup OBJECT-GROUP
    OBJECTS {	erd4upsStatus, erd4upsBeeperStatus, erd4upsShutdownStatus, erd4upsLastBatteryChange,
				erd4upsShutdownTime, erd4upsTurnOnTime, erd4upsStartShutdownTurnOn, erd4upsStartTurnOn, erd4upsStartLongTest,
				erd4pidInput, erd4pidOutputPercent, erd4pidOutputReal, erd4pidYp, erd4pidYi, erd4pidYd, erd4pidError
			}
    STATUS  current
    DESCRIPTION
            "A collection of objects providing SNR-ERD-4 different parameters"
    ::= { erd4Groups 7 }
	
erd4Compliance MODULE-COMPLIANCE
	STATUS     current
	DESCRIPTION
				"The compliance statement for SNR-ERD-4 that support all functions."
	MODULE -- this module
	MANDATORY-GROUPS { 	erd4NotificationsGroup, 
						erd4MeasurementsGroup,
						erd4dioGroup,
						erd4serialGroup,
						erd4modulesGroup,
						erd4optionsGroup,
						erd4diffparamsGroup}
	::= { erd4Compliances 1 }
