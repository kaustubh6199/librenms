erd4NotificationsGroup		NOTIFICATION-GROUP  
	NOTIFICATIONS 
	{
		erd4di1Alarm,
		erd4di1Release,
		erd4di2Alarm,
		erd4di2Release,
		erd4di3Alarm,
		erd4di3Release,
		erd4di4Alarm,
		erd4di4Release,
		erd4di5Alarm,
		erd4di5Release,
		erd4uSenseAlarm,
		erd4uSenseRelease,
		erd4reloutOn,
		erd4reloutOff,
		erd4do1On,
		erd4do1Off,
		erd4do2On,
		erd4do2Off,
		erd4do3On,
		erd4do3Off,
		erd4do4On,
		erd4do4Off,
		erd4do5On,
		erd4do5Off,
		erd4tempSensorAlarm,
		erd4tempSensorRelease,
		erd4adcSensorAlarm,
		erd4adcSensorRelease,
		erd4humSensorAlarm,
		erd4humSensorRelease,
		erd4task1Done,
		erd4task2Done,
		erd4task3Done,
		erd4task4Done,
		erd4pingLost,
		erd4batteryChargeLow,
		erd4upsUtilityFail,
		erd4upsUtilityOk
	}
	STATUS			current
	DESCRIPTION		"Notifications"
	::= { erd4Groups 1 }

erd4MeasurementsGroup OBJECT-GROUP
    OBJECTS {	erd4adcSensor, erd4temperatureDHT, erd4humidityDHT,
				erd4dtsName, erd4dtsTemp, erd4dtsTempCritNotif, erd4dtsTempCritMin, erd4dtsTempCritMax,
				erd4shuntName, erd4shuntTemp, erd4shuntVolt, erd4shuntCurrent, erd4shuntChrgLvl, erd4shuntChrgCap, erd4shuntDischCap, erd4shuntEndOfChrg, erd4shuntDiscn, erd4shuntBatLive, erd4shuntDischTime,
				erd4rsshtpName, erd4rsshtpTemp, erd4rsshtpHum, erd4rsshtpPssr, erd4rsshtpTempCritNotif, erd4rsshtpTempCritMin, erd4rsshtpTempCritMax,
				erd4rscounterName,
				erd4rsshtp1WName, erd4rsshtp1WTemp, erd4rsshtp1WMaster, erd4rsshtp1WTempCritNotif, erd4rsshtp1WTempCritMin, erd4rsshtp1WTempCritMax,
				erd4rsem115ApparentPower, 
				erd4sensorsTemplateValue, erd4sensorsTemplateName, erd4sensorsTemplateTotalConnected
			}
    STATUS  current
    DESCRIPTION
            "A collection of objects providing SNR-ERD-4 measurements"
    ::= { erd4Groups 2 }
	
