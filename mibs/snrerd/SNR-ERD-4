-- dio

-- diTable

erd4diTable		OBJECT-TYPE
	SYNTAX		SEQUENCE OF Erd4DiEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION	"A list of digital inputs values"
	::= { erd4dio 1 }

erd4diEntry		OBJECT-TYPE
	SYNTAX		Erd4DiEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION	"An entry (conceptual row) 
	in the diTable"
	INDEX   { erd4diName } 
	::= { erd4diTable 1 }

Erd4DiEntry ::= SEQUENCE {
	erd4diName		DisplayString,
	erd4diAlarmName	DisplayString,
	erd4diState		INTEGER,
	erd4diCnt		Unsigned32
}

erd4diName		OBJECT-TYPE
	SYNTAX		DisplayString (SIZE(0..32))
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION	"Digital input name"
	::= { erd4diEntry 1 }

erd4diAlarmName	OBJECT-TYPE
	SYNTAX		DisplayString
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"Alarm name"
	::= { erd4diEntry 2 }

erd4diState		OBJECT-TYPE
	SYNTAX		INTEGER {
		low(0),
		high(1)
	}
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION	"Digital input state"
	::= { erd4diEntry 3 }

erd4diCnt		OBJECT-TYPE
	SYNTAX		Unsigned32
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"Digital input impulse counter. 
	Writable for reset only. Set 0 to reset"
	::= { erd4diEntry 4 }


-- doTable

erd4doTable		OBJECT-TYPE
	SYNTAX		SEQUENCE OF Erd4DoEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION	"A list of digital outputs values"
	::= { erd4dio 2 }

erd4doEntry		OBJECT-TYPE
	SYNTAX		Erd4DoEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION	"An entry (conceptual row) 
	in the doTable"
	INDEX   { erd4doName } 
	::= { erd4doTable 1 }

Erd4DoEntry ::= SEQUENCE {
	erd4doName			DisplayString,
	erd4doDeviceName	DisplayString,
	erd4doState			INTEGER,
	erd4doResetTime		Integer32
}

erd4doName		OBJECT-TYPE
	SYNTAX		DisplayString (SIZE(0..32))
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION	"Digital output name"
	::= { erd4doEntry 1 }

erd4doDeviceName	OBJECT-TYPE
	SYNTAX			DisplayString
	MAX-ACCESS		read-write
	STATUS			current
	DESCRIPTION		"Digital output custom name"
	::= { erd4doEntry 2 }

erd4doState		OBJECT-TYPE
	SYNTAX		INTEGER
			{
				off(0),
				on(1),
				reload(2)
			}
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"Digital output state"
	::= { erd4doEntry 3 }

erd4doResetTime	OBJECT-TYPE
	SYNTAX		Integer32
	UNITS		"seconds"
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"Reload period (sec)"
	::= { erd4doEntry 4 }

