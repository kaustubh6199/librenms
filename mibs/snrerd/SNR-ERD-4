            "A collection of objects providing SNR-ERD-4 serial ports"
    ::= { erd4Groups 4 }
	
erd4modulesGroup OBJECT-GROUP
    OBJECTS {	erd4gsmModuleStatus, erd4gsmStrength, erd4gsmConnectCount
			}
    STATUS  current
    DESCRIPTION
            "A collection of objects providing SNR-ERD-4 modules"
    ::= { erd4Groups 5 }
	
erd4optionsGroup OBJECT-GROUP
    OBJECTS {	erd4dataType, 
				erd4reboot,
				erd4humCrit, erd4adcCritMin, erd4adcCritMax, erd4adcHysteresis
