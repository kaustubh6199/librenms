sonusAddressContextZoneH323SigPortCurrentStatisticsTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusAddressContextZoneH323SigPortCurrentStatisticsEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "
             H323 Signaling Port Statistics For File.
             "
    ::= { sonusAddressContextMIB 161 }

sonusAddressContextZoneH323SigPortCurrentStatisticsEntry OBJECT-TYPE
    SYNTAX          SonusAddressContextZoneH323SigPortCurrentStatisticsEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            ""
    INDEX           { sonusAddressContextName, 
                      sonusAddressContextZoneName, 
                      sonusAddressContextZoneH323SigPortCurrentStatisticsName, 
                      sonusAddressContextZoneH323SigPortCurrentStatisticsIndex }
    ::= { sonusAddressContextZoneH323SigPortCurrentStatisticsTable 1 }

SonusAddressContextZoneH323SigPortCurrentStatisticsEntry ::=
    SEQUENCE {
        sonusAddressContextZoneH323SigPortCurrentStatisticsName                       String,
        sonusAddressContextZoneH323SigPortCurrentStatisticsIndex                        Unsigned32,
        sonusAddressContextZoneH323SigPortCurrentStatisticsStatus                         INTEGER,
        sonusAddressContextZoneH323SigPortCurrentStatisticsIngressCallsCompleted                                        Integer32,
        sonusAddressContextZoneH323SigPortCurrentStatisticsEgressCallsCompleted                                       Integer32,
        sonusAddressContextZoneH323SigPortCurrentStatisticsIngressCallsAttempted                                        Integer32,
        sonusAddressContextZoneH323SigPortCurrentStatisticsEgressCallsAttempted                                       Integer32,
        sonusAddressContextZoneH323SigPortCurrentStatisticsH225MessagesSent                                   Counter64,
        sonusAddressContextZoneH323SigPortCurrentStatisticsH225MessageBytesSent                                       Counter64,
        sonusAddressContextZoneH323SigPortCurrentStatisticsH225MessagesRcvd                                   Counter64,
        sonusAddressContextZoneH323SigPortCurrentStatisticsH225MessageBytesRcvd                                       Counter64,
        sonusAddressContextZoneH323SigPortCurrentStatisticsH245MessagesSent                                   Counter64,
        sonusAddressContextZoneH323SigPortCurrentStatisticsH245MessageBytesSent                                       Counter64,
        sonusAddressContextZoneH323SigPortCurrentStatisticsH245MessagesRcvd                                   Counter64,
        sonusAddressContextZoneH323SigPortCurrentStatisticsH245MessageBytesRcvd                                       Counter64,
        sonusAddressContextZoneH323SigPortCurrentStatisticsActiveCalls                              Integer32,
        sonusAddressContextZoneH323SigPortCurrentStatisticsH225TcpPortsOpened                                     Integer32,
        sonusAddressContextZoneH323SigPortCurrentStatisticsH245TcpPortsOpened                                     Integer32,
        sonusAddressContextZoneH323SigPortCurrentStatisticsH245TcpPortsAllocated                                        Integer32
    }

