SonusAddressContextZoneCallCurrentStatisticsEntry ::=
    SEQUENCE {
        sonusAddressContextZoneCallCurrentStatisticsName                String,
        sonusAddressContextZoneCallCurrentStatisticsInUsage                   Unsigned32,
        sonusAddressContextZoneCallCurrentStatisticsOutUsage                    Unsigned32,
        sonusAddressContextZoneCallCurrentStatisticsInCalls                   Unsigned32,
        sonusAddressContextZoneCallCurrentStatisticsOutCalls                    Unsigned32,
        sonusAddressContextZoneCallCurrentStatisticsInCallAttempts                          Unsigned32,
        sonusAddressContextZoneCallCurrentStatisticsOutCallAttempts                           Unsigned32,
        sonusAddressContextZoneCallCurrentStatisticsMaxCompletedCalls                             Unsigned32,
        sonusAddressContextZoneCallCurrentStatisticsCallSetupTime                         Unsigned32,
        sonusAddressContextZoneCallCurrentStatisticsCallSetups                      Unsigned32,
        sonusAddressContextZoneCallCurrentStatisticsRoutingAttempts                           Unsigned32,
        sonusAddressContextZoneCallCurrentStatisticsInBwUsage                     Unsigned32,
        sonusAddressContextZoneCallCurrentStatisticsOutBwUsage                      Unsigned32,
        sonusAddressContextZoneCallCurrentStatisticsMaxActiveBwUsage                            Unsigned32,
        sonusAddressContextZoneCallCurrentStatisticsCallsWithPktOutage                              Unsigned32,
        sonusAddressContextZoneCallCurrentStatisticsCallsWithPktOutageAtEnd                                   Unsigned32,
        sonusAddressContextZoneCallCurrentStatisticsTotalPktOutage                          Unsigned32,
        sonusAddressContextZoneCallCurrentStatisticsMaxPktOutage                        Unsigned32,
        sonusAddressContextZoneCallCurrentStatisticsPodEvents                     Unsigned32,
        sonusAddressContextZoneCallCurrentStatisticsPlayoutBufferGood                             Unsigned32,
        sonusAddressContextZoneCallCurrentStatisticsPlayoutBufferAcceptable                                   Unsigned32,
        sonusAddressContextZoneCallCurrentStatisticsPlayoutBufferPoor                             Unsigned32,
        sonusAddressContextZoneCallCurrentStatisticsPlayoutBufferUnacceptable                                     Unsigned32,
        sonusAddressContextZoneCallCurrentStatisticsSipRegAttempts                          Unsigned32,
        sonusAddressContextZoneCallCurrentStatisticsSipRegCompletions                             Unsigned32,
        sonusAddressContextZoneCallCurrentStatisticsCallsWithPsxDips                            Unsigned32,
        sonusAddressContextZoneCallCurrentStatisticsTotalPsxDips                        Unsigned32,
        sonusAddressContextZoneCallCurrentStatisticsActiveRegs                      Unsigned32,
        sonusAddressContextZoneCallCurrentStatisticsMaxActiveRegs                         Unsigned32,
        sonusAddressContextZoneCallCurrentStatisticsActiveSubs                      Unsigned32,
        sonusAddressContextZoneCallCurrentStatisticsMaxActiveSubs                         Unsigned32,
        sonusAddressContextZoneCallCurrentStatisticsPeakCallRate                        Unsigned32
    }

