SonusAddressContextZoneTrafficControlIntervalStatisticsEntry ::=
    SEQUENCE {
        sonusAddressContextZoneTrafficControlIntervalStatisticsNumber                             Unsigned32,
        sonusAddressContextZoneTrafficControlIntervalStatisticsName                           String,
        sonusAddressContextZoneTrafficControlIntervalStatisticsIntervalValid                                    INTEGER,
        sonusAddressContextZoneTrafficControlIntervalStatisticsTime                           Unsigned32,
        sonusAddressContextZoneTrafficControlIntervalStatisticsSilc                           Unsigned32,
        sonusAddressContextZoneTrafficControlIntervalStatisticsStrCant                              Unsigned32,
        sonusAddressContextZoneTrafficControlIntervalStatisticsStrSkip                              Unsigned32,
        sonusAddressContextZoneTrafficControlIntervalStatisticsSkip                           Unsigned32,
        sonusAddressContextZoneTrafficControlIntervalStatisticsCant                           Unsigned32,
        sonusAddressContextZoneTrafficControlIntervalStatisticsCanf                           Unsigned32,
        sonusAddressContextZoneTrafficControlIntervalStatisticsAccCant                              Unsigned32,
        sonusAddressContextZoneTrafficControlIntervalStatisticsAccSkip                              Unsigned32,
        sonusAddressContextZoneTrafficControlIntervalStatisticsRouteAttemptsIRR                                       Unsigned32,
        sonusAddressContextZoneTrafficControlIntervalStatisticsRouteAttemptsSIRR                                        Unsigned32,
        sonusAddressContextZoneTrafficControlIntervalStatisticsRouteAttemptsORR                                       Unsigned32,
        sonusAddressContextZoneTrafficControlIntervalStatisticsRouteAttemptsSORR                                        Unsigned32,
        sonusAddressContextZoneTrafficControlIntervalStatisticsSuccessfulIRR                                    Unsigned32,
        sonusAddressContextZoneTrafficControlIntervalStatisticsSuccessfulSIRR                                     Unsigned32,
        sonusAddressContextZoneTrafficControlIntervalStatisticsSuccessfulORR                                    Unsigned32,
        sonusAddressContextZoneTrafficControlIntervalStatisticsSuccessfulSORR                                     Unsigned32
    }

sonusAddressContextZoneTrafficControlIntervalStatisticsNumber OBJECT-TYPE
    SYNTAX          Unsigned32
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "
             A sequence number which identifies the
             interval for which the set of statistics is required.
             The interval N identifies the recently completed interval,
             Interval N is identifed by the last sequence value and N-( number of intervals)
             is the earlest completed interval. 
             "
    ::= { sonusAddressContextZoneTrafficControlIntervalStatisticsEntry 1 }

sonusAddressContextZoneTrafficControlIntervalStatisticsName OBJECT-TYPE
    SYNTAX          String (SIZE(0..23))
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "
             The name of this trunk group.  
             "
    ::= { sonusAddressContextZoneTrafficControlIntervalStatisticsEntry 2 }

