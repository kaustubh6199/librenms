sonusAddressContextZoneSipIntervalStatisticsTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusAddressContextZoneSipIntervalStatisticsEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "
             The SIP trunk group interval SIP statistics.
             "
    ::= { sonusAddressContextMIB 109 }

sonusAddressContextZoneSipIntervalStatisticsEntry OBJECT-TYPE
    SYNTAX          SonusAddressContextZoneSipIntervalStatisticsEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            ""
    INDEX           { sonusAddressContextName, 
                      sonusAddressContextZoneName, 
                      sonusAddressContextZoneSipIntervalStatisticsNumber, 
                      sonusAddressContextZoneSipIntervalStatisticsName }
    ::= { sonusAddressContextZoneSipIntervalStatisticsTable 1 }

SonusAddressContextZoneSipIntervalStatisticsEntry ::=
    SEQUENCE {
        sonusAddressContextZoneSipIntervalStatisticsNumber                  Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsName                String,
        sonusAddressContextZoneSipIntervalStatisticsIntervalValid                         INTEGER,
        sonusAddressContextZoneSipIntervalStatisticsTime                Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsRcvInvite                     Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsSndInvite                     Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsRcvAck                  Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsSndAck                  Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsRcvPrack                    Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsSndPrack                    Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsRcvInfo                   Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsSndInfo                   Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsRcvRefer                    Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsSndRefer                    Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsRcvBye                  Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsSndBye                  Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsRcvCancel                     Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsSndCancel                     Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsRcvRegister                       Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsSndRegister                       Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsRcvUpdate                     Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsSndUpdate                     Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsRcv18x                  Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsSnd18x                  Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsRcv1xx                  Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsSnd1xx                  Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsRcv2xx                  Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsSnd2xx                  Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsRcvNonInv2xx                        Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsSndNonInv2xx                        Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsRcv3xx                  Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsSnd3xx                  Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsRcv4xx                  Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsSnd4xx                  Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsRcv5xx                  Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsSnd5xx                  Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsRcv6xx                  Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsSnd6xx                  Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsRcvNonInvErr                        Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsSndNonInvErr                        Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsRcvUnknownMsg                         Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsRcvSubscriber                         Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsSndSubscriber                         Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsRcvNotify                     Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsSndNotify                     Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsRcvOption                     Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsSndOption                     Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsInvReTransmit                         Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsRegReTransmit                         Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsByeRetransmit                         Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsCancelReTransmit                            Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsOtherReTransmit                           Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsRcvMessage                      Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsSndMessage                      Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsRcvPublish                      Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsSndPublish                      Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsEmergencyAccept                           Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsEmergencyRejectBWCall                                 Unsigned32,
        sonusAddressContextZoneSipIntervalStatisticsEmergencyRejectPolicer                                  Unsigned32,
