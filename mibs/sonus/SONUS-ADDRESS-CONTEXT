sonusAddressContextZoneCallIntervalStatisticsTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusAddressContextZoneCallIntervalStatisticsEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "
             The trunk group interval call statistics.
             "
    ::= { sonusAddressContextMIB 102 }

sonusAddressContextZoneCallIntervalStatisticsEntry OBJECT-TYPE
    SYNTAX          SonusAddressContextZoneCallIntervalStatisticsEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            ""
    INDEX           { sonusAddressContextName, 
                      sonusAddressContextZoneName, 
                      sonusAddressContextZoneCallIntervalStatisticsNumber, 
                      sonusAddressContextZoneCallIntervalStatisticsName }
    ::= { sonusAddressContextZoneCallIntervalStatisticsTable 1 }

SonusAddressContextZoneCallIntervalStatisticsEntry ::=
    SEQUENCE {
        sonusAddressContextZoneCallIntervalStatisticsNumber                   Unsigned32,
        sonusAddressContextZoneCallIntervalStatisticsName                 String,
        sonusAddressContextZoneCallIntervalStatisticsIntervalValid                          INTEGER,
        sonusAddressContextZoneCallIntervalStatisticsTime                 Unsigned32,
        sonusAddressContextZoneCallIntervalStatisticsInUsage                    Unsigned32,
        sonusAddressContextZoneCallIntervalStatisticsOutUsage                     Unsigned32,
        sonusAddressContextZoneCallIntervalStatisticsInCalls                    Unsigned32,
        sonusAddressContextZoneCallIntervalStatisticsOutCalls                     Unsigned32,
        sonusAddressContextZoneCallIntervalStatisticsInCallAttempts                           Unsigned32,
        sonusAddressContextZoneCallIntervalStatisticsOutCallAttempts                            Unsigned32,
        sonusAddressContextZoneCallIntervalStatisticsMaxCompletedCalls                              Unsigned32,
        sonusAddressContextZoneCallIntervalStatisticsCallSetupTime                          Unsigned32,
        sonusAddressContextZoneCallIntervalStatisticsCallSetups                       Unsigned32,
        sonusAddressContextZoneCallIntervalStatisticsRoutingAttempts                            Unsigned32,
        sonusAddressContextZoneCallIntervalStatisticsInBwUsage                      Unsigned32,
        sonusAddressContextZoneCallIntervalStatisticsOutBwUsage                       Unsigned32,
        sonusAddressContextZoneCallIntervalStatisticsMaxActiveBwUsage                             Unsigned32,
        sonusAddressContextZoneCallIntervalStatisticsCallsWithPktOutage                               Unsigned32,
        sonusAddressContextZoneCallIntervalStatisticsCallsWithPktOutageAtEnd                                    Unsigned32,
        sonusAddressContextZoneCallIntervalStatisticsTotalPktOutage                           Unsigned32,
        sonusAddressContextZoneCallIntervalStatisticsMaxPktOutage                         Unsigned32,
        sonusAddressContextZoneCallIntervalStatisticsPodEvents                      Unsigned32,
        sonusAddressContextZoneCallIntervalStatisticsPlayoutBufferGood                              Unsigned32,
        sonusAddressContextZoneCallIntervalStatisticsPlayoutBufferAcceptable                                    Unsigned32,
        sonusAddressContextZoneCallIntervalStatisticsPlayoutBufferPoor                              Unsigned32,
        sonusAddressContextZoneCallIntervalStatisticsPlayoutBufferUnacceptable                                      Unsigned32,
        sonusAddressContextZoneCallIntervalStatisticsSipRegAttempts                           Unsigned32,
        sonusAddressContextZoneCallIntervalStatisticsSipRegCompletions                              Unsigned32,
        sonusAddressContextZoneCallIntervalStatisticsCallsWithPsxDips                             Unsigned32,
        sonusAddressContextZoneCallIntervalStatisticsTotalPsxDips                         Unsigned32,
        sonusAddressContextZoneCallIntervalStatisticsActiveRegs                       Unsigned32,
        sonusAddressContextZoneCallIntervalStatisticsMaxActiveRegs                          Unsigned32,
        sonusAddressContextZoneCallIntervalStatisticsActiveSubs                       Unsigned32,
        sonusAddressContextZoneCallIntervalStatisticsMaxActiveSubs                          Unsigned32,
        sonusAddressContextZoneCallIntervalStatisticsPeakCallRate                         Unsigned32
    }

sonusAddressContextZoneCallIntervalStatisticsNumber OBJECT-TYPE
    SYNTAX          Unsigned32
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "
             A sequence number which identifies the
             interval for which the set of statistics is required.
             The interval N identifies the recently completed interval,
             Interval N is identifed by the last sequence value and N-( number of intervals)
             is the earlest completed interval. 
             "
    ::= { sonusAddressContextZoneCallIntervalStatisticsEntry 1 }

sonusAddressContextZoneCallIntervalStatisticsName OBJECT-TYPE
    SYNTAX          String (SIZE(0..23))
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "
             The name of this trunk group.  
             "
    ::= { sonusAddressContextZoneCallIntervalStatisticsEntry 2 }

sonusAddressContextZoneCallIntervalStatisticsIntervalValid OBJECT-TYPE
    SYNTAX          INTEGER { true(1), false(2) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "
             The member indicating the validity of the interval.
             "
    ::= { sonusAddressContextZoneCallIntervalStatisticsEntry 3 }

sonusAddressContextZoneCallIntervalStatisticsTime OBJECT-TYPE
    SYNTAX          Unsigned32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "
             The system up time when the interval statisitic is collected.
             "
    ::= { sonusAddressContextZoneCallIntervalStatisticsEntry 4 }

sonusAddressContextZoneCallIntervalStatisticsInUsage OBJECT-TYPE
    SYNTAX          Unsigned32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "
             The usage in the inbound direction for this trunk group for
             the specified interval.  Usage is defined as the time media
             bandwidth is activated to the time it is deactivated.
             "
    ::= { sonusAddressContextZoneCallIntervalStatisticsEntry 5 }

sonusAddressContextZoneCallIntervalStatisticsOutUsage OBJECT-TYPE
    SYNTAX          Unsigned32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "
             The usage in the outbound direction for this trunk group for
             the specified interval.  Usage is defined as the time media
             bandwidth is activated to the time it is deactivated.
             "
    ::= { sonusAddressContextZoneCallIntervalStatisticsEntry 6 }

sonusAddressContextZoneCallIntervalStatisticsInCalls OBJECT-TYPE
    SYNTAX          Unsigned32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "
             The number of completed inbound calls on this trunk group 
             for the specified interval.
             "
    ::= { sonusAddressContextZoneCallIntervalStatisticsEntry 7 }

sonusAddressContextZoneCallIntervalStatisticsOutCalls OBJECT-TYPE
    SYNTAX          Unsigned32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "
             The number of completed outbound calls on this trunk 
             group for the specified interval.
             "
    ::= { sonusAddressContextZoneCallIntervalStatisticsEntry 8 }

sonusAddressContextZoneCallIntervalStatisticsInCallAttempts OBJECT-TYPE
    SYNTAX          Unsigned32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "
             The number of inbound call attempts on this trunk 
             group for the specified interval.
             "
    ::= { sonusAddressContextZoneCallIntervalStatisticsEntry 9 }

sonusAddressContextZoneCallIntervalStatisticsOutCallAttempts OBJECT-TYPE
    SYNTAX          Unsigned32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "
             The number of outbound call attempts on this trunk 
             group for the specified interval.  
             "
    ::= { sonusAddressContextZoneCallIntervalStatisticsEntry 10 }

sonusAddressContextZoneCallIntervalStatisticsMaxCompletedCalls OBJECT-TYPE
    SYNTAX          Unsigned32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "
             Displayed as maxActiveCalls. The high water mark of total
             number of active calls in both the inbound and outbound
             directions on the trunk group for the specified interval.
             This statistic accounts for calls that are setting up, 
             stable, or tearing down.
             "
    ::= { sonusAddressContextZoneCallIntervalStatisticsEntry 11 }

sonusAddressContextZoneCallIntervalStatisticsCallSetupTime OBJECT-TYPE
    SYNTAX          Unsigned32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "
             The total call setup time for all completed calls
             in the inbound and outbound directions for this trunk group
             for the specified interval measured in hundredths of a second.
             This object can be used as the numerator for calculating 
             average call setup time.
             "
    ::= { sonusAddressContextZoneCallIntervalStatisticsEntry 12 }

sonusAddressContextZoneCallIntervalStatisticsCallSetups OBJECT-TYPE
    SYNTAX          Unsigned32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "
             The total number of calls setup but not necessarily
             completed in the inbound and outbound directions for 
             this trunk group for the specified interval.  This object 
             can be used as the denominator for calculating average 
             call setup time.
             "
    ::= { sonusAddressContextZoneCallIntervalStatisticsEntry 13 }

sonusAddressContextZoneCallIntervalStatisticsRoutingAttempts OBJECT-TYPE
    SYNTAX          Unsigned32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "
             The number of routing attempts for this trunk group
             for the specified interval.
             "
    ::= { sonusAddressContextZoneCallIntervalStatisticsEntry 14 }

sonusAddressContextZoneCallIntervalStatisticsInBwUsage OBJECT-TYPE
    SYNTAX          Unsigned32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "
             The sum of BW usage (expected data rate in bits per second multiplied
             by call duration in seconds) for every inbound call associated with
             this trunk group for the specified interval. 
             "
    ::= { sonusAddressContextZoneCallIntervalStatisticsEntry 15 }

sonusAddressContextZoneCallIntervalStatisticsOutBwUsage OBJECT-TYPE
    SYNTAX          Unsigned32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "
             The sum of BW usage (expected data rate in bits per second multiplied
             by call duration in seconds) for every outbound call associated with
             this trunk group for the specified interval.
             "
    ::= { sonusAddressContextZoneCallIntervalStatisticsEntry 16 }

sonusAddressContextZoneCallIntervalStatisticsMaxActiveBwUsage OBJECT-TYPE
    SYNTAX          Unsigned32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "
             The high water mark of BW usage in either direction associated
             with this trunk group for the specified interval.
             "
    ::= { sonusAddressContextZoneCallIntervalStatisticsEntry 17 }

sonusAddressContextZoneCallIntervalStatisticsCallsWithPktOutage OBJECT-TYPE
    SYNTAX          Unsigned32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "
             The number of calls with a maximum packet outage whose duration
             exceeds the configured minimum for this trunk group
             for the specified interval.
             "
    ::= { sonusAddressContextZoneCallIntervalStatisticsEntry 18 }

sonusAddressContextZoneCallIntervalStatisticsCallsWithPktOutageAtEnd OBJECT-TYPE
    SYNTAX          Unsigned32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "
             The number of calls whose maximum packet outage occurs at the end
             of the call for this trunk group for the specified interval.  This is 
             an indication that the call
             may have been terminated the because of poor quality. 
             "
    ::= { sonusAddressContextZoneCallIntervalStatisticsEntry 19 }

sonusAddressContextZoneCallIntervalStatisticsTotalPktOutage OBJECT-TYPE
    SYNTAX          Unsigned32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "
             The summation of packet outage durations (in milliseconds) 
             whose duration exceeds the configured minimum 
             which is reported during the specified performance interval.
             The average packet outage duration can be calculated by dividing
             this field by the number of calls reporting packet outages.
             "
    ::= { sonusAddressContextZoneCallIntervalStatisticsEntry 20 }

sonusAddressContextZoneCallIntervalStatisticsMaxPktOutage OBJECT-TYPE
    SYNTAX          Unsigned32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "
             The single longest maximum reported packet outage duration (in  milliseconds) 
             experienced during the specified performance interval for this
             trunk group.
             "
    ::= { sonusAddressContextZoneCallIntervalStatisticsEntry 21 }

sonusAddressContextZoneCallIntervalStatisticsPodEvents OBJECT-TYPE
    SYNTAX          Unsigned32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "
             The number of Packet Outage Detection (POD) Events detected for this
             peeer group for the specified interval.  A POD event occurs when a 
             configurable number of calls experience a packet
             outage with duration exceeding a programmable threshold. 
             "
    ::= { sonusAddressContextZoneCallIntervalStatisticsEntry 22 }

sonusAddressContextZoneCallIntervalStatisticsPlayoutBufferGood OBJECT-TYPE
    SYNTAX          Unsigned32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "
             Number of calls with all sub-intervals reporting GOOD playout
             buffer quality for this trunk group for the specified interval.
             "
    ::= { sonusAddressContextZoneCallIntervalStatisticsEntry 23 }

sonusAddressContextZoneCallIntervalStatisticsPlayoutBufferAcceptable OBJECT-TYPE
    SYNTAX          Unsigned32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "
             Number of calls with all sub-intervals reporting ACCEPTABLE or
             better playout buffer quality for this trunk group for the specified interval.
             "
    ::= { sonusAddressContextZoneCallIntervalStatisticsEntry 24 }

sonusAddressContextZoneCallIntervalStatisticsPlayoutBufferPoor OBJECT-TYPE
    SYNTAX          Unsigned32
    MAX-ACCESS      read-only
