        sonusAddressContextZoneMtrmConnPortPeerStatusShelfId                    Integer32,
        sonusAddressContextZoneMtrmConnPortPeerStatusPeerRole                     SonusMtrmPeerRoleType,
        sonusAddressContextZoneMtrmConnPortPeerStatusIpAddress                      InetAddressIP,
        sonusAddressContextZoneMtrmConnPortPeerStatusConnectionType                           SonusMtrmPeerConnectionType
    }

