    SEQUENCE {
        sonusAddressContextZoneTrafficControlCurrentStatisticsName                          String,
        sonusAddressContextZoneTrafficControlCurrentStatisticsSilc                          Unsigned32,
        sonusAddressContextZoneTrafficControlCurrentStatisticsStrCant                             Unsigned32,
        sonusAddressContextZoneTrafficControlCurrentStatisticsStrSkip                             Unsigned32,
        sonusAddressContextZoneTrafficControlCurrentStatisticsSkip                          Unsigned32,
        sonusAddressContextZoneTrafficControlCurrentStatisticsCant                          Unsigned32,
        sonusAddressContextZoneTrafficControlCurrentStatisticsCanf                          Unsigned32,
        sonusAddressContextZoneTrafficControlCurrentStatisticsAccCant                             Unsigned32,
        sonusAddressContextZoneTrafficControlCurrentStatisticsAccSkip                             Unsigned32,
        sonusAddressContextZoneTrafficControlCurrentStatisticsRouteAttemptsIRR                                      Unsigned32,
        sonusAddressContextZoneTrafficControlCurrentStatisticsRouteAttemptsSIRR                                       Unsigned32,
        sonusAddressContextZoneTrafficControlCurrentStatisticsRouteAttemptsORR                                      Unsigned32,
        sonusAddressContextZoneTrafficControlCurrentStatisticsRouteAttemptsSORR                                       Unsigned32,
        sonusAddressContextZoneTrafficControlCurrentStatisticsSuccessfulIRR                                   Unsigned32,
        sonusAddressContextZoneTrafficControlCurrentStatisticsSuccessfulSIRR                                    Unsigned32,
        sonusAddressContextZoneTrafficControlCurrentStatisticsSuccessfulORR                                   Unsigned32,
        sonusAddressContextZoneTrafficControlCurrentStatisticsSuccessfulSORR                                    Unsigned32
    }

