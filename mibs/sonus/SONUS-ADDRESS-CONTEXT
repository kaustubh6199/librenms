    SYNTAX          String (SIZE(0..255))
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "
             The name of the compute element that hosts this link detection group.
             "
    ::= { sonusAddressContextLinkDetectionGroupEntry 2 }

