             "
    ::= { sonusAddressContextZoneTrunkGroupIntervalStatisticsEntry 13 }

sonusAddressContextZoneTrunkGroupIntervalStatisticsPacketOutDetectState OBJECT-TYPE
    SYNTAX          SonusTgPacketOutageDetectionStateType
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "
             This object indicates the packet outage detection state.
             normal              - No packet outage has been declared.
             packetOutageState   - Packet outage has been declared.
             The current bandwidth limit has been 
             reduced by packet outage detection 
             bandwidth limit reduction.
             "
    ::= { sonusAddressContextZoneTrunkGroupIntervalStatisticsEntry 14 }

sonusAddressContextZoneMtrmConnPortPeerStatusTable OBJECT-TYPE
