    SYNTAX          String (SIZE(0..23))
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "
             The name of this link detection group.  
             "
    ::= { sonusAddressContextLinkDetectionGroupEntry 1 }

