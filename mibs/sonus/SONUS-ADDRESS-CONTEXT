    ::= { sonusAddressContextLinkDetectionGroupTable 1 }

SonusAddressContextLinkDetectionGroupEntry ::=
    SEQUENCE {
        sonusAddressContextLinkDetectionGroupName         String,
        sonusAddressContextLinkDetectionGroupCeName           String,
        sonusAddressContextLinkDetectionGroupType         SonusLvmInterfaceType
    }

