sonusAddressContextZoneSipCurrentStatisticsTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusAddressContextZoneSipCurrentStatisticsEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "
             The SIP trunk group current SIP statistics.
             "
    ::= { sonusAddressContextMIB 108 }

sonusAddressContextZoneSipCurrentStatisticsEntry OBJECT-TYPE
    SYNTAX          SonusAddressContextZoneSipCurrentStatisticsEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            ""
    INDEX           { sonusAddressContextName, 
                      sonusAddressContextZoneName, 
                      sonusAddressContextZoneSipCurrentStatisticsName }
    ::= { sonusAddressContextZoneSipCurrentStatisticsTable 1 }

SonusAddressContextZoneSipCurrentStatisticsEntry ::=
    SEQUENCE {
        sonusAddressContextZoneSipCurrentStatisticsName               String,
        sonusAddressContextZoneSipCurrentStatisticsRcvInvite                    Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsSndInvite                    Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsRcvAck                 Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsSndAck                 Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsRcvPrack                   Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsSndPrack                   Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsRcvInfo                  Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsSndInfo                  Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsRcvRefer                   Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsSndRefer                   Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsRcvBye                 Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsSndBye                 Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsRcvCancel                    Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsSndCancel                    Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsRcvRegister                      Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsSndRegister                      Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsRcvUpdate                    Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsSndUpdate                    Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsRcv18x                 Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsSnd18x                 Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsRcv1xx                 Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsSnd1xx                 Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsRcv2xx                 Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsSnd2xx                 Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsRcvNonInv2xx                       Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsSndNonInv2xx                       Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsRcv3xx                 Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsSnd3xx                 Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsRcv4xx                 Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsSnd4xx                 Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsRcv5xx                 Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsSnd5xx                 Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsRcv6xx                 Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsSnd6xx                 Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsRcvNonInvErr                       Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsSndNonInvErr                       Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsRcvUnknownMsg                        Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsRcvSubscriber                        Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsSndSubscriber                        Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsRcvNotify                    Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsSndNotify                    Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsRcvOption                    Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsSndOption                    Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsInvReTransmit                        Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsRegReTransmit                        Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsByeRetransmit                        Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsCancelReTransmit                           Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsOtherReTransmit                          Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsRcvMessage                     Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsSndMessage                     Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsRcvPublish                     Unsigned32,
        sonusAddressContextZoneSipCurrentStatisticsSndPublish                     Unsigned32,
