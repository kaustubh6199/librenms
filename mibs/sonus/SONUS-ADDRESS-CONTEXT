        sonusAddressContextVmgGeneralStatisticsNumberOfWildCardedAuditValueRequests                                           Counter32,
        sonusAddressContextVmgGeneralStatisticsNumberOfAuditValueErrors                               Counter32,
        sonusAddressContextVmgGeneralStatisticsNumberOfModifyRequests                             Counter32,
