sonusAddressContextZoneCallFailureCurrentStatisticsEntry OBJECT-TYPE
    SYNTAX          SonusAddressContextZoneCallFailureCurrentStatisticsEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            ""
    INDEX           { sonusAddressContextName, 
                      sonusAddressContextZoneName, 
                      sonusAddressContextZoneCallFailureCurrentStatisticsName }
    ::= { sonusAddressContextZoneCallFailureCurrentStatisticsTable 1 }

SonusAddressContextZoneCallFailureCurrentStatisticsEntry ::=
    SEQUENCE {
        sonusAddressContextZoneCallFailureCurrentStatisticsName                       String,
        sonusAddressContextZoneCallFailureCurrentStatisticsInCallFailNoRoutes                                     Unsigned32,
        sonusAddressContextZoneCallFailureCurrentStatisticsInCallFailNoResources                                        Unsigned32,
        sonusAddressContextZoneCallFailureCurrentStatisticsInCallFailNoService                                      Unsigned32,
        sonusAddressContextZoneCallFailureCurrentStatisticsInCallFailInvalidCall                                        Unsigned32,
        sonusAddressContextZoneCallFailureCurrentStatisticsInCallFailNetworkFailure                                           Unsigned32,
        sonusAddressContextZoneCallFailureCurrentStatisticsInCallFailProtocolError                                          Unsigned32,
        sonusAddressContextZoneCallFailureCurrentStatisticsInCallFailUnspecified                                        Unsigned32,
        sonusAddressContextZoneCallFailureCurrentStatisticsOutCallFailNoRoutes                                      Unsigned32,
        sonusAddressContextZoneCallFailureCurrentStatisticsOutCallFailNoResources                                         Unsigned32,
        sonusAddressContextZoneCallFailureCurrentStatisticsOutCallFailNoService                                       Unsigned32,
        sonusAddressContextZoneCallFailureCurrentStatisticsOutCallFailInvalidCall                                         Unsigned32,
        sonusAddressContextZoneCallFailureCurrentStatisticsOutCallFailNetworkFailure                                            Unsigned32,
        sonusAddressContextZoneCallFailureCurrentStatisticsOutCallFailProtocolError                                           Unsigned32,
        sonusAddressContextZoneCallFailureCurrentStatisticsOutCallFailUnspecified                                         Unsigned32,
        sonusAddressContextZoneCallFailureCurrentStatisticsRoutingFailuresResv                                      Unsigned32,
        sonusAddressContextZoneCallFailureCurrentStatisticsAllocFailBwLimit                                   Unsigned32,
        sonusAddressContextZoneCallFailureCurrentStatisticsAllocFailCallLimit                                     Unsigned32,
        sonusAddressContextZoneCallFailureCurrentStatisticsNoPsxRoute                             Unsigned32,
        sonusAddressContextZoneCallFailureCurrentStatisticsCallAbandoned                                Unsigned32,
        sonusAddressContextZoneCallFailureCurrentStatisticsCallFailPolicing                                   Unsigned32,
        sonusAddressContextZoneCallFailureCurrentStatisticsSipRegFailPolicing                                     Unsigned32,
        sonusAddressContextZoneCallFailureCurrentStatisticsSipRegFailInternal                                     Unsigned32,
        sonusAddressContextZoneCallFailureCurrentStatisticsSipRegFailOther                                  Unsigned32,
        sonusAddressContextZoneCallFailureCurrentStatisticsSecurityFail                               Unsigned32,
        sonusAddressContextZoneCallFailureCurrentStatisticsRegCallsFailed                                 Unsigned32,
        sonusAddressContextZoneCallFailureCurrentStatisticsNonMatchSrcIpCallsFail                                         Unsigned32,
        sonusAddressContextZoneCallFailureCurrentStatisticsInvalidSPCallsFailed                                       Unsigned32,
        sonusAddressContextZoneCallFailureCurrentStatisticsAllocFailParentConstraint                                            Unsigned32,
        sonusAddressContextZoneCallFailureCurrentStatisticsSipSubsFailPolicing                                      Unsigned32,
        sonusAddressContextZoneCallFailureCurrentStatisticsSipOtherReqFailPolicing                                          Unsigned32
    }

