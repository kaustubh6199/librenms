sonusAddressContextZoneCallFailureIntervalStatisticsEntry OBJECT-TYPE
    SYNTAX          SonusAddressContextZoneCallFailureIntervalStatisticsEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            ""
    INDEX           { sonusAddressContextName, 
                      sonusAddressContextZoneName, 
                      sonusAddressContextZoneCallFailureIntervalStatisticsNumber, 
                      sonusAddressContextZoneCallFailureIntervalStatisticsName }
    ::= { sonusAddressContextZoneCallFailureIntervalStatisticsTable 1 }

SonusAddressContextZoneCallFailureIntervalStatisticsEntry ::=
    SEQUENCE {
        sonusAddressContextZoneCallFailureIntervalStatisticsNumber                          Unsigned32,
        sonusAddressContextZoneCallFailureIntervalStatisticsName                        String,
        sonusAddressContextZoneCallFailureIntervalStatisticsIntervalValid                                 INTEGER,
        sonusAddressContextZoneCallFailureIntervalStatisticsTime                        Unsigned32,
        sonusAddressContextZoneCallFailureIntervalStatisticsInCallFailNoRoutes                                      Unsigned32,
        sonusAddressContextZoneCallFailureIntervalStatisticsInCallFailNoResources                                         Unsigned32,
        sonusAddressContextZoneCallFailureIntervalStatisticsInCallFailNoService                                       Unsigned32,
        sonusAddressContextZoneCallFailureIntervalStatisticsInCallFailInvalidCall                                         Unsigned32,
        sonusAddressContextZoneCallFailureIntervalStatisticsInCallFailNetworkFailure                                            Unsigned32,
        sonusAddressContextZoneCallFailureIntervalStatisticsInCallFailProtocolError                                           Unsigned32,
        sonusAddressContextZoneCallFailureIntervalStatisticsInCallFailUnspecified                                         Unsigned32,
        sonusAddressContextZoneCallFailureIntervalStatisticsOutCallFailNoRoutes                                       Unsigned32,
        sonusAddressContextZoneCallFailureIntervalStatisticsOutCallFailNoResources                                          Unsigned32,
        sonusAddressContextZoneCallFailureIntervalStatisticsOutCallFailNoService                                        Unsigned32,
        sonusAddressContextZoneCallFailureIntervalStatisticsOutCallFailInvalidCall                                          Unsigned32,
        sonusAddressContextZoneCallFailureIntervalStatisticsOutCallFailNetworkFailure                                             Unsigned32,
        sonusAddressContextZoneCallFailureIntervalStatisticsOutCallFailProtocolError                                            Unsigned32,
        sonusAddressContextZoneCallFailureIntervalStatisticsOutCallFailUnspecified                                          Unsigned32,
        sonusAddressContextZoneCallFailureIntervalStatisticsRoutingFailuresResv                                       Unsigned32,
        sonusAddressContextZoneCallFailureIntervalStatisticsAllocFailBwLimit                                    Unsigned32,
        sonusAddressContextZoneCallFailureIntervalStatisticsAllocFailCallLimit                                      Unsigned32,
        sonusAddressContextZoneCallFailureIntervalStatisticsNoPsxRoute                              Unsigned32,
        sonusAddressContextZoneCallFailureIntervalStatisticsCallAbandoned                                 Unsigned32,
        sonusAddressContextZoneCallFailureIntervalStatisticsCallFailPolicing                                    Unsigned32,
        sonusAddressContextZoneCallFailureIntervalStatisticsSipRegFailPolicing                                      Unsigned32,
        sonusAddressContextZoneCallFailureIntervalStatisticsSipRegFailInternal                                      Unsigned32,
        sonusAddressContextZoneCallFailureIntervalStatisticsSipRegFailOther                                   Unsigned32,
        sonusAddressContextZoneCallFailureIntervalStatisticsSecurityFail                                Unsigned32,
        sonusAddressContextZoneCallFailureIntervalStatisticsNonRegCallsFailed                                     Unsigned32,
        sonusAddressContextZoneCallFailureIntervalStatisticsNonMatchSrcIpCallsFail                                          Unsigned32,
        sonusAddressContextZoneCallFailureIntervalStatisticsInvalidSPCallsFailed                                        Unsigned32,
        sonusAddressContextZoneCallFailureIntervalStatisticsAllocFailParentConstraint                                             Unsigned32,
        sonusAddressContextZoneCallFailureIntervalStatisticsSipSubsFailPolicing                                       Unsigned32,
        sonusAddressContextZoneCallFailureIntervalStatisticsSipOtherReqFailPolicing                                           Unsigned32,
        sonusAddressContextZoneCallFailureIntervalStatisticsRegCallsFailed                                  Unsigned32
    }

sonusAddressContextZoneCallFailureIntervalStatisticsNumber OBJECT-TYPE
    SYNTAX          Unsigned32
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "
             A sequence number which identifies the
             interval for which the set of statistics is required.
             The interval N identifies the recently completed interval,
             Interval N is identifed by the last sequence value and N-( number of intervals)
             is the earlest completed interval. 
             "
    ::= { sonusAddressContextZoneCallFailureIntervalStatisticsEntry 1 }

sonusAddressContextZoneCallFailureIntervalStatisticsName OBJECT-TYPE
    SYNTAX          String (SIZE(0..23))
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "
             The name of this trunk group.  
             "
    ::= { sonusAddressContextZoneCallFailureIntervalStatisticsEntry 2 }

