        sonusAddressContextIpRouteStatusDestIpAddress             InetAddressIP,
        sonusAddressContextIpRouteStatusPrefix      Unsigned32,
        sonusAddressContextIpRouteStatusNextHop       InetAddressIP,
        sonusAddressContextIpRouteStatusIpInterfaceName               String,
        sonusAddressContextIpRouteStatusProtocol        SonusIpRouteProtoType
    }

sonusAddressContextIpRouteStatusCeName OBJECT-TYPE
    SYNTAX          String (SIZE(0..23))
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "
             The name of the compute element that hosts this static route.
             "
    ::= { sonusAddressContextIpRouteStatusEntry 1 }

sonusAddressContextIpRouteStatusDestIpAddress OBJECT-TYPE
    SYNTAX          InetAddressIP
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "
             The destination IP address of this route.
             
             This object may not take a Multicast (Class  D)
             address value.
             
             Any assignment (implicit or  otherwise)  of  an
             instance  of  this  object to a value x must be
             rejected if the bitwise logical-AND of  x  with
             the  value of the coarresponding instance of the
             ipCidrRouteMask object is not equal to x.
             "
    ::= { sonusAddressContextIpRouteStatusEntry 2 }

sonusAddressContextIpRouteStatusPrefix OBJECT-TYPE
    SYNTAX          Unsigned32 (0..128)
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "
             Indicates the prefix for the
             destination  address  before  being compared to
             the value  in  the  ipCidrRouteDest  field.   For
             those  systems  that  do  not support arbitrary
             subnet masks, an agent constructs the value  of
             the  ipCidrRouteMask  by  reference to the IP Ad-
             dress Class.
             
             Any assignment (implicit or  otherwise)  of  an
             instance  of  this  object to a value x must be
             rejected if the bitwise logical-AND of  x  with
             the  value of the corresponding instance of the
             ipCidrRouteDest object is not equal to ipCidrRoute-
             Dest.
             "
    ::= { sonusAddressContextIpRouteStatusEntry 3 }

sonusAddressContextIpRouteStatusNextHop OBJECT-TYPE
    SYNTAX          InetAddressIP
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "
             On remote routes, the address of the next sys-
             tem en route; Otherwise, 0.0.0.0.
             "
    ::= { sonusAddressContextIpRouteStatusEntry 4 }

sonusAddressContextIpRouteStatusIpInterfaceName OBJECT-TYPE
    SYNTAX          String (SIZE(0..23))
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "
             The name which identifies  the  local
             interface  through  which  the next hop of this
             route should be reached.
             "
    ::= { sonusAddressContextIpRouteStatusEntry 5 }

sonusAddressContextIpRouteStatusProtocol OBJECT-TYPE
    SYNTAX          SonusIpRouteProtoType
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "
             The routing mechanism via which this route was
             learned.  Inclusion of values for gateway rout-
             ing protocols is not  intended  to  imply  that
             hosts should support those protocols.
             "
    ::= { sonusAddressContextIpRouteStatusEntry 6 }

