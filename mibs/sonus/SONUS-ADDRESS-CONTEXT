sonusAddressContextZoneTrunkGroupStatusTotalCallsAvailable OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "
             The sum of all available or unblocked calls for this trunk group.
             "
    ::= { sonusAddressContextZoneTrunkGroupStatusEntry 3 }

sonusAddressContextZoneTrunkGroupStatusTotalCallsInboundReserved OBJECT-TYPE
    SYNTAX          Unsigned32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "
             This is meaningful for IP trunk groups that are configured for
             inbound or both directions only.  This is the reserved count 
             as specified by the percentage configured for this IP trunk group.
             This object shows the total number of calls that are reserved
             for inbound calls on this IP trunk group.
             "
    ::= { sonusAddressContextZoneTrunkGroupStatusEntry 4 }

sonusAddressContextZoneTrunkGroupStatusInboundCallsUsage OBJECT-TYPE
    SYNTAX          Unsigned32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "
             This is only meaningful for IP trunk groups that are configured
             for inbound or both directions.  This reflects the current 
             inbound or incoming usage count of this IP trunk group.  
             "
    ::= { sonusAddressContextZoneTrunkGroupStatusEntry 5 }

sonusAddressContextZoneTrunkGroupStatusOutboundCallsUsage OBJECT-TYPE
    SYNTAX          Unsigned32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "
             This reflects the current outbound, non-priority usage count of 
             this IP trunk group. 
             "
    ::= { sonusAddressContextZoneTrunkGroupStatusEntry 6 }

sonusAddressContextZoneTrunkGroupStatusTotalCallsConfigured OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "
             This object reflects the total number
             of calls that are configured on this IP trunk group.
             "
    ::= { sonusAddressContextZoneTrunkGroupStatusEntry 7 }

sonusAddressContextZoneTrunkGroupStatusPriorityCallUsage OBJECT-TYPE
    SYNTAX          Unsigned32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "
             This is only meaningful for IP trunk groups that are configured 
             with call reserved state enabled.  This reflects the current 
             priority usage count of this IP trunk group. 
             "
    ::= { sonusAddressContextZoneTrunkGroupStatusEntry 8 }

sonusAddressContextZoneTrunkGroupStatusTotalOutboundCallsReserved OBJECT-TYPE
    SYNTAX          Unsigned32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "
             This is meaningful for IP trunk groups that are configured for
             incoming call reservation.  This is the reserved count 
             as specified by the percentage configured for this IP trunk group.
             This object reflects the total number of calls that are reserved
             for outbound calls on this IP trunk group.
             "
    ::= { sonusAddressContextZoneTrunkGroupStatusEntry 9 }

