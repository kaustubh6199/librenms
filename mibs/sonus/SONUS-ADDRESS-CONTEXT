        sonusAddressContextZoneTrunkGroupStatusTotalCallsInboundReserved                                Unsigned32,
        sonusAddressContextZoneTrunkGroupStatusInboundCallsUsage                        Unsigned32,
        sonusAddressContextZoneTrunkGroupStatusOutboundCallsUsage                         Unsigned32,
        sonusAddressContextZoneTrunkGroupStatusTotalCallsConfigured                           Integer32,
        sonusAddressContextZoneTrunkGroupStatusPriorityCallUsage                        Unsigned32,
        sonusAddressContextZoneTrunkGroupStatusTotalOutboundCallsReserved                                 Unsigned32,
        sonusAddressContextZoneTrunkGroupStatusBwCurrentLimit                     Integer32,
        sonusAddressContextZoneTrunkGroupStatusBwAvailable                  Integer32,
        sonusAddressContextZoneTrunkGroupStatusBwInboundUsage                     Unsigned32,
        sonusAddressContextZoneTrunkGroupStatusBwOutboundUsage                      Unsigned32,
        sonusAddressContextZoneTrunkGroupStatusPacketOutDetectState                           SonusTgPacketOutageDetectionStateType
    }

