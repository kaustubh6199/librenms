    SYNTAX          SonusLiChnStatusType
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "
             The primary channel status (in service, out of service).
             "
    ::= { sonusAddressContextInterceptInterceptCallDataChannelStatisticsEntry 31 }

sonusAddressContextSipSubCountCurrentStatisticsTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusAddressContextSipSubCountCurrentStatisticsEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "
             This table presents SIP relayed subscription count statistics for the sip-endpoints
             "
    ::= { sonusAddressContextMIB 822 }

sonusAddressContextSipSubCountCurrentStatisticsEntry OBJECT-TYPE
    SYNTAX          SonusAddressContextSipSubCountCurrentStatisticsEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            ""
    INDEX           { sonusAddressContextName, 
                      sonusAddressContextSipSubCountCurrentStatisticsName }
    ::= { sonusAddressContextSipSubCountCurrentStatisticsTable 1 }

SonusAddressContextSipSubCountCurrentStatisticsEntry ::=
    SEQUENCE {
        sonusAddressContextSipSubCountCurrentStatisticsName                   INTEGER,
        sonusAddressContextSipSubCountCurrentStatisticsSipSubCountAttempts                                  Counter32,
        sonusAddressContextSipSubCountCurrentStatisticsSipSubCountCumCompletions                                        Counter32,
        sonusAddressContextSipSubCountCurrentStatisticsSipSubCountStable                                Counter32,
        sonusAddressContextSipSubCountCurrentStatisticsSipSubCountTotal                               Counter32
    }

sonusAddressContextSipSubCountCurrentStatisticsName OBJECT-TYPE
    SYNTAX          INTEGER { entry(1) }
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "
             Name of the system.
             "
    ::= { sonusAddressContextSipSubCountCurrentStatisticsEntry 1 }

sonusAddressContextSipSubCountCurrentStatisticsSipSubCountAttempts OBJECT-TYPE
    SYNTAX          Counter32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "
             Indicates the number of SIP relayed subscription attempts.
             "
    ::= { sonusAddressContextSipSubCountCurrentStatisticsEntry 2 }

