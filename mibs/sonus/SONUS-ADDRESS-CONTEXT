    SYNTAX          SonusLvmInterfaceType
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "
             Interface type supported by this link detection group.
             "
    DEFVAL          { ip }
    ::= { sonusAddressContextLinkDetectionGroupEntry 3 }

