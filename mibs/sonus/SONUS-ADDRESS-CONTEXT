        sonusAddressContextZoneTrunkGroupCurrentStatisticsBwOutboundUsage                                 Unsigned32,
        sonusAddressContextZoneTrunkGroupCurrentStatisticsPacketOutDetectState                                      SonusTgPacketOutageDetectionStateType
    }

