SonusBiccsgSvcGrpProfileEntry ::=
    SEQUENCE {
        sonusBiccsgSvcGrpProfileListIndex Integer32,
        sonusBiccsgSvcGrpProfileName    SonusName,
        sonusBiccsgSvcGrpProfileTG      SonusNameReference,
        sonusBiccsgSvcGrpProfileRevision INTEGER,
        sonusBiccsgSvcGrpProfileHuntAlgorithm     INTEGER,
        sonusBiccsgSvcGrpProfileCost    Integer32,
        sonusBiccsgSvcGrpProfileCicControl  INTEGER,
        sonusBiccsgSvcGrpProfileAdminState  SonusAdminState,
        sonusBiccsgSvcGrpProfileRowStatus RowStatus,
        sonusBiccsgSvcGrpProfileDiscTreatmentProfileName                SonusNameReference,
        sonusBiccsgSvcGrpProfileGrsStartup  INTEGER,
        sonusBiccsgSvcGrpProfileInspectionTime      DateAndTime,
        sonusBiccsgSvcGrpProfileInspectionFreq      INTEGER,
        sonusBiccsgSvcGrpProfileInspectionState       SonusAdminState,
        sonusBiccsgSvcGrpProfileSignalingProfileName            SonusNameReference,
        sonusBiccsgSvcGrpProfileInrInfProfileName         SonusNameReference,
        sonusBiccsgSvcGrpProfileClusterRouting      INTEGER,
        sonusBiccsgSvcGrpProfileAnsSupervisionTimeout             Integer32,
        sonusBiccsgSvcGrpProfileAnsSupervisionTimeoutAction                   INTEGER,
        sonusBiccsgSvcGrpProfileOverrideCutThruOnRcvAcmWithBciIw                        INTEGER,
        sonusBiccsgSvcGrpProfileCrankBackProfile        Integer32,
        sonusBiccsgSvcGrpProfileSequentialGRS     SonusSupportFlag,
        sonusBiccsgSvcGrpProfileCongRelTimeout      Integer32,
        sonusBiccsgSvcGrpProfileMessageThrottling         SonusSupportFlag,
        sonusBiccsgSvcGrpProfileMessageThrottlingTimer              Integer32,
        sonusBiccsgSvcGrpProfileCallCleanupCallsPerSegment                  Integer32,
        sonusBiccsgSvcGrpProfileCallCleanupInterSegmentTimer                    Integer32,
        sonusBiccsgSvcGrpProfileLdcTimeout  Integer32,
        sonusBiccsgSvcGrpProfileLdcAction INTEGER,
        sonusBiccsgSvcGrpProfileLdcRelCause   Integer32,
        sonusBiccsgSvcGrpProfileLdcEmergencyCalls         INTEGER,
        sonusBiccsgSvcGrpProfileLocationOfREL34       INTEGER,
        sonusBiccsgSvcGrpProfileLocationOfREL42       INTEGER,
        sonusBiccsgSvcGrpProfileTandemLimitCounter          Integer32,
        sonusBiccsgSvcGrpProfileIPBCPVersion    Integer32,
        sonusBiccsgSvcGrpProfileLocationOfREL     INTEGER,
        sonusBiccsgSvcGrpProfileDeleteIxcTcCode       DisplayString,
        sonusBiccsgSvcGrpProfileSkipCrankBackProfile            Unsigned32,
        sonusBiccsgSvcGrpProfileIpbcpT1 Integer32,
        sonusBiccsgSvcGrpProfileIpbcpT2 Integer32,
        sonusBiccsgSvcGrpProfileNIFGroupName    SonusNameReference,
        sonusBiccsgSvcGrpProfileSourceAddressFiltering              SonusAdminState,
        sonusBiccsgSvcGrpProfileHopCounterState       SonusAdminState,
        sonusBiccsgSvcGrpProfileHopCounterValue       Integer32
    }

