    SEQUENCE {
        sonusBiccsgServiceGroupStatListIndex    Integer32,
        sonusBiccsgServiceGroupStatName SonusNameReference,
        sonusBiccsgServiceGroupStatStatus INTEGER,
        sonusBiccsgServiceGroupStatLastInspectionTime             DateAndTime,
        sonusBiccsgServiceGroupStatNextInspectionTime             DateAndTime
    }

