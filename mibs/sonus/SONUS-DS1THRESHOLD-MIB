sonusDsx1ThresholdReportMinorCriticalTca OBJECT-TYPE
    SYNTAX          SonusAdminState
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "Controls whether Minor-level and Critical-level threshold Crossing
             Alerts (TCA) are reported (via an event log and trap). Setting this
             object to Disabled will prevent all Minor and Critical TCAs from being
             reported."
    DEFVAL          { disabled }
    ::= { sonusDsx1ThresholdEntry 68 }

sonusDsx1ThresholdReportDs1OosTca OBJECT-TYPE
    SYNTAX          SonusAdminState
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "Controls whether threshold Crossing Alerts (TCA) are reported
             (via an event log and trap) while the DS1 is Out of Service. Setting
             this object to Enabled allows TCAs to be reported while the DS1 is
             Out of Service."
    DEFVAL          { disabled }
    ::= { sonusDsx1ThresholdEntry 69 }

sonusDs1PmonIntervalType OBJECT-TYPE
    SYNTAX          INTEGER { fifteenMin(1), day(2) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Pmon interval type."
    ::= { sonusDsx1MIBNotificationsObjects 8 }

sonusDsx1PerfmonThresholdCrossingNotification NOTIFICATION-TYPE
    OBJECTS         { sonusShelfIndex, sonusSlotIndex, sonusPortIndex, 
                      sonusDs1Index, sonusDs1ThresholdType, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
    STATUS          current
    DESCRIPTION     
            "A sonusDsx1PerfmonThresholdCrossingNotification trap indicates that a T1/E1 span
             performance monitoring threshold has been crossed. The type indicates the type
             of threshold."
    ::= { sonusDsx1MIBNotificationsPrefix 8 }

sonusDsx1PerfmonIntervalStartedNotification NOTIFICATION-TYPE
    OBJECTS         { sonusShelfIndex, sonusSlotIndex, sonusPortIndex, 
                      sonusDs1Index, sonusDs1PmonIntervalType, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
    STATUS          current
    DESCRIPTION     
            "A  sonusDsx1PerfmonIntervalStartedNotification trap indicates that a new
             15 minute or 24 hour performance monitoring interval has started."
    ::= { sonusDsx1MIBNotificationsPrefix 10 }

END -- end of module SONUS-DS1THRESHOLD-MIB.
