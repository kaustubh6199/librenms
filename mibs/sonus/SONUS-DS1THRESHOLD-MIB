sonusDsx1Threshold OBJECT IDENTIFIER
    ::= { sonusDsx1MIBObjects 13 }

sonusDsx1ThresholdTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusDsx1ThresholdEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            " "
    ::= { sonusDsx1Threshold 1 }

sonusDsx1ThresholdEntry OBJECT-TYPE
    SYNTAX          SonusDsx1ThresholdEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "Entry in Sonus Dsx1 Threshold Table"
    INDEX           { sonusDsx1ThresholdShelfIndex, 
                      sonusDsx1ThresholdSlotIndex, 
                      sonusDsx1ThresholdPortIndex, 
                      sonusDsx1ThresholdDs1Index }
    ::= { sonusDsx1ThresholdTable 1 }

SonusDsx1ThresholdEntry ::=
    SEQUENCE {
        sonusDsx1ThresholdShelfIndex    SonusShelfIndex,
        sonusDsx1ThresholdSlotIndex     SonusSlotIndex,
        sonusDsx1ThresholdPortIndex     Integer32,
        sonusDsx1ThresholdDs1Index      Integer32,
        sonusDsx1ThresholdMinorLCVThreshold   Integer32,
        sonusDsx1ThresholdMinorLESThreshold   Integer32,
        sonusDsx1ThresholdMinorLSESThreshold    Integer32,
        sonusDsx1ThresholdMinorPCVThreshold   Integer32,
        sonusDsx1ThresholdMinorPESThreshold   Integer32,
        sonusDsx1ThresholdMinorPSESThreshold    Integer32,
        sonusDsx1ThresholdMinorCSSThreshold   Integer32,
        sonusDsx1ThresholdMinorSASThreshold   Integer32,
        sonusDsx1ThresholdMinorUASThreshold   Integer32,
        sonusDsx1ThresholdDayMinorLCVThreshold      Integer32,
        sonusDsx1ThresholdDayMinorLESThreshold      Integer32,
        sonusDsx1ThresholdDayMinorLSESThreshold       Integer32,
        sonusDsx1ThresholdDayMinorPCVThreshold      Integer32,
        sonusDsx1ThresholdDayMinorPESThreshold      Integer32,
        sonusDsx1ThresholdDayMinorPSESThreshold       Integer32,
        sonusDsx1ThresholdDayMinorCSSThreshold      Integer32,
        sonusDsx1ThresholdDayMinorSASThreshold      Integer32,
        sonusDsx1ThresholdDayMinorUASThreshold      Integer32,
        sonusDsx1ThresholdMajorLCVThreshold   Integer32,
        sonusDsx1ThresholdMajorLESThreshold   Integer32,
        sonusDsx1ThresholdMajorLSESThreshold    Integer32,
        sonusDsx1ThresholdMajorPCVThreshold   Integer32,
        sonusDsx1ThresholdMajorPESThreshold   Integer32,
        sonusDsx1ThresholdMajorPSESThreshold    Integer32,
        sonusDsx1ThresholdMajorCSSThreshold   Integer32,
        sonusDsx1ThresholdMajorSASThreshold   Integer32,
        sonusDsx1ThresholdMajorUASThreshold   Integer32,
        sonusDsx1ThresholdDayMajorLCVThreshold      Integer32,
        sonusDsx1ThresholdDayMajorLESThreshold      Integer32,
        sonusDsx1ThresholdDayMajorLSESThreshold       Integer32,
        sonusDsx1ThresholdDayMajorPCVThreshold      Integer32,
        sonusDsx1ThresholdDayMajorPESThreshold      Integer32,
        sonusDsx1ThresholdDayMajorPSESThreshold       Integer32,
        sonusDsx1ThresholdDayMajorCSSThreshold      Integer32,
        sonusDsx1ThresholdDayMajorSASThreshold      Integer32,
        sonusDsx1ThresholdDayMajorUASThreshold      Integer32,
        sonusDsx1ThresholdCriticalLCVThreshold      Integer32,
        sonusDsx1ThresholdCriticalLESThreshold      Integer32,
        sonusDsx1ThresholdCriticalLSESThreshold       Integer32,
        sonusDsx1ThresholdCriticalPCVThreshold      Integer32,
        sonusDsx1ThresholdCriticalPESThreshold      Integer32,
        sonusDsx1ThresholdCriticalPSESThreshold       Integer32,
        sonusDsx1ThresholdCriticalCSSThreshold      Integer32,
        sonusDsx1ThresholdCriticalSASThreshold      Integer32,
        sonusDsx1ThresholdCriticalUASThreshold      Integer32,
        sonusDsx1ThresholdDayCriticalLCVThreshold         Integer32,
        sonusDsx1ThresholdDayCriticalLESThreshold         Integer32,
        sonusDsx1ThresholdDayCriticalLSESThreshold          Integer32,
        sonusDsx1ThresholdDayCriticalPCVThreshold         Integer32,
        sonusDsx1ThresholdDayCriticalPESThreshold         Integer32,
        sonusDsx1ThresholdDayCriticalPSESThreshold          Integer32,
        sonusDsx1ThresholdDayCriticalCSSThreshold         Integer32,
        sonusDsx1ThresholdDayCriticalSASThreshold         Integer32,
        sonusDsx1ThresholdDayCriticalUASThreshold         Integer32,
        sonusDsx1ThresholdLCVTcaFilter  SonusAdminState,
        sonusDsx1ThresholdLESTcaFilter  SonusAdminState,
        sonusDsx1ThresholdLSESTcaFilter SonusAdminState,
        sonusDsx1ThresholdPCVTcaFilter  SonusAdminState,
        sonusDsx1ThresholdPESTcaFilter  SonusAdminState,
        sonusDsx1ThresholdPSESTcaFilter SonusAdminState,
        sonusDsx1ThresholdCSSTcaFilter  SonusAdminState,
        sonusDsx1ThresholdSASTcaFilter  SonusAdminState,
        sonusDsx1ThresholdUASTcaFilter  SonusAdminState,
        sonusDsx1ThresholdReportMinorCriticalTca        SonusAdminState,
        sonusDsx1ThresholdReportDs1OosTca SonusAdminState
    }

