    ::= { sonusDsx3MIBNotificationsPrefix 3 }

sonusDsx3PerfmonIntervalStartedNotification NOTIFICATION-TYPE
    OBJECTS         { sonusShelfIndex, sonusSlotIndex, sonusPortIndex, 
