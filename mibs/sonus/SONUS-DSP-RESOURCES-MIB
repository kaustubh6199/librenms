    OBJECTS         { sonusShelfIndex, sonusSlotIndex, 
                      sonusDspSlotStatPacketOutageTotal, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
    STATUS          current
    DESCRIPTION     
            "Generated after a packet outage exceeds a configured threshold 
             for a VoIP call on the specified shelf and slot."
    ::= { sonusPadMIBNotificationsPrefix 2 }

sonusPadPacketOutageThresholdClearedNotification NOTIFICATION-TYPE
    OBJECTS         { sonusShelfIndex, sonusSlotIndex, 
                      sonusDspSlotStatPacketOutageInterval, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
    STATUS          current
    DESCRIPTION     
            "Generated 10 seconds after the last packet outage exceeds a 
             configured threshold for a VoIP call on the specified shelf and slot."
    ::= { sonusPadMIBNotificationsPrefix 3 }

sonusPadDspFailureNotification NOTIFICATION-TYPE
    OBJECTS         { sonusShelfIndex, sonusSlotIndex, 
                      sonusDspAdmnDspIndex, sonusEventDescription, 
                      sonusEventClass, sonusEventLevel, sonusSequenceId, 
                      sonusEventTime }
    STATUS          current
    DESCRIPTION     
            "Generated after a DSP has not responded to consecutive DSP stat
             commands."
    ::= { sonusPadMIBNotificationsPrefix 4 }

sonusPadThresholdPerSlotNotification NOTIFICATION-TYPE
    OBJECTS         { sonusShelfIndex, sonusSlotIndex, 
                      sonusPadThresholdType, sonusPadThresholdState, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
    STATUS          current
    DESCRIPTION     
            "Generated when a PAD resource usage of a slot exceeds a configured the threshold value.
             The type indicates the type reseource."
    ::= { sonusPadMIBNotificationsPrefix 5 }

sonusDspActDeActFailureNotification NOTIFICATION-TYPE
    OBJECTS         { sonusShelfIndex, sonusSlotIndex, 
                      sonusDspAdmnDspIndex, sonusEventDescription, 
                      sonusEventClass, sonusEventLevel, sonusSequenceId, 
                      sonusEventTime }
    STATUS          current
    DESCRIPTION     
            "Generated when DSP resources could not be activated or deactivated.
             This typically indicates a resource was not cleaned up."
    ::= { sonusPadMIBNotificationsPrefix 6 }

sonusPadDspChecksumFailure NOTIFICATION-TYPE
    OBJECTS         { sonusShelfIndex, sonusSlotIndex, 
                      sonusDspAdmnDspIndex, sonusPadAdmnChecksumAction, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
    STATUS          current
    DESCRIPTION     
            "Generated after a DSP has detected a checksum failure."
    ::= { sonusPadMIBNotificationsPrefix 7 }

sonusPadDspChecksumErrorRepaired NOTIFICATION-TYPE
    OBJECTS         { sonusShelfIndex, sonusSlotIndex, 
                      sonusDspAdmnDspIndex, sonusEventDescription, 
                      sonusEventClass, sonusEventLevel, sonusSequenceId, 
                      sonusEventTime }
    STATUS          current
    DESCRIPTION     
            "Generated after a DSP has detected a checksum failure and repaired the error successfully."
    ::= { sonusPadMIBNotificationsPrefix 8 }

sonusPadDspChecksumErrorRepairFailed NOTIFICATION-TYPE
    OBJECTS         { sonusShelfIndex, sonusSlotIndex, 
                      sonusDspAdmnDspIndex, sonusEventDescription, 
                      sonusEventClass, sonusEventLevel, sonusSequenceId, 
                      sonusEventTime }
    STATUS          current
    DESCRIPTION     
            "Generated after a DSP has detected a checksum failure and the error could not be repaired."
    ::= { sonusPadMIBNotificationsPrefix 9 }

END -- end of module SONUS-DSP-RESOURCES-MIB.
