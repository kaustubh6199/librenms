    SYNTAX          SonusPadAdmnEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "Entry in Table consisting of the Sonus Packet Assembler Dis-assembler Administartion information"
    INDEX           { sonusPadAdmnShelfIndex }
    ::= { sonusPadAdmnTable 1 }

SonusPadAdmnEntry ::=
    SEQUENCE {
        sonusPadAdmnShelfIndex          SonusShelfIndex,
        sonusPadAdmnJitterEvalPeriod    Integer32,
        sonusPadAdmnJitterMinOccThsh    Integer32,
        sonusPadAdmnRtpG711ALaw         Integer32,
        sonusPadAdmnRtpG711ULaw         Integer32,
        sonusPadAdmnRtpG729A            Integer32,
        sonusPadAdmnRtpG729AB           Integer32,
        sonusPadAdmnG729ABThreshold     Integer32,
        sonusPadAdmnG729ABThresholdState SonusAdminState,
        sonusPadAdmnToneThreshold       Integer32,
        sonusPadAdmnToneThresholdState  SonusAdminState,
        sonusPadAdmnRowStatus           RowStatus,
        sonusPadAdmnRtpDtmfRelay        Integer32,
        sonusPadAdmnRtpFaxRelay         Integer32,
        sonusPadAdmnSidMinTime          Integer32,
        sonusPadAdmnSidMaxTime          Integer32,
        sonusPadAdmnSidHangoverTime     Integer32,
        sonusPadAdmnSidMinNoiseFloor    Integer32,
        sonusPadAdmnSidMaxNoiseFloor    Integer32,
        sonusPadAdmnComfortEnergy       Integer32,
        sonusPadAdmnPacketOutageThreshold Integer32,
        sonusPadAdmnPacketOutageTotalReset  INTEGER,
        sonusPadAdmnG7231Threshold      Integer32,
        sonusPadAdmnG7231ThresholdState SonusAdminState,
        sonusPadAdmnG726Threshold       Integer32,
        sonusPadAdmnG726ThresholdState  SonusAdminState,
        sonusPadAdmnSignalingTone       INTEGER,
        sonusPadAdmnNlpDisable          Integer32,
        sonusPadAdmnNlpEnable           Integer32,
        sonusPadAdmnNarrowbandDetection INTEGER,
        sonusPadAdmnUniversalCompressionThreshold         Integer32,
        sonusPadAdmnUniversalCompressionThresholdState              SonusAdminState,
        sonusPadAdmnPlayoutTimeseriesPeriod   Integer32,
        sonusPadAdmnPlayoutTimeseriesThreshold0       Integer32,
        sonusPadAdmnPlayoutTimeseriesThreshold1       Integer32,
        sonusPadAdmnPlayoutTimeseriesThreshold2       Integer32,
        sonusPadAdmnPacketOutageInterval Integer32,
        sonusPadAdmnPacketLossEvalInterval  Integer32,
        sonusPadAdmnPacketLossThreshold Integer32,
        sonusPadAdmnPacketLossThresholdDuration       Integer32,
        sonusPadAdmnCnProfile           INTEGER,
        sonusPadAdmnCnRateControl       Integer32,
        sonusPadAdmnChecksumAction      INTEGER,
        sonusPadAdmnDcsTcfGap           Integer32,
        sonusPadAdmnEpt                 SonusAdminState,
        sonusPadAdmnDTMFConfigDuration  Integer32,
        sonusPadAdmnDryupTimer          Integer32,
        sonusPadAdmnAudioTxDuring2833   SonusAdminState
    }

