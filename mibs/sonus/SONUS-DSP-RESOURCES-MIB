    SYNTAX          Integer32 (1..4)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Index into the GSM Redundancy Profile table"
    ::= { sonusGsmRedundancyEntry 2 }

sonusGsmRedundancyCodecMode OBJECT-TYPE
    SYNTAX          Integer32 (1..4)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "AMR Codec Mode used by this Profile"
    DEFVAL          { 1 }
    ::= { sonusGsmRedundancyEntry 3 }

sonusGsmRedundancyPktRed OBJECT-TYPE
    SYNTAX          Integer32 (0..2)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "Packet Redundancy (0,1,2) associated with the Codec Mode(sonusGsmRedundancyCodecMode), 0
             indicates Redundancy is disabled for AMR GANC mode"
    DEFVAL          { 0 }
    ::= { sonusGsmRedundancyEntry 4 }

sonusGsmRedundancyPktLossThreshold OBJECT-TYPE
    SYNTAX          Integer32 (0..63)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "Packet Loss Threshold value associated with the Codec Mode(sonusGsmRedundancyCodecMode),
             expressed in number of AMR frames, only apply if Redundancy value (sonusGsmRedundancyPktRed)
             is non-zero; This value is a percentage of the total AMR frames in PacketLossEvaluationInteval
             (sonusPadAdmnPacketLossEvalInterval) and is used to calculate the frame loss ratio for AMR 
             codecs"
    DEFVAL          { 15 }
    ::= { sonusGsmRedundancyEntry 5 }

sonusGsmRedundancyPktLossHysteresis OBJECT-TYPE
    SYNTAX          Integer32 (0..15)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "Packet Loss Hysteresis value associated with the Codec Mode(sonusGsmRedundancyCodecMode),
             expressed in number of AMR frames, only apply if Redundancy value (sonusGsmRedundancyPktRed)
             is non-zero; This value is a percentage of the total AMR frames in PacketLossEvaluationInteval
             (sonusPadAdmnPacketLossEvalInterval) and is used to calculate the frame loss ratio for AMR 
             codecs"
    DEFVAL          { 2 }
    ::= { sonusGsmRedundancyEntry 6 }

