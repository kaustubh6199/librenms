    SEQUENCE {
        sonusGsmRedundancyRowStatus     RowStatus,
        sonusGsmRedundancyIndex         Integer32,
        sonusGsmRedundancyCodecMode     Integer32,
        sonusGsmRedundancyPktRed        Integer32,
        sonusGsmRedundancyPktLossThreshold  Integer32,
        sonusGsmRedundancyPktLossHysteresis   Integer32
    }

