    SYNTAX          INTEGER { nfsOOS(0), lnkFail(1), lnkCong(2) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "This object defines the stat reason"
    DEFVAL          { 1 }
    ::= { sonusDflushNetworkFailureEventStatEntry 5 }

