sonusGr303NodeStatus OBJECT IDENTIFIER
    ::= { sonusGr303MIBObjects 16 }

sonusGr303NodeStatSubProvIntervalRemaining OBJECT-TYPE
    SYNTAX          Unsigned32 (0..65535)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "This is the time remaining in the current Subscriber Provisioning 
             interval timer passed down by the PSX in the last polling operation"
    ::= { sonusGr303NodeStatus 1 }

sonusGr303NodeStatSubProvPsxPolls OBJECT-TYPE
    SYNTAX          Unsigned32 (0..4294967295)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "Number of times the GSX has polled the PSX for Subscriber info"
    ::= { sonusGr303NodeStatus 2 }

sonusGr303NodeStatSubProvPsxPartialUpdates OBJECT-TYPE
    SYNTAX          Unsigned32 (0..4294967295)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "Number of times the GSX has polled the PSX for partial subscriber info"
    ::= { sonusGr303NodeStatus 3 }

sonusGr303NodeStatSubProvPsxFullUpdates OBJECT-TYPE
    SYNTAX          Unsigned32 (0..4294967295)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "Number of times the GSX has polled the PSX for all the data"
    ::= { sonusGr303NodeStatus 4 }

sonusGr303NodeStatSubProvPsxPollFailures OBJECT-TYPE
    SYNTAX          Unsigned32 (0..4294967295)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "Number of times the GSX timed out waiting for a response to a poll message from the PSX"
    ::= { sonusGr303NodeStatus 5 }

sonusGr303NodeStatSubProvPsxPollRetries OBJECT-TYPE
    SYNTAX          Unsigned32 (0..4294967295)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "Number of times the GSX retried sending a poll message to the PSX"
    ::= { sonusGr303NodeStatus 6 }

sonusGr303MIBNotifications OBJECT IDENTIFIER
    ::= { sonusGr303MIBObjects 50 }

sonusGr303MIBNotificationsPrefix OBJECT IDENTIFIER
    ::= { sonusGr303MIBNotifications 0 }

sonusGr303MIBNotificationsObjects OBJECT IDENTIFIER
    ::= { sonusGr303MIBNotifications 1 }

sonusGr303TerminationId OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(1..46))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The unknown Gr303 termination ID."
    ::= { sonusGr303MIBNotificationsObjects 1 }

sonusGr303EocRdtAlarmCategory OBJECT-TYPE
    SYNTAX          INTEGER { indeterminate(0), equipment(1), 
                        transmission(2), environmental(3), software(4), 
                        service(5), thresholdAlert(6), 
                        scheduledManagementOperations(7) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The EOC RDT alarm cateogry."
    ::= { sonusGr303MIBNotificationsObjects 2 }

sonusGr303EocRdtAlarmSeverity OBJECT-TYPE
    SYNTAX          INTEGER { cleared(0), indeterminate(1), warning(2), 
                        minor(3), major(4), critical(5) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The EOC RDT alarm severity."
    ::= { sonusGr303MIBNotificationsObjects 3 }

sonusGr303MgcDownNotification NOTIFICATION-TYPE
    OBJECTS         { sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
    STATUS          current
    DESCRIPTION     
            "This trap indicates that this node has lost communications with
             its Media Gateway Controller (MGC) and all attempts to regain
             communications have expired."
    ::= { sonusGr303MIBNotificationsPrefix 1 }

sonusGr303MgcUpNotification NOTIFICATION-TYPE
    OBJECTS         { sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
    STATUS          current
    DESCRIPTION     
            "This trap indicates that this node has just regained
             communication with its Media Gateway Controller (MGC)."
    ::= { sonusGr303MIBNotificationsPrefix 2 }

sonusGr303ShoweringLineNotification NOTIFICATION-TYPE
    OBJECTS         { sonusGr303IgName, sonusGr303IgCrvIndex, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
    STATUS          current
    DESCRIPTION     
            "This trap indicates that a showering line condition has
             been detected on a GR303 Interface Group subscrivber CRV."
    ::= { sonusGr303MIBNotificationsPrefix 3 }

sonusGr303ShoweringLineClearedNotification NOTIFICATION-TYPE
    OBJECTS         { sonusGr303IgName, sonusGr303IgCrvIndex, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
    STATUS          current
    DESCRIPTION     
            "This trap indicates that a showering line condition has
             been cleared on a GR303 Interface Group subscriber CRV."
    ::= { sonusGr303MIBNotificationsPrefix 4 }

sonusGr303TmcDownNotification NOTIFICATION-TYPE
    OBJECTS         { sonusGr303IgName, sonusEventDescription, 
                      sonusEventClass, sonusEventLevel, sonusSequenceId, 
                      sonusEventTime }
    STATUS          current
    DESCRIPTION     
            "This trap indicates that this GR303 Interface Group lacks
             an active TMC path with an established LAPD payload link."
    ::= { sonusGr303MIBNotificationsPrefix 5 }

sonusGr303TmcUpNotification NOTIFICATION-TYPE
    OBJECTS         { sonusGr303IgName, sonusEventDescription, 
                      sonusEventClass, sonusEventLevel, sonusSequenceId, 
                      sonusEventTime }
    STATUS          current
    DESCRIPTION     
            "This trap indicates that a LAPD payload link is established
             on the active TMC path of this GR303 Interface Group."
    ::= { sonusGr303MIBNotificationsPrefix 6 }

sonusGr303UnknownTerminationIdNotification NOTIFICATION-TYPE
    OBJECTS         { sonusGr303TerminationId, sonusEventDescription, 
                      sonusEventClass, sonusEventLevel, sonusSequenceId, 
                      sonusEventTime }
    STATUS          current
    DESCRIPTION     
            "This trap indicates that an Unknown Termination ID cause code
             (430) for a termination id is received from Media Gateway
             Controller (MGC) in response to a H.248 message sent by this GSX."
    ::= { sonusGr303MIBNotificationsPrefix 7 }

sonusGr303UnknownTerminationIdClearedNotification NOTIFICATION-TYPE
    OBJECTS         { sonusGr303TerminationId, sonusEventDescription, 
                      sonusEventClass, sonusEventLevel, sonusSequenceId, 
                      sonusEventTime }
    STATUS          current
    DESCRIPTION     
            "This trap indicates that this Unknown Termination ID cause code
             (430) for a termination id is cleared."
    ::= { sonusGr303MIBNotificationsPrefix 8 }

sonusGr303EocRdtAlarmNotification NOTIFICATION-TYPE
    OBJECTS         { sonusGr303IgName, sonusGr303EocRdtAlarmCategory, 
                      sonusGr303EocRdtAlarmSeverity, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
    STATUS          current
    DESCRIPTION     
            "This trap reports a GR303 Interface Group's EOC RDT alarm severity
             of a specified category."
    ::= { sonusGr303MIBNotificationsPrefix 9 }

END -- end of module SONUS-GR303-MIB.
