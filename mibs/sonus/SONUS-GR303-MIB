        sonusGr303StatIgCrvDs1Num       Unsigned32,
        sonusGr303StatIgCrvDs0Num       Unsigned32,
        sonusGr303StatIgCrvLastCauseCode4Crv    Unsigned32,
        sonusGr303StatIgCrvEocPriServiceState     INTEGER,
        sonusGr303StatIgCrvEocSecServiceState     BITS,
        sonusGr303StatIgCrvEocLineCircuitAddress        DisplayString,
        sonusGr303StatIgCrvDuration     Unsigned32,
        sonusGr303StatIgCrvEocReturnStatus  INTEGER
    }

