sonusGr303CountIgLapdEntry OBJECT-TYPE
    SYNTAX          SonusGr303CountIgLapdEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "An entry in the Sonus Interface Group LAPD Counter table."
    INDEX           { sonusGr303CountIgLapdIgIndex, 
                      sonusGr303CountIgLapdLinkNum }
    ::= { sonusGr303CountIgLapdTable 1 }

SonusGr303CountIgLapdEntry ::=
    SEQUENCE {
        sonusGr303CountIgLapdIgIndex    Unsigned32,
        sonusGr303CountIgLapdLinkNum    Unsigned32,
        sonusGr303CountIgLapdTxPackets  Counter32,
        sonusGr303CountIgLapdRxPackets  Counter32,
        sonusGr303CountIgLapdT200Timeouts Counter32,
        sonusGr303CountIgLapdLinkEstSuccesses     Counter32,
        sonusGr303CountIgLapdLinkEstFailures    Counter32,
        sonusGr303CountIgLapdAckedPackets Counter32,
        sonusGr303CountIgLapdRcvdInvalidFrames      Counter32,
        sonusGr303CountIgLapdTimesOwnRcvrBusy     Counter32,
        sonusGr303CountIgLapdTimesPeerRcvrBusy      Counter32,
        sonusGr303CountIgLapdReturnStatus INTEGER
    }

sonusGr303CountIgLapdIgIndex OBJECT-TYPE
    SYNTAX          Unsigned32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The index of the GR303 Interface Group that this LAPD link belongs to."
    ::= { sonusGr303CountIgLapdEntry 1 }

sonusGr303CountIgLapdLinkNum OBJECT-TYPE
    SYNTAX          Unsigned32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The link number of this LAPD link.
             The meaning is as follows:
             1 - Primary,   TMC
             2 - Primary,   TMC, PPS
             3 - Primary,   EOC
             4 - Primary,   EOC, PPS
             5 - Secondary, TMC
             6 - Secondary, TMC, PPS
             7 - Secondary, EOC
             8 - Secondary, EOC, PPS"
    ::= { sonusGr303CountIgLapdEntry 2 }

