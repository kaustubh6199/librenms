sonusGr303IgRdtAlarmCountTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusGr303IgRdtAlarmCountEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "This table presents the RDT alarm counters reported to a GR303 Interface Group."
    ::= { sonusGr303MIBObjects 15 }

sonusGr303IgRdtAlarmCountEntry OBJECT-TYPE
    SYNTAX          SonusGr303IgRdtAlarmCountEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "This entry presents the RDT alarm counters reported to a GR303 Interface Group."
    INDEX           { sonusGr303IgRdtAlarmIgIndex }
    ::= { sonusGr303IgRdtAlarmCountTable 1 }

SonusGr303IgRdtAlarmCountEntry ::=
    SEQUENCE {
        sonusGr303IgRdtAlarmIgIndex     Unsigned32,
        sonusGr303IgRdtAlarmIndeterminateIndeterminate              Unsigned32,
        sonusGr303IgRdtAlarmIndeterminateWarning        Unsigned32,
        sonusGr303IgRdtAlarmIndeterminateMinor      Unsigned32,
        sonusGr303IgRdtAlarmIndeterminateMajor      Unsigned32,
        sonusGr303IgRdtAlarmIndeterminateCritical         Unsigned32,
        sonusGr303IgRdtAlarmEquipmentIndeterminate          Unsigned32,
        sonusGr303IgRdtAlarmEquipmentWarning    Unsigned32,
        sonusGr303IgRdtAlarmEquipmentMinor  Unsigned32,
        sonusGr303IgRdtAlarmEquipmentMajor  Unsigned32,
        sonusGr303IgRdtAlarmEquipmentCritical     Unsigned32,
        sonusGr303IgRdtAlarmTransmissionIndeterminate             Unsigned32,
        sonusGr303IgRdtAlarmTransmissionWarning       Unsigned32,
        sonusGr303IgRdtAlarmTransmissionMinor     Unsigned32,
        sonusGr303IgRdtAlarmTransmissionMajor     Unsigned32,
        sonusGr303IgRdtAlarmTransmissionCritical        Unsigned32,
        sonusGr303IgRdtAlarmEnvironmentalIndeterminate              Unsigned32,
        sonusGr303IgRdtAlarmEnvironmentalWarning        Unsigned32,
        sonusGr303IgRdtAlarmEnvironmentalMinor      Unsigned32,
        sonusGr303IgRdtAlarmEnvironmentalMajor      Unsigned32,
        sonusGr303IgRdtAlarmEnvironmentalCritical         Unsigned32,
        sonusGr303IgRdtAlarmSoftwareIndeterminate         Unsigned32,
        sonusGr303IgRdtAlarmSoftwareWarning   Unsigned32,
        sonusGr303IgRdtAlarmSoftwareMinor Unsigned32,
        sonusGr303IgRdtAlarmSoftwareMajor Unsigned32,
        sonusGr303IgRdtAlarmSoftwareCritical    Unsigned32,
        sonusGr303IgRdtAlarmServiceIndeterminate        Unsigned32,
        sonusGr303IgRdtAlarmServiceWarning  Unsigned32,
        sonusGr303IgRdtAlarmServiceMinor Unsigned32,
        sonusGr303IgRdtAlarmServiceMajor Unsigned32,
        sonusGr303IgRdtAlarmServiceCritical   Unsigned32,
        sonusGr303IgRdtAlarmThresholdAlertIndeterminate               Unsigned32,
        sonusGr303IgRdtAlarmThresholdAlertWarning         Unsigned32,
        sonusGr303IgRdtAlarmThresholdAlertMinor       Unsigned32,
        sonusGr303IgRdtAlarmThresholdAlertMajor       Unsigned32,
        sonusGr303IgRdtAlarmThresholdAlertCritical          Unsigned32,
        sonusGr303IgRdtAlarmScheduledManagementOperationsIndeterminate                              Unsigned32,
        sonusGr303IgRdtAlarmScheduledManagementOperationsWarning                        Unsigned32,
        sonusGr303IgRdtAlarmScheduledManagementOperationsMinor                      Unsigned32,
        sonusGr303IgRdtAlarmScheduledManagementOperationsMajor                      Unsigned32,
        sonusGr303IgRdtAlarmScheduledManagementOperationsCritical                         Unsigned32,
        sonusGr303IgRdtAlarmReturnStatus INTEGER
    }

