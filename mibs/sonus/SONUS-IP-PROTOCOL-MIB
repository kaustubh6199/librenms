    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Count of SA failure due to proposal failures"
    ::= { sonusNrsIsakmpdIkeStatEntry 12 }

sonusNrsIsakmpdIkeInvalidIDinfoCnt OBJECT-TYPE
    SYNTAX          Counter32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Count of SA failure due to invalid ID information"
    ::= { sonusNrsIsakmpdIkeStatEntry 13 }

sonusNrsIsakmpdIkeInvalidHashCnt OBJECT-TYPE
    SYNTAX          Counter32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Count of SA failure due to invalid hash information"
    ::= { sonusNrsIsakmpdIkeStatEntry 14 }

sonusNrsIsakmpdIkeV2AuthFailedCnt OBJECT-TYPE
    SYNTAX          Counter32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Count of SA failed during AUTH for IKEv2"
    ::= { sonusNrsIsakmpdIkeStatEntry 15 }

sonusNrsIsakmpdIkeTSFailedCnt OBJECT-TYPE
    SYNTAX          Counter32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Count of SA failed due to Traffic Selector"
    ::= { sonusNrsIsakmpdIkeStatEntry 16 }

sonusNrsIsakmpdIkePeerIpAddress OBJECT-TYPE
    SYNTAX          IpAddress
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The ISAKMPD Peer IP address."
    ::= { sonusNrsIsakmpdIkeStatEntry 17 }

