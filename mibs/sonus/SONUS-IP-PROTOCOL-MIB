SonusSctpGeneralGroupEntry ::=
    SEQUENCE {
        sonusSctpActiveAssociations     Counter32,
        sonusSctpActiveStreams          Counter32,
        sonusSctpAttemptFails           Counter32,
        sonusSctpCurrEstab              Counter32,
        sonusSctpInPkts                 Counter32,
        sonusSctpInBadchecksum          Counter32,
        sonusSctpInErrs                 Counter32,
        sonusSctpInChunks               Counter32,
        sonusSctpInInitChunks           Counter32,
        sonusSctpInDataChunks           Counter32,
        sonusSctpInSackChunks           Counter32,
        sonusSctpInHeartBeatChunks      Counter32,
        sonusSctpInAbortChunks          Counter32,
        sonusSctpInErrChunks            Counter32,
        sonusSctpInOotbChunks           Counter32,
        sonusSctpOutPkts                Counter32,
        sonusSctpOutChunks              Counter32,
        sonusSctpOutInitChunks          Counter32,
        sonusSctpOutDataChunks          Counter32,
        sonusSctpOutSackChunks          Counter32,
        sonusSctpOutHeartBeatChunks     Counter32,
        sonusSctpOutAbortChunks         Counter32,
        sonusSctpOutErrChunks           Counter32,
        sonusSctpGeneralGroupShelf      SonusShelfIndex,
        sonusSctpGeneralGroupSlot       SonusSlotIndex,
        sonusSctpGeneralGroupInstance   Integer32
    }

