    SYNTAX          INTEGER { afInet(2), afInet6(24) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The SPD traffic selector local IP address type."
    ::= { sonusNrsIsakmpdIkeStatEntry 19 }

