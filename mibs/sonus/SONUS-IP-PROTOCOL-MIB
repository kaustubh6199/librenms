    SYNTAX          Counter32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Count of SA failed due to DPD Failure"
    ::= { sonusNrsIsakmpdIkeStatEntry 20 }

sonusIpTrap OBJECT IDENTIFIER
    ::= { sonusMib-2 10 }

sonusIpsecMIBNotifications OBJECT IDENTIFIER
    ::= { sonusIpTrap 1 }

sonusIpsecMIBNotificationsPrefix OBJECT IDENTIFIER
    ::= { sonusIpsecMIBNotifications 0 }

sonusIpsecMIBNotificationsObjects OBJECT IDENTIFIER
    ::= { sonusIpsecMIBNotifications 1 }

sonusIpsecIkeFailureNotification NOTIFICATION-TYPE
    OBJECTS         { sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
    STATUS          current
    DESCRIPTION     
            "This trap indicates that IKE/IPSEC failure"
    ::= { sonusIpsecMIBNotificationsPrefix 1 }

