SonusRogueMediaBySrcPolicerDiscardRingEntry ::=
    SEQUENCE {
        sonusRogueMediaBySrcPolicerDiscardRingShelfIndex                SonusShelfIndex,
        sonusRogueMediaBySrcPolicerDiscardRingSlotIndex               SonusSlotIndex,
        sonusRogueMediaBySrcPolicerDiscardRingPositionIndex                   Unsigned32,
        sonusRogueMediaBySrcPolicerDiscardRingNIFSIFName                SonusNameReference,
        sonusRogueMediaBySrcPolicerDiscardRingSourceIpAddress                     IpAddress,
        sonusRogueMediaBySrcPolicerDiscardRingDestIpAddress                   IpAddress,
        sonusRogueMediaBySrcPolicerDiscardRingDiscardCount                  Counter32,
        sonusRogueMediaBySrcPolicerDiscardRingType          INTEGER,
        sonusRogueMediaBySrcPolicerDiscardRingSourceIpPort                  Unsigned32,
        sonusRogueMediaBySrcPolicerDiscardRingDestIpPort                Unsigned32,
        sonusRogueMediaBySrcPolicerDiscardRingDestUnique                INTEGER,
        sonusRogueMediaBySrcPolicerDiscardRingSourceIp6Address                      Ipv6Address,
        sonusRogueMediaBySrcPolicerDiscardRingSourceIpVer                 INTEGER,
        sonusRogueMediaBySrcPolicerDiscardRingDestIp6Address                    Ipv6Address,
        sonusRogueMediaBySrcPolicerDiscardRingDestIpVer               INTEGER
    }

