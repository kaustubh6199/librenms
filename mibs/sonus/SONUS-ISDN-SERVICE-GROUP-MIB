    SYNTAX          SonusNameReference
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "Specifies a named reference to a tone to play for dial tone."
    ::= { sonusIsdnsgProfileEntry 37 }

sonusIsdnsgProfileMinDigitsForQuery OBJECT-TYPE
    SYNTAX          Integer32 (0..30)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "If Overlap Receiving is supported and CalledAddress
             received is not complete, then minimum number of
             CalledAddress Digits to receive before we go for Route
             Query."
    DEFVAL          { 0 }
    ::= { sonusIsdnsgProfileEntry 38 }

sonusIsdnsgProfileImmediateDiscWithInbandTones OBJECT-TYPE
    SYNTAX          INTEGER { disabled(1), enabled(2), establishing(3), 
                        allstates(4) }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "When a DISCONNECT with inband tones is recieved, to maintain
             the voicepath a PROGRESS is propagated.  This parameter causes
             an immediate disconnect instead.  The options are explaned below.
             	disabled:		maintain voice path
             	enabled:		immediate disconnect in stable states
             	establishing	immediate disconnect in establishing states
             	allstates		immediate disconnect in all states"
    DEFVAL          { disabled }
    ::= { sonusIsdnsgProfileEntry 39 }

sonusIsdnsgProfileConfirmationToneOverride OBJECT-TYPE
    SYNTAX          INTEGER { disabled(1), enabled(2) }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "If supported, informs CC to play a C-Tone for all
             calls using this service group.  If unsupported, 
             informs CC to use the enable flag from the PSX to
             decide whether or not to play a C-Tone."
    DEFVAL          { disabled }
