                        bchOffset18(18), bchOffset19(19), 
                        bchOffset20(20), bchOffset21(21), 
                        bchOffset22(22), bchOffset23(23), 
                        bchOffset24(24), bchOffset25(25), 
                        bchOffset26(26), bchOffset27(27), 
                        bchOffset28(28), bchOffset29(29), 
                        bchOffset30(30), bchOffset31(31) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "B-channel maintenance status bitmap."
    ::= { sonusIsdnServiceGroupMIBNotificationsObjects 3 }

sonusIsdnSvcGrpInServiceNotification NOTIFICATION-TYPE
    OBJECTS         { sonusIsdnSvcGrpName, sonusEventDescription, 
                      sonusEventClass, sonusEventLevel, sonusSequenceId, 
                      sonusEventTime }
    STATUS          current
    DESCRIPTION     
            "This trap indicates that the specified ISDN Service Group
             was placed in service."
    ::= { sonusIsdnServiceGroupMIBNotificationsPrefix 1 }

