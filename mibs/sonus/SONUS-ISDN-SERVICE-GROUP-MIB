sonusIsdnsgProfile OBJECT IDENTIFIER
    ::= { sonusIsdnServiceGroupMIBObjects 3 }

sonusIsdnsgProfileNextIndex OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The next valid index to use when creating a new sonusIsdnsgProfileEntry"
    ::= { sonusIsdnsgProfile 1 }

sonusIsdnsgProfileTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusIsdnsgProfileEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "This table contains ISDN Service Group Profiles."
    ::= { sonusIsdnsgProfile 2 }

sonusIsdnsgProfileEntry OBJECT-TYPE
    SYNTAX          SonusIsdnsgProfileEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "This table describes the ISDN Service Groups profile."
    INDEX           { sonusIsdnsgProfileListIndex }
    ::= { sonusIsdnsgProfileTable 1 }

SonusIsdnsgProfileEntry ::=
    SEQUENCE {
        sonusIsdnsgProfileListIndex     Integer32,
        sonusIsdnsgProfileName          SonusName,
        sonusIsdnsgProfileTrunkGroup    SonusNameReference,
        sonusIsdnsgProfileHuntAlgorithm INTEGER,
        sonusIsdnsgProfileCost          Integer32,
        sonusIsdnsgProfileSwitchSide    INTEGER,
        sonusIsdnsgProfileSwitchType    INTEGER,
        sonusIsdnsgProfileSwitchProfile DisplayString,
        sonusIsdnsgProfileMode          SonusServiceState,
        sonusIsdnsgProfileAdminState    SonusAdminState,
        sonusIsdnsgProfileRowStatus     RowStatus,
        sonusIsdnsgProfileDefaultDirectoryNumber        DisplayString,
        sonusIsdnsgProfileCallingPartyNumberProvision             INTEGER,
        sonusIsdnsgProfileCallingPartyNumberDiscard           INTEGER,
        sonusIsdnsgProfileCallingPartyNumberDelivery            INTEGER,
        sonusIsdnsgProfileCallingPartySubAddressTransfer                INTEGER,
        sonusIsdnsgProfileCallingPartySubAddressDelivery                INTEGER,
        sonusIsdnsgProfileCallingPartyNumberPresentation                INTEGER,
        sonusIsdnsgProfileCallingPartyNameDelivery          INTEGER,
        sonusIsdnsgProfileCalledPartySubAddressTransfer               INTEGER,
        sonusIsdnsgProfileCalledPartySubAddressDelivery               INTEGER,
        sonusIsdnsgProfileTonePackageName SonusNameReference,
        sonusIsdnsgProfileAlertToneName SonusNameReference,
        sonusIsdnsgProfileIsupIwfEarlyACM INTEGER,
        sonusIsdnsgProfileIsupEarlyCutThruAudibleRing             INTEGER,
        sonusIsdnsgProfileIsupEarlyCutThruNetworkInterworking                     INTEGER,
        sonusIsdnsgProfileDiscTreatmentProfileName          SonusNameReference,
        sonusIsdnsgProfileDiscSspProfileName    SonusNameReference,
        sonusIsdnsgProfileBearerCapabilityFallback          INTEGER,
        sonusIsdnsgProfileDialToneName  SonusNameReference,
        sonusIsdnsgProfileTbctEnabled   INTEGER,
        sonusIsdnsgProfileTbctAcknowledge INTEGER,
        sonusIsdnsgProfileTbctNotificationToTransferredUsers                    INTEGER,
        sonusIsdnsgProfileTbctNotificationToController              INTEGER,
        sonusIsdnsgProfileTbctActiveLimit Integer32,
        sonusIsdnsgProfileTbctIntervalLimit   Integer32,
        sonusIsdnsgProfileConfirmationToneName      SonusNameReference,
        sonusIsdnsgProfileMinDigitsForQuery   Integer32,
        sonusIsdnsgProfileImmediateDiscWithInbandTones              INTEGER,
        sonusIsdnsgProfileConfirmationToneOverride          INTEGER,
        sonusIsdnsgProfileAnsSupervisionTimeout       Integer32,
        sonusIsdnsgProfileAnsSupervisionTimeoutAction             INTEGER,
        sonusIsdnsgProfileCrankBackProfile  Integer32,
        sonusIsdnsgProfileCongRelTimeout Integer32,
        sonusIsdnsgProfileGuardTimer    Integer32,
        sonusIsdnsgProfileCallingPartyNameReceiveTimer              Integer32,
        sonusIsdnsgProfileLdcTimeout    Integer32,
        sonusIsdnsgProfileLdcAction     INTEGER,
        sonusIsdnsgProfileLdcRelCause   Integer32,
        sonusIsdnsgProfileLdcEmergencyCalls   INTEGER,
        sonusIsdnsgProfileOliDelivery   INTEGER,
        sonusIsdnsgProfileBchannelNegotiation     INTEGER,
        sonusIsdnsgProfileSendProgressOnAcmWithCause            INTEGER,
        sonusIsdnsgProfileSkipCrankBackProfile      Unsigned32,
        sonusIsdnsgProfilePrecedenceParamForwarding           INTEGER,
        sonusIsdnsgProfileBillableCallForConwithNonCharge                 INTEGER,
        sonusIsdnsgProfileChannelIdentification       INTEGER,
        sonusIsdnsgProfileUSIUpdate     INTEGER,
        sonusIsdnsgProfileCauseOfNoId   INTEGER,
        sonusIsdnsgProfileCRLRelCause   Integer32,
        sonusIsdnsgProfileColpColrIGW   INTEGER,
        sonusIsdnsgProfileTransferConnectConferenceUri              SonusNameReference,
        sonusIsdnsgProfileChanIDinAlertAndConnect         INTEGER,
        sonusIsdnsgProfileSuspendResume INTEGER,
        sonusIsdnsgProfileDisplayInRestart  DisplayString,
        sonusIsdnsgProfileForwardNotify INTEGER
    }

