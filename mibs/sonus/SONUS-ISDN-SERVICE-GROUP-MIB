sonusIsdnsgServiceGroupStatTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusIsdnsgServiceGroupStatEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "This table contains status information about each ISDN service group
             in the GSX9000 Switch node."
    ::= { sonusIsdnServiceGroupMIBObjects 2 }

sonusIsdnsgServiceGroupStatEntry OBJECT-TYPE
    SYNTAX          SonusIsdnsgServiceGroupStatEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "This table describes the status of the Service Groups that are configured
             as destinations ISDN nodes for the GSX9000 Switch node."
    INDEX           { sonusIsdnsgServiceGroupStatListIndex }
    ::= { sonusIsdnsgServiceGroupStatTable 1 }

SonusIsdnsgServiceGroupStatEntry ::=
    SEQUENCE {
        sonusIsdnsgServiceGroupStatListIndex    Integer32,
        sonusIsdnsgServiceGroupStatStatus INTEGER,
        sonusIsdnsgServiceGroupStatTgMode INTEGER,
        sonusIsdnsgServiceGroupStatNumBchConfigured           Integer32,
        sonusIsdnsgServiceGroupStatNumBchAvailable          Integer32,
        sonusIsdnsgServiceGroupStatInCallsActive        Counter32,
        sonusIsdnsgServiceGroupStatInCalls  Counter32,
        sonusIsdnsgServiceGroupStatInConnected      Counter32,
        sonusIsdnsgServiceGroupStatOutCallsActive         Counter32,
        sonusIsdnsgServiceGroupStatOutCalls   Counter32,
        sonusIsdnsgServiceGroupStatOutConnected       Counter32,
        sonusIsdnsgServiceGroupStatPrimaryDchannel          INTEGER,
        sonusIsdnsgServiceGroupStatPrimaryDchInPkts           Counter32,
        sonusIsdnsgServiceGroupStatPrimaryDchOutPkts            Counter32,
        sonusIsdnsgServiceGroupStatPrimaryDchInBytes            Counter32,
        sonusIsdnsgServiceGroupStatPrimaryDchOutBytes             Counter32,
        sonusIsdnsgServiceGroupStatBackupDchannel         INTEGER,
        sonusIsdnsgServiceGroupStatBackupDchInPkts          Counter32,
        sonusIsdnsgServiceGroupStatBackupDchOutPkts           Counter32,
        sonusIsdnsgServiceGroupStatBackupDchInBytes           Counter32,
        sonusIsdnsgServiceGroupStatBackupDchOutBytes            Counter32,
        sonusIsdnsgServiceGroupStatTbctAttempts       Counter32,
        sonusIsdnsgServiceGroupStatTbctActiveCount          Counter32,
        sonusIsdnsgServiceGroupStatTbctActiveLimitRejects                 Counter32,
        sonusIsdnsgServiceGroupStatTbctIntervalCount            Counter32,
        sonusIsdnsgServiceGroupStatTbctIntervalLimitRejects                   Counter32,
        sonusIsdnsgServiceGroupStatTbctTotalRejects           Counter32,
        sonusIsdnsgServiceGroupStatDchannelService          DisplayString,
        sonusIsdnsgServiceGroupStatDchannelPrimMonitorTest                  DisplayString,
        sonusIsdnsgServiceGroupStatDchannelBckMonitorTest                 DisplayString
    }

sonusIsdnsgServiceGroupStatListIndex OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The index of the ISDN Service Group."
    ::= { sonusIsdnsgServiceGroupStatEntry 1 }

