    SYNTAX          RowStatus
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "This is for SNMP use."
    ::= { sonusDsLpTransEntryObject 5 }

sonusDsLpTransLblObjects OBJECT IDENTIFIER
    ::= { sonusLocalPolicyServicesMIBObjects 10 }

sonusDsLpTransLblNextIndex OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The next valid index to use when creating a new sonusDsLpTransLblEntry"
    ::= { sonusDsLpTransLblObjects 1 }

sonusDsLpTransLblTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusDsLpTransLblEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "Each entry of this table defines a Translation Labelin the
             Local Policy server."
    ::= { sonusDsLpTransLblObjects 2 }

sonusDsLpTransLblEntry OBJECT-TYPE
    SYNTAX          SonusDsLpTransLblEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "Definition of a Local Policy Routing Label."
    INDEX           { sonusDsLpTransLblIndex }
    ::= { sonusDsLpTransLblTable 1 }

SonusDsLpTransLblEntry ::=
    SEQUENCE {
        sonusDsLpTransLblIndex          Unsigned32,
        sonusDsLpTransLblName           SonusName,
        sonusDsLpTransLblType           INTEGER,
        sonusDsLpTransLblDnsZoneName    SonusName,
        sonusDsLpTransLblSignalingZoneName  SonusName,
        sonusDsLpTransLblEnumRoot       SonusName,
        sonusDsLpTransLblAdminState     SonusAdminState,
        sonusDsLpTransLblRowStatus      RowStatus
    }

sonusDsLpTransLblIndex OBJECT-TYPE
    SYNTAX          Unsigned32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "A unique index for each Local Policy Routing Label."
    ::= { sonusDsLpTransLblEntry 1 }

sonusDsLpTransLblName OBJECT-TYPE
    SYNTAX          SonusName
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The name of this Local Policy Routing Label."
    ::= { sonusDsLpTransLblEntry 2 }

sonusDsLpTransLblType OBJECT-TYPE
    SYNTAX          INTEGER { none(1), enumaor(2), enumlnp(3) }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The label type for this entry. "
    DEFVAL          { none }
    ::= { sonusDsLpTransLblEntry 3 }

sonusDsLpTransLblDnsZoneName OBJECT-TYPE
    SYNTAX          SonusName
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The DNS Zone Name ."
    ::= { sonusDsLpTransLblEntry 4 }

sonusDsLpTransLblSignalingZoneName OBJECT-TYPE
    SYNTAX          SonusName
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The Signaling Zone Name."
    ::= { sonusDsLpTransLblEntry 5 }

sonusDsLpTransLblEnumRoot OBJECT-TYPE
    SYNTAX          SonusName
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The ENUM root string."
    ::= { sonusDsLpTransLblEntry 6 }

sonusDsLpTransLblAdminState OBJECT-TYPE
    SYNTAX          SonusAdminState
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The admin state of the secondary route. The route is skipped
             if not ENABLED."
    DEFVAL          { disabled }
    ::= { sonusDsLpTransLblEntry 7 }

sonusDsLpTransLblRowStatus OBJECT-TYPE
    SYNTAX          RowStatus
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "This is for SNMP use."
    ::= { sonusDsLpTransLblEntry 8 }

sonusDsLpDomainSelTblObjects OBJECT IDENTIFIER
    ::= { sonusLocalPolicyServicesMIBObjects 11 }

sonusDsLpDomainSelTbl OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusDsLpDoaminSelTblEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "Each entry of this table defines a mapping for a domain
             name to a trunk group based on signalling zone."
    ::= { sonusDsLpDomainSelTblObjects 1 }

sonusDsLpDoaminSelTblEntry OBJECT-TYPE
    SYNTAX          SonusDsLpDoaminSelTblEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "Definition of a domain selection table entry."
    INDEX           { sonusDsLpDomainSelTblIndex }
    ::= { sonusDsLpDomainSelTbl 1 }

SonusDsLpDoaminSelTblEntry ::=
    SEQUENCE {
        sonusDsLpDomainSelTblIndex      Unsigned32,
        sonusDsLpDomainName             DisplayString,
        sonusDsLpSigZoneId              Unsigned32,
        sonusDsLpTgName                 DisplayString
    }

sonusDsLpDomainSelTblIndex OBJECT-TYPE
    SYNTAX          Unsigned32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "A unique index for each entry."
    ::= { sonusDsLpDoaminSelTblEntry 1 }

sonusDsLpDomainName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(1..120))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The Domain name."
    ::= { sonusDsLpDoaminSelTblEntry 2 }

sonusDsLpSigZoneId OBJECT-TYPE
    SYNTAX          Unsigned32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The signalling zone id associated with that domain name. "
    ::= { sonusDsLpDoaminSelTblEntry 3 }

sonusDsLpTgName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(1..120))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The Trunk Group index to be selected for the domain name. "
    ::= { sonusDsLpDoaminSelTblEntry 4 }

END -- end of module SONUS-LOCAL-POLICY-SERVICES-MIB.
