    SYNTAX          SEQUENCE OF SonusMtrmPeerStatEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "Table of all the MTRM Peers based on the connection"
    ::= { sonusMtrmPeerObjects 1 }

sonusMtrmPeerStatEntry OBJECT-TYPE
    SYNTAX          SonusMtrmPeerStatEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "Table of Peer status entries. This table will hold
             all the client connections on the MTRM server and the
             server connection for the MTRM client"
    INDEX           { sonusMtrmPeerStatMtrmIndex, sonusMtrmPeerStatIndex }
    ::= { sonusMtrmPeerStatTable 1 }

SonusMtrmPeerStatEntry ::=
    SEQUENCE {
        sonusMtrmPeerStatMtrmIndex      Integer32,
        sonusMtrmPeerStatIndex          Integer32,
        sonusMtrmPeerStatPeerName       DisplayString,
        sonusMtrmPeerStatConnStatus     INTEGER,
        sonusMtrmPeerStatShelfId        Integer32,
        sonusMtrmPeerStatPeerRole       INTEGER,
        sonusMtrmPeerStatRTO            Integer32,
        sonusMtrmPeerStatIRRTBDepth     Integer32,
        sonusMtrmPeerStatFRRTBDepth     Integer32,
        sonusMtrmPeerStatIpAddress      IpAddress,
        sonusMtrmPeerStatMsgsSent       Integer32,
        sonusMtrmPeerStatMsgsRcvd       Integer32,
        sonusMtrmPeerStatShelfRegReqMsgsSent    Integer32,
        sonusMtrmPeerStatShelfRegReqMsgsRcvd    Integer32,
        sonusMtrmPeerStatShelfRegRespMsgsSent     Integer32,
        sonusMtrmPeerStatShelfRegRespMsgsRcvd     Integer32,
        sonusMtrmPeerStatHealthCheckReqMsgsSent       Integer32,
        sonusMtrmPeerStatHealthCheckReqMsgsRcvd       Integer32,
        sonusMtrmPeerStatHealthCheckRespMsgsSent        Integer32,
        sonusMtrmPeerStatHealthCheckRespMsgsRcvd        Integer32,
        sonusMtrmPeerStatMtrgRegReqMsgsSent   Integer32,
        sonusMtrmPeerStatMtrgRegReqMsgsRcvd   Integer32,
        sonusMtrmPeerStatMtrgRegRespMsgsSent    Integer32,
        sonusMtrmPeerStatMtrgRegRespMsgsRcvd    Integer32,
        sonusMtrmPeerStatIpAuditReqMsgsSent   Integer32,
        sonusMtrmPeerStatIpAuditReqMsgsRcvd   Integer32,
        sonusMtrmPeerStatIpAuditRespMsgsSent    Integer32,
        sonusMtrmPeerStatIpAuditRespMsgsRcvd    Integer32,
        sonusMtrmPeerStatBwAllocReqMsgsSent   Integer32,
        sonusMtrmPeerStatBwAllocReqMsgsRcvd   Integer32,
        sonusMtrmPeerStatBwAllocRespMsgsSent    Integer32,
        sonusMtrmPeerStatBwAllocRespMsgsRcvd    Integer32,
        sonusMtrmPeerStatCallAllocReqMsgsSent     Integer32,
        sonusMtrmPeerStatCallAllocReqMsgsRcvd     Integer32,
        sonusMtrmPeerStatCallAllocRespMsgsSent      Integer32,
        sonusMtrmPeerStatCallAllocRespMsgsRcvd      Integer32,
        sonusMtrmPeerStatBwFreeReqMsgsSent  Integer32,
        sonusMtrmPeerStatBwFreeReqMsgsRcvd  Integer32,
        sonusMtrmPeerStatCallFreeReqMsgsSent    Integer32,
        sonusMtrmPeerStatCallFreeReqMsgsRcvd    Integer32,
        sonusMtrmPeerStatTakeBackBwIndMsgsSent      Integer32,
        sonusMtrmPeerStatTakeBackBwIndMsgsRcvd      Integer32,
        sonusMtrmPeerStatTakeBackCallsIndMsgsSent         Integer32,
        sonusMtrmPeerStatTakeBackCallsIndMsgsRcvd         Integer32,
        sonusMtrmPeerStatMtrgDeregReqMsgsSent     Integer32,
        sonusMtrmPeerStatMtrgDeregReqMsgsRcvd     Integer32,
        sonusMtrmPeerStatMtrgUnavailableMsgsSent        Integer32,
        sonusMtrmPeerStatMtrgUnavailableMsgsRcvd        Integer32,
        sonusMtrmPeerStatIp6Address     Ipv6Address,
        sonusMtrmPeerStatIpVersion      INTEGER,
        sonusMtrmPeerStatConnType       INTEGER
    }

sonusMtrmPeerStatMtrmIndex OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Index of the local MTRM object to which this
             connection is being used for communication."
    ::= { sonusMtrmPeerStatEntry 1 }

sonusMtrmPeerStatIndex OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Index of this MTRM peer connection."
    ::= { sonusMtrmPeerStatEntry 2 }

sonusMtrmPeerStatPeerName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..23))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The name of the peer MTRM. If during registration a
             name is not present then the IP address of the intial
             connection will be presnted here in string form."
    ::= { sonusMtrmPeerStatEntry 3 }

sonusMtrmPeerStatConnStatus OBJECT-TYPE
    SYNTAX          INTEGER { available(1), unavailable(2), pending(3) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The Status of the peer connection. If the connection
             is up and active this will be set to available. If the
             connection is down then it will be set to
             unavailable."
    ::= { sonusMtrmPeerStatEntry 4 }

sonusMtrmPeerStatShelfId OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "This is the shelf ID that was assiged to this
             connection by the server."
    ::= { sonusMtrmPeerStatEntry 5 }

sonusMtrmPeerStatPeerRole OBJECT-TYPE
    SYNTAX          INTEGER { client(1), server(2) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "This is the shelf ID that was assiged to this
             connection by the server."
    ::= { sonusMtrmPeerStatEntry 6 }

sonusMtrmPeerStatRTO OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The current Retransmission Time Out in seconds. The
             messages that are in the retransmission buffer will be
             sent at this interval."
    ::= { sonusMtrmPeerStatEntry 7 }

sonusMtrmPeerStatIRRTBDepth OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "This the infinite retry retransmission buffer. The
             messages in this buffer will be attempted to be sent
             to the peer as long as the connection is alive. If
             the connection to the peer is lost this buffer will be 
             flushed."
    ::= { sonusMtrmPeerStatEntry 8 }

sonusMtrmPeerStatFRRTBDepth OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "This the finite retry retransmission buffer. The
             messages in this buffer will be attempted to be sent
             to the server at most twice. Failure to send this
             message to the server will result in the message being
             discarded. If the connection to the peer is lost this 
             buffer will be flushed."
    ::= { sonusMtrmPeerStatEntry 9 }

sonusMtrmPeerStatIpAddress OBJECT-TYPE
    SYNTAX          IpAddress
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "IP Address of the MTRM Peer."
    ::= { sonusMtrmPeerStatEntry 10 }

