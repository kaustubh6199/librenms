sonusMtrmPortAvailNotification NOTIFICATION-TYPE
    OBJECTS         { sonusMtrmPortIndex, sonusMtrmServerAddr, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
    STATUS          obsolete
    DESCRIPTION     
            "This trap is generated from the client and indicates that a MTRM server Port has become available.
             This trap is replaced by trap sonusMtrmPortConnAvailNotification. "
    ::= { sonusMtrmMIBNotificationsPrefix 1 }

sonusMtrmPortUnAvailNotification NOTIFICATION-TYPE
    OBJECTS         { sonusMtrmPortIndex, sonusMtrmServerAddr, 
                      sonusMtrmConnCloseReasonCode, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
    STATUS          obsolete
    DESCRIPTION     
            "This trap indicates that a MTRM connection to the peer has become unavailable.
             This trap is replaced by trap sonusMtrmPortConnUnAvailNotification. "
    ::= { sonusMtrmMIBNotificationsPrefix 2 }

sonusMtrmPortRegisterAttemptNotification NOTIFICATION-TYPE
    OBJECTS         { sonusMtrmPortIndex, sonusMtrmServerAddr, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
    STATUS          obsolete
    DESCRIPTION     
            "This trap is being removed.
             This trap indicates that a MTRM client has attempted to contact the MTRM server."
    ::= { sonusMtrmMIBNotificationsPrefix 3 }

