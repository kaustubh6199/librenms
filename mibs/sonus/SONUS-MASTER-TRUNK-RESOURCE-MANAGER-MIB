sonusMtrmMtrgClientObjects OBJECT IDENTIFIER
    ::= { sonusMasterTrunkResourceManagerMIBObjects 4 }

sonusMtrmMtrgClientTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusMtrmMtrgClientEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "Table of MTRM communication MTRG client configurations."
    ::= { sonusMtrmMtrgClientObjects 1 }

sonusMtrmMtrgClientEntry OBJECT-TYPE
    SYNTAX          SonusMtrmMtrgClientEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "MTRM client  communication configuration."
    INDEX           { sonusMtrmMtrgClientIndex }
    ::= { sonusMtrmMtrgClientTable 1 }

SonusMtrmMtrgClientEntry ::=
    SEQUENCE {
        sonusMtrmMtrgClientIndex        Integer32,
        sonusMtrmMtrgClientName         SonusName,
        sonusMtrmMtrgClientType         INTEGER,
        sonusMtrmMtrgClientMaxCalls     Integer32,
        sonusMtrmMtrgClientMaxBW        Integer32,
        sonusMtrmMtrgClientHuntAlgorithm INTEGER,
        sonusMtrmMtrgClientMtrmIndex    Integer32,
        sonusMtrmMtrgClientMode         SonusServiceState,
        sonusMtrmMtrgClientState        SonusAdminState,
        sonusMtrmMtrgClientRowStatus    RowStatus
    }

sonusMtrmMtrgClientIndex OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Identifies the Client MTRG entry."
    ::= { sonusMtrmMtrgClientEntry 1 }

sonusMtrmMtrgClientName OBJECT-TYPE
    SYNTAX          SonusName
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "Name of the Client MTRG object that is to be associated with
             the trunk groups. This name will be used as the key to
             collect the trunk groups together."
    ::= { sonusMtrmMtrgClientEntry 2 }

sonusMtrmMtrgClientType OBJECT-TYPE
    SYNTAX          INTEGER { ip(1), pstn(2) }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The rest of the configuration parameters depend on
             this. Some parameters are not applicable to IP and
             others may not be applicable to PSTN."
    DEFVAL          { ip }
    ::= { sonusMtrmMtrgClientEntry 3 }

sonusMtrmMtrgClientMaxCalls OBJECT-TYPE
    SYNTAX          Integer32 (-1..2147483647)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The number of calls that can be made on this Master
             Trunk Group. This value is only of concern if this
             object refers to an MTRM server. In the client
             incarnamtion the local maximas prevail. This value is
             currently only used when the MTRG is of type IP.
             A negative value is unlimited."
    DEFVAL          { -1 }
    ::= { sonusMtrmMtrgClientEntry 4 }

sonusMtrmMtrgClientMaxBW OBJECT-TYPE
    SYNTAX          Integer32 (-1..2147483647)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The amount of bandwidth that can be allocated in
             total. This value is only of concern to an MTRM
             Server. This value is currently only used when the
             MTRG is of type IP."
    DEFVAL          { -1 }
    ::= { sonusMtrmMtrgClientEntry 5 }

sonusMtrmMtrgClientHuntAlgorithm OBJECT-TYPE
    SYNTAX          INTEGER { circuithi2lo(1), circuitlo2hi(2), 
                        mostnleastidle(3), circularbottom2top(4), 
                        circulartop2bottom(5) }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "This value determines the hunt algorithm used for
             this MTRG. This value is only of significance when set
             against trunk groups populated on the MTRM server."
    DEFVAL          { mostnleastidle }
    ::= { sonusMtrmMtrgClientEntry 6 }

sonusMtrmMtrgClientMtrmIndex OBJECT-TYPE
    SYNTAX          Integer32 (0..128)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "This index refers to the MTRM object this MTRG
             belongs to. The MTRM should be configured and enabled
             for this object to be set. A value of 0 means no index."
    ::= { sonusMtrmMtrgClientEntry 7 }

sonusMtrmMtrgClientMode OBJECT-TYPE
    SYNTAX          SonusServiceState
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "Mode of the MTRM Communication Port."
    DEFVAL          { inService }
    ::= { sonusMtrmMtrgClientEntry 8 }

sonusMtrmMtrgClientState OBJECT-TYPE
    SYNTAX          SonusAdminState
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "State of the MTRM Communication Port."
    DEFVAL          { disabled }
    ::= { sonusMtrmMtrgClientEntry 9 }

sonusMtrmMtrgClientRowStatus OBJECT-TYPE
