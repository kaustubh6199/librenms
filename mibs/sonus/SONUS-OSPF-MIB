sonusOspfHostAdminStat OBJECT-TYPE
    SYNTAX          SonusOspfStatus
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "The OSPF  host route's  administrative  status.
             When enabled this host route will be advertised as
             an internal route.  When disabled, OSPF stops to
             advertise this route."
    DEFVAL          { enabled }
    ::= { sonusOspfHostEntry 6 }

sonusOspfHostType OBJECT-TYPE
    SYNTAX          INTEGER { mns-reachable(1), pns-reachable(2), 
                        normal(3) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "The type of host route.  MNS signaling and management
             addresses are reachable only from management interfaces.
             PNS signaling is reachable from any of the PNS interfaces
             on that card.  By default all host addresses are considered
             normal host routes.  Normal host routes are reachable from
             both MNS and PNS interfaces."
    DEFVAL          { normal }
    ::= { sonusOspfHostEntry 7 }

sonusOspfHostReachableSlot OBJECT-TYPE
    SYNTAX          Integer32 (0..16)
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Specifies what particular card to use to receive traffic
             on.  This host route is advertised only on those interfaces
             on that card. Card 0 indicates host route is recahble from
             all slots."
    DEFVAL          { 0 }
    ::= { sonusOspfHostEntry 8 }

sonusOspfHostReachableIntf OBJECT-TYPE
    SYNTAX          IpAddress
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Specifies what particular port on a given card the traffic
             meant for this host address will be received.  If it is
             MNS, traffic will be received on the corresponding port of
             the standby port also.  This value is useless unless
             ReachableSlot is > 0 "
    ::= { sonusOspfHostEntry 9 }

sonusOspfHostShelf OBJECT-TYPE
    SYNTAX          SonusShelfIndex
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "A 32-bit integer identifying the OSPF Shelf"
    ::= { sonusOspfHostEntry 10 }

