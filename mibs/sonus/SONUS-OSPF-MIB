        sonusOspfVirtNbrLsRetransQLen   Gauge32,
        sonusOspfVirtNbrHelloSuppressed TruthValue,
        sonusOspfVirtNbrShelf           SonusShelfIndex,
        sonusOspfVirtNbrSlot            SonusSlotIndex,
        sonusOspfVirtNbrInstance        Integer32
    }

