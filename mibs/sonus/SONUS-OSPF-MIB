sonusOspfNbrTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusOspfNbrEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "A table of non-virtual neighbor information."
    REFERENCE       
            "OSPF Version 2, Section 10  The  Neighbor  Data
             Structure"
    ::= { sonusOspf 10 }

sonusOspfNbrEntry OBJECT-TYPE
    SYNTAX          SonusOspfNbrEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "The information regarding a single neighbor."
    REFERENCE       
            "OSPF Version 2, Section 10  The  Neighbor  Data
             Structure"
    INDEX           { sonusOspfNbrShelf, sonusOspfNbrSlot, 
                      sonusOspfNbrInstance, sonusOspfNbrIpVersion, 
                      sonusOspfNbrIpAddr, sonusOspfNbrIp6Addr, 
                      sonusOspfNbrAddressLessIndex }
    ::= { sonusOspfNbrTable 1 }

SonusOspfNbrEntry ::=
    SEQUENCE {
        sonusOspfNbrIpAddr              IpAddress,
        sonusOspfNbrAddressLessIndex    SonusOspfInterfaceIndex,
        sonusOspfNbrRtrId               SonusOspfRouterID,
        sonusOspfNbrOptions             Integer32,
        sonusOspfNbrPriority            SonusOspfDesignatedRouterPriority,
        sonusOspfNbrState               INTEGER,
        sonusOspfNbrEvents              Counter32,
        sonusOspfNbrLsRetransQLen       Gauge32,
        sonusOspfNbmaNbrStatus          RowStatus,
        sonusOspfNbmaNbrPermanence      INTEGER,
        sonusOspfNbrHelloSuppressed     TruthValue,
        sonusOspfNbrShelf               SonusShelfIndex,
        sonusOspfNbrSlot                SonusSlotIndex,
        sonusOspfNbrInstance            Integer32,
        sonusOspfNbrIp6Addr             Ipv6Address,
        sonusOspfNbrIpVersion           INTEGER
    }

sonusOspfNbrIpAddr OBJECT-TYPE
    SYNTAX          IpAddress
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The IP address this neighbor is using  in  its
             IP  Source  Address.  Note that, on addressless
             links, this will not be 0.0.0.0,  but  the  ad-
             dress of another of the neighbor's interfaces."
    ::= { sonusOspfNbrEntry 1 }

sonusOspfNbrAddressLessIndex OBJECT-TYPE
    SYNTAX          SonusOspfInterfaceIndex
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "On an interface having an  IP  Address,  zero.
             On  addressless  interfaces,  the corresponding
             value of ifIndex in the Internet Standard  MIB.
             On  row  creation, this can be derived from the
             instance."
    ::= { sonusOspfNbrEntry 2 }

sonusOspfNbrRtrId OBJECT-TYPE
    SYNTAX          SonusOspfRouterID
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "A 32-bit integer (represented as a type  IpAd-
             dress)  uniquely  identifying  the  neighboring
             router in the Autonomous System."
    DEFVAL          { '00000000'H }
    ::= { sonusOspfNbrEntry 3 }

sonusOspfNbrOptions OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "A Bit Mask corresponding to the neighbor's op-
             tions field.
             
             Bit 0, if set, indicates that the  system  will
             operate  on  Type of Service metrics other than
             TOS 0.  If zero, the neighbor will  ignore  all
             metrics except the TOS 0 metric.
             
             Bit 1, if set, indicates  that  the  associated
             area  accepts and operates on external informa-
             tion; if zero, it is a stub area.
             
             Bit 2, if set, indicates that the system is ca-
             pable  of routing IP Multicast datagrams; i.e.,
             that it implements the Multicast Extensions  to
             OSPF.
             
             Bit 3, if set, indicates  that  the  associated
             area  is  an  NSSA.  These areas are capable of
             carrying type 7 external advertisements,  which
             are  translated into type 5 external advertise-
             ments at NSSA borders."
    REFERENCE       
            "OSPF Version 2, Section 12.1.2 Options"
    DEFVAL          { 0 }
    ::= { sonusOspfNbrEntry 4 }

sonusOspfNbrPriority OBJECT-TYPE
    SYNTAX          SonusOspfDesignatedRouterPriority
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "The priority of this neighbor in the designat-
             ed router election algorithm.  The value 0 sig-
             nifies that the neighbor is not eligible to be-
             come  the  designated router on this particular
             network."
    DEFVAL          { 1 }
    ::= { sonusOspfNbrEntry 5 }

sonusOspfNbrState OBJECT-TYPE
    SYNTAX          INTEGER { down(1), attempt(2), init(3), twoWay(4), 
                        exchangeStart(5), exchange(6), loading(7), 
                        full(8) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The State of the relationship with this Neigh-
             bor."
    REFERENCE       
            "OSPF Version 2, Section 10.1 Neighbor States"
    DEFVAL          { down }
    ::= { sonusOspfNbrEntry 6 }

sonusOspfNbrEvents OBJECT-TYPE
    SYNTAX          Counter32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The number of times this neighbor relationship
             has changed state, or an error has occurred."
    ::= { sonusOspfNbrEntry 7 }

