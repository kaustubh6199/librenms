sonusOspfExtLsdbTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusOspfExtLsdbEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "The OSPF Process's Links State Database."
    REFERENCE       
            "OSPF Version 2, Section 12  Link  State  Adver-
             tisements"
    ::= { sonusOspf 12 }

sonusOspfExtLsdbEntry OBJECT-TYPE
    SYNTAX          SonusOspfExtLsdbEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "A single Link State Advertisement."
    INDEX           { sonusOspfExtLsdbShelf, sonusOspfExtLsdbSlot, 
                      sonusOspfExtLsdbInstance, sonusOspfExtLsdbType, 
                      sonusOspfExtLsdbLsid, sonusOspfExtLsdbRouterId }
    ::= { sonusOspfExtLsdbTable 1 }

SonusOspfExtLsdbEntry ::=
    SEQUENCE {
        sonusOspfExtLsdbType            INTEGER,
        sonusOspfExtLsdbLsid            IpAddress,
        sonusOspfExtLsdbRouterId        SonusOspfRouterID,
        sonusOspfExtLsdbSequence        Integer32,
        sonusOspfExtLsdbAge             Integer32,
        sonusOspfExtLsdbChecksum        Integer32,
        sonusOspfExtLsdbAdvertisement   OCTET STRING,
        sonusOspfExtLsdbShelf           SonusShelfIndex,
        sonusOspfExtLsdbSlot            SonusSlotIndex,
        sonusOspfExtLsdbInstance        Integer32
    }

sonusOspfExtLsdbType OBJECT-TYPE
    SYNTAX          INTEGER { asExternalLink(5) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The type  of  the  link  state  advertisement.
             Each  link state type has a separate advertise-
             ment format."
    REFERENCE       
            "OSPF Version 2, Appendix A.4.1 The  Link  State
             Advertisement header"
    ::= { sonusOspfExtLsdbEntry 1 }

sonusOspfExtLsdbLsid OBJECT-TYPE
    SYNTAX          IpAddress
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The Link State ID is an LS Type Specific field
             containing either a Router ID or an IP Address;
             it identifies the piece of the  routing  domain
             that is being described by the advertisement."
    REFERENCE       
            "OSPF Version 2, Section 12.1.4 Link State ID"
    ::= { sonusOspfExtLsdbEntry 2 }

sonusOspfExtLsdbRouterId OBJECT-TYPE
    SYNTAX          SonusOspfRouterID
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The 32 bit number that uniquely identifies the
             originating router in the Autonomous System."
    REFERENCE       
            "OSPF Version 2, Appendix C.1 Global parameters"
    ::= { sonusOspfExtLsdbEntry 3 }

sonusOspfExtLsdbSequence OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The sequence number field is a  signed  32-bit
             integer.   It  is used to detect old and dupli-
             cate link state advertisements.  The  space  of
             sequence  numbers  is  linearly  ordered.   The
             larger the sequence number the more recent  the
             advertisement."
    REFERENCE       
            "OSPF Version  2,  Section  12.1.6  LS  sequence
             number"
    ::= { sonusOspfExtLsdbEntry 4 }

sonusOspfExtLsdbAge OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "This field is the age of the link state adver-
             tisement in seconds."
    REFERENCE       
            "OSPF Version 2, Section 12.1.1 LS age"
    ::= { sonusOspfExtLsdbEntry 5 }

sonusOspfExtLsdbChecksum OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "This field is the  checksum  of  the  complete
             contents  of  the  advertisement, excepting the
             age field.  The age field is excepted  so  that
             an   advertisement's  age  can  be  incremented
             without updating the  checksum.   The  checksum
             used  is  the same that is used for ISO connec-
             tionless datagrams; it is commonly referred  to
             as the Fletcher checksum."
    REFERENCE       
            "OSPF Version 2, Section 12.1.7 LS checksum"
    ::= { sonusOspfExtLsdbEntry 6 }

sonusOspfExtLsdbAdvertisement OBJECT-TYPE
    SYNTAX          OCTET STRING (SIZE(36))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The entire Link State Advertisement, including
             its header."
    REFERENCE       
            "OSPF Version 2, Section 12  Link  State  Adver-
             tisements"
    ::= { sonusOspfExtLsdbEntry 7 }

sonusOspfExtLsdbShelf OBJECT-TYPE
    SYNTAX          SonusShelfIndex
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "A 32-bit integer identifying the OSPF Shelf"
    ::= { sonusOspfExtLsdbEntry 8 }

sonusOspfExtLsdbSlot OBJECT-TYPE
    SYNTAX          SonusSlotIndex
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "A 32-bit integer identifying the OSPF Slot"
    ::= { sonusOspfExtLsdbEntry 9 }

sonusOspfExtLsdbInstance OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "A 32-bit integer identifying the OSPF Instance"
    ::= { sonusOspfExtLsdbEntry 10 }

