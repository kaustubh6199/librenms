    ::= { sonusOspfStubAreaTable 1 }

SonusOspfStubAreaEntry ::=
    SEQUENCE {
        sonusOspfStubAreaId             SonusOspfAreaID,
        sonusOspfStubTOS                SonusOspfTOSType,
        sonusOspfStubMetric             SonusOspfBigMetric,
        sonusOspfStubStatus             RowStatus,
        sonusOspfStubMetricType         INTEGER,
        sonusOspfStubAreaShelf          SonusShelfIndex,
        sonusOspfStubAreaSlot           SonusSlotIndex,
        sonusOspfStubAreaInstance       Integer32
    }

