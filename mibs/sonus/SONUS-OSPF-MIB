sonusOspfVirtIfEntry OBJECT-TYPE
    SYNTAX          SonusOspfVirtIfEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "Information about a single Virtual Interface."
    INDEX           { sonusOspfVirtIfShelf, sonusOspfVirtIfSlot, 
                      sonusOspfVirtIfInstance, sonusOspfVirtIfAreaId, 
                      sonusOspfVirtIfNeighbor }
    ::= { sonusOspfVirtIfTable 1 }

SonusOspfVirtIfEntry ::=
    SEQUENCE {
        sonusOspfVirtIfAreaId           SonusOspfAreaID,
        sonusOspfVirtIfNeighbor         SonusOspfRouterID,
        sonusOspfVirtIfTransitDelay     SonusOspfUpToMaxAge,
        sonusOspfVirtIfRetransInterval  SonusOspfUpToMaxAge,
        sonusOspfVirtIfHelloInterval    SonusOspfHelloRange,
        sonusOspfVirtIfRtrDeadInterval  SonusOspfPositiveInteger,
        sonusOspfVirtIfState            INTEGER,
        sonusOspfVirtIfEvents           Counter32,
        sonusOspfVirtIfAuthKey          OCTET STRING,
        sonusOspfVirtIfStatus           RowStatus,
        sonusOspfVirtIfAuthType         Integer32,
        sonusOspfVirtIfShelf            SonusShelfIndex,
        sonusOspfVirtIfSlot             SonusSlotIndex,
        sonusOspfVirtIfInstance         Integer32
    }

sonusOspfVirtIfAreaId OBJECT-TYPE
    SYNTAX          SonusOspfAreaID
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The  Transit  Area  that  the   Virtual   Link
             traverses.  By definition, this is not 0.0.0.0"
    ::= { sonusOspfVirtIfEntry 1 }

sonusOspfVirtIfNeighbor OBJECT-TYPE
    SYNTAX          SonusOspfRouterID
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The Router ID of the Virtual Neighbor."
    ::= { sonusOspfVirtIfEntry 2 }

sonusOspfVirtIfTransitDelay OBJECT-TYPE
    SYNTAX          SonusOspfUpToMaxAge
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "The estimated number of seconds  it  takes  to
             transmit  a link- state update packet over this
             interface."
    DEFVAL          { 1 }
    ::= { sonusOspfVirtIfEntry 3 }

sonusOspfVirtIfRetransInterval OBJECT-TYPE
    SYNTAX          SonusOspfUpToMaxAge
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "The number of seconds between  link-state  ad-
             vertisement  retransmissions,  for  adjacencies
             belonging to this  interface.   This  value  is
             also used when retransmitting database descrip-
             tion  and  link-state  request  packets.   This
             value  should  be well over the expected round-
             trip time."
    DEFVAL          { 5 }
    ::= { sonusOspfVirtIfEntry 4 }

sonusOspfVirtIfHelloInterval OBJECT-TYPE
    SYNTAX          SonusOspfHelloRange
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "The length of time, in  seconds,  between  the
             Hello  packets that the router sends on the in-
             terface.  This value must be the same  for  the
             virtual neighbor."
    DEFVAL          { 10 }
    ::= { sonusOspfVirtIfEntry 5 }

sonusOspfVirtIfRtrDeadInterval OBJECT-TYPE
    SYNTAX          SonusOspfPositiveInteger
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "The number of seconds that  a  router's  Hello
             packets  have  not been seen before it's neigh-
             bors declare the router down.  This  should  be
             some  multiple  of  the  Hello  interval.  This
             value must be the same for the  virtual  neigh-
             bor."
    DEFVAL          { 60 }
    ::= { sonusOspfVirtIfEntry 6 }

sonusOspfVirtIfState OBJECT-TYPE
    SYNTAX          INTEGER { down(1), pointToPoint(4) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "OSPF virtual interface states."
    DEFVAL          { down }
    ::= { sonusOspfVirtIfEntry 7 }

