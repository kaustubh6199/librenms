        sonusSonetVTTca24hUAS           TruthValue,
        sonusSonetVTTca24hCV            TruthValue,
        sonusSonetVTTcaFarEnd15mES      TruthValue,
        sonusSonetVTTcaFarEnd15mSES     TruthValue,
        sonusSonetVTTcaFarEnd15mUAS     TruthValue,
        sonusSonetVTTcaFarEnd15mCV      TruthValue,
        sonusSonetVTTcaFarEnd24hES      TruthValue,
        sonusSonetVTTcaFarEnd24hSES     TruthValue,
        sonusSonetVTTcaFarEnd24hUAS     TruthValue,
        sonusSonetVTTcaFarEnd24hCV      TruthValue
    }

sonusSonetVTTcaIfIndex OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "The ifIndex assigned to this port when it was created.
             This will not change until the port is deleted."
    ::= { sonusSonetVTTcaEntry 1 }

