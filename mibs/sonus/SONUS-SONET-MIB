                        sectionFarendErroredSec24hr(34), 
                        sectionFarendSeverelyErroredSec24hr(35), 
                        sectionFarendSeverelyErroredFrameSec24hr(36), 
                        lineFarendCodingViolation24hr(37), 
                        lineFarendErroredSec24hr(38), 
