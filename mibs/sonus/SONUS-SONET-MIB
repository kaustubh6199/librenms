    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Count of Remote Error Indications that have occured."
    ::= { sonusSonetVTStatisticsEntry 9 }

sonusSonetVTLayer OBJECT IDENTIFIER
    ::= { sonusSonetObjectsVT 3 }

sonusSonetVTLayerTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusSonetVTLayerEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "The SONET VT layer table.  This table maps SONET VT ifIndex
             to a shelf, slot, port, and vt."
    ::= { sonusSonetVTLayer 1 }

sonusSonetVTLayerEntry OBJECT-TYPE
    SYNTAX          SonusSonetVTLayerEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "An entry in the SONET VT layer table. This entry 
             maps the ifIndex of the SONET VT to a shelf, slot,
             port, and vt.  A row is added to this table when a SONET
             VT is created."
    INDEX           { sonusSonetVTLayerShelfIndex, 
                      sonusSonetVTLayerSlotIndex, 
                      sonusSonetVTLayerPortIndex, 
                      sonusSonetVTLayerVTIndex }
    ::= { sonusSonetVTLayerTable 1 }

SonusSonetVTLayerEntry ::=
    SEQUENCE {
        sonusSonetVTLayerShelfIndex     SonusShelfIndex,
        sonusSonetVTLayerSlotIndex      SonusSlotIndex,
        sonusSonetVTLayerPortIndex      Integer32,
        sonusSonetVTLayerVTIndex        Integer32,
        sonusSonetVTLayerIfIndex        Integer32,
        sonusSonetVTLayerRowStatus      RowStatus
    }

sonusSonetVTLayerShelfIndex OBJECT-TYPE
    SYNTAX          SonusShelfIndex
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "Shelf index for this table."
    ::= { sonusSonetVTLayerEntry 1 }

sonusSonetVTLayerSlotIndex OBJECT-TYPE
    SYNTAX          SonusSlotIndex
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "Slot index for this table."
    ::= { sonusSonetVTLayerEntry 2 }

sonusSonetVTLayerPortIndex OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "Port index for this table."
    ::= { sonusSonetVTLayerEntry 3 }

sonusSonetVTLayerVTIndex OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "VT index for this table."
    ::= { sonusSonetVTLayerEntry 4 }

sonusSonetVTLayerIfIndex OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The ifIndex assigned to this shelf, slot, port, vt when the 
             SONET VT was created."
    ::= { sonusSonetVTLayerEntry 5 }

sonusSonetVTLayerRowStatus OBJECT-TYPE
    SYNTAX          RowStatus
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "This object is used to create, delete,
             or modify a row in this table."
    ::= { sonusSonetVTLayerEntry 6 }

sonusSonetPortAdminStatusNotification NOTIFICATION-TYPE
    OBJECTS         { ifIndex, sonusShelfIndex, sonusSlotIndex, 
                      sonusPortIndex, sonusSonetAdminStatus, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
    STATUS          current
    DESCRIPTION     
            "This trap indicates the Sonet port's administrative status
             has changed due to an operator-initiated command. Possible
             admin status is EnabledInService, EnabledOutOfService,
             Disabled, and DryingUp."
    ::= { sonusSonetMIBNotificationsPrefix 1 }

sonusSonetPortOperStatusNotification NOTIFICATION-TYPE
    OBJECTS         { ifIndex, sonusShelfIndex, sonusSlotIndex, 
                      sonusPortIndex, sonusSonetOperStatus, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel }
    STATUS          obsolete
    DESCRIPTION     
            "NO LONGER SUPPORTED. This trap indicates the Sonet
             port's operational status
             has changed."
    ::= { sonusSonetMIBNotificationsPrefix 2 }

sonusSonetLineAlarmNotification NOTIFICATION-TYPE
    OBJECTS         { ifIndex, sonusShelfIndex, sonusSlotIndex, 
                      sonusPortIndex, sonusSonetAlarmStatus, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime, 
                      sonusPimIndex }
    STATUS          current
    DESCRIPTION     
            "This trap indicates the line Section/Line-layer alarm
             state of the Sonet port has changed."
    ::= { sonusSonetMIBNotificationsPrefix 3 }

sonusSonetLineThresholdCrossNotification NOTIFICATION-TYPE
    OBJECTS         { ifIndex, sonusShelfIndex, sonusSlotIndex, 
                      sonusPortIndex, sonusSonetThresholdType, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime, 
                      sonusPimIndex }
    STATUS          current
    DESCRIPTION     
            "This trap indicates that a Section/Line-layer performance
             monitoring threshold has been crossed."
    ::= { sonusSonetMIBNotificationsPrefix 4 }

sonusSonetPathAdminStatusNotification NOTIFICATION-TYPE
    OBJECTS         { ifIndex, sonusShelfIndex, sonusSlotIndex, 
                      sonusPortIndex, sonusPathIndex, 
                      sonusSonetAdminStatus, sonusEventDescription, 
                      sonusEventClass, sonusEventLevel, sonusSequenceId, 
                      sonusEventTime }
    STATUS          current
    DESCRIPTION     
            "This trap indicates the Sonet path's administrative status
             has changed due to an operator-initiated command. Possible
             admin status is Equipped or Unequipped."
    ::= { sonusSonetMIBNotificationsPrefix 5 }

sonusSonetPathOperStatusNotification NOTIFICATION-TYPE
    OBJECTS         { ifIndex, sonusShelfIndex, sonusSlotIndex, 
                      sonusPortIndex, sonusPathIndex, 
                      sonusSonetOperStatus, sonusEventDescription, 
                      sonusEventClass, sonusEventLevel }
    STATUS          obsolete
    DESCRIPTION     
            "NO LONGER SUPPORTED. This trap indicates the Sonet
             path's operational status has changed."
    ::= { sonusSonetMIBNotificationsPrefix 6 }

sonusSonetPathAlarmNotification NOTIFICATION-TYPE
    OBJECTS         { ifIndex, sonusShelfIndex, sonusSlotIndex, 
                      sonusPortIndex, sonusPathIndex, 
                      sonusSonetAlarmStatus, sonusEventDescription, 
                      sonusEventClass, sonusEventLevel, sonusSequenceId, 
                      sonusEventTime }
    STATUS          current
    DESCRIPTION     
            "This trap indicates the line Path-layer alarm state
             of the Sonet path has changed."
    ::= { sonusSonetMIBNotificationsPrefix 7 }

sonusSonetPathThresholdCrossNotification NOTIFICATION-TYPE
    OBJECTS         { ifIndex, sonusShelfIndex, sonusSlotIndex, 
                      sonusPortIndex, sonusPathIndex, 
                      sonusSonetThresholdType, sonusEventDescription, 
                      sonusEventClass, sonusEventLevel, sonusSequenceId, 
                      sonusEventTime }
    STATUS          current
    DESCRIPTION     
            "This trap indicates that a Path-layer performance
             monitoring threshold has been crossed."
    ::= { sonusSonetMIBNotificationsPrefix 8 }

sonusSonetPortOutOfServiceNotification NOTIFICATION-TYPE
    OBJECTS         { ifIndex, sonusShelfIndex, sonusSlotIndex, 
                      sonusPortIndex, sonusSonetOutOfServiceReason, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
    STATUS          current
    DESCRIPTION     
            "This trap indicates the Sonet port went out of service.
             Reason indicates why the port went out of service. Possible
             reasons include admin or hardware failure."
    ::= { sonusSonetMIBNotificationsPrefix 9 }

sonusSonetPathOutOfServiceNotification NOTIFICATION-TYPE
    OBJECTS         { ifIndex, sonusShelfIndex, sonusSlotIndex, 
                      sonusPortIndex, sonusPathIndex, 
                      sonusSonetOutOfServiceReason, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
    STATUS          current
    DESCRIPTION     
            "This trap indicates the Sonet path went out of service.
             Reason indicates why the port went out of service. Possible
             reasons include admin or hardware failure."
    ::= { sonusSonetMIBNotificationsPrefix 10 }

sonusSonetVTAlarmNotification NOTIFICATION-TYPE
    OBJECTS         { ifIndex, sonusShelfIndex, sonusSlotIndex, 
                      sonusPortIndex, sonusVTIndex, 
                      sonusSonetAlarmStatus, sonusEventDescription, 
