sonusSonetVTStatisticsTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusSonetVTStatisticsEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "The Sonus SONET VT Statistics table."
    ::= { sonusSonetVT 11 }

sonusSonetVTStatisticsEntry OBJECT-TYPE
    SYNTAX          SonusSonetVTStatisticsEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "An entry in the Sonus SONET VT Statistics table."
    INDEX           { sonusSonetVTStatisticsIfIndex }
    ::= { sonusSonetVTStatisticsTable 1 }

SonusSonetVTStatisticsEntry ::=
    SEQUENCE {
        sonusSonetVTStatisticsIfIndex   Integer32,
        sonusSonetVTStatisticsDefectAIS Integer32,
        sonusSonetVTStatisticsDefectLOP Integer32,
        sonusSonetVTStatisticsDefectRDI Integer32,
        sonusSonetVTStatisticsAlarmAIS  Integer32,
        sonusSonetVTStatisticsAlarmLOP  Integer32,
        sonusSonetVTStatisticsAlarmRFI  Integer32,
        sonusSonetVTStatisticsCodeViolation   Integer32,
        sonusSonetVTStatisticsREI       Integer32
    }

sonusSonetVTStatisticsIfIndex OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "The ifIndex assigned to this port when it was created.
             This will not change until the port is deleted."
    ::= { sonusSonetVTStatisticsEntry 1 }

sonusSonetVTStatisticsDefectAIS OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Count of Alarm Indication Signal defects that have occured."
    ::= { sonusSonetVTStatisticsEntry 2 }

sonusSonetVTStatisticsDefectLOP OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Count of Loss of Pointer defects that have occured."
    ::= { sonusSonetVTStatisticsEntry 3 }

sonusSonetVTStatisticsDefectRDI OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Count of Remote Defect Indication defects that have occured."
    ::= { sonusSonetVTStatisticsEntry 4 }

sonusSonetVTStatisticsAlarmAIS OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Count of Alarm Indication Signal alarms that have occured."
    ::= { sonusSonetVTStatisticsEntry 5 }

sonusSonetVTStatisticsAlarmLOP OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Count of Loss of Pointer alarms that have occured."
    ::= { sonusSonetVTStatisticsEntry 6 }

sonusSonetVTStatisticsAlarmRFI OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Count of Remote Failure Indication alarms that have occured."
    ::= { sonusSonetVTStatisticsEntry 7 }

