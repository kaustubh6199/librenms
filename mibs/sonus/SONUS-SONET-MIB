sonusSonetPathThresholdTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusSonetPathThresholdEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "The Sonus SONET Path Threshold Table. This table adds objects to
             set the Performance Monitoring thresholds for generating a
             Threshold Crossing Alert."
    ::= { sonusSonetPath 1 }

sonusSonetPathThresholdEntry OBJECT-TYPE
    SYNTAX          SonusSonetPathThresholdEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "An entry in the Sonus SONET Path Threshold table."
    INDEX           { sonusSonetPathThresholdIfIndex }
    ::= { sonusSonetPathThresholdTable 1 }

SonusSonetPathThresholdEntry ::=
    SEQUENCE {
        sonusSonetPathThresholdIfIndex  Integer32,
        sonusSonetPathThresholdPathIndex Integer32,
        sonusSonetPathThreshold15mCVs   Integer32,
        sonusSonetPathThreshold15mESs   Integer32,
        sonusSonetPathThreshold15mSESs  Integer32,
        sonusSonetPathThreshold15mUASs  Integer32,
        sonusSonetPathThreshold24hCVs   Integer32,
        sonusSonetPathThreshold24hESs   Integer32,
        sonusSonetPathThreshold24hSESs  Integer32,
        sonusSonetPathThreshold24hUASs  Integer32,
        sonusSonetPathThresholdCVTcaFilter  SonusAdminState,
        sonusSonetPathThresholdESTcaFilter  SonusAdminState,
        sonusSonetPathThresholdSESTcaFilter   SonusAdminState,
        sonusSonetPathThresholdUASTcaFilter   SonusAdminState
    }

