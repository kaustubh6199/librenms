sonusSonetPathCurrentTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusSonetPathCurrentEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "The Sonus SONET Path Current table."
    ::= { sonusSonetPath 2 }

sonusSonetPathCurrentEntry OBJECT-TYPE
    SYNTAX          SonusSonetPathCurrentEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "An entry in the Sonus SONET Path Current table."
    INDEX           { sonusSonetPathCurrentIfIndex }
    ::= { sonusSonetPathCurrentTable 1 }

SonusSonetPathCurrentEntry ::=
    SEQUENCE {
        sonusSonetPathCurrentIfIndex    Integer32,
        sonusSonetPathCurrentPathIndex  Integer32,
        sonusSonetPathCurrentWidth      INTEGER,
        sonusSonetPathCurrentStatus     Integer32,
        sonusSonetPathCurrentESs        PerfCurrentCount,
        sonusSonetPathCurrentSESs       PerfCurrentCount,
        sonusSonetPathCurrentUASs       PerfCurrentCount,
        sonusSonetPathCurrentCVs        PerfCurrentCount,
        sonusSonetPathCurrentFCs        PerfCurrentCount,
        sonusSonetPathCurrentTraceStatus INTEGER,
        sonusSonetPathCurrentElapsedTime Integer32,
        sonusSonetPathCurrentDayElapsedTime   Integer32,
        sonusSonetPathCurrentERDICode   INTEGER,
        sonusSonetPathCurrentSignalLabel INTEGER,
        sonusSonetPathCurrentPatternMonitor   INTEGER,
        sonusSonetPathCurrentPatternBER INTEGER,
        sonusSonetPathCurrentPatternErrorCnt    Integer32
    }

sonusSonetPathCurrentIfIndex OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "The ifIndex assigned to this path when it was created.
             This will not change until the path is deleted."
    ::= { sonusSonetPathCurrentEntry 1 }

sonusSonetPathCurrentPathIndex OBJECT-TYPE
    SYNTAX          Integer32 (1..3)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "STS1 path index for this table."
    ::= { sonusSonetPathCurrentEntry 2 }

sonusSonetPathCurrentWidth OBJECT-TYPE
    SYNTAX          INTEGER { sts1(1), sts3cSTM1(2), sts12cSTM4(3), 
                        sts24c(4), sts48cSTM16(5) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "A value that indicates the type of the SONET/SDH Path.
             For SONET, the assigned types are the STS-Nc SPEs,
             where N = 1, 3, 12, 24, and 48. STS-1 is equal to 51.84
             Mbps. For SDH, the assigned types are the STM-Nc VCs,
             where N = 1, 4, and 16."
    ::= { sonusSonetPathCurrentEntry 3 }

sonusSonetPathCurrentStatus OBJECT-TYPE
    SYNTAX          Integer32 (1..128)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "This variable indicates the status of the interface. It is
             a bit map represented as a sum, therefore, it can represent
             multiple defects simultaneously. The sonetPathNoDefect
             should be set if and only if no other flag is set.
             
             The various bit positions are:
             1   sonetPathNoDefect
             2   sonetPathSTSLOP
             4   sonetPathSTSAIS
             8   sonetPathSTSRDI
             16  sonetPathUnequiped
             32  sonetPathSignalLabelMismatch
             64  sonetPathTraceMismatch
             128 sonetPathNotMonitored"
    ::= { sonusSonetPathCurrentEntry 4 }

sonusSonetPathCurrentESs OBJECT-TYPE
    SYNTAX          PerfCurrentCount
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The counter associated with the number of Errored Seconds
             encountered by a SONET/SDH Path in the current 24 hour
             interval."
    ::= { sonusSonetPathCurrentEntry 5 }

sonusSonetPathCurrentSESs OBJECT-TYPE
    SYNTAX          PerfCurrentCount
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The counter associated with the number of Severely Errored
             Seconds encountered by a SONET/SDH Path in the current 
             24 hour interval."
    ::= { sonusSonetPathCurrentEntry 6 }

sonusSonetPathCurrentUASs OBJECT-TYPE
    SYNTAX          PerfCurrentCount
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The counter associated with the number of Unavailable
             Seconds encountered by a SONET/SDH Path in the current
             24 hour interval."
    ::= { sonusSonetPathCurrentEntry 7 }

sonusSonetPathCurrentCVs OBJECT-TYPE
