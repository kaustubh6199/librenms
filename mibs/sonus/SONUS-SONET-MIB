sonusSonetVTAdmnStatAction OBJECT-TYPE
    SYNTAX          INTEGER { init15MinuteNearEnd(1), 
                        init15MinuteFarEnd(2), init24HourNearEnd(4), 
                        init24HourFarEnd(8) }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "Initialize Performance Monitoring Statistics.  A PM initialization
             action initiates one request for the action.  Requests to initialize
             more than one group of statistics can be accomplished by OR-ing the 
             appropriate bits (e.g., to initialize all 15 minute PM statistics, 
             set bits 1 and 2). Reading this variable always returns zero."
    ::= { sonusSonetVTAdmnEntry 7 }

sonusSonetVTAdmnRDIMode OBJECT-TYPE
    SYNTAX          INTEGER { normal(1), enhanced(2) }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The desired type of SONET port VT Remote Defect Indication to
             use.
             		 (1) normal - Use one-bit RDI-V signalling using bit 5 of G1.
             		 (2) enhanced - Use three-bit RDI-V (ERDI-V) using bits 5-7 of G1."
    DEFVAL          { enhanced }
    ::= { sonusSonetVTAdmnEntry 8 }

sonusSonetVTAdmnTraceState OBJECT-TYPE
    SYNTAX          SonusAdminState
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The desired state of SONET VT Path Trace.
             (1) enabled - Enables the generation of SONET VT Path Trace Messages
             in the J1 bytes of the VT Path overhead, and enables the reception and
             checking of received VT Path Trace Messages with the Expected VT Path 
             Trace Message.
             (2) disabled - Transmits all zero's in the J1 bytes of the VT Path
             overhead, and disables the reception and checking of received VT
             Path Trace Messages with the Expected VT Path Trace Message."
    DEFVAL          { disabled }
    ::= { sonusSonetVTAdmnEntry 9 }

sonusSonetVTAdmnTraceExpMsg OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..15))
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The expected VT path trace string to be received on the SONET port in the
             J1 bytes of the VT Path overhead."
    ::= { sonusSonetVTAdmnEntry 10 }

sonusSonetVTAdmnTraceXmtMsg OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..15))
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The VT path trace string to be transmitted on the SONET port in the J1
             bytes of the VT Path overhead."
    ::= { sonusSonetVTAdmnEntry 11 }

sonusSonetVTAdmnTraceRcvMsg OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..15))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The VT path trace string received on the SONET port in the J1
             bytes of the VT Path overhead."
    ::= { sonusSonetVTAdmnEntry 12 }

sonusSonetVTAdmnOperStatus OBJECT-TYPE
    SYNTAX          INTEGER { up(1), down(2), dryingUp(3), remoteDown(4) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The actual status of the Sonet VT. VT is Up if its corresponding
             port is administratively in service and there are no VT layer 
             failures or hardware failures. VT is Down if the port is 
             administratively in service and there are VT layer failures or 
             hardware failures, or if the port is administratively out of service. 
             VT is DryingUp if the port is administratively out of service and is 
             drying up (no new calls, still carrying	in-progress calls)."
    ::= { sonusSonetVTAdmnEntry 13 }

sonusSonetVTAdmnEquippedState OBJECT-TYPE
    SYNTAX          INTEGER { equipped(1), unequipped(2) }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "This object is used to set the VT to the Equipped or
             Unequipped state. If the VT is Unequipped, the Unequipped
             signal label is sent in the VT overhead, and AIS-V is
             forced."
    DEFVAL          { equipped }
    ::= { sonusSonetVTAdmnEntry 14 }

sonusSonetVTAdmnSTS1Idx OBJECT-TYPE
    SYNTAX          Integer32 (1..3)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "STS-1/VC4 index for the VT."
    ::= { sonusSonetVTAdmnEntry 15 }

