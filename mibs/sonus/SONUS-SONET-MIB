sonusSonetLine OBJECT IDENTIFIER
    ::= { sonusSonetObjects 3 }

sonusSonetLineThresholdTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusSonetLineThresholdEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "The Sonus SONET Line Threshold Table. This table adds objects
             to set the Performance Monitoring thresholds for generating a
             Threshold Crossing Alert."
    ::= { sonusSonetLine 1 }

sonusSonetLineThresholdEntry OBJECT-TYPE
    SYNTAX          SonusSonetLineThresholdEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "An entry in the Sonus SONET Line Threshold table."
    INDEX           { sonusSonetLineThresholdIfIndex }
    ::= { sonusSonetLineThresholdTable 1 }

SonusSonetLineThresholdEntry ::=
    SEQUENCE {
        sonusSonetLineThresholdIfIndex  Integer32,
        sonusSonetLineThreshold15mCVs   Integer32,
        sonusSonetLineThreshold15mESs   Integer32,
        sonusSonetLineThreshold15mSESs  Integer32,
        sonusSonetLineThreshold15mUASs  Integer32,
        sonusSonetLineThreshold24hCVs   Integer32,
        sonusSonetLineThreshold24hESs   Integer32,
        sonusSonetLineThreshold24hSESs  Integer32,
        sonusSonetLineThreshold24hUASs  Integer32,
        sonusSonetLineThresholdCVTcaFilter  SonusAdminState,
        sonusSonetLineThresholdESTcaFilter  SonusAdminState,
        sonusSonetLineThresholdSESTcaFilter   SonusAdminState,
        sonusSonetLineThresholdUASTcaFilter   SonusAdminState
    }

sonusSonetLineThresholdIfIndex OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "The ifIndex assigned to this port when it was created.
             This will not change until the port is deleted."
    ::= { sonusSonetLineThresholdEntry 1 }

sonusSonetLineThreshold15mCVs OBJECT-TYPE
    SYNTAX          Integer32 (0..16383)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "Threshold for 15 minute interval Line layer Coding Violations."
    ::= { sonusSonetLineThresholdEntry 2 }

sonusSonetLineThreshold15mESs OBJECT-TYPE
    SYNTAX          Integer32 (0..900)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "Threshold for 15 minute interval Line layer Errored Seconds."
    ::= { sonusSonetLineThresholdEntry 3 }

sonusSonetLineThreshold15mSESs OBJECT-TYPE
    SYNTAX          Integer32 (0..900)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "Threshold for 15 minute interval Line layer Severely Errored
             Seconds."
