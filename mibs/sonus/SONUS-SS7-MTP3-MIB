    SYNTAX          Integer32 (0..255)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The successive restoration attempts the link has performed
             since the link became unavailble."
    DEFVAL          { 0 }
    ::= { sonusSs7LinkStatusEntry 14 }

sonusSs7LinkSuccessiveSltTests OBJECT-TYPE
    SYNTAX          Integer32 (0..255)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The successive SLTM/SLTA tests the link has performed in
             either bringing the link to the available state, or 
             executing the periodic SLTs."
    DEFVAL          { 0 }
    ::= { sonusSs7LinkStatusEntry 15 }

