    SYNTAX          INTEGER { activated(0), deactivated(1) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The current activation/deactivation status of the link.
             Note that a link can be activated/deactivated by the link
             or its belongling linkset."
    ::= { sonusSs7LinkStatusEntry 10 }

sonusSs7LinkPeriodicSltStatus OBJECT-TYPE
    SYNTAX          INTEGER { on(0), off(1) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The current periodic SLT status of the link."
    ::= { sonusSs7LinkStatusEntry 11 }

sonusSs7LinkCongestionLevel OBJECT-TYPE
    SYNTAX          Integer32 (0..3)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The current congestion level of the link."
    DEFVAL          { 0 }
    ::= { sonusSs7LinkStatusEntry 12 }

