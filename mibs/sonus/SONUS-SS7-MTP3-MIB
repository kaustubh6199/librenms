    SYNTAX          BITS { reserved0(0), unavl(1), cong(2), lsAvl(3), 
                        tfa(4), tfp(5), tfr(6), avl(7) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The current state of the SS7 route:
             unavl(1),				Route unavailable,
             cong(2),				Route congested,
             lsAvl(3),				Associated linkset available,
             tfa(4),					Route allowed,
             tfp(5),					Route prohibited,
             tfr(6),					Route restricted,
             avl(7),					Route available"
    ::= { sonusSs7RouteStatusEntry 2 }

