    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The index of the SS7 link."
    ::= { sonusSs7LinkStatusEntry 1 }

sonusSs7LinkState OBJECT-TYPE
    SYNTAX          BITS { unavl(0), reserved1(1), reserved2(2), 
                        reserved3(3), reserved4(4), reserved5(5), 
                        reserved6(6), reserved7(7), reserved8(8), 
                        reserved9(9), forceUninhInProg(10), 
                        coTimeCtrlPending(11), coEmrgPending(12), 
                        coNormPending(13), coPending(14), 
                        coTimeCtrlInProg(15), coEmrgInProg(16), 
                        coNormInProg(17), coInProg(18), 
                        cbTimeCtrlPending(19), cbSeqCtrlPending(20), 
                        cbPending(21), cbTimeCtrlInProg(22), 
                        cbSeqCtrlInProg(23), cbInProg(24), 
                        localUninhInProg(25), localInhInProg(26), 
                        linh(27), rinh(28), lblk(29), rblk(30), 
                        l2oos(31) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The current state of the SS7 link:
             unavl(0),				Link unavailable
             forceUninhInProg(10),	Forced uninhibit in progress,
             coTimeCtrlPending(11),	Changeover in time control mode pending,
             coEmrgPending(12),		Changeover in emergency mode pending,
             coNormPending(13),		Changeover in normal mode pending,
             coPending(14),			Changeover pending,
             coTimeCtrlInProg(15),	Changeover in time control mode in progress,
             coEmgyInProg(16),		Changeover in emergency mode in progress,
             coNormInProg(17),		Changeover in normal mode in progress, 
             coInProg(18),			Changeover in progress,
             cbTimeCtrlPending(19),	Changeback in time control mode pending,
             cbSeqCtrlPending(20),	Changeback in sequence control mode pending,
             cbPending(21),			Changeback pending,
             cbTimeCtrlInProg(22),	Changeback in time control mode in progress,
             cbSeqCtrlInProg(23),	Changeback in sequence control mode in progress,
             cbInProg(24),			Changeback in progress,
             localUninhInProg(25),	Local un-inhibit in progress,
             localInhInProg(26),		Local inhibit in progress,
             linh(27),				Local inhibited,
             rinh(28),				Remote inhibited,
             lblk(29),				Local blocked,
             rblk(30),				Remote blocked,
             l2oos(31),				Layer 2 out of service"
    ::= { sonusSs7LinkStatusEntry 2 }

