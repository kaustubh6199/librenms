    DEFVAL          { 0 }
    ::= { sonusSs7RouteStatusEntry 3 }

sonusSs7Mtp3Mgmt OBJECT IDENTIFIER
    ::= { sonusSs7Mtp3MIBObjects 7 }

sonusSs7Mtp3MgmtNextIndex OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The next valid index to use when creating an entry in
             the sonusMtp3MgmtTable."
    ::= { sonusSs7Mtp3Mgmt 1 }

sonusSs7Mtp3MgmtTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusSs7Mtp3MgmtEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "This table specifies an MTP3 management."
    ::= { sonusSs7Mtp3Mgmt 2 }

sonusSs7Mtp3MgmtEntry OBJECT-TYPE
    SYNTAX          SonusSs7Mtp3MgmtEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            ""
    INDEX           { sonusSs7Mtp3MgmtIndex }
    ::= { sonusSs7Mtp3MgmtTable 1 }

SonusSs7Mtp3MgmtEntry ::=
    SEQUENCE {
        sonusSs7Mtp3MgmtIndex           Integer32,
        sonusSs7Mtp3MgmtName            SonusName,
        sonusSs7Mtp3MgmtRowStatus       RowStatus,
        sonusSs7Mtp3MgmtStackType       INTEGER,
        sonusSs7Mtp3MgmtNetworkIndicator INTEGER,
        sonusSs7Mtp3MgmtMaxLocalPcs     Integer32,
        sonusSs7Mtp3MgmtMaxDestinations Integer32,
        sonusSs7Mtp3MgmtMaxLinks        Integer32,
        sonusSs7Mtp3MgmtMaxLinksets     Integer32,
        sonusSs7Mtp3MgmtMaxRoutes       Integer32,
        sonusSs7Mtp3MgmtProfileName     DisplayString,
        sonusSs7Mtp3MgmtStatisticsMode  INTEGER,
        sonusSs7Mtp3MgmtStatisticsInterval  Integer32,
        sonusSs7Mtp3MgmtTraceLevel      INTEGER,
        sonusSs7Mtp3MgmtActivationMode  INTEGER,
        sonusSs7Mtp3MgmtRestartMode     INTEGER,
        sonusSs7Mtp3MgmtCongestionPriority  INTEGER
    }

sonusSs7Mtp3MgmtIndex OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The index of the MTP3 management entry."
    ::= { sonusSs7Mtp3MgmtEntry 1 }

sonusSs7Mtp3MgmtName OBJECT-TYPE
    SYNTAX          SonusName
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The name of the MTP3 management."
    ::= { sonusSs7Mtp3MgmtEntry 2 }

sonusSs7Mtp3MgmtRowStatus OBJECT-TYPE
    SYNTAX          RowStatus
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            ""
    ::= { sonusSs7Mtp3MgmtEntry 3 }

sonusSs7Mtp3MgmtStackType OBJECT-TYPE
    SYNTAX          INTEGER { itu(0), ansi(1) }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The stack type of the MTP3 stack."
    DEFVAL          { itu }
    ::= { sonusSs7Mtp3MgmtEntry 4 }

sonusSs7Mtp3MgmtNetworkIndicator OBJECT-TYPE
    SYNTAX          INTEGER { intl0(0), intl1(1), natl0(2), natl1(3) }
    MAX-ACCESS      read-write
    STATUS          obsolete
    DESCRIPTION     
            "The network indicator of the MTP3 stack. This object is no
             longer needed, as the Hughes stack uses the corresponding
             object associated with each node to overwrite it."
    DEFVAL          { natl0 }
    ::= { sonusSs7Mtp3MgmtEntry 5 }

sonusSs7Mtp3MgmtMaxLocalPcs OBJECT-TYPE
    SYNTAX          Integer32 (1..8)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The max. number of local point codes of the MTP3 stack."
    ::= { sonusSs7Mtp3MgmtEntry 6 }

sonusSs7Mtp3MgmtMaxDestinations OBJECT-TYPE
    SYNTAX          Integer32 (1..256)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The max. number of remote point codes of the MTP3 stack."
    ::= { sonusSs7Mtp3MgmtEntry 7 }

sonusSs7Mtp3MgmtMaxLinks OBJECT-TYPE
    SYNTAX          Integer32 (1..256)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The max. number of SS7 links of the MTP3 stack."
    ::= { sonusSs7Mtp3MgmtEntry 8 }

sonusSs7Mtp3MgmtMaxLinksets OBJECT-TYPE
    SYNTAX          Integer32 (1..256)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The max. number of SS7 linksets of the MTP3 stack."
    ::= { sonusSs7Mtp3MgmtEntry 9 }

sonusSs7Mtp3MgmtMaxRoutes OBJECT-TYPE
    SYNTAX          Integer32 (1..512)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The max. number of SS7 routes of the MTP3 stack."
    ::= { sonusSs7Mtp3MgmtEntry 10 }

sonusSs7Mtp3MgmtProfileName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(1..23))
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The name of the MTP3 profile this MGMT uses."
    ::= { sonusSs7Mtp3MgmtEntry 11 }

sonusSs7Mtp3MgmtStatisticsMode OBJECT-TYPE
    SYNTAX          INTEGER { enabled(0), reset(1), disabled(2) }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The statistics collection mode of the MTP3 stack.
             The enabled mode enables the MTP3 stack statistics collection
             at the rate that is specified by 
             sonusSs7Mtp3MgmtStatisticsInterval.  The disabled mode 
             disables the collection, and the reset mode zeros all the 
             statistics counters when the collection is enabled."
    DEFVAL          { enabled }
    ::= { sonusSs7Mtp3MgmtEntry 12 }

sonusSs7Mtp3MgmtStatisticsInterval OBJECT-TYPE
    SYNTAX          Integer32 (3..30)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The interval in minutes at which the MTP3 statistics 
             are collected."
    DEFVAL          { 5 }
    ::= { sonusSs7Mtp3MgmtEntry 13 }

sonusSs7Mtp3MgmtTraceLevel OBJECT-TYPE
    SYNTAX          INTEGER { off(0), brief(1), detailed(2), override(3) }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The trace level of the MTP3 stack.
             off -- to turn the trace off.
             brief -- to log the major events,
             detailed -- to log the maximum information at numerous points
             in the flow of execution.
             override -- logs at maximum trace level, bypassing the sonus 
             event level logging"
    DEFVAL          { off }
    ::= { sonusSs7Mtp3MgmtEntry 14 }

sonusSs7Mtp3MgmtActivationMode OBJECT-TYPE
    SYNTAX          INTEGER { default(0), allEmergency(1), 
                        firstLinkEmergency(2) }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The activation mode of the MTP3 stack.
             
             ITU, ETSI:
             default -- all active links in each link set are activated
             	using the normal alignment.
             allEmergency -- all active links in each link set are
             	activated using the emergency alignment,
             firstLinkEmergency -- the first active link that was 
             	configured in each linkset is activated using the
             	emergency alignment.  The remaining are activated using
             	the normal alignment.
             
             ANSI:
             default -- same as firstLinkEmergency
             allEmergency -- all active links in each link set are
             	activated using the emergency alignment,
             firstLinkEmergency -- the first active link that was 
             	configured in each linkset is activated using the
             	emergency alignment.  The remaining are activated using
             	the normal alignment."
    DEFVAL          { default }
    ::= { sonusSs7Mtp3MgmtEntry 15 }

sonusSs7Mtp3MgmtRestartMode OBJECT-TYPE
    SYNTAX          INTEGER { full(0), partial(1) }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The MTP restart mode.  Note that this applies to ANSI only."
    DEFVAL          { partial }
    ::= { sonusSs7Mtp3MgmtEntry 16 }

sonusSs7Mtp3MgmtCongestionPriority OBJECT-TYPE
    SYNTAX          INTEGER { on(0), off(1) }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "This applies to the national network, indicating whether
             the congestion priority should be on or off."
    DEFVAL          { on }
    ::= { sonusSs7Mtp3MgmtEntry 17 }

sonusSs7Mtp3MgmtStatusTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusSs7Mtp3MgmtStatusEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "Status Table of the SS7 MTP3 MGMT for the GSX9000 node."
    ::= { sonusSs7Mtp3Mgmt 3 }

sonusSs7Mtp3MgmtStatusEntry OBJECT-TYPE
    SYNTAX          SonusSs7Mtp3MgmtStatusEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "SS7 link entry."
    INDEX           { sonusSs7Mtp3MgmtStatusIndex }
    ::= { sonusSs7Mtp3MgmtStatusTable 1 }

SonusSs7Mtp3MgmtStatusEntry ::=
    SEQUENCE {
        sonusSs7Mtp3MgmtStatusIndex     Integer32,
        sonusSs7Mtp3MgmtState           BITS
    }

sonusSs7Mtp3MgmtStatusIndex OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The index of the SS7 Mtp3 Mgmt."
    ::= { sonusSs7Mtp3MgmtStatusEntry 1 }

sonusSs7Mtp3MgmtState OBJECT-TYPE
    SYNTAX          BITS { deactivated(0), reserved1(1), reserved2(2), 
                        reserved3(3), reserved4(4), reserved5(5), 
                        reserved6(6), reserved7(7), reserved8(8), 
                        reserved9(9), reserved10(10), reserved11(11), 
                        reserved12(12), reserved13(13), reserved14(14), 
                        reserved15(15), reserved16(16), reserved17(17), 
                        reserved18(18), reserved19(19), reserved20(20), 
                        reserved21(21), reserved22(22), reserved23(23), 
                        reserved24(24), reserved25(25), reserved26(26), 
                        reserved27(27), reserved28(28), 
                        serverInactive(29), noRoute(30), noIsup(31) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The current state of the SS7 link:
             deactivated(0),			Mtp3 Mgmt deactivated
             serverInactive(29),		Server (MNS, or GNS) inactive,
             noRoute(30),			No routes configured,
             noIsup(31),				No ISUP CIC configured"
    ::= { sonusSs7Mtp3MgmtStatusEntry 2 }

sonusSs7Mtp3MIBNotifications OBJECT IDENTIFIER
    ::= { sonusSs7Mtp3MIB 2 }

sonusSs7Mtp3MIBNotificationsPrefix OBJECT IDENTIFIER
    ::= { sonusSs7Mtp3MIBNotifications 0 }

sonusSs7Mtp3MIBNotificationsObjects OBJECT IDENTIFIER
    ::= { sonusSs7Mtp3MIBNotifications 1 }

sonusSs7DestinationStateChangeNotification NOTIFICATION-TYPE
    OBJECTS         { sonusSs7NodeName, sonusSs7DestinationName, 
                      sonusSs7DestinationCongLevel, 
                      sonusSs7DestinationDiscardLevel, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
    STATUS          current
    DESCRIPTION     
            "This trap indicates that the state of the speicified SS7 
             destionation has chagned as described in the event description."
    ::= { sonusSs7Mtp3MIBNotificationsPrefix 1 }

sonusSs7LinkStateChangeNotification NOTIFICATION-TYPE
    OBJECTS         { sonusSs7LinkName, sonusSs7NodeName, 
                      sonusSs7DestinationName, sonusSs7LinkSlc, 
                      sonusSs7LinkState, sonusEventDescription, 
                      sonusEventClass, sonusEventLevel, sonusSequenceId, 
                      sonusEventTime }
    STATUS          current
    DESCRIPTION     
            "This trap indicates that the state of the speicified SS7 
             link has chagned as described in the event description."
    ::= { sonusSs7Mtp3MIBNotificationsPrefix 2 }

sonusSs7RouteStateChangeNotification NOTIFICATION-TYPE
    OBJECTS         { sonusSs7RouteName, sonusSs7NodeName, 
                      sonusSs7DestinationName, sonusSs7LinksetName, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
    STATUS          current
    DESCRIPTION     
            "This trap indicates that the state of the speicified SS7 
             route has chagned as described in the event description."
    ::= { sonusSs7Mtp3MIBNotificationsPrefix 3 }

END -- end of module SONUS-SS7-MTP3-MIB.
