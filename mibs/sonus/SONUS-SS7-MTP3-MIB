        sonusSs7LinkUnavlStopTime       Unsigned32,
        sonusSs7LinkUnavlDuration       Unsigned32,
        sonusSs7LinkActivationStatus    INTEGER,
        sonusSs7LinkPeriodicSltStatus   INTEGER,
        sonusSs7LinkCongestionLevel     Integer32,
        sonusSs7LinkDiscardLevel        Integer32,
