    SYNTAX          INTEGER { afInet(2), afInet6(24) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "IP Address Type"
    ::= { sonusDBLEndpointStatEntry 13 }

sonusDBLApplicationRuleStatTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusDBLApplicationRuleStatEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "The collection of DBL list policer status."
    ::= { sonusTransportResourcesMIBObjects 11 }

sonusDBLApplicationRuleStatEntry OBJECT-TYPE
    SYNTAX          SonusDBLApplicationRuleStatEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "The collection of DBL list policer status."
    INDEX           { sonusDBLAppRuleStatAppType, 
                      sonusDBLAppRuleStatAppIndex, 
                      sonusDBLAppRuleStatRuleIndex }
    ::= { sonusDBLApplicationRuleStatTable 1 }

