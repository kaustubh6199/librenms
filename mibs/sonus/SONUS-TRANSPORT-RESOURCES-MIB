        sonusDBLRuleActionEffectivePeriod Unsigned32,
        sonusDBLRuleRowStatus           RowStatus
    }

sonusDBLRuleProfileIndex OBJECT-TYPE
    SYNTAX          Unsigned32 (1..10)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The index of the parent profile for this rule."
    ::= { sonusDBLRuleEntry 1 }

sonusDBLRuleIndex OBJECT-TYPE
    SYNTAX          Unsigned32 (1..2)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Identifies the DBL rule within a DBL profile."
    ::= { sonusDBLRuleEntry 2 }

sonusDBLRuleEventType OBJECT-TYPE
    SYNTAX          INTEGER { sipRegistFailure(1), badSipMessage(2), 
                        sipEpCacAggrRej(3) }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The DBL rule event type."
    ::= { sonusDBLRuleEntry 3 }

sonusDBLRuleActionType OBJECT-TYPE
    SYNTAX          INTEGER { watch(1), blacklist(2) }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The action to take when a specified event meets the criteria for this rule."
    ::= { sonusDBLRuleEntry 4 }

sonusDBLRuleActionDBLExcludeWhiteState OBJECT-TYPE
    SYNTAX          INTEGER { disabled(1), enabled(2) }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The behavior whether to DBL an entry when the entry is in static white list.
             When enabled, entry will not be in the DBL list if it is already in white list.
             When disabled, entry will be in the DBL list regardless whether it's in whitelist or not."
    DEFVAL          { disabled }
    ::= { sonusDBLRuleEntry 5 }

sonusDBLRuleEventPerDayThreshold OBJECT-TYPE
    SYNTAX          Unsigned32 (0..86400)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The number of events per day which is used as fillrate of token bucket policer. Value 0 means zero tolerance, the DBL agent takes action immediately if gets an event."
    DEFVAL          { 0 }
    ::= { sonusDBLRuleEntry 6 }

sonusDBLRuleActionEffectivePeriod OBJECT-TYPE
    SYNTAX          Unsigned32 (60..86400)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The DBL action enforcement period (in seconds) the DBL policer entry remains effective."
    DEFVAL          { 60 }
    ::= { sonusDBLRuleEntry 7 }

sonusDBLRuleRowStatus OBJECT-TYPE
    SYNTAX          RowStatus
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The Row Status of this entry used by SNMP only."
    ::= { sonusDBLRuleEntry 8 }

sonusDBLEndpointStatTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusDBLEndpointStatEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "The collection of DBL list endpoint policer status."
    ::= { sonusTransportResourcesMIBObjects 10 }

sonusDBLEndpointStatEntry OBJECT-TYPE
    SYNTAX          SonusDBLEndpointStatEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "The collection of DBL list policer status."
    INDEX           { sonusDBLEndpointStatIfIndex, 
                      sonusDBLEndpointStatSrcIpAddress, 
                      sonusDBLEndpointStatSrcIp6Address, 
                      sonusDBLEndpointStatSrcIpVer }
    ::= { sonusDBLEndpointStatTable 1 }

