    SEQUENCE {
        sonusDBLAppRuleStatAppType      Unsigned32,
        sonusDBLAppRuleStatAppIndex     Unsigned32,
        sonusDBLAppRuleStatRuleIndex    Unsigned32,
        sonusDBLAppRuleStatCumulativeEnforcements         Counter64,
        sonusDBLAppRuleStatCurrentPolicerEnforcements             Counter32,
        sonusDBLAppRuleStatPreemptedPolicerEnforcements               Counter32
    }

sonusDBLAppRuleStatAppType OBJECT-TYPE
    SYNTAX          Unsigned32 (1)
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "Application type. 
             -- sip (1) "
    ::= { sonusDBLApplicationRuleStatEntry 1 }

sonusDBLAppRuleStatAppIndex OBJECT-TYPE
    SYNTAX          Unsigned32 (1..2048)
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "The unique per server assigned application index when it was created.
             For SIP application type, this index is the same as SIP service group index."
    ::= { sonusDBLApplicationRuleStatEntry 2 }

sonusDBLAppRuleStatRuleIndex OBJECT-TYPE
    SYNTAX          Unsigned32 (1..2)
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "The Index of the DBL profile rule of this entry."
    ::= { sonusDBLApplicationRuleStatEntry 3 }

sonusDBLAppRuleStatCumulativeEnforcements OBJECT-TYPE
    SYNTAX          Counter64
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The cumulative DBL policer enforcements during the lifetime of the effective profile/rule."
    ::= { sonusDBLApplicationRuleStatEntry 4 }

sonusDBLAppRuleStatCurrentPolicerEnforcements OBJECT-TYPE
    SYNTAX          Counter32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            " The current DBL policer enforcements (black list on NP) ."
    ::= { sonusDBLApplicationRuleStatEntry 5 }

sonusDBLAppRuleStatPreemptedPolicerEnforcements OBJECT-TYPE
    SYNTAX          Counter32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The preempted/overridden DBL policer enforcements due to 
             the (endpoint, AC) is already on the white list."
    ::= { sonusDBLApplicationRuleStatEntry 6 }

sonusDBLApplicationPolicerStatTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusDBLApplicationPolicerStatEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "The collection of DBL list policer status."
    ::= { sonusTransportResourcesMIBObjects 12 }

