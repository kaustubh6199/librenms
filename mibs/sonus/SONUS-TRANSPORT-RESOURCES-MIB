    SEQUENCE {
        sonusDBLAppPolicerStatAppType   Unsigned32,
        sonusDBLAppPolicerStatAppIndex  Unsigned32,
        sonusDBLAppPolicerStatSlotIndex SonusSlotIndex,
        sonusDBLAppPolicerStatTableIndex Unsigned32,
        sonusDBLAppPolicerStatPolicerHashIndex      Unsigned32,
        sonusDBLAppPolicerStatPolicerSubIndex     Unsigned32,
        sonusDBLAppPolicerStatTriggerEvent  INTEGER,
        sonusDBLAppPolicerStatSrcIpAddress  IpAddress,
        sonusDBLAppPolicerStatIfIndex   Unsigned32,
        sonusDBLAppPolicerStatRemainEffectivePeriod           Unsigned32,
        sonusDBLAppPolicerStatNPDiscardCnt  Counter32,
        sonusDBLAppPolicerStatSrcIp6Address   Ipv6Address,
        sonusDBLAppPolicerStatSrcIpVer  INTEGER
    }

sonusDBLAppPolicerStatAppType OBJECT-TYPE
    SYNTAX          Unsigned32 (1)
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "The application type.
             ApplicationType 1 is for SIP."
    ::= { sonusDBLApplicationPolicerStatEntry 1 }

sonusDBLAppPolicerStatAppIndex OBJECT-TYPE
    SYNTAX          Unsigned32 (1..2048)
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "The unique per server assigned application index when it was created.
             For SIP application type, this index is the same as SIP service group index."
    ::= { sonusDBLApplicationPolicerStatEntry 2 }

sonusDBLAppPolicerStatSlotIndex OBJECT-TYPE
    SYNTAX          SonusSlotIndex
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "The slot of the ingress NIF or SIF where the event is detected, and
             where BL would be installed"
    ::= { sonusDBLApplicationPolicerStatEntry 3 }

sonusDBLAppPolicerStatTableIndex OBJECT-TYPE
    SYNTAX          Unsigned32 (1..2)
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "Application specific table index indicating a DBL manager or agent.
             DBL manager and DBL agent maitains different set of policers, 
             and DBL manager and DBL agent may coexists on same server card.
                  - index 1 refers to DBL manager policer table,
                  - index 2 refers to DBL agent policer table"
    ::= { sonusDBLApplicationPolicerStatEntry 4 }

sonusDBLAppPolicerStatPolicerHashIndex OBJECT-TYPE
    SYNTAX          Unsigned32 (1..1024)
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "Application specific hash index of a pociler hash table."
    ::= { sonusDBLApplicationPolicerStatEntry 5 }

sonusDBLAppPolicerStatPolicerSubIndex OBJECT-TYPE
    SYNTAX          Unsigned32 (1..100)
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "Application specific sub-index of the policers of a policer hash table."
    ::= { sonusDBLApplicationPolicerStatEntry 6 }

sonusDBLAppPolicerStatTriggerEvent OBJECT-TYPE
    SYNTAX          INTEGER { sipRegistFailure(1), badSipMessage(2), 
                        sipEpCacAggrRej(3) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
