    SYNTAX          Unsigned32 (1..65535)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The LIF ifIndex of the ingress NIF or SIF where the DBL is created"
    ::= { sonusDBLApplicationPolicerStatEntry 9 }

sonusDBLAppPolicerStatRemainEffectivePeriod OBJECT-TYPE
    SYNTAX          Unsigned32 (60..86400)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The remaining period of time the DBL policer entry remains effective."
    ::= { sonusDBLApplicationPolicerStatEntry 10 }

