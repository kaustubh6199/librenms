    SYNTAX          IpAddress
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The source IPv4 address being policed."
    ::= { sonusDBLEndpointStatEntry 2 }

sonusDBLEndpointStatTriggerRule OBJECT-TYPE
    SYNTAX          Unsigned32 (1..2)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The DBL profile rule this entry is created against."
    ::= { sonusDBLEndpointStatEntry 3 }

sonusDBLEndpointStatApplicationType OBJECT-TYPE
    SYNTAX          Unsigned32 (1)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The application type.
             sip(1) "
    ::= { sonusDBLEndpointStatEntry 4 }

sonusDBLEndpointStatApplicationIndex OBJECT-TYPE
    SYNTAX          Unsigned32 (1..2048)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The unique per server assigned application index when it was created."
    ::= { sonusDBLEndpointStatEntry 5 }

sonusDBLEndpointStatSlot OBJECT-TYPE
    SYNTAX          SonusSlotIndex
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
