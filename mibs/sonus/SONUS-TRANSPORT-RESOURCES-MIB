    SYNTAX          INTEGER { unblacklist(1) }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The action to unblacklist the DBL endpoint from XNS."
    ::= { sonusDBLEndpointStatEntry 10 }

sonusDBLEndpointStatTriggerEvent OBJECT-TYPE
    SYNTAX          INTEGER { sipRegistFailure(1), badSipMessage(2), 
                        sipEpCacAggrRej(3) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
