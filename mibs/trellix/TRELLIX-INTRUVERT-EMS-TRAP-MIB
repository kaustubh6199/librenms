TRELLIX-INTRUVERT-EMS-TRAP-MIB DEFINITIONS ::= BEGIN

IMPORTS
	OBJECT-TYPE, MODULE-IDENTITY, Integer32, 
	IpAddress, TimeTicks, NOTIFICATION-TYPE,
	Unsigned32
		FROM SNMPv2-SMI
	Ipv6Address
		FROM IPV6-TC
	TimeInterval, TruthValue, RowStatus, 
	MacAddress, DateAndTime, DisplayString,
	TimeStamp
		FROM SNMPv2-TC
	trellix-intruvert
		FROM TRELLIX-INTRUVERT-SMI;

ivEMSTrapMIB MODULE-IDENTITY
	LAST-UPDATED "201201100000Z"
	ORGANIZATION "MUSARUBRA US LLC"
	CONTACT-INFO "Trellix Customer Service Department
 		      Postal: 6220 American Center Drive
                	      San Jose CA 95002-2563
                	      USA
		      Tel: +1 800 338 8754
		      E-mail: support@mcafee.com"
	DESCRIPTION  "The definition of the Trellix-IntruShield Element Management System 
		     Traps, which will be forwarded from the north bound interface 
		     to the 3rd-party Fault management system."
	REVISION     "201201100000Z"
	DESCRIPTION  "Initial version"
	::= { trellix-intruvert 5 }

-- *****************************************************************
-- top level groups in the TRELLIX-INTRUVERT-EMS-TRAP-MIB
-- *****************************************************************

ivEMSNotificationTrap		OBJECT IDENTIFIER ::= { ivEMSTrapMIB 0 }
ivEMSNotificationAlertData	OBJECT IDENTIFIER ::= { ivEMSTrapMIB 2 }
ivEMSNotificationSysEventData	OBJECT IDENTIFIER ::= { ivEMSTrapMIB 3 }
ivEMSGeneratedEventData		OBJECT IDENTIFIER ::= { ivEMSTrapMIB 4 }
ivEnterceptAlertData		OBJECT IDENTIFIER ::= { ivEMSTrapMIB 5 }
ivEMSNotificationAuditData	OBJECT IDENTIFIER ::= { ivEMSTrapMIB 6 }

-- *****************************************************************
-- IntruShield Alert Common Data Fields Group
-- *****************************************************************

ivAlertCommonGrp	OBJECT IDENTIFIER ::= { ivEMSNotificationAlertData 1 }

-- This group contains the data fields that shared by multiple Alerts
-- types, which defines some generic information about the Alert

ivSensorName  OBJECT-TYPE
	SYNTAX 	DisplayString
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The sensor name in EMS to represents the sensor from where the event is 
	generated."
	::= { ivAlertCommonGrp 1 }

ivAdministratorDomainName  OBJECT-TYPE
	SYNTAX 	DisplayString
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The administrator domain name in EMS where the sensor that generates the
	event belongs to."
	::= { ivAlertCommonGrp 2 }

ivAlarmSeverity  OBJECT-TYPE
	SYNTAX 	INTEGER {
		critical  (1),
		error  (2),
		warning  (3),
		info	(4)
	}
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The severity of the fault."
	::= { ivAlertCommonGrp 3 }


ivGlobalAttackName  OBJECT-TYPE
	SYNTAX 	DisplayString
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The global attack name. Each attack has its own unique integer
	identifier, which is defined by the Trellix Intrusion Detection 
	Department and is mapped to the attack name."
	::= { ivAlertCommonGrp 4 }

ivAlertTimeStamp  OBJECT-TYPE
	SYNTAX 	DateAndTime
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The time that the Alert is get generated."
	::= { ivAlertCommonGrp 5 }

ivAlertCount  OBJECT-TYPE
	SYNTAX 	Integer32
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The number of alerts that were raised by the same signature as the 
	current one, but were suppressed by the sensor."
	::= { ivAlertCommonGrp 6 }

ivAlertDuration  OBJECT-TYPE
	SYNTAX 	INTEGER (0..65535)
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The time interval from the first unsent alert, which raised by the same
	signature as current one but suppressed, till the current alert, which 
	sent after one or more suppressed alerts raised by the same signature.
	The time is in seconds."
	::= { ivAlertCommonGrp 7 }

ivGlobalVIDSName  OBJECT-TYPE
	SYNTAX 	DisplayString
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION
	"The VIDS name.  Each VIDS has a global identity number.  VIDS -- 
	Virtual Intrusion Detection System."
	::= { ivAlertCommonGrp 8 }

ivSensorSlotId  OBJECT-TYPE
	SYNTAX 	INTEGER (0..255)
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The slot number on the sensor that detected the attack/abnormal."
	::= { ivAlertCommonGrp 9 }

ivSensorPortId  OBJECT-TYPE
	SYNTAX 	INTEGER (0..255)
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The port number of slot <ivSensorSlotId> on the sensor that detected the
	attack/abnormal."
	::= { ivAlertCommonGrp 10 }

ivGlobalProtocolName  OBJECT-TYPE
	SYNTAX 	DisplayString
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The global protocol Name.  Each protocol has a unique identification number 
	defined by Trellix Intrusion Detection Department."
	::= { ivAlertCommonGrp 11 }

ivGlobalNIName  OBJECT-TYPE
	SYNTAX 	DisplayString
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The NI name. Each NI has an unique ID number in EMS."
	::= { ivAlertCommonGrp 12 }

ivAlertCategory  OBJECT-TYPE
	SYNTAX 	INTEGER {
		exploit  (111),
		doSandDDoS  (112),
		reconnaissance  (113),
		policyViolation	(114)
	}
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The impact category, i.e. attack type. It describes the general types of the 
	 impacts that can be performed to a system.
	 Exploit -- This category covers the majority of attacks that actively seek to
	 compromises systems, gain unauthorized access to confidential information or
	 service, or tamper normal system operations by exploiting known or potential
	 system vulnerability.
	 DoSandDDoS -- (Distributed) deny of service attack. Possibly harm the ability
	 of the network or system to respond or continue providing services.
	 Reconnaissance -- This type activities is for the purpose of intelligence 
	 gathering to prepare for future attacks, for example, port scan.
	 PolicyViolation -- An attacker performed an action that goes against the 
	 organizational or system policy, possibly by attempting to gain access 
	 they are not authorized to have."
	::= { ivAlertCommonGrp 13 }

ivAlertSubCategory  OBJECT-TYPE
	SYNTAX 	DisplayString
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The detailed impact category that can be performed to a system."
	::= { ivAlertCommonGrp 14 }
 
ivDetectionMechanism  OBJECT-TYPE
	SYNTAX 	DisplayString
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The method used to detect attack.  Each method relates to a specific 
	 attack category."
	::= { ivAlertCommonGrp 15 }

ivAttackResult  OBJECT-TYPE
	SYNTAX 	INTEGER {
		attack-Successful (100),
		inconclusive  (200),
		attack-Failed  (300),
		not-Applicable  (400),
		doS-Blocking-Activated  (888),
		attack-Blocked  (999),
		blocking-Simulated-Attack-Successful (10100),
		blocking-Simulated-Inconclusive (10200),
		blocking-Simulated-Attack-Failed (10300),
		blocking-Simulated (10400),
		smart-blocked (777)
	}
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The result of detected attacks.
	 Attack Successful -- the attack was either successful or possibly successful.
	 Inconclusive -- the result of the attack is unknown. This is most likely due 
	 to a generic policy, such as the default or all-inclusive policy are not
	 environment specific.
	 Attack Failed -- the attack had no impact.
	 Not Applicable -- the alert was raised for suspicious, but not necessarily 
	 malicious, traffic. This result is common for Reconnaissance attacks due 
	 to the nature of port scanning and host sweeping.
	 DoS Blocking Activated -- Drop action for DOS attacks.
	 Attack Blocked -- attacks blocked by a 'Drop packets' sensor response."
	::= { ivAlertCommonGrp 16 }

ivAlertDirection  OBJECT-TYPE
	SYNTAX 	INTEGER {
		outbound  (0),
		inbound  (1),
		unknown  (2),
		biDirection  (3)
	}
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"Inbound and Outbound refer to the direction that traffic is flowing in
	 regards to the network.  Inbound refers to the traffic destined for the
	 internal network, and Outbound refers to traffic destined for the 
	 external network."
	::= { ivAlertCommonGrp 17 } 

ivAlertSeverity  OBJECT-TYPE
	SYNTAX 	INTEGER {
		high  (1),
		medium  (2),
		low  (3),
		info	(4)
	}
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The severity of the alert.  Distinguish it from ivAlarmSeverity as required."
	::= { ivAlertCommonGrp 18 }

ivAlertRawSeverity  OBJECT-TYPE
	SYNTAX 	INTEGER (0..9)
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The raw severity of the alert. 0 - informational; 1,2,3 - low; 
	4,5,6 - medium; 7,8,9 - high"
	::= { ivAlertCommonGrp 19 }

ivAlertIsHostIsolationEnabled  OBJECT-TYPE
	SYNTAX 	INTEGER {
		notEnabled  (0),
		enabled  (1) }
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"Flag to indicating if Host Isolation is enabled for the alert, 0 - Not Enabled, 1 - Enabled"
	::= { ivAlertCommonGrp 20 }


ivAlertMPENotification  OBJECT-TYPE
	SYNTAX 	INTEGER {
		notEnabled  (0),
		enabled  (1) 
	}
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"McAffee policy Enforcer (MPE) notification status of the alert, 0 - Not Enabled, 1 - Enabled"
	::= { ivAlertCommonGrp 21 }

ivAlertSensorUUID  OBJECT-TYPE
	SYNTAX 	DisplayString
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
		"Unique ID generated for every alert."
	::= { ivAlertCommonGrp 22 }
	
ivAlertDeviceType  OBJECT-TYPE
	SYNTAX 	DisplayString
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
		"Device type from which the alert was raised.IPS Sensor (0), NTBA Device (1), HIPS (2)"
	::= { ivAlertCommonGrp 27 }
	
ivAlertZoneName  OBJECT-TYPE
	SYNTAX 	DisplayString
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION
		"Zone in which the alert was raised. Applicable only to NTBA alerts."
	::= { ivAlertCommonGrp 28 }	

ivAlertSrcPhone  OBJECT-TYPE
	SYNTAX 	DisplayString
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
		"Source phone number."
	::= { ivAlertCommonGrp 29 }	

ivAlertSrcIMSI  OBJECT-TYPE
	SYNTAX 	DisplayString
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
		"Source phone IMSI."
	::= { ivAlertCommonGrp 30 }	
	
ivAlertSrcAPN  OBJECT-TYPE
	SYNTAX 	DisplayString
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
		"Source phone APN."
	::= { ivAlertCommonGrp 31 }	
	
ivAlertDestPhone  OBJECT-TYPE
	SYNTAX 	DisplayString
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
		"Destination phone number."
	::= { ivAlertCommonGrp 32 }	

ivAlertDestIMSI  OBJECT-TYPE
	SYNTAX 	DisplayString
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
		"Destination phone IMSI."
	::= { ivAlertCommonGrp 33 }	
	
ivAlertDestAPN  OBJECT-TYPE
	SYNTAX 	DisplayString
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
		"Destination phone APN."
	::= { ivAlertCommonGrp 34 }	

ivSensorIPAddress  OBJECT-TYPE
	SYNTAX 	IpAddress
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
		"The sensor ipAddress from where the event is generated. This is the ipAddress of the sensor represented by ivSensorName"
	::= { ivAlertCommonGrp 35 }

ivSensorIPv6Address  OBJECT-TYPE
	SYNTAX 	Ipv6Address
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
		"The sensor IP V6 Address from where the event is generated. This is the IPv6Address of the sensor represented by ivSensorName"
	::= { ivAlertCommonGrp 36 }
	
ivExecutableName  OBJECT-TYPE
	SYNTAX 	DisplayString
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
		"Executable Name."
	::= { ivAlertCommonGrp 37 }	
	

ivExecutableHash  OBJECT-TYPE
	SYNTAX 	DisplayString
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
		"Executable Hash"
	::= { ivAlertCommonGrp 38 }	
	
ivExecutableMalwareConfidence  OBJECT-TYPE
	SYNTAX 	DisplayString
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
		"Executable Malware Confidence"
	::= { ivAlertCommonGrp 39 }	
	
	
-- *****************************************************************

-- IntruShield Alert 5 Tuple Fields Group
-- *****************************************************************

ivAlert5TupleGrp	OBJECT IDENTIFIER ::= { ivEMSNotificationAlertData 2 }

-- The attributes to describe a specific network session

ivNetworkProtocolName  OBJECT-TYPE
	SYNTAX 	DisplayString
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The well-known 8 bits protocol number in the IP header.  The protocol 
	value is set to zero in both Port Scan and Host Sweep Alerts.  Here shows
	as string."
	::= { ivAlert5TupleGrp 1 }

ivSourceIPAddress  OBJECT-TYPE
	SYNTAX 	IpAddress
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The source IP address of the attack get detected."
	::= { ivAlert5TupleGrp 2 }

ivDestinationIPAddress  OBJECT-TYPE
	SYNTAX 	IpAddress
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The destination IP address of the attack get detected.  The 
	destination IP address value is set to zero by the sensor in the 
	Host Sweep Alert."
	::= { ivAlert5TupleGrp 3 }

ivSourcePort  OBJECT-TYPE
	SYNTAX 	INTEGER (0..65535)
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The source port of the attack get detected."
	::= { ivAlert5TupleGrp 4 }

ivDestinationPort  OBJECT-TYPE
	SYNTAX 	INTEGER (0..65535)
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The destination port of the attack get detected.  The destination
	port value is set to zero by the sensor in the Port Scan Alert."
	::= { ivAlert5TupleGrp 5 }
	
ivSourceIPv6Address  OBJECT-TYPE
	SYNTAX 	Ipv6Address
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The source IP V6 address of the attack get detected."
	::= { ivAlert5TupleGrp 6 }

ivDestinationIPv6Address  OBJECT-TYPE
	SYNTAX 	Ipv6Address
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The destination IP V6 address of the attack get detected.  The 
	destination IP address value is set to zero by the sensor in the 
	Host Sweep Alert."
	::= { ivAlert5TupleGrp 7 }

-- *****************************************************************
-- IntruShield Signature Alert Specific Data Fields Group
-- *****************************************************************

ivSignatureAlertGrp	OBJECT IDENTIFIER ::= { ivEMSNotificationAlertData 3 }

-- The specific data fields that belongs to the Signature Alert

ivSignatureId  OBJECT-TYPE
	SYNTAX 	DisplayString
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The signature identifier name of a specific attack which is defined by
	<ivGlobalAttackId>."
	::= { ivSignatureAlertGrp 1 }

ivSigAlertQualifier1  OBJECT-TYPE
	SYNTAX 	Integer32
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"Internal state of protocol parsing machine, TBD."
	::= { ivSignatureAlertGrp 2 }

ivSigAlertQualifier2  OBJECT-TYPE
	SYNTAX 	Integer32
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"Internal state of protocol parsing machine, TBD."
	::= { ivSignatureAlertGrp 3 }

ivSigAlertProtocolParsingStateId  OBJECT-TYPE
	SYNTAX 	INTEGER (0..255)
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"Internal state of protocol parsing machine, TBD."
	::= { ivSignatureAlertGrp 4 }

ivSuppressedSignatureIds  OBJECT-TYPE
	SYNTAX 	DisplayString
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"A group of signature identifier names that are related to the alert 
	 other than the one specified in ivSignatureId field, which means 
	 the attack fits in multiple signatures.  The names are separated by 
	 ';'.  If the alert has only one signatureId, this field would be empty
	 or as text 'No Others' "
	::= { ivSignatureAlertGrp 5 }


-- *****************************************************************
-- IntruShield Statistical Anomaly Alert Specific Data Fields Group
-- *****************************************************************

ivStatAnormalyGrp	OBJECT IDENTIFIER ::= { ivEMSNotificationAlertData 4 }

-- The specific data fields that belongs to the Statistical Anomaly Alert

ivFirstAlarmTime  OBJECT-TYPE
	SYNTAX 	DateAndTime
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The first alarm time in the statistical anomaly."
	::= { ivStatAnormalyGrp 1 }

ivAccumulateTime  OBJECT-TYPE
	SYNTAX 	Integer32
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The alarm accumulate time in the statistical anomaly since the first
	alarm get raised. The time is in seconds."
	::= { ivStatAnormalyGrp 2 }

ivStatMeasureCount  OBJECT-TYPE
	SYNTAX 	INTEGER (1..16)
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The number of measures existed in the statistical Anomaly Alert. Each 
	measure pair contains a short term measure and a long term measure."
	::= { ivStatAnormalyGrp 3 }

ivStatMeasureTable  OBJECT-TYPE
	SYNTAX 	SEQUENCE OF IvStatMeasureEntry
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The table that contains all the statistical measures in the Anomaly
	alert."
	::= { ivStatAnormalyGrp 4 }

ivStatMeasureEntry  OBJECT-TYPE
	SYNTAX 	IvStatMeasureEntry
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The entry contains a pair of Short Term Measure and a Long Term Measure."
	INDEX  { ivStatMeasureId }
	::= { ivStatMeasureTable 1 }

IvStatMeasureEntry  ::= SEQUENCE  {
	ivStatMeasureId  INTEGER,
	ivMeasureBinCount  INTEGER
}

ivStatMeasureId  OBJECT-TYPE
	SYNTAX 	INTEGER (1..16)
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The Measure Id of the specific measure shown in the Anomaly Alert."
	::= { ivStatMeasureEntry 1 }

ivMeasureBinCount  OBJECT-TYPE
	SYNTAX 	INTEGER (1..32)
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The number of the Distributions(in short Term)/Probabilities(in long 
	Term) in the Measure, Which should be the number of entries in the 
	ivMeaDistributionTable."
	::= { ivStatMeasureEntry 2 }

ivMeaDistributionTable  OBJECT-TYPE
	SYNTAX 	SEQUENCE OF IvMeaDistributionEntry
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The table that contains all the Short Term distributions and Long
	Term Probabilities for the specific measure in the Anomaly alert."
	::= { ivStatAnormalyGrp 5 }

ivMeaDistributionEntry  OBJECT-TYPE
	SYNTAX 	IvMeaDistributionEntry
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The entry that contains all the Short Term distributions and Long
	Term Probabilities."
	INDEX  { ivStatMeasureId, ivDistOrProbIndex }
	::= { ivMeaDistributionTable 1 }

IvMeaDistributionEntry  ::= SEQUENCE  {
	ivDistOrProbIndex  INTEGER,
	ivShortTermDistribution  DisplayString,
	ivLongTermProbability  DisplayString
}

ivDistOrProbIndex  OBJECT-TYPE
	SYNTAX 	INTEGER (1..32)
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The index of the short term Distribution or Long term Probability
	value in the values list."
	::= { ivMeaDistributionEntry 1 }

ivShortTermDistribution  OBJECT-TYPE
	SYNTAX 	DisplayString
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The short term Distribution value.  In fact this value is a float type,
	but the value may not be parsed by the 3rdparty(just display), so send the
	value as a display String, like 0.7895."
	::= { ivMeaDistributionEntry 2 }

ivLongTermProbability  OBJECT-TYPE
	SYNTAX 	DisplayString
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The long term Probability value.  In fact this value is a float type,
	but the value may not be parsed by the 3rdparty(just display), so send the
	value as a display String, like 0.7895."
	::= { ivMeaDistributionEntry 3 }

-- **********************************************************************
-- IntruShield Simple Threshold Anomaly Alert Specific Data Fields Group
-- **********************************************************************

ivSimpleThresholdAnormalyGrp  OBJECT IDENTIFIER ::= { ivEMSNotificationAlertData 5 }

-- The specific data fields that belongs to the Simple Threshold Anomaly Alert

ivObservedValue  OBJECT-TYPE
	SYNTAX 	INTEGER
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The actual value get by the sensor that went over the threshold. The
	threshold itself is defined in the Policy(EMS)."
	::= { ivSimpleThresholdAnormalyGrp 1 }

ivThresholdName  OBJECT-TYPE
	SYNTAX 	DisplayString
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The threshold name. Each threshold has an unique identity number."
	::= { ivSimpleThresholdAnormalyGrp 2 }

ivThresholdValue  OBJECT-TYPE
	SYNTAX 	Integer32
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The current value of the Threshold."
	::= { ivSimpleThresholdAnormalyGrp 3 }

ivThresholdDuration  OBJECT-TYPE
	SYNTAX 	Integer32
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The accumulate time to get the current Threshold value."
	::= { ivSimpleThresholdAnormalyGrp 4 }



-- **************************************************************
-- IntruShield Port Scan  Alert Specific Data Fields Group
-- **************************************************************

ivPortScanAlertGrp	OBJECT IDENTIFIER ::= { ivEMSNotificationAlertData 6 }

-- The specific data fields that belongs to the Port Scan Alert

ivPSAlertPortCount  OBJECT-TYPE
	SYNTAX 	INTEGER (1..255)
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The number of ports that get scanned in the Port Scan Alert."
	::= { ivPortScanAlertGrp 1 }

ivPSAlertDestPortTable  OBJECT-TYPE
	SYNTAX 	SEQUENCE OF IvPSAlertDestPortEntry
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The ports list that get scanned in the Port Scan Alert."
	::= { ivPortScanAlertGrp 2 }

ivPSAlertDestPortEntry  OBJECT-TYPE
	SYNTAX 	IvPSAlertDestPortEntry
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"One of the ports that get scanned in the Port Scan Alert."
	INDEX { ivPSAlertDestPortIndex }
	::= { ivPSAlertDestPortTable 1 }

IvPSAlertDestPortEntry  ::= SEQUENCE  {
	ivPSAlertDestPortIndex  INTEGER,
	ivPSAlertDestinationPort  INTEGER
}

ivPSAlertDestPortIndex  OBJECT-TYPE
	SYNTAX 	INTEGER (0..255)
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The index of a destination port get scanned in the Port Scan Alert."
	::= { ivPSAlertDestPortEntry 1 }

ivPSAlertDestinationPort  OBJECT-TYPE
	SYNTAX 	INTEGER (0..65535)
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"A destination port number get scanned in the Port Scan Alert."
	::= { ivPSAlertDestPortEntry 2 }

-- **************************************************************
-- IntruShield Host Sweep Alert Specific Data Fields Group
-- **************************************************************

ivHostSweepAlertGrp	OBJECT IDENTIFIER ::= { ivEMSNotificationAlertData 7 }

-- The specific data fields that belongs to the Host Sweep Alert

ivHSAlertIPAddrCount  OBJECT-TYPE
	SYNTAX 	INTEGER (1..255)
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The number of IP Addresses that get scanned in the Host Sweep 
	Alert."
	::= { ivHostSweepAlertGrp 1 }

ivHSAlertIPAddrTable  OBJECT-TYPE
	SYNTAX 	SEQUENCE OF IvHSAlertIPAddrEntry
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The IP Addresses list that get scanned in the Host Sweep Alert."
	::= { ivHostSweepAlertGrp 2 }

ivHSAlertIPAddrEntry  OBJECT-TYPE
	SYNTAX 	IvHSAlertIPAddrEntry
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"One of the IP Addresses that get scanned in the Host Sweep Alert."
	INDEX { ivHSAlertIPAddrIndex }
	::= { ivHSAlertIPAddrTable 1 }

IvHSAlertIPAddrEntry  ::= SEQUENCE  {
	ivHSAlertIPAddrIndex  INTEGER,
	ivHSAlertDestinationIPAddress  IpAddress,
	ivHSAlertDestinationIPv6Address  Ipv6Address
}

ivHSAlertIPAddrIndex  OBJECT-TYPE
	SYNTAX 	INTEGER (0..255)
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The index of a destination port get scanned in the Host Sweep Alert."
	::= { ivHSAlertIPAddrEntry 1 }

ivHSAlertDestinationIPAddress  OBJECT-TYPE
	SYNTAX 	IpAddress
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"A destination IP Address get scanned in the Host Sweep Alert."
	::= { ivHSAlertIPAddrEntry 2 }

ivHSAlertDestinationIPv6Address  OBJECT-TYPE
	SYNTAX 	Ipv6Address
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"A destination IP v6 Address get scanned in the Host Sweep Alert."
	::= { ivHSAlertIPAddrEntry 3 }

-- *****************************************************************
-- IntruShield System Event Data Fields Group
-- *****************************************************************

ivEventTimeStamp OBJECT-TYPE
	SYNTAX 	DateAndTime
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
	"The time stamp of the system event generated in the IntruShield box."
	::= { ivEMSNotificationSysEventData 1 }

ivEventTemperatureStatus  OBJECT-TYPE
	SYNTAX 	INTEGER {
		normal (1),
		abnormal (2),
		critical (3)
	}
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION 
